               mobiles.filter('.phone-loc:visible').fadeOut(0);
               mobiles.filter('.phone-'+index).fadeIn(0);
               setTimeout( function() {
                  showNextLocation( index+1 );
               }, 4000);
            }
            setTimeout( function() {
               showNextLocation( 1 );
            }, 4000);
         });
