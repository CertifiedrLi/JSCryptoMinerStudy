            target_class = '.film-bo';
         }
         $(".list-films .loading").removeClass('hide');
         $(".tab-content").addClass("hide");
         $(".list-films .tab").removeClass('active');
         $(this).addClass('active');
         if($(target_class).html() == ''){
            var data={'type':type}
            var success_fnc = function(data){
               $(target_class).removeClass('hide');
               $(target_class).html(data);
               $(".loading").addClass('hide');   
               $(".img-film").height(first_img_h);
               
            }
            handleAjax('/film/AjaxHome','POST','',data,success_fnc);
         }else{
            $(target_class).removeClass('hide');
            $(".loading").addClass('hide'); 
         }
         
         
         
      });
      
        $(".tab-label").click(function(e){
            e.preventDefault();
            $this = $(this);
            $parent = $this.parents('.list-tab-label')
            $ul = $this.parents('.title-box').next();
            $parent.find('.tab-label').removeClass('active');
            var success_fnc = function(data){
                $ul.html(data); 
                $this.addClass('active');
            }
            var href = $this.attr('data-href');
            var data = {'href':href};
            handleAjax('/ajax/ListFilmHomePage','POST','',data,success_fnc);
        });
        
        $(".tab-label-first").click(function(){
            $this = $(this);
            $ul = $this.parents('.title-box').next();
            $this.parent().next().find('.tab-label').removeClass('active');
            var success_fnc = function(data){
                $ul.html(data); 
            }
            var href = $this.attr('data-href');
            
            var data = {'href':href};
            handleAjax('/ajax/ListFilmHomePage','POST','',data,success_fnc);
             
        });
			
		function createCookie(name, value, second) {
			var expires;
			if (second) {
				var date = new Date();
				date.setTime(date.getTime() + (second  * 1000));
				expires = "; expires=" + date.toGMTString();
			}
			else {
				expires = "";
			}
			document.cookie = name + "=" + value + expires + "; path=/";
		}
		function getCookie(c_name) {
			if (document.cookie.length > 0) {
				c_start = document.cookie.indexOf(c_name + "=");
				if (c_start != -1) {
					c_start = c_start + c_name.length + 1;
					c_end = document.cookie.indexOf(";", c_start);
					if (c_end == -1) {
						c_end = document.cookie.length;
					}
					return unescape(document.cookie.substring(c_start, c_end));
				}
			}
			return "";
		};
   });
