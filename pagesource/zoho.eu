            $(".signing").html("<a target='_blank' href='https://help.zoho.com/portal/home' >Support</a> <a class='signup' href='https://home.zoho.eu/' > Access Your Apps</a>");
            $(".bottom-signup-band").css('display', 'none');
            changeUrl();
        }
    }

    function resetH() {
        var getDomain = document.domain
        var splitUrl = getDomain.split('.');
        domainOne = splitUrl.pop();
        domain = splitUrl.pop();
    }
