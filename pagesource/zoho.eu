<script>
    /* Remove the sites24*7 for zoho.eu*/
    var zohoDomain = document.domain;
    if (zohoDomain == "www.zoho.eu") {

        /* zoho .eu cookie policy */

        function getCookie(name) {
            var cookiename = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                if (typeof String.prototype.trim !== 'function') {
                    String.prototype.trim = function() {
                        return this.replace(/^\s+|\s+$/g, '');
                    }
                }
                if (c.indexOf(cookiename) == 0) return c.substring(cookiename.length, c.length);
            }
            return c;
        }
        $(document).ready(function(e) {

            var cookieHtml = '<div id="cookie-clear-bar" style="text-align:center;">We use cookies to give you the best online experience. By using our website you agree to our use of cookies in accordance with our cookie policy.<button id="CookieClear" onclick="CookieClearAccept()">OK</button><a href="https://www.zoho.eu/cookiepolicy.html">Learn More.</a></div>'
            $('body').append(cookieHtml);

            jQuery("html").css("margin-top", "0");
            var uidcookieVal = getCookie();
            if (uidcookieVal.indexOf('euCookiepolicy') >= 0) {
                jQuery("#cookie-clear-bar").hide();
                jQuery("html").css("margin-top", "0");
                jQuery('.signing').removeAttr('style');
            } else {
                jQuery("#cookie-clear-bar").show();
                //jQuery("html").css("margin-top", "32px");
                //jQuery('.signing').css('top', '35px');
            }
        });

        function CookieClearAccept() {
            days = 30;
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            var expires = "; expires=" + date.toGMTString();
            document.cookie = "euCookiepolicy=true" + expires + "; path=/";
            jQuery("#cookie-clear-bar").hide();
            jQuery("html").css("margin-top", "0");
            jQuery('.signing').removeAttr('style');
        }
    }




