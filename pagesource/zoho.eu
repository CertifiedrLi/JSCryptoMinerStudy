        function play(video) {
            var getsharUrl = video.getAttribute('data-sbg-url');
            var getsharText = video.getAttribute('data-share-text');
            var getsharSummary = video.getAttribute('data-share-summary');

            $('body').css({
                'overflow': 'hidden'
            }); // get target url & inserted ..//
            active_video = video;
            var getPlayvideo = $(active_video).attr('data-video-url');
            var video_src = getPlayvideo + '?&autoplay=1&rel=0&showinfo=0&autohide=1';
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                window.location.href = video_src;
            } else {
                var $this_oembed = $('<iframe src="' + video_src + '" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>');
                var $this_player = $player;
                $this_player.fadeIn(duration);
                $this_oembed.addClass('playing');
                setTimeout(function() {
                    $this_player.find('.player').append($this_oembed);
                    //   $this_player.find('.player').append($shrBtn);
                    $('.sbg-button-facebook').attr({
                        'data-share-summary': getsharSummary,
                        'data-sbg-url': getsharUrl
                    })
                    $('.sbg-button-twitter').attr({
                        'data-sbg-text': getsharText
                    })

                    setTimeout(function() {
                        $('.share-btn-container').css({
                            'opacity': '1'
                        });
                        setTimeout(function() {
                            $('.share-btn-container').css({
                                'margin-top': 0
                            });
                            sbg();
                        }, 500)
                    }, 1000)
                    //$this_player.find('.player').append($this_oembed);

                    resizePlayer();
                }, duration);
            }
        }

        function resizePlayer() { // resizing the iframe ......//
            var ratio = 16 / 9;
            var $wrap = $player.find('.player');
            var $this_oembed = $player.find('iframe');
            var max_height = $wrap.height();
            var max_width = $wrap.width();
            var wrap_ratio = max_width / max_height;
            if (wrap_ratio > ratio) {
                // limit iframe width
                $this_oembed.css('width', (max_height * ratio) + 1);
                $('.share-btn-container').css('width', (max_height * ratio));

                $this_oembed.css('height', '');
            }
            if (wrap_ratio < ratio) {
                // limit iframe height
                $this_oembed.css('width', '');
                $('.share-btn-container').css('width', '100%');
                $this_oembed.css('height', (max_width / ratio));
            }
        }

        function close() { // close video popup.........//
            if (active_video) {
                video = active_video;
            } else {
                return;
            }
            var this_duration = duration;
            var $this_player = $player;
            $this_oembed = $this_player.find('iframe');
            $this_oembed.attr('src', "about:blank");
            $this_oembed.remove();
            $('.share-btn-container').remove();
            active_video = null;
            setTimeout(function() {
                $this_player.fadeOut(this_duration, function() {
                    active_video = null;
                });
                $('body').css({
                    'overflow': 'auto'
                });
            }, duration - 500);
        }

        $(window).on('resize orientationchange', function(e) { // resizing called ..........//
            resizePlayer();
        });
    });
    /* ZOHO VIDEO POPUP END */



