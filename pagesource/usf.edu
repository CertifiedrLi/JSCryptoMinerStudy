          calURL = decodeURIComponent(calURL);
          //Take link data and build li
          li = usfDateLi(arrEvents[dateItem].summary, calURL, formatGCalTime(arrEvents[dateItem].start.utcdate), formatGCalTime(arrEvents[dateItem].end.utcdate));
          // append the list item onto the unordered list
          ul.appendChild(li);
          //arrEvents[dateItem].summary
        }
        var calHeader = document.createElement('h2');
