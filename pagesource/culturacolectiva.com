        var generateResourceName = "count:" + JSON.stringify(objectShareThis);
        var shareThisSuccess = setInterval(checkShareThis, 1000)
        function checkShareThis () {
            if (window.__sharethis__) {
                var countNumber = window.__sharethis__.load_counts_cache[generateResourceName];
                if (countNumber !== undefined && countNumber.status === 'complete') {
                    clearInterval(shareThisSuccess)
                    document.getElementById('cc-share-id').innerHTML = countNumber.response.total.label + ' <i class="fa fa-share-alt"></i>';
                }
            } else {
