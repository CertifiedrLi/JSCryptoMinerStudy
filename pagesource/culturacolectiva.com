            }
        }
        // End hacking implementation js for share this

        // Getting embed iframes
        var elementsIframes = $('article .ql-iframe-embed iframe')

        // Get class by type iframe
        function getTypeIframe (source) {
            if (source.indexOf('facebook') !== -1) {
                if (source.indexOf('post') !== -1) {
                    return 'cc-iframe-facebook-post'
                } else if (source.indexOf('video') !== -1) {
                    return 'cc-iframe-facebook-video'
                } else {
                    return 'cc-iframe-facebook'
                }
            } else if (source.indexOf('soundcloud') !== -1) {
                return 'cc-iframe-soundcloud'
            } else if (source.indexOf('spotify') !== -1) {
                return 'cc-iframe-spotify'
            } else if (source.indexOf('applemusic') !== -1) {
                return 'cc-apple-music'
            } else if (source.indexOf('google') !== -1) {
                return 'cc-google'
            } else {
                return 'cc-original-iframe'
            }
        }

        // Method for fixing sizes in iframes
        function fixingIframe (element) {
            var widthVideo = $(element).attr('width')
            var heightVideo = $(element).attr('height')
            var decimal = (((heightVideo/widthVideo).toFixed(4)) * 100).toFixed(2)
            $(element).parent().css('padding-bottom', decimal + '%')
        }

        $.each(elementsIframes, function (index, element) {
            var iframeSource = $(element).attr('src')
            var typeIframe = getTypeIframe(iframeSource)

            $(element).parent().addClass(getTypeIframe(iframeSource))
            console.log(getTypeIframe(iframeSource), iframeSource, $(element).parent())

            if (typeIframe == 'cc-iframe-facebook-video') {
                fixingIframe(element)
            }
        })
