    <div class="baoliao-wrap" style="display:none;">
        <div class="baoliao-cont">
            <div class="baoliao-close">X</div>
            <input id="contact" type="text" name="name" placeholder="输入邮箱或手机号码" value="" />
            <div class="contact-err-msg">请正确填写手机号码或邮箱！</div>
            <div class="editor">
                <textarea id="editor_id" name="txtContentBl" style="width:700px;height:300px;">
                    &lt;strong&gt;HTML内容&lt;/strong&gt;
                </textarea>
            </div>
            <div class="content-err-msg">请输入内容</div>
            <div id="baoliao-send" class="baoliao-send">发送</div>
        </div>
    </div>
    <div class="ie-mask" style="display:none;"></div>
    <!--爆料end-->
    <script type="text/javascript" src="//static.cifnews.com/yuguo/js/editor/kindeditor-min.js?v=20180314002"></script>
    <script>
    (function () {
        var editor = KindEditor.create('textarea[name="txtContentBl"]', {
            id : 'editor_id',
            resizeType: 1,
            pasteType: 1,
            width:"100%",
            height:"350px",
            allowFileManager: false,
            items: [
                        'fontname', 'fontsize', '|', 'forecolor', 'hilitecolor', 'bold', 'italic', 'underline',
                        'removeformat', '|', 'justifyleft', 'justifycenter', 'justifyright', 'insertorderedlist',
                        'insertunorderedlist', '|', 'emoticons', 'image', 'link']
        });
        editor.html('这里输入文字...');

        function addEventHandler(target,type,func){ 
            if(target.addEventListener){ 
                //监听IE9，谷歌和火狐 
                target.addEventListener(type, func, false); 
            }else if(target.attachEvent){ 
                target.attachEvent("on" + type, func); 
            }else{ 
                target["on" + type] = func; 
            } 
        } 

        addEventHandler(document.getElementById('baoliao-send'),'click', function () {
            var regPhone = /^(((13[0-9]{1})|(15[0-9]{1})|(18[0-9]{1}))+\d{8})$/;
            var regEmail = /[a-zA-Z0-9]{1,10}@[a-zA-Z0-9]{1,5}\.[a-zA-Z0-9]{1,5}/;
            var contactMsg=$.trim($('#contact').val());
            var content=editor.html();
            //判断联系方式
            if (!(regPhone.test(contactMsg) || regEmail.test(contactMsg))) {
                $('.contact-err-msg').show();
                return false;
            }else{
                $('.contact-err-msg').hide();
            }
            //判断内容
            if (content=="这里输入文字..." || content.length==0) {
                $('.content-err-msg').show();
                return false;
            }else{
                $('.content-err-msg').hide();
            }

            // 读取 html
            var postData = {
                'content': content,
                'contact': contactMsg
            };
            $.ajax({
                url: '/tools/BreakNewsPost',
                type:'POST',
                data: postData,
                success: function (result) {
                    var res=$.parseJSON(result);
                    alert(res['msgbox']);
                    if(res.msg==1){
                        $('.baoliao-wrap,.ie-mask').hide();
                    }
                },
                error: function () {
                    console.log("fail");
                }
            });

        })
        //关闭按钮
        $('.baoliao-close').click(function(){
            $('.baoliao-wrap,.ie-mask').hide();
        });
        $('#js-bliao').click(function(){
            $('.baoliao-wrap,.ie-mask').show();
        });
    })();
    </script>

    

    

