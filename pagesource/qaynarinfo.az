<script type="text/javascript">
(function(){
	function searchIn() {
    	var th = $(".search input");
    	if (!th.hasClass("active")) {
    		th.addClass("active");
    		$(".closeSearch").removeClass("hide");
    		$(".searchIcon").addClass("hide");
    	}
    }

    function searchOut() {
    	var th = $(".search input");
    	if (th.hasClass("active")) {
    		th.removeClass("active");
    		$(".closeSearch").addClass("hide");
    		$(".searchIcon").removeClass("hide");
            $(".searchResultPage").stop().slideUp();
    		th.val("");
    		syncVal = "";
    	}
    }


    $(".search input").on("focus",function(){
    	searchIn();
    	$(".searchInput").css("width","95%");
    });

    $(".search-icon").on("click",function(){
    	searchIn();
    	$(".searchInput").css("width","95%");
    });

    $(".search input").on("blur",function(){
    	if ($(this).val() == "") {
    		$(".searchInput").css("width","0px");
    		searchOut();
    	}
    });

    $(".closeSearch").on("click",function(){
    	$(".searchInput").css("width","0px");
    	searchOut();
    });

    $(".search input").on("keyup",function(){
    	syncVal = $(this).val();
    })

	$(".language-box__current").click(function(){
		$(".language-box__list").toggleClass("language-box__list_active")
	});



    })();
</script>






