    <script type="text/javascript">
        var bannerSwiper = new Swiper('.swiper-container', {
            autoplay: 4000,
            loop: true,
            onlyExternal: true,
            autoplayDisableOnInteraction: false,
            onSlideChangeStart: function(swiper) {
                $('.swiperThumbnail ul li').eq(bannerSwiper.activeLoopIndex).addClass("on").siblings().removeClass("on");
                //$('.swiper-wrapper li').eq(0).siblings().show();
            },
        });
        
        $('.swiperThumbnail ul li').click(function() {
            var _index = $(this).index();
            bannerSwiper.swipeTo(_index, 500, true);
            $(this).addClass("on").siblings().removeClass("on");
        });

        var webWidth = 736;

        function changeSwiper1(obj) {
            var _width = jQuery(window).width();
            var _is = _width <= webWidth;
            var _this = jQuery(obj);
            var arr = new Array();
            var ahtml = '';
            _this.find('li').each(function(e) {
                arr[e] = jQuery(this).html();
            });

            var arrlen = arr.length;
            var len = 0;
            if (_is) {
                if (arrlen % 2 > 0) {
                    len = parseInt(arrlen / 2) + 1;
                } else {
                    len = parseInt(arrlen / 2);
                }
            } else {
                if (arrlen % 8 > 0) {
                    len = parseInt(arrlen / 8) + 1;
                } else {
                    len = parseInt(arrlen / 8);
                }
            }


            var j = 0;
            var no = 8;
            for (var i = 0; i < len; i++) {
                var ihtml = '';
                if (_is) {
                    no = 2;
                }
                for (var k = 0; k < no; k++, j++) {
                    if (j < arrlen) {
                        ihtml += '<li>' + arr[j] + '</li>';
                    }
                }
                ahtml += '<div class="swiper-slide"><ul class="clear">' + ihtml + '</ul></div>';
                //j++;
            }
            _this.find('.swiper-wrapper').html(ahtml);
        }
        changeSwiper1('.xfswiper0');
        changeSwiper1('.xfswiper1');
        changeSwiper1('.xfswiper2');
        changeSwiper1('.xfswiper3');
        changeSwiper1('.xfswiper4');
        changeSwiper1('.xfswiper5');
        changeSwiper1('.xfswiper6');
        jQuery(window).resize(function() {
            changeSwiper1('.xfswiper0');
            changeSwiper1('.xfswiper1');
            changeSwiper1('.xfswiper2');
            changeSwiper1('.xfswiper3');
            changeSwiper1('.xfswiper4');
            changeSwiper1('.xfswiper5');
            changeSwiper1('.xfswiper6');
        });

        jQuery(function() {
            var t;
            jQuery(".swichmainbox .title .t_h2 .swichtitle a").hover(function() {
                var temp = jQuery(this);
                t = setTimeout(function() {
                    temp.parent().find("a").removeClass("this");
                    temp.addClass("this");
                    temp.parent().parent().parent().parent().find(".one").hide();
                    temp.parent().parent().parent().parent().find(".one:eq(" + temp.index() + ")").show();
                }, 100);
            }, function() {
                clearTimeout(t);
            });

            jQuery(".opennav").toggle(function() {
                jQuery(".opennav").addClass("closenav");
                jQuery(".opennav").attr("title", "关闭");
                jQuery(".mobilenavfc").animate({
                    height: '100%'
                }, 300);
            }, function() {
                jQuery(".opennav").removeClass("closenav");
                jQuery(".opennav").attr("title", "展开");
                jQuery(".mobilenavfc").animate({
                    height: '0'
                }, 300);
            });

            jQuery("#selectbtn").toggle(function() {
                jQuery(".selectfcbox").show();
            }, function() {
                jQuery(".selectfcbox").hide();
            });

            jQuery(".weixin").hover(function() {
                jQuery(".weixinfcbox").show();
            }, function() {
                jQuery(".weixinfcbox").hide();
            });

            jQuery(".app").toggle(function() {
                jQuery(".appdownfcbox").show();
            }, function() {
                jQuery(".appdownfcbox").hide();
            });

            jQuery("#selectlist p").click(function() {
                var txt = $(this).text();
                jQuery("#selectbtn").html(txt);
                var value = $(this).attr("rel");
                jQuery("#selectlist").hide();
            });

        });

        var week = new Date().getDay();
        jQuery(".xf_news .pk_time a").removeClass("current");
        jQuery(".xf_news .time_pic .time_con").hide();
        if (week != 0) {
            jQuery(".xf_news .pk_time a:eq(" + (week - 1) + ")").addClass("current");
            jQuery(".xf_news .time_pic .time_con:eq(" + (week - 1) + ")").show();
            new Swiper(".xfswiper" + (week - 1), {
                nextButton: '.xfswiper-button-next' + (week - 1),
                prevButton: '.xfswiper-button-prev' + (week - 1),
                spaceBetween: 30
            });
        } else {
            jQuery(".xf_news .pk_time a:last").addClass("current");
            jQuery(".xf_news .time_pic .time_con:last").show();
            new Swiper(".xfswiper7", {
                nextButton: '.xfswiper-button-next7',
                prevButton: '.xfswiper-button-prev7',
                spaceBetween: 30
            });
        }

        jQuery(".xf_news .pk_time a").each(function(index) {
            jQuery(this).click(function() {
                jQuery(".xf_news .pk_time a").removeClass("current");
                jQuery(this).addClass("current");
                jQuery(".xf_news .time_pic .time_con").hide();
                jQuery(".xf_news .time_pic .time_con:eq(" + index + ")").show();
                new Swiper(".xfswiper" + index, {
                    nextButton: '.xfswiper-button-next' + index,
                    prevButton: '.xfswiper-button-prev' + index,
                    spaceBetween: 30
                });
            });
        });
