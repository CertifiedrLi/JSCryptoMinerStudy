 function error_ch(id, con) {
 
 $('#' + id).parent().find('i').html(con);
 
 if(id == 'apply_icode' || id == 'apply_pcode')
 {
 $('#' + id).parent().find('i').show();
 $('#' + id).parent().addClass("active");
 }
 else {
 $('#' + id).parent().find('i').show();
 $('#' + id).parent().addClass("active");
 }

 }
 function error_qx(id) {
 $('#' + id).parent().find('i').hide();
 $('#' + id).parent().removeClass("active");
 }


 function settime() {
 if (countdown < 1) {

 $('.dx_yzm').html("获取验证码");
 countdown = 60;
 $('.dx_yzm').attr("href", "javascript:sendcode();");
 return;
 } else {

 $('.dx_yzm').html("重新发送(" + countdown + ")");
 countdown--;
 $('.dx_yzm').attr("href","javascript:;");

 }
 setTimeout(function () { settime() }, 1000);
 }



