function getProducts(industry)
{
 //产品推荐
 var strkeys= decodeURI($.cookie("B2BKeyword_Search"));
 //alert(strkeys); //alert(decodeURI(strkeys));
 var key1="";
 var key2="";
 var key3="";
 if(strkeys!=""&&strkeys!=null)
 {
 var arrKeys=strkeys.split('^');
 var arr=arrKeys.reverse(); 
 var uniqueKeys= arr.unique3(); 
 if(uniqueKeys.length==1)
 {
 key1=uniqueKeys[0];
 
 }else if(uniqueKeys.length==2)
 {
 key1=uniqueKeys[0];
 key2=uniqueKeys[1];
 }else
 {
 key1=uniqueKeys[0];
 key2=uniqueKeys[1];
 key3=uniqueKeys[2];
 }
 if(key1=="null")
 {
 key1="";
 }
 
 }
 
 var url = 'http://passportphp.b2b.cn';
 $.ajax({
 url: url + '/interface/Getsoruceforhomepage/Getproducts_new?jsoncallback=?',
 dataType: 'json',
 type: 'get',
 data: 'key1=' +encodeURIComponent(key1)+"&key2="+encodeURIComponent(key2)+"&key3="+encodeURIComponent(key3)+"&c="+industry,
 success: function (data) {
 //alert(data);
 var str = "";
 for (var i = 0; i < data.length; i++) {

 str += "<li><a href=\""+data[i].productUrl+"\" target=\"_blank\"><span class=\"sp_pic\"><img data-url=\""+data[i].productImgPath+"\" src=\"../20171123/images/pixel.gif\" class=\"scrollLoading\" alt=\"\"/></span><p>"+data[i].productTitle+"</p>";
 str+="<span>价位：<i>"+data[i].price+"</i></span><em>起订量：<i>"+data[i].mincount+"件</i></em><b></b></a></li>";
 
 }

 $("#prolist").html(str);
 $(".scrollLoading").scrollLoading();
 }
 });

}

