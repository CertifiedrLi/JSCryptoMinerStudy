    <script type="text/javascript">

        jQuery(document).ready(function () {
            updateStoreFederalBar();
            jQuery(document).on("setFavoriteStore", function () {
                updateStoreFederalBar();
            });
        });

        function updateStoreFederalBar() {
            if (BELL.shop.FavoriteStore.IsFavoriteStoreSet()) {
                var favoriteStore = BELL.shop.FavoriteStore.GetMyStoreCookie();
                var address;
                if (!favoriteStore.locationName) {
                    var array = favoriteStore.address.split(" ");
                    var splicedAddress = removeItems(array, 3);
                    address = splicedAddress.join(" ").replace(/,\s*$/, "");
                } else {
                    address = favoriteStore.locationName;
                }
                address = favoriteStore.storeName + " - " + address;
                if (address.length > 30)
                    jQuery(".rsx-federal-bar-links .favorite-store-info").css("font-size", "12px");
                else
                    jQuery(".rsx-federal-bar-links .favorite-store-info").css("font-size", "14px");
                jQuery(".rsx-federal-bar-links .favorite-store-info").text(address);
                jQuery(".rsx-federal-bar-links .rsx-connector-active-lob-favorite").removeClass("rsx-hide");
            }
        }

        function goToFavoriteStoreFederalBar() {
            var favoriteStore = BELL.shop.FavoriteStore.GetMyStoreCookie();
            s_oTrackPage({
                s_oAPT: "647-0-0",
                s_oBTN: "my store - header"
            });
            window.location.href = "/Store_Locator?key=" + favoriteStore.id;
        }

        function removeItems(arr, item) {
            for (var i = 0; i < item; i++) {
                arr.pop();
            }
            return arr;
        }
    </script>

