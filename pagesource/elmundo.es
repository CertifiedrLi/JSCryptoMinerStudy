                MBL.addEvent(window, "scroll", destroyMenuHandler);
                MBL.delEvent(MBL.menuDiv, 'transitionend', endTrasitionHandler);
                MBL.menuDiv.removeAttribute('style');
                if (window.statusMenu=='close') {
                    MBL.menuDiv.style.display = 'none';
                }
            }
            delete window["_MBL_"];
        };
