    <script>
        var date = new Date();
        var confDate = new Date(date.getFullYear() - 12, date.getMonth(), date.getDate());

        $("#userEdit-BirthDate").datepicker({

            isRTL: true,
            changeMonth: true,
            changeYear: true,
            yearRange: "-150:+0",
            dateFormat: "yy/mm/dd",
            maxDate: confDate,
            numberOfMonths: 1,
            showButtonPanel: true
        });

        $(".my-profile-modal")
            .click(function (e) {
                e.preventDefault();

                $.ajax({
                    type: "POST",
                    url: "/AirCtr.aspx/LoadUserInfo",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    error: function (err) {
                        toastr.error("شما از سیستم خارج شده اید. لطفا دوباره وارد شوید");
                        console.error(err);
                    },
                    success: function (data) {
                        if (data == null) {
                            toastr.error("شما از سیستم خارج شده اید. لطفا دوباره وارد شوید");
                        } else {
                            $("#userEdit-Uname").html(data.d.Uname);
                            $("#userEdit-Fname").val(data.d.Fname);
                            $("#userEdit-Lname").val(data.d.Lname);
                            $("#userEdit-Mobile").val(data.d.Mobile);
                            $("#userEdit-NationalId").val(data.d.NationalId);
                            $("#userEdit-BirthDate").val(data.d.BirthDate);
                            $("#userEdit-Gender").val(data.d.Gender);
                            $("#UserCompleteInfo").modal("toggle");
                            $.fn.modal.Constructor.prototype.enforceFocus = function () { };
                        }
                    }
                });
            });

        $(".submitProfile")
            .click(function () {
                var fname = $("#userEdit-Fname").val().trim();
                var lname = $("#userEdit-Lname").val().trim();
                var nationalId = $("#userEdit-NationalId").val().trim();
                var mobile = $("#userEdit-Mobile").val().trim();
                var birthDate = $("#userEdit-BirthDate").val().trim();
                var gender = $("#userEdit-Gender").val();

                if (fname.length === 0 || fname.indexOf('  ') >= 0) {
                    toastr.error("نام خود را درست وارد نکردید");
                    return false;
                }
                else if (lname.length === 0 || lname.indexOf('  ') >= 0) {
                    toastr.error("نام خانوادگی خود را درست وارد نکردید");
                    return false;
                }
                else if (nationalId.length === 0 || (nationalId.length > 0 && nationalId.length !== 10) || nationalId.indexOf(' ') >= 0) {
                    toastr.error("کد ملی خود را درست وارد نکردید");
                    return false;
                }
                else if (birthDate.length === 0 || (birthDate.length > 0 && birthDate.length !== 10) || birthDate.indexOf(' ') >= 0) {
                    toastr.error("تاریخ تولد خود را درست وارد نکردید");
                    return false;
                }
                else if (mobile.length !== 11 || mobile.indexOf(' ') >= 0) {
                    toastr.error("شماره همراه خود را درست وارد نکردید");
                    return false;
                }
                else if (gender.length === 0) {
                    toastr.error("جنسیت خود را مشخص نکردید");
                    return false;
                }
                $.ajax({
                    type: "POST",
                    url: "/AirCtr.aspx/UpdateUserInfo",
                    data: JSON.stringify({
                        firstName: fname,
                        lastName: lname,
                        gender: $("#userEdit-Gender").val(),
                        nationalId: nationalId,
                        mobile: mobile,
                        birthDate: birthDate
                    }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    error: function (err) {
                        console.error(err);
                    },
                    success: function (data) {
                        if (data.d) {
                            toastr.success("اطلاعات کاربری شما ثبت شد");
                        } else {
                            toastr.error("اطلاعات خود را صحیح وارد نکردید");
                        }
                    }
                });
            });

        $("#ChangePassword").click(function (e) {
            e.preventDefault();

            var $this = $(this);

            //set button on loading
            $this.button('loading');
            var newPass = $('#txtNewPass').val();
            var newPassConf = $('#txtNewPassConfirm').val();
            if (newPass !== newPassConf) {
                console.log(newPass);
                console.log(newPassConf);
                toastr.error("پسورد شما یکسان نیست");
                console.log("sdsdsds");
                $this.button('reset');
                return false;
            }
            if (newPass.length < 6) {
                toastr.error("لطفا رمز عبور خود رابا فرمت صحیح (حداقل 6 کارکتر) وارد کنید");
                $this.button('reset');
            }

            $.ajax({
                type: "POST",
                url: "/AirCtr.aspx/ChangePassword",
                data: JSON.stringify({
                    oldPass: $('#txtOldPass').val(),
                    newPass: newPass,
                    newPassConf: newPassConf
                }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    if (response.d == null || response.d == "") {
                        //show success
                        toastr.info("تغییر رمز با موفقیت انجام شد.");

                        //close modal
                        $("#ResetPasswordModal").modal("toggle");

                        //reset form
                        $('#txtOldPass').val("");
                        $('#txtNewPass').val("");
                        $('#txtPassConfirm').val("");
                    } else {
                        //show error
                        toastr.error(response.d);
                    }
                },
                complete: function () {
                    //reset button from loading
                    $this.button('reset');
                }
            });
        });

        $(".resetPassBtn")
            .click(function (e) {
                $("#ResetPasswordModal").modal("toggle");
                $("#UserCompleteInfo").modal("toggle");
            });

