    <script>

        function validateForm() {
            //var cap = $('.off-frame .txtRegisterCaptcha').val();
            var email = $('#txtemail').val();
            var mobile = $('#txtmobile').val();

            if (mobile == null || mobile == "" || email == null || email == "") {
                toastr.error("فیلدهای مورد نظر را پر کنید");
                return false;
            }
            else if ($.cookie("tc-email") == email && $.cookie("tc-mobile") == mobile) {
                toastr.error("شما قبلا از این ایمیل و شماره تلفن استفاده کرده اید");
                return false;
            }

            else {
                //console.log(email + " * " + cap + " * " + isfirst + " * " + mobile);
                $.ajax({
                    type: 'POST',
                    url: '/AirCtr.aspx/GetDiscountTrain',
                    data: JSON.stringify({
                        email: email,
                        //captcha: cap,
                        //isfirst: isfirst,
                        mobile: mobile,
                        isTrain: "1"
                    }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    error: function (response) {
                        debugger;
                        toastr.error("ارتباط به مشکل خورده است. لطفا دوباره سعی کنید")
                    },
                    success: function (data) {
                        if (data.d.Successful) {
                            debugger;

                            $(".off-frame").addClass("done");
                            $.cookie('gotcode', true);
                            $.cookie("tc-email", email);
                            $.cookie("tc-mobile", mobile);
                            toastr.success(data.d.Message);
                        }
                        else {
                            debugger;

                            //showCaptcha();
                            toastr.error(data.d.Message);
                        }
                    }
                });
            }
        }

