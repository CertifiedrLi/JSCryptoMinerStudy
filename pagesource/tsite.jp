<script type="text/javascript">
    var xrostString = "XROST_TLSC";
    var xrost = getCookie(xrostString);
    if (replaceAll(xrost,"\"","") != "") {
        var acdt_my = replaceAll(replaceAll(replaceAll(document.location.host + "_" + xrost + "_" + "acdt_my","=","_s"),"/","_i"),"+","_t");
        var access_date = getCookie(acdt_my);
        var today = getToday();
        if (access_date == "" || access_date != today) {
            setCookie(acdt_my, today);
            (function() {
                var ul = document.createElement('SCRIPT');
                ul.type = 'text/javascript';
                ul.async = true;
                ul.src = ('https:' === document.location.protocol ? 'https://ssl.'
                        : 'http://') + 'my.tsite.jp/js/srpCCm002_' + xrostString + '.js';
                var s = document.getElementsByTagName('SCRIPT')[0];
                s.parentNode.insertBefore(ul, s);
            })();
        }
    }

    function getToday() {
        var dd = new Date();
        var yy = dd.getYear();
        var mm = dd.getMonth() + 1;
        var dd = dd.getDate();
        if (yy < 2000) {
            yy += 1900;
        }
        if (mm < 10) {
            mm = "0" + mm;
        }
        if (dd < 10) {
            dd = "0" + dd;
        }
        return yy + "/" + mm + "/" + dd;
    }

    function setCookie(key, val) {
        var tmp = key + "=" + escape(val) + "; path=/; ";
        var date = new Date();
        date.setTime(date.getTime() + (24 * 60 * 60 * 1000));
        tmp += "expires=" + date.toUTCString() + ";";
        document.cookie = tmp;
    }

    function getCookie(key) {
        var cookies = " " + document.cookie + ";";
        var i, j, k;
        i = j = k = 0;
        var tmp;
        var len = cookies.length;
        while (i < len) {
            j = cookies.indexOf(";", i);
            tmp = cookies.substring(i + 1, j);
            k = tmp.indexOf("=");
            if (tmp.substring(0, k) == key) {
                return (unescape(tmp.substring(k + 1, j - i - 1)));
            }
            i = j + 1;
        }
        return ("");
    }

    function replaceAll(expression, org, dest){   
        return expression.split(org).join(dest);   
    }
</script>

