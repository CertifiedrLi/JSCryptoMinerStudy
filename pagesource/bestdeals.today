window.timePageInfo = {
  TIME_LAPSE_FOR_SEND: 5000,

  data: {
    time_on_site:   { type: 'time_active', value: 0 },
    time_no_active: { type: 'time_inactive', value: 0 },
    scroll:         { type: 'scroll', value: 0 },
    mouseMove:      { type: 'mouse_move', value: 0 },
    clicks:         { type: 'click', value: 0 }
  },

  init: function(){
    this.recordEvents();
    this.clock.start();
  },

  initSendData: function(){
    var _this = this;
    setInterval(function(){
      _this.sendData();
    }, this.TIME_LAPSE_FOR_SEND);
  },

  updateData: function(key){
    this.clock.start();
    this.data[key].value += 1;
  },

  recordEvents: function(){
    document.addEventListener('mousemove', function() { window.timePageInfo.updateData('mouseMove') } );
    document.addEventListener('click', function() { window.timePageInfo.updateData('clicks') });
    document.addEventListener('scroll', function() { window.timePageInfo.updateData('scroll') });
  },

  timeOnSite: function() {
    return this.clock.currentTime();
  },

  timeInactiveOnSite: function() {
    return ((this.TIME_LAPSE_FOR_SEND / 1000) - this.clock.currentTime());
  },

  // resets both pagedata and time data
  reset: function(){
    for ( var key in  this.data ){
      this.data[key].value = 0;
    }
    this.clock.reset();
  }
}

window.timePageInfo.clock = {
  TIME_LAPSE_NO_ACTIVITY: 10000,
  TIME_UNIT: 1000, //SECONDS
  time: 0,
  sleep:0,
  start: function (){
    if (!this.timer) {
      this.count();
    }
    this.sleep = 0;
    return this.time;
  },

  stop: function() {
    clearTimeout(this.timer);
    this.timer = 0;
    return this.time;
  },

  count: function(){
    _self = this;
    this.time  += 1;
    this.sleep += 1;
    if (this.sleep == 10) return this.stop();
    this.timer = setTimeout(function(){  _self.count() }, _self.TIME_UNIT);
  },

  reset: function(){
    var time = this.time;
    this.time = 0;
    this.stop();
    return time;
  },

  currentTime: function(){
    return this.time;
  }
};

timePageInfo.init();

