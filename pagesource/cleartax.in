<script>
    window.smoothScroll = function (element, duration, offset) {
        var startingY = window.pageYOffset;
        var elementY = getElementY(element) - offset;
        var targetY = document.body.scrollHeight - elementY < window.innerHeight ? document.body.scrollHeight - window.innerHeight : elementY;
        var diff = targetY - startingY;

        var easing = function (t) { return t < .5 ? 4 * t * t * t : (t - 1) * (2 * t - 2) * (2 * t - 2) + 1 }
        var start

        if (!diff) return

        window.requestAnimationFrame(function step(timestamp) {
            if (!start) start = timestamp
            var time = timestamp - start
            var percent = Math.min(time / duration, 1)
            percent = easing(percent)

            window.scrollTo(0, startingY + diff * percent)

            if (time < duration) {
                window.requestAnimationFrame(step)
            }
        });
    };


    var mobileAccordionMap = {
        "mobile-for-you-title": "mobile-for-you-accordion",
        "mobile-for-ca-title": "mobile-for-ca-accordion",
        "mobile-for-business-title": "mobile-for-business-accordion",
        "mobile-my-account-title": "mobile-my-account-accordion"
    };
    var overlayIdsMap = {
        "for_you_top": "for_you_overlay",
        "ca_legal_top": "ca_legal_overlay",
        "for_business_top": "for_business_overlay",
        "my-account": "account-dropdown"
    };

    var navTabColorMap = {
        "for_you_top": "#FCA65F",
        "ca_legal_top": "#00C693",
        "for_business_top": "#8278E8"
    };

    Object.keys(overlayIdsMap).map(function (id) {
        var el = document.getElementById(overlayIdsMap[id]);
        if (el) {
            el.onmouseleave = overlayMouseLeave.bind(null, id);
        }
    });


    function overlayMouseLeave(id, el) {
        if (el.relatedTarget && el.relatedTarget.id !== id && el.clientY !== document.getElementById(id).getBoundingClientRect().bottom) {
            window.setTimeout(function (el) {
                el.target.classList.add("hidden");
                document.getElementById(id).style.borderBottom = "none";
            }.bind(null, el), 200);
        }
    }


    Object.keys(overlayIdsMap).map(function (id) {
        var el = document.getElementById(id);
        if (el) {
            var timer;
            el.onmouseenter = function mouseEnterHandler(e) {
                timer = setTimeout(function () {
                    var overlayEl = document.getElementById(overlayIdsMap[id]);
                    if (overlayEl.classList.contains("hidden")) {
                        overlayEl.classList.remove("hidden");
                        el.style.borderBottom = "2px solid " + navTabColorMap[id];
                    }
                }, 200);
            };

            el.onmouseleave = function mouseLeaveHandler(e) {
                clearTimeout(timer);
                var overlayEl = document.getElementById(overlayIdsMap[id]);
                if (!overlayEl.classList.contains("hidden") && !(document.getElementById(overlayIdsMap[id]).contains(e.relatedTarget))) {
                    window.setTimeout(function (overlayEl) {
                        overlayEl.classList.add("hidden");
                        el.style.borderBottom = "none";
                    }.bind(null, overlayEl), 210);
                }
            }
        }


    });

    var hamburgerIcon = document.getElementById("hamburger-icon");
    var mobileMenu = document.getElementById("mobile-menu");
    var navClose = document.getElementById("nav-close");

    hamburgerIcon.onclick = function () {
        mobileMenu.classList.remove("hidden");
        document.body.classList.add("no-scroll");

    }

    navClose.onclick = function () {
        mobileMenu.classList.add("hidden");
        document.body.classList.remove("no-scroll");
    }

    Object.keys(mobileAccordionMap).map(function (id) {
        var el = document.getElementById(id);
        if (el) {
            el.onclick = function () {
                el.classList.toggle("active");
                Object.keys(mobileAccordionMap).filter(function (key) { return key !== id }).forEach(function (key) {
                    var other = document.getElementById(mobileAccordionMap[key]);
                    if (other && !other.classList.contains("hidden")) {
                        other.classList.add("hidden");
                    }
                });
                var acc = document.getElementById(mobileAccordionMap[id]);
                if (acc.classList.contains("hidden")) {
                    acc.classList.remove("hidden");
                } else {
                    acc.classList.add("hidden");
                }
            }
        }
    });

    function getElementY(query) {
        return window.pageYOffset + document.querySelector(query).getBoundingClientRect().top;
    }

    function closeLoginModal(event, id) {
        document.getElementById(id).classList.add('hidden');
    }

    function openLoginModal(event, id) {
        navClose.onclick();
        document.getElementById(id).classList.remove('hidden');
    }

    var loginModal = document.getElementById('content-login-modal');
    var signupModal = document.getElementById('content-signup-modal');

    loginModal.addEventListener('click', function (e) {
        if (!e.target.getAttribute('data-tracking-action')) {
            e.stopPropagation();
        }

    });
</script>


