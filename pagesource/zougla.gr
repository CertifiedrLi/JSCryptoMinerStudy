<script type="text/javascript">
  $(document).ready(function () {
    var $container = $('#ctl00_ctl00_main_SingleColumnZoneBottom_ctl00_articlesCyclePanelWithPromo');
		var $slideContainer = $('#ctl00_ctl00_main_SingleColumnZoneBottom_ctl00_articlesCyclePanelWithPromo .main-container');
    var $banner = $container.find('.banner-container .banner .adman-banner');

		$slideContainer.on('cycle-before', function (e, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {
			$(incomingSlideEl).find('.photo img').each(function () {
				if ($(this).attr('src') != $(this).attr('_src'))
					$(this).attr('src', $(this).attr('_src'));
			});
		});

		$slideContainer.find('.prev').click(function (event) {
			event.preventDefault();
			$slideContainer.cycle('prev');
		});

		$slideContainer.find('.next').click(function (event) {
			event.preventDefault();
			$slideContainer.cycle('next');
		});

    // in case of less than 2 items, hide navigation arrows
    if ($slideContainer.find('.panel').length < 2)    
      $slideContainer.find('span.prev, span.next').css('display', 'none');

		$slideContainer.cycle({
			fx: 'fade',
			speed: 500,
			timeout: 0,
			swipe: true,
			slides: '.panel',
			pagerTemplate: '',
		});

    // in case of no banner, show articles instead
    if ($banner.length == 0) {
      $container.addClass('no-banner');
    } else {
      $banner.on('banner-loading', function() {
        setTimeout(function() { if ($banner.width() == 0) $container.addClass('no-banner'); }, 5000);
      });

      $banner.on('banner-loaded', function() {
        var iterations = 20;

        var bannerInterval = setInterval(function() {
          if ($banner.width() > 0) {
            clearInterval(bannerInterval);
          } else if (--iterations == 0) {
            $container.addClass('no-banner');
            clearInterval(bannerInterval);
          }
        }, 500);
      });
    }
	});
</script>
