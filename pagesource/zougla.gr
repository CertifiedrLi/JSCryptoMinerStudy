<script type="text/javascript">
	function InitFrontStorySlider(/*int*/ slideDelayInSeconds) {
		var isAutoScrollPaused = false;
		var isAutoScrollStopped = false;

		var container = $('.box#frontstory .content-container-top');

		// bind events
		$(document).ready(function () {
			SetFrontStorySliderHeight(container);

			window.setTimeout(function () { SetFrontStorySliderHeight(container); }, 2000);
		});

		$(window).on('resize orientationchange container-resize', function () {
			SetFrontStorySliderHeight(container);
		});

		container.on('cycle-initialized', function (e, opts) {
			SetFrontStorySliderHeight(container);
		});

		container.on('cycle-before', function (e, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {
			$(incomingSlideEl).find('.photo img').each(function () {
				if ($(this).attr('src') != $(this).attr('_src')) {
					// Trigger the next slide for loading and recalculate contained height
					$(this).attr('src', $(this).attr('_src')).load(function () {
						container.stop().animate({ height: $(incomingSlideEl).outerHeight(false) }, 250, null);
					});
				} else {
					container.stop().animate({ height: $(incomingSlideEl).outerHeight(false) }, 250, null);
				}
			});

			$('.box#frontstory .thumbnail-container img').css({ opacity: 0.7 });
			$('.box#frontstory .thumbnail-container div.idx_' + $(incomingSlideEl).data('index') + ' img').fadeTo('fast', 1);
		});

		$('.box#frontstory .content-container').css('visibility', 'visible').cycle({
			fx: 'fade',
			speed: 500,
			timeout: 0,
			swipe: true,
			slides: '.slide',
			pagerTemplate: '',
		});

		$(".box#frontstory .thumbnail-container").css('visibility', 'visible').find('div').each(function () {
			$(this).css('left', 118 * $(this).data('index'));
		});

		$(".box#frontstory .thumbnail-container div").click(function () {
			isAutoScrollStopped = true;
			$('.box#frontstory .content-container').cycle('goto', $(this).data('index'));
		});

		$('.box#frontstory .prev').click(function (event) {
			event.preventDefault();
			isAutoScrollStopped = true;
			$('.box#frontstory .content-container').cycle('prev');
		});

		$('.box#frontstory .next').click(function (event) {
			event.preventDefault();
			isAutoScrollStopped = true;
			$('.box#frontstory .content-container').cycle('next');
		});

		$('.box#frontstory .content-container-top .slide').hover(function () {
			isAutoScrollPaused = true;
			$('.box#frontstory .next, .box#frontstory .prev').stop().fadeIn(250);
		}, function () {
			isAutoScrollPaused = false;
			$('.box#frontstory .next, .box#frontstory .prev').stop().fadeOut(250);
		});

		// auto-slide only if frontstory control is fully loaded and visible
		$(window).load(function () {
			if (slideDelayInSeconds > 0) {
				window.setInterval(function () {
					if (!isAutoScrollPaused && !isAutoScrollStopped && ($(window).scrollTop() < ($('#frontstory').offset().top + 100))) {
						$('.box#frontstory .content-container').cycle('next');
					}
				}, slideDelayInSeconds * 1000);
			}
		});
	};

	function SetFrontStorySliderHeight(container) {
		var opts = container.data('cycle.opts');

		if (opts) {
			height = $(opts.slides[opts.currSlide]).outerHeight(false);
			opts.container.height(height);
		}
	};
</script>
