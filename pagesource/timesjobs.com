archText);ajaxController.jbAutoSuggest(jbUrl,{async:false,callback:function(response){var jsonObj=jQuery.parseJSON(response);var li_data="";jQuery.each(jsonObj,function(i,element){var hyperlink="javascript:click2addCompanySuggestList('"+element.companyId+"','"+textId+"','"+autoSuggestListId+"');";li_data+="<li><a id="+element.companyId+" href="+hyperlink+">"+element.companyName+"</a></li>";});var auList="<ul>"+li_data+"</ul>";jQuery("#"+autoSuggestListId).html(auList);if(li_data!=""){jQuery("#"+autoSuggestListId).show();}else{jQuery("#"+autoSuggestListId).hide();}}});}}function BasicResFileUpload(){new AjaxUpload('resumeFile_basic',{action:'/candidate/servlet/FileUploadServlet',responseType:'true',name:'resumeFile_basic',onSubmit:function(){document.getElementById("uploadResume_basic").style.display="none";document.getElementById("uploadingResume_basic").style.display="block";},onComplete:function(file,response){var displayFilename=file;if(file.length>18){displayFilename=file.substring(0,18)+"..";}response=response.replace(/'/g,'"');var jsonObj=jQuery.parseJSON(response);jsonObj["filename"]=displayFilename;ajaxController.setSessionAttribute('tjindexResumeUploadMsg',jsonObj["MESSAGE"]);response=JSON.stringify(jsonObj);ajaxController.setSessionAttribute('tjindexResumeUpload',response);window.location='/candidate/register.html';}});}BasicResFileUpload();</script>
