<script type="text/javascript">
    
    var topRankCompanyChartData = [];
    $(document).ready(function () {
        var url = $("#hdnTopRankCompaniesURL").val();
        GetChartData(url, 'TCMarketValue');
    });
    function GetChartData(url, tabID) {
        var jqhx = $.ajax({
            type: "GET",
            url: url,
            beforeSend: function () {
                $("#topRankCompaniesLoading").css("display", "block");
                //$(".loading-div").show();
            },
        }).done(function (data) {
            topRankCompanyChartData = [];
            for (var i = 0; i < data.length; i++) {
                topRankCompanyChartData.push({
                    title: data[i][0],
                    value: data[i][1],
                    name: data[i][2]
                });
            }
            DrawChart(topRankCompanyChartData);
            $("[relf-tc-tab='toprankcompanies']").removeClass("active");
            $("#" + tabID).addClass("active");
        }).always(function () {
            $(".loading-div").hide();
        });
    }
    function DrawChart(topRankCompanyChartData) {
        var chart = AmCharts.makeChart("funnelChart", {
            "type": "funnel",
            "theme": "none",
            "dataProvider": topRankCompanyChartData,
            "balloon": {
                "fixedPosition": true
            },
            "valueField": "value",
            "titleField": "name",
            "marginRight": 50,
            "marginLeft": 50,
            "startX": -500,
            "outlineAlpha": 1,
            "depth3D": 50,
            "angle": 20,
            "outlineColor": "#FFFFFF",
            "outlineThickness": 2,
            "labelPosition": "center",
            "balloonText": "[[title]]: [[value]]",
            "colors": ["#5AB8DF", "#E27683", "#7CC576", "#FFAD62", "#BD8DBF"]
        });
        setTimeout(function () {
            $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "https://www.argaamplus.com").html("").css("top", "0px");
            $("#topRankCompaniesLoading").css("display", "none");
        }, 100);
    }
    jQuery('.chart-input').off().on('input change', function () {
        var property = jQuery(this).data('property');
        var target = chart;
        var value = Number(this.value);
        chart.startDuration = 0;

        if (property == 'innerRadius') {
            value += "%";
        }

        target[property] = value;
        chart.validateNow();
        $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "https://www.argaamplus.com").html("").css("top", "0px");
    });
</script>



