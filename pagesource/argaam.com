<script>
    var InternationalMarketChart;
    var InternationalMarketChartData = [];
    var count;
    var previousDayClosePrice;
    var maxValue;
    var minValue;
    $(document).ready(function () {
        $(".priRow").first().addClass("selected");
        $(".priRow").click(function () {
            $(".priRow").each(function () { $(this).removeClass("selected"); });
            $(this).addClass("selected");
        });

        var internationalMarketID = $(".priRow.selected").attr("internationalmarketid");
        var days = $("#internationalMarketTabs a.active").attr("relf-days");
        //DrawInternationalMarketChartForAllData(days, internationalMarketID);
        $("#activeInternationalMarketID").val(internationalMarketID);
        $(".priRow").first().trigger('click');
    });
    function GetInternationalMarketData(item) {
        //$("[relf-international-market]").css("display", "none");
        var internationalMarketID = $(item).attr("internationalMarketID");
        var days = $("#internationalMarketTabs a.active").attr("relf-days");
        DrawInternationalMarketChartForAllData(days, internationalMarketID);
        //$("[relf-international-market='" + internationalMarketID + "']").css("display", "block");

        if ($("[relf-international-market='" + internationalMarketID + "']").is(':visible')) {
            $("[relf-international-market='" + internationalMarketID + "']").toggle();
        }
        else {
            $("[relf-international-market]").css("display", "none");
            $("[relf-international-market='" + internationalMarketID + "']").css("display", "block");
        }



        $("#activeInternationalMarketID").val(internationalMarketID);
        $("[relf-international-market='" + internationalMarketID + "'] li a[relf-days='" + days + "']").addClass("active");
    }
    function InternationalChartTab(item) {
        var internationalMarketID = $("#activeInternationalMarketID").val();
        var days = $(item).attr("relf-days");
        DrawInternationalMarketChartForAllData(days, internationalMarketID);
        $("#internationalMarketTabs a").removeClass("active");
        $(item).addClass("active");
        $("#activeInternationalMarketID").val(internationalMarketID);
    }
    function generateInternationalMarketChartData(chartData, days, InternationalMarketID, callback) {
        $("[relf-loading-id='imc_loading_" + InternationalMarketID + "']").show();
        var url2 = "/InteractiveCharts/GetInternationalMarketChartData?InternationalMarketId=" + InternationalMarketID;
        if (days != "") {
            url2 = url2 + "&days=" + days;
        }
        chartData.length = 0;
        var jqhx = $.ajax({
            type: "GET",
            url: url2,
            cache: false,
            async: true
        })
        .done(function (data) {
            count = data.length;
            for (var i = 0; i < data.length; i++) {
                if (data[i] != null) {
                    var date = data[i][0];
                    var open = data[i][1];
                    var min = data[i][4];
                    var max = data[i][3];
                    var close = data[i][2];
                    var category = data[i][5];
                    chartData.push({
                        date: date,
                        open: open,
                        close: close,
                        max: max,
                        min: min,
                        category: AmCharts.stringToDate(category, "DD-MM-YYYY")
                    });
                }
            }
            callback();
        });
    }
    var imc_timout = 5000;
    function DrawInternationalMarketChartForAllData(days, InternationalMarketID) {
        generateInternationalMarketChartData(InternationalMarketChartData, days, InternationalMarketID, function () {
            InternationalMarketChart = new AmCharts.AmSerialChart();
            InternationalMarketChart.pathToImages = "/Scripts/amcharts/images/";
            InternationalMarketChart.dataProvider = InternationalMarketChartData;
            InternationalMarketChart.categoryField = "category";
            InternationalMarketChart.dateFormats = [{ period: 'fff', format: 'MMM DD' }, { period: 'ss', format: 'MMM DD' }, { period: 'mm', format: 'MMM DD' }, { period: 'hh', format: 'MMM DD' }, { period: 'DD', format: 'MMM DD' }, { period: 'WW', format: 'MMM DD' }, { period: 'MM', format: 'MMM' }, { period: 'YYYY', format: 'YYYY' }];
            var categoryAxis = InternationalMarketChart.categoryAxis;
            categoryAxis.parseDates = true;
            categoryAxis.minPeriod = "mm";
            //categoryAxis.showLastLabel = true;
            categoryAxis.showFirstLabel = true;
            categoryAxis.equalSpacing = false;
            categoryAxis.gridPosition = "start";
            categoryAxis.dateFormats = [{ period: 'fff', format: 'MMM DD' }, { period: 'ss', format: 'MMM DD' }, { period: 'mm', format: 'MMM DD' }, { period: 'hh', format: 'MMM DD' }, { period: 'DD', format: 'MMM DD' }, { period: 'WW', format: 'MMM DD' }, { period: 'MM', format: 'MMM' }, { period: 'YYYY', format: 'YYYY' }];
            categoryAxis.axisColor = "#DADADA";
            categoryAxis.autoGridCount = false;
            categoryAxis.gridCount = 5;
            categoryAxis.markPeriodChange = false;

            var valueAxis = new AmCharts.ValueAxis();
            valueAxis.position = "right";
            InternationalMarketChart.addValueAxis(valueAxis);

            var graph = new AmCharts.AmGraph();
            graph.type = "line";
            graph.title = "Market";
            graph.valueField = "close";
            //graph.lineAlpha = 4;
            graph.lineColor = "#1F588A";
            graph.lineThickness = 2;
            //graph.fillColors = ["#35628D", "#A7D2EE"];
            graph.fillColors = ["#D6E7F2"];
            graph.fillAlphas = 0.7;
            graph.pointPosition = "start";
            graph.connect = true;
            graph.showBalloon = false;
            InternationalMarketChart.addGraph(graph);

            var chartCursor = new AmCharts.ChartCursor();
            chartCursor.cursorPosition = "mouse";
            chartCursor.categoryBalloonDateFormat = "DD-MMM-YYYY";
            chartCursor.bulletsEnabled = true;
            chartCursor.bulletSize = 8;
            chartCursor.categoryBalloonEnabled = false;
            chartCursor.cursorAlpha = 0;
            InternationalMarketChart.addChartCursor(chartCursor);

            var legend = new AmCharts.AmLegend();
            legend.position = "top";
            legend.top = 1;
            legend.markerSize = 2;
            legend.labelText = "";
            legend.markerType = "none";
            legend.markerLabelGap = 1;
            legend.valueText = "Close: [[value]] Date: [[category]]";
            legend.verticalGap = 0;
            InternationalMarketChart.addLegend(legend);
            InternationalMarketChart.write("internationalMarketChart_" + InternationalMarketID);
            $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "https://www.argaamplus.com").html("").css("top", "0px");
            $("[relf-loading-id='imc_loading_" + InternationalMarketID + "']").hide();

        });
        //setTimeout(function () {
           
        //}, imc_timout);
        //imc_timout = 3000;
    }
</script>

