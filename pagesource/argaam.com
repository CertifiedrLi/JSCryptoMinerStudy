                DrawCommodityChart(o, days);
            }

            //SetChartTabActiveInActive(o, days);
        }

        function SetChartTabActiveInActive(o, days)
        {
            var oT = $('#hdnCmpTimeRange').val();
            var oChtTbId = '#chrtTab_' + oT

            $(oChtTbId).removeClass('active');
            if (days>0) {
                $("#chrtTab_9125").removeClass('active');
            }
            $(o).addClass('active');

            $('#hdnCmpTimeRange').val(days);
        }

        $(window).load(function()
        {
            DrawCommodityChartData($("#chrtTab_90"), 90);
        });

    </script>

