    $(document).ready(function () {
        $('#slideNext').click(function () {

            var index = parseInt($('#slideNext').attr('relf'));
            var maxIndex = parseInt($('#next_prev_section').attr('maxIndex'));

            if (index == maxIndex) {
                // disable next link
                $('#slideNext').addClass('disabled');
            }
            else {

                OnNextPrevClick(index);

                $('#slidePrev').attr('relf', index - 1);// enable prev link
                $('#slidePrev').removeClass('disabled');
                $('#slideNext').attr('relf', index + 1);

                if (index == (maxIndex - 1)) {
                    // disable next link
                    $('#slideNext').addClass('disabled');
                }
            }

            return false;
        });
        $('#slidePrev').click(function () {

            var index = parseInt($('#slidePrev').attr('relf'));

            if (index < 0) {
                // disable prev link
                $('#slidePrev').addClass('disabled');
            }
            else {

                OnNextPrevClick(index);

                $('#slidePrev').attr('relf', index - 1);
                $('#slideNext').attr('relf', index + 1);// enable next link
                $('#slideNext').removeClass('disabled');

                if (index == 0) {
                    // disable prev link
                    $('#slidePrev').addClass('disabled');
                }
            }

            return false;
        });
    });

    function OnNextPrevClick(index) {
        $('#lblComp').fadeOut(500, function () {
            $('#lblComp').html(a[index][1]);
            var moreUrl = $('#ProdCapMoreURL').attr('relf');
