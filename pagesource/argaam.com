    <script type="text/javascript">
        function GetHomePagePieChart(item) {
            $("#pieChartContainer").empty();
            var bsChartData = [];
            var forDate = $(item).attr("for-date");
            $("[bs-pie-chart-data][bs-for-date='" + forDate + "']").each(function () {
                var bsValue = $(this).attr("bs-pie-chart-data");
                var bsName = $(this).attr("bs-pie-chart-name");
                if (bsValue.indexOf("-") == -1 && bsValue.indexOf("null") == -1) {
                    bsChartData.push({
                        name: bsName,
                        value: bsValue
                    });
                }
            });
            if (bsChartData.length != 0) {
                var x = $(item).position().left + $(item).outerWidth();
