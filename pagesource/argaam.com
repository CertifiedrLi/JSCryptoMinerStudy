    function GeneratepetrochemcialChartData(period) {
        $("#divLoading1").show();
        $("#argaamPetrochemicalIndexPeriod").val(period);
        $(".chart_link").each(function () {
            $(this).removeClass("active");
        });
        $("#" + period).addClass("active");
        var jqhx = $.ajax({
            type: "GET",
            url: "/Petrochemical/GetArgaamPetrochemicalsIndex?period=" + period,
            async: true
        })
        .done(function (data) {
            petrochemcialChartData = [];
            for (var i = 0; i < data.length; i++) {
                petrochemcialChartData.push({
                    date: data[i][0],
                    index: data[i][1],
                });
            }
        });
        Draw();
    }
    function Draw() {
        setTimeout(function () {
            chart = new AmCharts.AmSerialChart();
            chart.pathToImages = "/Scripts/amcharts/images/";
            chart.dataProvider = petrochemcialChartData;
            chart.categoryField = "date";

            var categoryAxis = chart.categoryAxis;
            categoryAxis.parseDates = true;
            categoryAxis.minPeriod = "DD";
            categoryAxis.showLastLabel = true;
            categoryAxis.showFirstLabel = true;
            categoryAxis.equalSpacing = true;
            categoryAxis.gridPosition = "start";
            categoryAxis.dateFormats = [{ period: 'fff', format: 'JJ:NN:SS' }, { period: 'ss', format: 'JJ:NN:SS' }, { period: 'mm', format: 'JJ:NN' }, { period: 'hh', format: 'JJ:NN' }, { period: 'DD', format: 'MMM DD' }, { period: 'WW', format: 'MMM DD' }, { period: 'MM', format: 'MMM' }, { period: 'YYYY', format: 'YYYY' }];
            categoryAxis.axisColor = "#DADADA";
            categoryAxis.autoGridCount = false;
            categoryAxis.gridCount = 8;
            categoryAxis.markPeriodChange = false;

            var valueAxis = new AmCharts.ValueAxis();
            valueAxis.position = "right";
            chart.addValueAxis(valueAxis);

            var graph = new AmCharts.AmGraph();
            graph.type = "line";
            graph.title = "Argaam Petrochemical Index";
            graph.valueField = "index";
            graph.lineColor = "#98B2CA";
            graph.lineThickness = 1.5;
            graph.pointPosition = "start";
            graph.connect = true;
            graph.showBalloon = false;
            chart.addGraph(graph);

            var chartCursor = new AmCharts.ChartCursor();
            chartCursor.cursorPosition = "mouse";
            chartCursor.categoryBalloonDateFormat = "MMM, YYYY";
            chartCursor.bulletsEnabled = true;
            chartCursor.bulletSize = 8;
            chartCursor.categoryBalloonEnabled = false;
            chartCursor.cursorAlpha = 0;
            chart.addChartCursor(chartCursor);

            var legend = new AmCharts.AmLegend();
            legend.position = "top";
            legend.top = 1;
            legend.markerSize = 2;
            legend.labelText = "";
            legend.markerType = "none";
            legend.markerLabelGap = 1;
            legend.valueText = "Index: [[index]] Date: [[date]]";
            legend.verticalGap = 0;
            chart.addLegend(legend);
            chart.write("argaamPetrochemicalIndexChart");
            $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "https://www.argaamplus.com").html("").css("top", "0px");
            $("#divLoading1").hide();
        }, pcc_timeout);
        pcc_timeout = 2000;
    }
</script>

