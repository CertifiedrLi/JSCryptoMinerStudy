<script type="text/javascript">
    function Detail(item) {
        $("#calendarEventDetail").html("");
        var calendarEventID = $(item).attr("id");
        var url = "/ar/calendar/calendardetail/" + calendarEventID;
        var jqxhr = $.ajax({
            type: "get",
            url: url
        })
         .done(function (data) {
             $("body").css("overflow", "hidden");
             $("#calendarEventDetail").html(data);
             LoadGoogleMap();
             $("#colse-rpojects").click(function () {
                 $("#calendarEventDetail").html("");
                 return false;
             });
         });
    }
    function navigate(id) {
        var url = $(id).attr("nav-url");
        $("#calendarLoadingDiv").show();
        $.ajax({
            type: "GET",
            url: url
        })
       .done(function myfunction(data) {
           $("#homeCalnederDetail").html(data);
           $("#calendarLoadingDiv").hide();
       })
        .error(function myfunction() {
            $("#calendarLoadingDiv").hide();
        });
    }
    var EventsDic = new Object();
    var tempCurrentMonthEventDays = new Array();
    function LoadCalendar(divID, EventsDic, date) {
        if(!$(divID).hasClass("hasDatepicker")){
            $(divID).datepicker(
             {
                 setDate: date,
                 onChangeMonthYear: function (year, month, widget) {
                     LoadEvents(month,year);
                 },
                 beforeShowDay: function (_date) {
                     if(tempCurrentMonthEventDays != undefined)
                     {
                         var day = _date.getDate();
                         if (day > 0 && tempCurrentMonthEventDays.indexOf(day) > -1)
                             return [true, "highlighted-day", "Events"];
                     }
                     return [true, "", ""];
                 },
                 dateFormat: 'mm-dd-yy',
                 onSelect: function (dateText, inst) {
                     var date = $(this).val();

                     $(this).css("color", "blue");

                     $("#calendarLoadingDiv").show();
                     $.ajax({
                         type: "GET",
                         url: "/" + 'ar' + "/calendar/calendarhomepartial/startdate/" + date + "/type?isNewHome=true",
                         async: false
                     })
                      .done(function myfunction(data) {
                          $("#homeCalnederDetail").html(data);
                          $("#calendarLoadingDiv").hide();
                      })
                      .error(function myfunction() {
                          $("#calendarLoadingDiv").hide();
                      });
                     $("#calendarLoadingDiv").hide();
                 }
             });
        }else{
            $(divID).datepicker("refresh");
        }
        $("#calendarLoadingDiv").css("display","none");
     }

    function LoadEvents(month, year) {
        $("#calendarLoadingDiv").css("display","block");
        tempCurrentMonthEventDays = undefined;
        if(EventsDic[year + "-"+ month] == undefined){
            $.ajax({
                type: "GET",
                url: "/" + 'ar' + "/calendar/getmonthevents/month/" + month + "/year/" + year,
                async: true
            })
               .done(function (monthEvents) {
                   EventsDic[ year + "-" + month ] = monthEvents;
                   tempCurrentMonthEventDays = monthEvents;
                   LoadCalendar("#divDate", EventsDic, new Date(year, month, 1));
               })
               .error(function () {
                   $("#calendarLoadingDiv").css("display","none");
               });
        }else{
            tempCurrentMonthEventDays = EventsDic[year + "-" + month];
            LoadCalendar("#divDate", EventsDic, new Date(year, month, 1));
        }
     }
     $(document).ready(function () {
         LoadEvents(3, 2018,new Date(2018, 3, 1));
         $(".ui-datepicker").css("width", "100%");
     });

</script>

