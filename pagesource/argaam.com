                            marketChartData.push({
                                date: date,
                                open: open,
                                close: close,
                                max: max,
                                min: min,
                                category: category,
                            });
                        }
                    }
                    var valueAxis = new AmCharts.ValueAxis();
                    valueAxis.position = "right";
                    valueAxis.minimum = saudiminValue;
                    valueAxis.maximum = saudimaxValue;
                    saudiMarketchart.addValueAxis(valueAxis);
                    saudiMarketchart.dataProvider = marketChartData;
                    saudiMarketchart.validateData();
                    $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "http://www.argaam.com").html("").css("top", "0px");
                    //}
                }
            });
    }, 60000);


    function generateChartData(marketID, divID) {
        var url = $("#marketChartDataURL").val() + "?marketid=" + marketID;
        var jqhx = $.ajax({
            type: "GET",
            url: url,
            async: true,
            cache: false
        })
        .done(function (data) {
            marketChartData = [];
            //saudiCount = data.length;
            if (data != null) {
                for (var i = 0; i < data.length; i++) {
                    //var date = parseDate(data[i][0]);
                    if (data[i] != null) {
                        var date = data[i][0];
                        var open = data[i][1];
                        var min = data[i][4];
                        var max = data[i][3];
                        var close = data[i][2];
                        saudipreviousDayClosePrice = data[i][7];
                        saudiminValue = data[i][5];
                        saudimaxValue = data[i][6];
                        category = data[i][8];
                        marketChartData.push({
                            date: date,
                            open: open,
                            close: close,
                            max: max,
                            min: min,
                            category: category,
                        });
                    }
                }
                DrawStockChart(marketID, divID);
            }
        });
    }
    function zoomChart() {
        saudiMarketchart.zoomToIndexes(-01, marketChartData.length - 1);
    }
</script>

