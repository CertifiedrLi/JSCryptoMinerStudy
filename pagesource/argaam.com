        var maxValue;
        var minValue;

        function generateCommodityChartData(chartData, days, callback) {

            var url2 = "/home/getcommoditychartdata?commodityid=" + 1;
        
            if (days != "")
            {
                url2 = url2 + "&days=" + days;
            }

            chartData.length = 0;

            var jqhx = $.ajax({
                type: "GET",
                url: url2,
                cache: false,
                async: true
            })
            .done(function (data) {
                count = data.length;
                for (var i = 0; i < data.length; i++) {
                    if (data[i] != null)
                    {
                        var date = data[i][0];
                        var open = data[i][1];
                        var min = data[i][4];
                        var max = data[i][3];
                        var close = data[i][2];
                        var category = data[i][5];
                        chartData.push({
                            date: date,
                            open: open,
                            close: close,
                            max: max,
                            min: min,
                            category: AmCharts.stringToDate(category, "DD-MM-YYYY")
                        });
                    }
                }
                callback();
            });
        }

        function generateCommodityChartDataForIntraDay(chartData, days, callback) {        

            var url1 = "/home/getintradaycommoditychartdata?commodityID=" + 1;

            chartData.length = 0;

            var jqhx = $.ajax({
                type: "GET",
                url: url1,
                cache: false,
                async: true
            })
            .done(function (data) {
                count = data.length;
                for (var i = 0; i < data.length; i++) {
                    if (data[i] != null)
                    {
                        //var date = parseDate(data[i][0]);
                        var date = data[i][0];
                        var open = data[i][1];
                        var close = data[i][2];
                        var max = data[i][3];
                        var min = data[i][4];
                        minValue = data[i][5];
                        maxValue = data[i][6];
                        previousDayClosePrice = data[i][7];
                        var category = data[i][8];
                        chartData.push({
                            date: date,
                            open: open,
                            close: close,
                            max: max,
                            min: min,
                            category: category
                        });
                    }
                }
                console.log(chartData);
                callback();
            });
        }

        function DrawCommodityChart(o, days) {
            generateCommodityChartData(commodityChartData, days, function() {
                commodityChart = new AmCharts.AmSerialChart();
                commodityChart.pathToImages = "/Scripts/amcharts/images/";
                commodityChart.dataProvider = commodityChartData;
                commodityChart.categoryField = "category";
                commodityChart.dateFormats = [{ period: 'fff', format: 'MMM DD' }, { period: 'ss', format: 'MMM DD' }, { period: 'mm', format: 'MMM DD' }, { period: 'hh', format: 'MMM DD' }, { period: 'DD', format: 'MMM DD' }, { period: 'WW', format: 'MMM DD' }, { period: 'MM', format: 'MMM' }, { period: 'YYYY', format: 'YYYY' }];
                var categoryAxis = commodityChart.categoryAxis;
                categoryAxis.parseDates = true;
                categoryAxis.minPeriod = "mm";
                categoryAxis.showLastLabel = true;
                categoryAxis.showFirstLabel = true;
                categoryAxis.equalSpacing = true;
                categoryAxis.gridPosition = "start";
                categoryAxis.dateFormats = [{ period: 'fff', format: 'MMM DD' }, { period: 'ss', format: 'MMM DD' }, { period: 'mm', format: 'MMM DD' }, { period: 'hh', format: 'MMM DD' }, { period: 'DD', format: 'MMM DD' }, { period: 'WW', format: 'MMM DD' }, { period: 'MM', format: 'MMM' }, { period: 'YYYY', format: 'YYYY' }];
                categoryAxis.axisColor = "#DADADA";
                categoryAxis.autoGridCount = false;
                categoryAxis.gridCount = 8;
                categoryAxis.markPeriodChange = false;

                var valueAxis = new AmCharts.ValueAxis();
                valueAxis.position = "right";
                commodityChart.addValueAxis(valueAxis);

                var graph = new AmCharts.AmGraph();
                graph.type = "line";
                graph.title = "Market";
                graph.valueField = "close";
                //graph.lineAlpha = 4;
                graph.lineColor = "#1F588A";
                graph.lineThickness = 2;
                //graph.fillColors = ["#35628D", "#A7D2EE"];
                graph.fillColors = ["#D6E7F2"];
                graph.fillAlphas = 0.7;
                graph.pointPosition = "start";
                graph.connect = true;
                graph.showBalloon = false;
                commodityChart.addGraph(graph);

                var chartCursor = new AmCharts.ChartCursor();
                chartCursor.cursorPosition = "mouse";
                chartCursor.categoryBalloonDateFormat = "DD-MMM-YYYY";
                chartCursor.bulletsEnabled = true;
                chartCursor.bulletSize = 8;
                chartCursor.categoryBalloonEnabled = false;
                chartCursor.cursorAlpha = 0;
                commodityChart.addChartCursor(chartCursor);

                var legend = new AmCharts.AmLegend();
                legend.position = "top";
                legend.top = 1;
                legend.markerSize = 2;
                legend.labelText = "";
                legend.markerType = "none";
                legend.markerLabelGap = 1;
                legend.valueText = "Close: [[value]] Date: [[category]]";
                legend.verticalGap = 0;
                commodityChart.addLegend(legend);
                commodityChart.write("commodityChartDiv");
                $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "https://www.argaamplus.com").html("").css("top", "0px");
                SetChartTabActiveInActive(o, days);
            });        
        }

        function DrawCommodityChartForIntraDay(o, days) {
            generateCommodityChartDataForIntraDay(commodityChartDataForDay, days, function () {
                if (commodityChart != null)
                    commodityChart.clear();      

                commodityChart = new AmCharts.AmSerialChart();
                commodityChart.pathToImages = "/Scripts/amcharts/images/";
                commodityChart.dataProvider = commodityChartDataForDay;
                commodityChart.categoryField = "date";

                var categoryAxis = commodityChart.categoryAxis;
                categoryAxis.parseDates = true;
                categoryAxis.minPeriod = "mm";
                categoryAxis.showLastLabel = true;
                categoryAxis.showFirstLabel = true;
                categoryAxis.equalSpacing = true;
                categoryAxis.gridPosition = "start";
                categoryAxis.dateFormats = [{ period: 'fff', format: 'JJ:NN:SS' }, { period: 'ss', format: 'JJ:NN:SS' }, { period: 'mm', format: 'JJ:NN' }, { period: 'hh', format: 'JJ:NN' }, { period: 'DD', format: 'MMM DD' }, { period: 'WW', format: 'MMM DD' }, { period: 'MM', format: 'MMM' }, { period: 'YYYY', format: 'YYYY' }];
                categoryAxis.axisColor = "#DADADA";
                categoryAxis.autoGridCount = false;
                categoryAxis.gridCount = 8;
                categoryAxis.markPeriodChange = false;

                var valueAxis = new AmCharts.ValueAxis();
                valueAxis.position = "right";
                valueAxis.minimum = minValue;
                valueAxis.maximum = maxValue;
                commodityChart.addValueAxis(valueAxis);

                // Let's add a guide
                var guide1 = new AmCharts.Guide();
                guide1.date = new Date(2014, 1, 30, 04, 55);
                guide1.lineColor = "red";
                guide1.lineAlpha = 2;
                guide1.parseDate = false;
                guide1.dashLength = 4;
                guide1.value = previousDayClosePrice;
                guide1.label = previousDayClosePrice;
                guide1.inside = true;
                guide1.labelRotation = 180;
                guide1.position = "left";
                valueAxis.addGuide(guide1);

                //chart.addChartScrollbar(chartScrollbar);

                var graph = new AmCharts.AmGraph();
                graph.type = "line";
                graph.title = "Market";
                graph.valueField = "close";
                //graph.lineAlpha = 4;
                graph.lineColor = "#98B2CA";
                graph.lineThickness = 1.5;
                //graph.fillColors = ["#35628D", "#A7D2EE"];
                graph.pointPosition = "start";
                graph.connect = true;
                graph.showBalloon = false;
                commodityChart.addGraph(graph);

                var chartCursor = new AmCharts.ChartCursor();
                chartCursor.cursorPosition = "mouse";
                chartCursor.categoryBalloonDateFormat = "JJ:NN";
                chartCursor.bulletsEnabled = true;
                chartCursor.bulletSize = 8;
                chartCursor.categoryBalloonEnabled = false;
                chartCursor.cursorAlpha = 0;
                commodityChart.addChartCursor(chartCursor);

                var legend = new AmCharts.AmLegend();
                legend.position = "top";
                legend.top = 1;
                legend.markerSize = 2;
                legend.labelText = "";
                legend.markerType = "none";
                legend.markerLabelGap = 1;
                legend.valueText = "Close: [[value]] Time: [[category]]";
                legend.verticalGap = 0;
                commodityChart.addLegend(legend);
                commodityChart.write("commodityChartDiv");
                $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "https://www.argaamplus.com").html("").css("top", "0px");
                SetChartTabActiveInActive(o, days);
            });
        }

        function DrawCommodityChartData(o, days)
        {
            $('#hdnCommChartDayVal').val(days);
            if (days > 0 && days <= 5)
            {
                DrawCommodityChartForIntraDay(o, days);
            }
            else
