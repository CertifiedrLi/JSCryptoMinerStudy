<script type="text/javascript">
    var saudiMarketchart;
    var marketChartData = [];
    var saudiCount;
    var saudipreviousDayClosePrice;
    var saudimaxValue;
    var saudiminValue;
    var marketID;
    var mc_timeout = 1000;
    function parseDate(dateString) {
        var dt = dateString.split(" ");
        var date = dt[0];
        var time = dt[1];

        var dateArray = date.split("/");
        var timeArray = time.split(":");

        var date = new Date(Number(dateArray[2]), Number(dateArray[1]), Number(dateArray[0]), Number(timeArray[0]), Number(timeArray[1]));
        return date;
    }
    $(document).ready(function () {
        MarketChartsTab();
        $("[id^='saudi_chart_market_id'].active").click();

    });
    function DrawStockChart(marketID, divID) {
        $("[smc-chart-id='" + divID + "']").show();
        setTimeout(function () {
            //$(".market-chart-wrapper").css("display", "none").slideUp();
            $(".market-chart-wrapper").each(function (i, itm) {
                if ($(itm).attr('chart-id') != 'chartDiv3' && $(itm).attr('chart-id') != 'chartDiv14')
                    $(itm).css("display", "none");
            });

            saudiMarketchart = new AmCharts.AmSerialChart();
            saudiMarketchart.pathToImages = "/Scripts/amcharts/images/";
            saudiMarketchart.dataProvider = marketChartData;
            saudiMarketchart.categoryField = "date";
            saudiMarketchart.addListener("dataUpdated", zoomChart);

            var categoryAxis = saudiMarketchart.categoryAxis;
            categoryAxis.parseDates = true;
            categoryAxis.minPeriod = "mm";
            categoryAxis.showLastLabel = true;
            categoryAxis.showFirstLabel = true;
            categoryAxis.equalSpacing = true;
            categoryAxis.gridPosition = "start";
            categoryAxis.dateFormats = [{ period: 'fff', format: 'JJ:NN:SS' }, { period: 'ss', format: 'JJ:NN:SS' }, { period: 'mm', format: 'JJ:NN' }, { period: 'hh', format: 'JJ:NN' }, { period: 'DD', format: 'MMM DD' }, { period: 'WW', format: 'MMM DD' }, { period: 'MM', format: 'MMM' }, { period: 'YYYY', format: 'YYYY' }];
            categoryAxis.axisColor = "#fff";
            categoryAxis.autoGridCount = false;
            if (marketID == 1) {
                categoryAxis.gridCount = 6;
            }
            else if (marketID == 3) {
                categoryAxis.gridCount = 8;
            }
            else if (marketID == 14) {
                categoryAxis.gridCount = 8;
            }
            else if (marketID == 4) {
                categoryAxis.gridCount = 6;
            }
            else if (marketID == 5) {
                categoryAxis.gridCount = 6;
            }
            else if (marketID == 6) {
                categoryAxis.minPeriod = "15mm";
                categoryAxis.gridCount = 4;
            }
            else if (marketID == 13) {
                categoryAxis.minPeriod = "15mm";
                categoryAxis.gridCount = 8;
                categoryAxis.equalSpacing = false;
            }
            else if (marketID == 7) {
                categoryAxis.minPeriod = "15mm";
                categoryAxis.gridCount = 10;
                categoryAxis.equalSpacing = false;
            }
            else if (marketID == 2) {
                categoryAxis.minPeriod = "15mm";
                categoryAxis.gridCount = 8;
                categoryAxis.equalSpacing = false;
            }

            categoryAxis.markPeriodChange = false;

            var valueAxis = new AmCharts.ValueAxis();
            valueAxis.position = "right";
            valueAxis.minimum = saudiminValue;
            valueAxis.maximum = saudimaxValue;
            saudiMarketchart.addValueAxis(valueAxis);

            // Let's add a guide
            var guide1 = new AmCharts.Guide();
            guide1.date = new Date(2014, 1, 30, 04, 55);
            guide1.lineColor = "red";
            guide1.lineAlpha = 2;
            guide1.parseDate = false;
            guide1.dashLength = 4;
            guide1.value = saudipreviousDayClosePrice;
            guide1.label = saudipreviousDayClosePrice;
            guide1.inside = true;
            guide1.labelRotation = 180;
            guide1.position = "left";
            valueAxis.addGuide(guide1);

            //chart.addChartScrollbar(chartScrollbar);

            var graph = new AmCharts.AmGraph();
            graph.type = "line";
            graph.title = "Market";
            graph.valueField = "close";
            //graph.lineAlpha = 4;
            graph.lineColor = "#1F588A";
            graph.lineThickness = 2;
            //graph.fillColors = ["#35628D", "#A7D2EE"];
            graph.fillColors = ["#D6E7F2"];
            graph.fillAlphas = 0.7;
            graph.pointPosition = "start";
            graph.connect = true;
            graph.showBalloon = false;
            saudiMarketchart.addGraph(graph);

            var chartCursor = new AmCharts.ChartCursor();
            chartCursor.cursorPosition = "mouse";
            chartCursor.categoryBalloonDateFormat = "JJ:NN";
            chartCursor.bulletsEnabled = true;
            chartCursor.bulletSize = 8;
            chartCursor.categoryBalloonEnabled = false;
            chartCursor.cursorAlpha = 0;
            //chartCursor.zoomable = false;
            saudiMarketchart.addChartCursor(chartCursor);

            var legend = new AmCharts.AmLegend();
            legend.position = "top";
            legend.top = 1;
            legend.markerSize = 2;
            legend.labelText = "";
            legend.markerType = "none";
            legend.markerLabelGap = 1;
            legend.valueText = "Close: [[value]] Time: [[category]]";
            legend.verticalGap = 0;
            saudiMarketchart.addLegend(legend);
            saudiMarketchart.write(divID);
            $("#" + divID).show();
            $("[chart-id='" + divID + "']").css("display", "block").slideDown(1000);
            $("#activeMarketID").val(marketID);
            $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "https://www.argaamplus.com").html("").css("top", "0px");
            $("[smc-chart-id='" + divID + "']").hide();
            // set up the chart to update every second
            setInterval(function () {
                var activeMarketID = $("#activeMarketID").val();
                var url = $("#marketChartDataURL").val() + "?marketid=" + activeMarketID;
                //chart.dataProvider.shift();
                var jqhx = $.ajax({
                    type: "GET",
                    url: url,
                    async: true,
                    cache:false
                })
            .done(function (data) {
                marketChartData = [];
                if (data != null) {
                    for (var i = 0; i < data.length; i++) {
                        if (data[i] != null) {
                            var date = data[i][0];
                            var open = data[i][1];
                            var min = data[i][4];
                            var max = data[i][3];
                            var close = data[i][2];
                            saudipreviousDayClosePrice = data[i][7];
                            var category = data[i][8];
                            saudiminValue = data[i][5];
                            saudimaxValue = data[i][6];
                            marketChartData.push({
                                date: date,
                                open: open,
                                close: close,
                                max: max,
                                min: min,
                                category: category,
                            });
                        }
                    }
                }
                var valueAxis = new AmCharts.ValueAxis();
                valueAxis.position = "right";
                valueAxis.minimum = saudiminValue;
                valueAxis.maximum = saudimaxValue;
                saudiMarketchart.addValueAxis(valueAxis);
                saudiMarketchart.dataProvider = marketChartData;
                saudiMarketchart.validateData();
                $("[href='https://www.amcharts.com/javascript-charts/']").attr("href", "http://www.argaam.com").html("").css("top", "0px");
                //}
            });
            }, 60000);
        }, mc_timeout);
        mc_timeout = 1000;
    }

    setInterval(function () {
        //var activeMarketID = $("#activeMarketID").val();
        //chart.dataProvider.shift();
        var url = $("#marketChartDataURL").val() + "?marketid=" + marketID;
        var jqhx = $.ajax({
            type: "GET",
            url: url,
            async: true,
            cache:false
        })

            .done(function (data) {

                marketChartData = [];
                if (data != null) {
                    for (var i = 0; i < data.length; i++) {

                        if (data[i] != null) {
                            var date = data[i][0];
                            var open = data[i][1];
                            var min = data[i][4];
                            var max = data[i][3];
                            var close = data[i][2];
                            saudipreviousDayClosePrice = data[i][7];
                            var category = data[i][8];
                            saudiminValue = data[i][5];
