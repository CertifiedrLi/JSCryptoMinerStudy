    function RedirectS3URLToAnotherPage(fileFormat, lang) {
        
        var urls = [];
        var S3BaseURL = '/argaamplus.s3.amazonaws.com/';
        var PDFpageURL = '/'+'ar'; 

        $("#financialCompanyPDFForhomepagediv a").each(function () {
            
            var url = $(this).attr("href");
             if (url != undefined && url.includes(S3BaseURL) && url.includes(fileFormat) && lang == 'ar') {
                var marketName='tasi';
                var companyName = $(this).attr("relf-companyName");
                var fiscalPeriod = $(this).attr("relf-quarter");
                var year = $(this).attr("relf-year");
                urls.push({
                    url: $(this).attr("href"),
                    lang: $(this).text(),
                    urlsplit: url.split("/"),
                    replacedURL: this.href.replace(url, PDFpageURL + "/" + url.split("/")[3])
                });
                this.href = PDFpageURL + "/" +marketName+ '/' + companyName.toLowerCase() + '/financial-report' + '/' +year +'/'+ fiscalPeriod + '/' + url.split("/")[3];
            }
        })
    }

</script>

