                  "precision": 2,
                  "min" : 0,
                  "max" : 20,
                  "increment" : 0.01
                },
                {
                  "precision": 2,
                  "min" : 20,
                  "max" : 50,
                  "increment" : 0.05
                }]
            };
            pbjs.setPriceGranularity(customGranularity);
            // Setup Prebid to send all bids to Google, instead of just the highest bidder
            // TODO: WILL BE DEPRECATED IN v1.0 and need to be passed in setConfig
            pbjs.enableSendAllBids();
        });
    } // eo prebid installation

    window.DFP_adTargets.beta = "true";window.DFP_adTargets.dnLoggedIn = "false";
    window.DFP_adTargetsParam = "beta%3Dtrue%26dnLoggedIn%3Dfalse";
    // Lets configure our ad slot paramaters for page load ads, to support roadblock ads
    // Back in the day, ads were all loaded on the fly, these days first page load ads are configured in the <head>
    // then all requested in a single google ad request so that ad dudes can target all ads for one campaign (called roadblocks)

    // Same ads on all pages
    var prebuilds = {
        dfpTop:{
            sizes:[
                "[[992, 480], [[728,90],[970,30],[970,90],[970,250]]]",
                "[[0, 0], [[320,50]]]"
            ],
            position: "Top",
            outOfPage: false,
            bid: true
        },
        dfpDMN1x1:{
            sizes:[
                "[[0, 0], [[1,1]]]"
            ],
            position: "DMN1x1",
            outOfPage: false,
            bid: false
        },
        dfpPosition1:{
            sizes:[
                "[[992, 480], [[300,250],[300,600]]]",
                "[[0, 0], [[320,50],[300,250]]]"
            ],
            position: "Position1",
            outOfPage: false,
            bid: true,
            additionalBids: {
                desktop: [
                    {
                        bidder: "criteo",
                        params: { zoneId: '799740' }
                    }
                ],
                mobile:[
                    {
                        bidder: "criteo",
                        params: { zoneId: '799741' }
                    }
                ]
            }
        },
        dfpPosition2:{
            sizes:[
                "[[992, 480], [[300,250]]]",
                "[[0, 0], [[300,250]]]"
            ],
            position: "Position2",
            outOfPage: false,
            bid: true
        },
        dfpAdhesion:{
            sizes:[
                "[[0, 0], [[320,50]]]"
            ],
            position: "adhesion",
            outOfPage: false,
            bid: false
        },
        dfpOutOfPage:{
            sizes:[],
            position: "DMNOOP",
            outOfPage: true,
            bid: false
        },
        dfpPosition3: {
            sizes:[
                "[[992, 480], [[300,250]]]",
                "[[0, 0], [[300,250]]]"
            ],
            position: "Position3",
            outOfPage: false,
            bid: true
        }
    };

        // Ad definitions for the front page
    prebuilds.dfpTop2 = {
            sizes:[
                "[[992, 480], [[728,90],[970,30],[970,90],[970,250]]]",
                "[[0, 0], [[320,50],[300,250]]]"
            ],
            position: "Top2",
            outOfPage: false,
            bid: true
        };
        // eo primary Ad Configurations we use for Google and Prebid


    // Prebid Slots, defined first so we can setup some targeting data for the ad
    if(window.DallasNews.settings.ADS_HEADER_BIDDING){
        for(var slot in prebuilds){
            var prebuild = prebuilds[slot];

            // Skip ads we are not bidding for
            if (!prebuild.bid) {
                continue;
            }

            // Skip ads without bids
            var pos = prebuild["position"];
            if (!(window.useMobileAds ? window.mobileBids[pos] : window.desktopBids[pos])) {
                continue;
            }

            var size = getPrebuildSizeForWindow(prebuild, window.innerWidth);
            size = size[1];

            // We can have various sizes of ads depending on window width
            // So we need to determine the appropriate size Google will be rendering
            var bidSlot = {
                code: slot,
                sizes: size,
                bids: window.useMobileAds ? window.mobileBids[pos] : window.desktopBids[pos]
            }

            // If the ad definition for this page included additional bidder stick them also in the list of normal bidders
            if ('additionalBids' in prebuild  && 'mobile' in prebuild.additionalBids) {
                for (var mb in prebuild.additionalBids.mobile) {
                    bidSlot.bids.unshift(prebuild.additionalBids.mobile[mb]);
                }
            }
            if ('additionalBids' in prebuild  && 'desktop' in prebuild.additionalBids) {
                for (var db in prebuild.additionalBids.desktop) {
                    bidSlot.bids.unshift(prebuild.additionalBids.desktop[db]);
                }
            }
            
            // Add this ad unit to the prebid auction list (which will be requested after the ads are prepped in integration)
            window.bidSlots.push(bidSlot);
        }
    } // eo prebid slot building for ads

    // Google DFP Ad, setup second, so if we are using prebid we can extend with prebid targeting data (once the cmd queue is processed)
    googletag.cmd.push(function () {

        // Build our slots
        for(var slot in prebuilds){

            var prebuild = prebuilds[slot];
            var dfpSlot = null;

            if (prebuild.outOfPage) {
                // Special ad type that is out of page
                dfpSlot = googletag.defineOutOfPageSlot('/' + DFP_networkId + '/' + DFP_sitepage, slot);
                dfpSlot.addService(googletag.pubads());
            } else {
                // Normal ads that appear in page with sizes
                var defaultSize = JSON.parse(prebuild.sizes[0]);

                // Iterate our sizes and add them
                var dfpSizeMappingBuilder = googletag.sizeMapping();
                for (var k = 0; k < prebuild.sizes.length; k++) {
                    var size = JSON.parse(prebuild.sizes[k]);
                    dfpSizeMappingBuilder.addSize(size[0], size[1]);
                }
                var dfpSizeMappingArray = dfpSizeMappingBuilder.build();

                // Build the slot
                dfpSlot = googletag.defineSlot('/' + DFP_networkId + '/' + DFP_sitepage, defaultSize[1], slot);
                dfpSlot.defineSizeMapping(dfpSizeMappingArray);
                dfpSlot.addService(googletag.pubads());
            }

            // Slot targetting
            dfpSlot.setTargeting("position", prebuild.position).
                                setTargeting("pageType", "home").
                                setCollapseEmptyDiv(true);
                if (window.DFP_adTargets) {
                    for (let key in window.DFP_adTargets) {
                        dfpSlot.setTargeting(key, window.DFP_adTargets[key])
                    }
                }
            dfpSlot.addService(googletag.pubads());

            // Window variables for our slot
            window[slot] = dfpSlot;
        }

        // Now that slots are built, if prebid was completed we have bid data to attach to the slots, before its sent off
        if (window.DallasNews.settings.ADS_HEADER_BIDDING) {
            pbjs.setTargetingForGPTAsync();
        }

        // Finalize Google Services
        // Make sure we load ads asynchronously so we dont block the page
        googletag.pubads().enableAsyncRendering();
        // Enable single request mode so all ads are fetched in one call, to support roadblock ads
        // (roadblock ads is how they can render a specific set of ads in each slot, usually because someone wanted to buy all ad space on a page)
        googletag.pubads().enableSingleRequest();
        // This would normally prevent google from loading ads on page load (until refresh() was called), but it is commented out so ads will load on page load
        // It is re-enabled after the first load of ads for dynamic ads in the Ad.js class
        // googletag.pubads().disableInitialLoad();
        // Enable google services for all defined ad slots
        googletag.enableServices();
    }); // eo google cmd for building the ads


    // If we are loading ads with Prebid, set a fallback timeout to load them if prebid is slow
    // Otherwise always load the ad server asap
    if(window.DallasNews.settings.ADS_HEADER_BIDDING){
        setTimeout(function () { 
            initAdserver();
        }, window.PREBID_TIMEOUT);

        runPrebid();
    }
    else{
        initAdserver();
    }
</script>
