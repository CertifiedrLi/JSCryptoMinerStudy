<script>
var timelineObj, timelineTag, idn;
function loadFall(){
	timelineObj.showloading();
	$.ajax({
		url : 'xmlTimeline.asp?type='+timelineTag+'&ipage='+(++idn),
		dataType : 'xml',
		success : function( rspXML ){
			var formatgastr = '';
            formatgastr = location.href.toLowerCase();
            formatgastr = formatgastr.replace('http://www.vogue.com.tw/', '');
            if(window.ga){
                window.ga('send', 'pageview',{ 'page': '' + formatgastr + '#readmore' });
            }else{
                if(window.pageTracker){
                    window.pageTracker._trackPageview(formatgastr + '#readmore');
                }
            }
			timelineObj.appendData( rspXML );
		},
		error : function(){
			//alert('error');
		}
	});
}
$(document).ready(function(){

	$('#bannerSlide').bannerSlide();
	$('#searchform').bindSearch();
	$('#v-timeline').find('div.v-widget-timeStamp').each(function(){$(this).timestamp();});
	$('#filter').doToogle("a",function(){
		timelineTag = $(this).attr('tag');
		idn = 0;
		timelineObj.clear();
	});

	/* 註冊元件 timeline(json templateName, bool donotClear) */
	timelineObj = $('#v-timeline').timeline({
		'default' : 'v-typeExpand',

		'clip' : 'v-typeClip',
		'left' : 'v-typeLeft',
		'right' : 'v-typeRight',
		'divider' : 'v-divider',
		'ad' : 'v-typeAd'
	}, true);
	timelineTag = $('#filter').children('a.selected').attr('tag');
	idn = 1;
	//loadFall();
});

</script>
