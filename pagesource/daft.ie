                    <script type="text/ng-template" id="frontend/home/templates/searchblock.html">
                    <div class="search-section" ng-class="{takeoverpadding: takeover.active}">
    <div class="container">
        <div class="row" ng-if="takeover.active !== true" ng-hide="hero.hide_search_text">
            <div class="col-xs-12">
                <h1 class="tagline">
                    <strong>Search Ireland&#8217;s No. 1 property website</strong>
                </h1>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <div class="search-block">
                    <div class="heading-box">
                        <ul ng-show="headingBox">
                            <li class="separator-blank"></li>
                            <li ng-class="{active: searchData.adType=='sale'}">
                                <a
                                    ng-click="updateType('sale')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="sale"
                                >For Sale</a>
                            </li>
                            <li class="separator"></li>
                            <li ng-class="{active: searchData.adType=='rental'}">
                                <a
                                    ng-click="updateType('rental')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="rental"
                                >To Rent</a>
                            </li>
                            <li class="separator"></li>
                            <li ng-class="{active: searchData.adType=='sharing'}">
                                <a
                                    ng-click="updateType('sharing')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="sharing"
                                >Sharing</a>
                            </li>
                            <li class="separator"></li>
                            <li ng-class="{active: searchData.adType=='commercial'}">
                                <a
                                    ng-click="updateType('commercial')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="commercial"
                                >Commercial</a>
                            </li>
                            <li class="separator"></li>
                            <li>
                                <a
                                    ng-click="switchHeading()"
                                    class="no-select last"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="more"
                                    >More</a>
                            </li>
                            <li class="separator-blank"></li>
                        </ul>
                        <ul ng-hide="headingBox" class="ng-hide">
                            <li class="separator-blank"></li>
                            <li ng-class="{active: searchData.adType=='shortterm'}">
                                <a
                                    ng-click="updateType('shortterm')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="shortterm"
                                >Short Term Lets</a>
                            </li>
                            <li class="separator"></li>
                            <li ng-class="{active: searchData.adType=='holiday_homes'}">
                                <a
                                    ng-click="updateType('holiday_homes')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="holiday_homes"
                                >Holiday Homes</a>
                            </li>
                            <li class="separator"></li>
                            <li ng-class="{active: searchData.adType=='new_developments'}">
                                <a
                                    ng-click="updateType('new_developments')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="new_developments"
                                >New Homes</a>
                            </li>
                            <li class="separator"></li>
                            <li ng-class="{active: searchData.adType=='international_sale'}">
                                <a
                                    ng-click="updateType('international_sale')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="international_sale"
                                >Overseas</a>
                            </li>
                            <li class="separator"></li>
                            <li ng-class="{active: searchData.adType=='parking'}">
                                <a
                                    ng-click="updateType('parking')"
                                    class="no-select"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="parking"
                                >Parking Spaces</a>
                            </li>
                            <li class="separator"></li>
                            <li>
                                <a
                                    ng-click="switchHeading()"
                                    class="no-select last"
                                    analytics-on="click"
                                    analytics-category="HomepageSearchBlock"
                                    analytics-label="back"
                                >Back</a>
                            </li>
                            <li class="separator-blank"></li>
                        </ul>
                    </div>
                    <div class="search-box">
                        <form class="search-form clearfix">
                            <div class="cell">
                                <div class="select-holder">
                                    <select
                                        id="selectCityCountry"
                                        ng-model="searchData.cityCountyContinent"
                                        ng-change="updateCityCountySubscribers()"
                                        class="search-select ng-hide"
                                        >
                                        <option selected="selected" value="">{{cityCountyContinent.heading}}</option>
                                        <option ng-if="cityCountyContinent.cities.length > 0" ng-repeat="city in cityCountyContinent.cities" value="{{city.id}}">{{city.name}}</option>
                                        <option ng-if="cityCountyContinent.counties.length > 0 && cityCountyContinent.cities.length > 0" disabled="disabled">----------</option>
                                        <option ng-if="cityCountyContinent.counties.length > 0" ng-repeat="county in cityCountyContinent.counties" value="{{county.id}}">{{county.name}}</option>
                                        <option ng-if="cityCountyContinent.continents.length > 0" ng-repeat="continent in cityCountyContinent.continents" value="{{continent.id}}">{{continent.name}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="cell">
                                <div id="choose-an-area" class="choose-an-area">
                                    <a href="#" class="opener-link disabled">{{ areas.heading }}</a>
                                    <div class="drop-block js-slide-hidden">
                                        <div class="holder jcf-scrollable">
                                            <div class="column" ng-repeat="column in areas">
                                                <div class="checkbox-holder" ng-repeat="area in column">
                                                    <input ng-model="searchData.areas[area.id]" ng-true-value="{{area.name}}" ng-change="updateAreasHeading()" id="{{area.id}}" type="checkbox">
                                                    <span class="text">
                                                        <label ng-if="false !== area.count" for="{{area.id}}">{{area.name}} [{{area.count}}]</label>
                                                        <label ng-if="false === area.count" for="{{area.id}}">{{area.name}}</label>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="column" ng-show="!areas.length">
                                                <div class="checkbox-holder">
                                                    No properties available in this area.
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="cell" ng-hide="searchData.adType === 'sale'">
                                <div class="select-holder">
                                    <select
                                        id="selectPropertyType"
                                        ng-model="searchData.propertyType"
                                        class="search-select ng-hide"
                                        ng-options="type.id as type.name for type in propertyTypes"
                                        >
                                    </select>
                                </div>
                            </div>
                            <div class="cell" ng-show="searchData.adType === 'sale'">
                                <div class="select-holder">
                                    <!-- Max price for sale only -->
                                    <select
                                        id="selectMaxPrice"
                                        ng-model="searchData.maxPrice"
                                        class="search-select ng-hide"
                                        >
                                        <option ng-repeat="price in priceRange" value="{{price.value}}" ng-bind-html="price.display">{{price.display}}</option>
                                    </select>
                                </div>
                            </div>
                            <button class="btn btn-primary btn-search" ng-class="{'btn-search-alternative': test === true}" ng-click="doSearch()">Search</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

            </script>
                    <script type="text/ng-template" id="frontend/home/templates/featured.html">
                    <section class="gallery-section">
    <div class="carousel-module" id="featured-carousel" ng-if="featured">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="carousel-module-mask">
                        <div class="carousel-module-slideset">

                            <div class="carousel-module-slide" ng-repeat="page in featuredPages">
                                <div class="carousel-module-holder">
                                    <div ng-repeat="ad in page">
                                        <div
                                            class="carousel-module-box featured-container {{ ad.type }}"
                                            ng-if="$parent.$index === 0 && $index === 0 && (ad.type == 'double' || ad.type == 'triple')"
                                            analytics-on="click"
                                            analytics-category="HomepageCarousel"
                                            analytics-label="{{ featured.source }}-{{ ($parent.$parent.$index * 6) + ($parent.$index + 1) }}-featured"
                                        >
                                            <div class="placeholder">
                                                <a href="{{ad.featured.url}}" target="_self">
                                                    <div class="featured-image">
                                                        <img ng-if="ad.type == 'double'" ng-src="{{ad.image}}" height="249" width="298" alt="">
                                                        <img ng-if="ad.type == 'triple'" ng-src="{{ad.image}}" height="249" width="634" alt="">
                                                    </div>
                                                    <div class="featured-info">
                                                        <h2>{{ad.featured.title}}</h2>
                                                        <span class="featured-address">{{ad.featured.subtitle}}</span>
                                                        <p>{{ad.featured.description}}</p>
                                                        <span class="featured-price">{{ad.featured.footer}}</span>
                                                        <img ng-if="ad.featured.logo" class="featured-logo" ng-src="{{ad.featured.logo}}" alt="{{ad.featured.title}}, {{ad.area}}, {{ad.county}}">
                                                    </div>
                                                    <div ng-if="ad.sticker" ng-style="ad.sticker_color && {'background-color': ad.sticker_color};" class="featured-sticker sticker">
                                                        {{ad.sticker}}
                                                    </div>
                                                </a>
                                            </div>
                                        </div>
                                        <div
                                            class="carousel-module-box"
                                            ng-if="!ad.featured"
                                            analytics-on="click"
                                            analytics-category="HomepageCarousel"
                                            analytics-label="{{ featured.source }}-{{ ($parent.$parent.$index * 6) + ($parent.$index + 1) }}"
                                        >
                                            <div class="placeholder">
                                                <a href="/{{ad.shortcode}}" target="_self">
                                                    <div ng-if="ad.sticker" class="sticker">{{ad.sticker}}</div>
                                                    <img ng-src="{{ad.image}}" height="249" width="298" alt="{{ad.area}}, {{ad.county}}">
                                                    <div class="footer">
                                                        <h2>{{ad.area}}</h2>
                                                        <div class="carousel-module-property">
                                                            <span ng-if="ad.price" class="price" title="{{ad.price}}">{{ad.price}}</span>
                                                            <strong class="title">{{ad.county}}</strong>
                                                        </div>
                                                    </div>
                                                </a>
                                            </div>
                                        </div>
