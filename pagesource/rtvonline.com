            var URL = 'http://www.rtvonline.com/search/google/?q='+keyword+'&cx='+encodeURIComponent('partner-pub-2223768610047094:4484609873')+'&cof='+encodeURIComponent('FORID:10')+'&ie=UTF-8&sa=Search';
            window.location.href = URL;
        }
    }    
	
