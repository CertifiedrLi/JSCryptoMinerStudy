    <script type="text/javascript">
        var MarketingData;
        var DAWideData34, DAWideData35;
        var DAWideData34LodingComplete = false, DAWideData35LodingComplete = false;   // WideData를 로드하면 1,, 그전까지는 로딩화면이 나오게 처리(0), 에러 발생시 -1

	    var getDAAdBanner = function (reqNo, elId, defaultImage) {
	        if (window.daAdController === undefined || daAdController === null) return;

	        var request = {
	            reqNo: reqNo,
	            pageType: daAdController.PageType.MAIN,
	            context: this,
	            successCb: function (banners) {
	                var adhtml = '', banner;

	                if (banners === undefined || banners === null || banners.length == 0) 
	                {
	                    if (defaultImage !== undefined )
	                    {
	                        adhtml += '<a href="http://promotion.auction.co.kr/promotion/md/eventview.aspx?txtMD=09265A46B5" target="_blank">';
	                        adhtml += '<img src="' + defaultImage + '" alt="배너광고" border:0;">';
	                        adhtml += '</a>';

	                        $(elId).html(adhtml);
	                    }
	                    return;
	                }

	                banner = banners[0];

	                adhtml += '<a href="' + banner.landingUrl + '" onclick="daAdController.sendAdLog(\'' + banner.clickUrl + '\');" target="_blank">';
	                adhtml += '<img src="' + banner.imageUrl + '" alt="' + banner.imageDesc + '" border:0;">';
	                adhtml += '</a>';
				    
	                adhtml += "<div class='ad_mark_wrap'>" +
								"<a href='#link_none' class='ad_btn'>광고배너 자세히보기</a>" +
								"<div class='ad_layer' style='z-index: 1; display: none;'>" +
									"<em></em>" +
									"<p>광고배너</p>" +
									"<a href='#' onclick=\"this.parentNode.style.display = 'none'; return false;\" class='ad_layer_close'>닫기</a>" +
								"</div>" +
							"</div>";

	                $(elId).html(adhtml);

	                if ($(elId).length > 0)
	                    daAdController.sendAdLog(banner.impUrl);                                           
	            },
	            errorCb: function (err) {
	                var adhtml = '';

	                if (defaultImage !== undefined) {
	                    adhtml += '<a href="http://promotion.auction.co.kr/promotion/md/eventview.aspx?txtMD=09265A46B5" target="_blank">';
	                    adhtml += '<img src="' + defaultImage + '" alt="배너광고" border:0;">';
	                    adhtml += '</a>';

	                    $(elId).html(adhtml);
	                }
	            }
	        };

	        daAdController.getDABanner(request);
	    };

	    getDAAdBanner(9000000032, "#divBrandAd32", "http://image.auction.co.kr/DA/base/amain_02_80240.jpg");
	    getDAAdBanner(9000000038, "#divBrandAd38", "http://image.auction.co.kr/DA/base/amain_03_80141.jpg");
	    getDAAdBanner(9000000036, "#divBrandAd36");
	    getDAAdBanner(9000000037, "#divBrandAd37", "http://image.auction.co.kr/DA/base/amain_07_155237.jpg");

	    var getDABanners = function (reqNo) {
	        if (window.daAdController === undefined || daAdController === null) return;

	        var request = {
	            reqNo: reqNo,
	            pageType: daAdController.PageType.MAIN,
	            context: this,
	            successCb: function (banners) {
	                if (banners === undefined || banners === null || banners.length == 0) {
	                    return;
	                }

	                if (reqNo == 9000000028) {
                        // 홈 브랜딩 처리
	                    MarketingData = banners;

	                    try {
	                        Home.DrawCorner('marketingbanner');
	                    } catch (ex) { SetSlotErrorLog("Home.DrawCorner('marketingbanner'", ex); }
	                } else if (reqNo == 9000000034) {
	                    
	                    // DAWide34 처리
	                    DAWideData34 = banners;
	                    DAWideData34LodingComplete = 1;
	                    WideBannerBiz.refreshAdBannerImage(5);
	                } else if (reqNo == 9000000035) {
	                    
	                    // DAWide35 처리
	                    DAWideData35 = banners;
	                    DAWideData35LodingComplete = 1;
	                    WideBannerBiz.refreshAdBannerImage(6);
	                }
	            },
	            errorCb: function (err) {
	                if (reqNo == 9000000028) {
	                    MarketingData = null;

	                    try {
	                        Home.DrawCorner('marketingbanner');
	                    } catch (ex) { SetSlotErrorLog("Home.DrawCorner('marketingbanner'", ex); }
	                } else if (reqNo == 9000000034) {
	                    DAWideData34LodingComplete = -1;
	                    WideBannerBiz.refreshAdBannerImage(5);
	                } else if (reqNo == 9000000035) {
	                    DAWideData35LodingComplete = -1;
	                    WideBannerBiz.refreshAdBannerImage(6);
	                }
	            }
	        };

	        daAdController.getDABanners(request);
	    };

	    if (typeof Home != "undefined") {
	        getDABanners(9000000028);
	    }

	    getDABanners(9000000034);
	    getDABanners(9000000035);
        
    </script>
    
