	<script id="experiencia_social_tmpl" type="text/template">
    <div class="<%= expSocialClass %>">
        <div class="reacoes-icones">
            <a class="tooltip-abre">
                <% var cont = 0 %>
                <% under.each(reacoes.lista, function(value) { %>
                    <% if(cont < 3) { %>        
                        <%= _.template($("#" + value.template).html())(value) %> 
                        <%  cont = cont + 1; %>    
                    <% } %>    
                <% }); %>
            </a>
            <% if(typeof reacoes.total !== "undefined" && reacoes.total > 0) { %><span class="nr-interacoes"><%= reacoes.total %></span><% } %>
            <%= _.template($("#tooltip_emojis_base_tmpl").html())(reacoes) %> 
        </div>
        
        <% if(expSocialClass !== "com-reacoes-sem-amigos") { %>
        <ul class="reacoes-amigos">
            <% if(typeof amigos !== "undefined" && expSocialClass !== "usuario-nao-logado") { %>
                <% under.each(amigos.lista, function(value) { %>
                    <li>
                        <span>
                            <img src="<%= value.foto %>" title="<%= value.nome_exibicao %>">
                        </span>
                         <% if(value.reacao !== false) { %>
                             <%= _.template($("#" + value.template_reacao).html())() %> 
                         <% } %>
                    </li>
                <% }); %>
                <% if(typeof amigos.total != "undefined" && amigos.total > amigos.limit) { %>   
                    <li class="saiba-mais">
                        <a href="" class="bt-abre-modal" title="Mais amigos"><span>Mais amigos</span></a>
                    </li>
                <% } %>
            <% } %>
            <% if(expSocialClass === "usuario-nao-logado") { %>
                <li>
                    <span>
                        <img src="/public/_imagens/icone-experiencia-social-usuario-01.png">
                    </span>
                </li>
                <li>
                    <span>
                        <img src="/public/_imagens/icone-experiencia-social-usuario-02.png">
                    </span>
                </li>
                <li>
                    <span>
                        <img src="/public/_imagens/icone-experiencia-social-usuario-03.png">
                    </span>
                </li>
                <li>
                    <a class="bt-saiba-quem-leu" href="" title="Saiba quem dos seus amigos leu">Saiba quem dos seus amigos leu</a>
                </li>          
            <% } %>
        </ul>
        <% } %>
        
        
    </div>
</script>

