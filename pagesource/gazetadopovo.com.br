<script>
$(window).load(function(){

	var arrayGTMProds = [];
	var idxPos = 1;
	var pathUrl = window.location.pathname;
	var pathUrlySplit = pathUrl.split('/');

	$('section').each(function(){
		var attr = $(this).attr('data-secao');
		var clss = $(this).attr('class');
		
		// Carrega matérias diagramadas na capa
		if (attr == 'gp-home-conteudo' || attr == 'gp-bn-curitiba' || attr == 'gp-bn-parana' || attr == 'gp-bn-nacional') {
			
	    	if (attr == 'gp-bn-curitiba' || attr == 'gp-bn-parana' || attr == 'gp-bn-nacional') {
	    		
	    		var articlesGPH = $(this).children('article');
	    		
				articlesGPH.each(function(){
					var IDMateriaGTM = encodeIDGTM('http://www.gazetadopovo.com.br'+$(this).children('a').attr('href'));
					var dadosGTM = $(this).attr('data-gtm');
					var arraySplit = dadosGTM.split('|');
					var arrayVal = {'id':IDMateriaGTM, 'name':encodeURIComponent(arraySplit[0]), 'list':'Home Principal', 'position': idxPos};
					
					arrayGTMProds.push(arrayVal);
					$(this).children('a').attr('data-position', idxPos);
					idxPos = idxPos + 1;
				});
	    	
	    	} else {
	    		
	    		var DivArticlesGPH = $(this).children('div');

				DivArticlesGPH.each(function(){
					var articlesGPH = $(this).children('article');
	
					articlesGPH.each(function(){
						var IDMateriaGTM = encodeIDGTM('http://www.gazetadopovo.com.br'+$(this).children('a').attr('href'));
						var dadosGTM = $(this).attr('data-gtm');
						var arraySplit = dadosGTM.split('|');
						var arrayVal = {'id':IDMateriaGTM, 'name':encodeURIComponent(arraySplit[0]), 'list':'Home Principal', 'position': idxPos};
						
						arrayGTMProds.push(arrayVal);
						$(this).children('a').attr('data-position', idxPos);
						idxPos = idxPos + 1;
					});
		        });
	    	}

		// Carrega matérias diagramadas na editoria
		} else if(attr == 'gp-editoria-conteudo') {
			
			var DivArticlesGPH = $(this).children('div');

			console.log('Carregando dados das chamadas da editoria para GTM...');
			DivArticlesGPH.each(function(){
				var articlesGPH = $(this).children('article');

				articlesGPH.each(function(){
					
					var IDMateriaGTM = encodeIDGTM('http://www.gazetadopovo.com.br'+$(this).children('a').attr('href'));
					var dadosGTM = $(this).attr('data-gtm');
					var arraySplit = dadosGTM.split('|');

					if(pathUrlySplit.length > 3){
						var nameList = 'Home Subeditoria';						
					}else{
						var nameList = 'Home Editoria';						
					}

					var arrayVal = {'id':IDMateriaGTM, 'name':encodeURIComponent(arraySplit[0]), 'list':nameList, 'position': idxPos};

					arrayGTMProds.push(arrayVal);
					
					$(this).children('a').attr('data-position', idxPos);
					
					idxPos = idxPos + 1;
				});

	        });
		
		} else if (clss && clss.indexOf('bloco-dinamico') > -1) {
			
			var contador = 1;
			var divFilha = $(this).children('div');
			var articles = divFilha.children('article');
			
	        articles.each(function(){
	        	$(this).children('a').attr('data-position', contador);
	        	contador++;
	        });
		}
	});
	
	if ($('body.ed-ultimas-noticias')) {
		var contUltimas = 1;
		$('article.ultimas-chamadas').each(function(){
			$(this).children('a').attr('data-position', contUltimas);
			contUltimas++;
		});
	}
	
	if ($('body.ed-populares')) {
		var contPopulares = 1;
		$('section.c-lista-ordenada ol li').each(function(){
			$(this).children('article').children('a').attr('data-position', contPopulares);
			contPopulares++;
		});
	}
	
	if (arrayGTMProds.length > 0) {

		if(pathUrlySplit.length > 3){
			var slugList = 'home-subeditoria';						
		}else{
			var slugList = 'home-editoria';						
		}
		
		var impressionsGTM = {'event':'productImpression',
				  'eventCategory':'gazeta-do-povo',
				  'eventAction': slugList+':lista-materias:load',
				  'eventLabel':'product-impressions',
				  'ecommerce':{'currencyCode':'BR', 'impressions' : arrayGTMProds}};


		var objEcommerce = new Object();
		objEcommerce = impressionsGTM;
		
		dataLayer.push(objEcommerce);
	}
	
	if(typeof GTM_EventL != "undefined"){

		var userType = GTM_EventL.split(':');
		
		dataLayer.push({
			'event':'clickEvent', 
			'eventCategory':'gazeta-do-povo', 
			'eventAction': GTM_EventA, 
			'eventLabel': GTM_EventL,
			'userID': GTM_UserID,
			'userType': userType[0]
		});
	}
	
	console.log(encodeIDGTM('19'));
	if(typeof productsGTM != "undefined"){
		dataLayer.push({
			'event':'productImpression',
			  'eventCategory':'gazeta-do-povo',
			  'eventAction': 'materia:product:load',
			  'eventLabel':'product-impressions',
			  'ecommerce': {'detail' : {'products' : productsGTM}}
		});
	}


});
</script>

