    function overlayOpen() {
        $(".app-promo-download-lnk"). on("click", function(e){
            e.preventDefault();
            var height = $(document).height(),
                layer = $(".c24-blocking-layer"),
                pageAds = $("#c24-info-frame-wrapper"),
                pageContainerFooter = $(".c24-info-frame"),
                pageContainerShow = $(".c24-blocking-layer, .c24-overlay-inner"),
                inputPhone   = $("input[name=phone]"),
                SMSLabel     = $("#trueSMS"),
                inputPhone   = $("input[name=phone]"),
                SMSError     = $(".errorsms");

                SMSLabel.hide();
                SMSError.hide();
                inputPhone.removeClass("error");
                inputPhone.removeClass("true");
                inputPhone.val("");


            $("#c24-info-frame-wrapper").css("top", ($(window).height() - $(".c24-info-frame").height())/2);
            $("#c24-info-frame-wrapper").css("left", ($(window).width() - $(".c24-info-frame").width())/2 - 30);



            layer.height(height);
            pageContainerShow.show();
            pageAds.show();
        });
    }

    function overlayClose() {
        var closeOverlay = $(".c24-info-frame-close, .c24-blocking-layer"),
            layer = $(".c24-blocking-layer"),
            pageAds = $("#c24-info-frame-wrapper");

        closeOverlay.on("click", function() {
            layer.removeAttr("style");
            pageAds.removeAttr("style");
        });

        $(document).keyup(function(e) {
            if (e.keyCode == 27) {

                layer.removeAttr("style");
                pageAds.removeAttr("style");
            }
        });
    }

    function ajaxHandler() {

        $("#c24-promo-app-push").on("submit", function(e) {

            e.preventDefault();
             if ($("input[name=phone]").val() == "") {
                Check24.buttonSpinner.removeSpinner("btn-promo-sms");

                $("input[name=phone]").addClass("error");
                $("#errorSMS").show();

                return false;
            }
            $.ajax({
                type:"POST",
                url : "/ajax/misc/app-promo/",
                data: $("#c24-promo-app-push").serialize(),

                success : function(data) {
                    var promoButton  = $("#btn-promo-sms"),
                        inputPhone   = $("input[name=phone]"),
                        SMSLabel     = $("#trueSMS"),
                        SMSError     = $(".errorsms"),
                        closeOverlay = $(".c24-info-frame-close, .c24-blocking-layer"),
                        layer        = $(".c24-blocking-layer"),
                        pageAds      = $("#c24-info-frame-wrapper");

                    if(!window.console) {
                        console.log(data);
                        console.log("SUCCESS SMS");
                    };

                    SMSLabel.show();
                    inputPhone.addClass("true");
                    promoButton.attr("disabled", true).addClass("disabled");

                    if(inputPhone.hasClass("error")) {
                        inputPhone.removeClass("error");
                        SMSError.hide();
                    }

                    setTimeout(
                        function() {
                            layer.removeAttr("style");
                            pageAds.removeAttr("style");
                            promoButton.attr("disabled", false).removeClass("disabled");
                            Check24.buttonSpinner.removeSpinner("btn-promo-sms");
                        },
                    5000);
                },
                error: function(data) {
                    var inputPhone   = $("input[name=phone]"),
                        SMSError     = $(".errorsms");

                    Check24.buttonSpinner.removeSpinner("btn-promo-sms");

                    inputPhone.addClass("error");
                    SMSError.show();

                }
            });
        });
    }

    $(document).ready(function() {
        overlayOpen();
        overlayClose();
        ajaxHandler();
    });


