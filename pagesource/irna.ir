                url: "/NetServices/ajax.aspx/InvoiceModuleMethodJson",
                data: "{modulepath : '/modules/misc/NewsletterReg.ascx' , methodname: 'RegNewsletter', args: ['" + $("#ctl00_ctl00_ButtomContentPlaceHolder_NewsLetterRegCustome_111_txtNewLetEmail").val() + "','" + $("#txtNewLetName").val() + "','','" + getValueUsingClass(".newsletgroups input[type='checkbox']:checked") + "' , '" + securecode + "','" + enterdsecurecode + "'] }",
