<script>
   // More GA
   (function() {
  function sendGaPv(key) {
    ga('send', 'pageview', key);
  }
  function sendGaEvent(key, type) {
    ga('send', 'event', type, 'click', key);
  }
  function sendGaEventAndRedirect(key, url, type) {
    var redirectTriggered = false;
    // event发送成功才跳转
    ga('send', 'event', type, 'click', key, {'hitCallback':
      function() {
        redirectTriggered = true;
        document.location = url;
      }
    });
    // 万一GA被墙, 防止页面卡死
    setTimeout(function() {
      if (!redirectTriggered) {
        document.location = url;
      }
    }, 1500);
  }
  function hackAnchor(el, key, type) {
    var isNewWindow = el.target=='_blank';
    if (isNewWindow) {
      _attachEvent(el, 'click', function() {
        sendGaEvent(key, type);
      });
    } else {
      // 需要跳转，需要确保GA事件发出后再跳转
      el.onclick = function(e) {
        // 取消默认行为
        doane(e);
        var url = el.href;
        sendGaEventAndRedirect(key, url, type);
      }
    }
  }
  function hackSubmitButton(idOrEl, key) {
    //var el = typeof(idOrEl) == "string" ? $(idOrEl) : idOrEl;
    //_attachEvent(el, 'click', function() {
      // TODO hack post or onclick
      //sendGaEvent(key, 'cz');
    //});
  }
  function hackAnchors(parentIdOrElArray, key, type) {
    if (!type) {
      type = 'tg';
    }
    for (var i=0; i<parentIdOrElArray.length; i++) {
      var idOrEl = parentIdOrElArray[i];
      var el = typeof(idOrEl) == "string" ? $(idOrEl) : idOrEl;
      if (!el) continue;
      if (el.nodeName && el.nodeName == "A") {
        hackAnchor(el, key, type);
      } else {
        var anchors = el.getElementsByTagName('a');
        for (var j=0; j<anchors.length; j++) {
          hackAnchor(anchors[j], key, type);
        }
      }
    }
  }

  var url = document.location.pathname + document.location.search;
  if(match = url.match(/^\/forum\.php.*(?:\?|&)(g|f|t)id=[0-9]+(?:&|$)/) || url.match(/^\/(t)hread-[0-9]+-/) || url.match(/^\/(f)orum-[0-9]+-/)) {
    var text = $('pt').firstElementChild.innerText;
    text = trim(text).replace("电视家网»论坛", "").replace(/ *› */g, "/");
    sendGaPv(text);
    switch(match[1]) {
      case 'g':
        // 大区
        hackAnchors(['frame635bLU'], 'daqu_appdl');
        hackAnchors(['frameuJzMjU'], 'daqu_jiaocheng');
        break;
      case 'f':
        // 板块
        var diy1El = $('diy1');
        if (diy1El) {
          var diyDivs = diy1El.childNodes;
          var userguideDiv = diyDivs[0];
          if (userguideDiv) {
            var userguideEls = [];
            var userguideLists = userguideDiv.getElementsByTagName('li');
            // 忽略最后一个“加入QQ群”
            for (var i=0; i<userguideLists.length - 1; i++) {
              userguideEls.push(userguideLists[i]);
            }
            hackAnchors(userguideEls, 'forum_userguide');
            hackAnchors(userguideDiv.getElementsByClassName('portal_block_summary'), 'download', 'cz');
          }
          var picAndTextDiv = diyDivs[1];
          if (picAndTextDiv) {
            hackAnchors(picAndTextDiv.getElementsByClassName('frame-1-1-l'), 'forum_pic');
            hackAnchors(picAndTextDiv.getElementsByClassName('frame-1-1-r'), 'forum_text');
          }
        }
        // 发帖
        hackSubmitButton('newspecial', 'forum_fatie');
        hackSubmitButton('fastpostsubmit', 'forum_fastfatie');
        break;
      case 't':
        // 帖子
        hackAnchors($C('a_pt'), 'thread_ad-texttop');
        hackAnchors($C('a_pb'), 'thread_ad-textdown');
        var diyEl = $('diy1');
        if (diyEl) {
          hackAnchors(diyEl.getElementsByClassName('frame-1-1-1-l'), 'thread_ad-pic1');
          hackAnchors(diyEl.getElementsByClassName('frame-1-1-1-c'), 'thread_ad-pic2');
          hackAnchors(diyEl.getElementsByClassName('frame-1-1-1-r'), 'thread_ad-pic3');
        }
        hackAnchors(document.getElementsByTagName('fieldset'), 'thread_ad-zonghe');
        // 发帖与回复
        hackSubmitButton('newspecial', 'thread_fatie');
        hackSubmitButton('post_reply', 'thread_huifulouzhu');
        hackSubmitButton('postsubmit', 'thread_huifu_suc');
        hackSubmitButton($C('fastre'), 'thread_huifulouceng');
        hackSubmitButton('fastpostsubmit', 'thread_fasthuifu');
        break;
    }
  } else if(url.match(/^\/(?:index|forum)\.php/) || document.location.pathname == '/') {
    // 首页
    hackAnchors(['portal_block_607_content'], 'index_fatie', 'cz');
    hackAnchors(['frameoxon8x_left', 'frameoXNlfy_left'], 'index_daohang');
    hackAnchors(['portal_block_852'], 'index_banner1');
    // 16.8.27 banner2、3拆分
    //hackAnchors(['portal_block_1136'], 'index_banner2');
    //hackAnchors(['portal_block_1904'], 'index_banner3');
    hackAnchors(['portal_block_2957'], 'index_banner2-1');
    hackAnchors(['portal_block_2958'], 'index_banner2-2');
    hackAnchors(['portal_block_2955'], 'index_banner3-1');
    hackAnchors(['portal_block_2956'], 'index_banner3-2');
    hackAnchors(['portal_block_107'], 'index_toutiao-pic');
    hackAnchors(['portal_block_109'], 'index_toutiao-text1');
    hackAnchors(['portal_block_232'], 'index_toutiao-text2');
    hackAnchors(['portal_block_638'], 'index_appdl');
    hackAnchors(['portal_block_641'], 'index_topic-zb');
    hackAnchors(['portal_block_642'], 'index_topic-db');
    hackAnchors(['portal_block_1134_content', 'portal_block_230_content', 'portal_block_853_content', 'portal_block_848_content'], 'index_hot1');
    hackAnchors(['portal_block_748_content', 'portal_block_1094_content', 'portal_block_1025_content', 'portal_block_1095_content'], 'index_hot2');
  } else if(match = url.match(/^\/(news|pingce)\//)) {
    // 门户列表页
    var type = match[1];
    hackAnchors(['portal_block_280'], 'news_list-hot');
    hackAnchors(['portal_block_281'], 'pingce_list-hot');
  } else if(match = url.match(/^\/portal\.php.*(?:\?|&)aid=([0-9]+)(?:&|$)/) || url.match(/^\/article-([0-9]+)-/)) {
    // 门户文章页
    var aid = match[1];
    var text = $('pt').firstElementChild.innerText;
    var type = text.match('门户 ›评测') ? 'pingce' : 'news';
    sendGaPv(type + '/' + aid);
    hackAnchors(['portal_block_282'], 'news_content-hot');
    hackAnchors(['portal_block_677'], 'pingce_content-hot');
    hackAnchors(['diyrightbttom'], 'news_content-pic');
    hackAnchors(['frameQFr8f7'], 'pingce_content-pic');
  } else if(url.match(/^\/forum\.php\?mod=misc&action=nav/)) {
    // 首页点发帖后跳转页
    hackSubmitButton('postbtn', 'index_fatie_suc');
  } else if(url.match(/^\/forum\.php\?mod=post&action=newthread&fid=/)) {
    // 发帖跳转页
    hackSubmitButton('postsubmit', 'forum_fatie_suc');
  }
})();
</script>
