		/*FIRST FOLD*/
	
		.first_fold {
			padding-top: 18px;
		}
	
		.first_fold .hero_story .summary {
			color: #616161;
			font-size: 16px;
			line-height:1.25em;
		}
	
		.sub_heroes,
		.hero_columnist {
			float: left;
		}
	
		.sub_heroes {
			width: 75%;
			display: flex;
			flex-direction: row;
			box-sizing: border-box;
		}
	
		.sub_heroes .sub_hero_item {
			padding: 0px 15px;
