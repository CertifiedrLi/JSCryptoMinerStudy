		function formatDateTime(){
			 $('time').each(function( index ) {
	
			  var type = $( this ).attr('type');
			  var the_date = $( this ).attr('datetime');
	
			  if(type == 'ago'){
				var day = moment(the_date).fromNow();
				day = day.replace(' minutes ago','m ago');
				day = day.replace(' hours ago','h ago');
				day = day.replace('an hour ago','1h ago');
				day = day.replace(' days ago','d ago');
				day = day.replace('a day ago','Yesterday');
			  }else if (type == 'date-time'){
				var day = moment(the_date).format("MMM D, h:mm a");
			  }else{
				if( moment(the_date).format("DD/MM/YYYY") == moment().format("DD/MM/YYYY")){
				  //var day = 'Hari ini, '+moment(the_date).format("h:mm a");
				  var day = moment(the_date).format("h:mm a");
				}else{
