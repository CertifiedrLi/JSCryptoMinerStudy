		}
	
		.uk-container::after {
			clear: both
		}
	
		.uk-container> :last-child {
			margin-bottom: 0;
		}
	
		.uk-container .uk-container {
			padding-left: 0;
			padding-right: 0
		}
	
		.uk-container-small {
			max-width: 900px
		}
	
		.uk-container-large {
			max-width: 1600px
		}
	
		.uk-container-expand {
			max-width: none
		}
	
		.uk-grid {
			display: -ms-flexbox;
			display: -webkit-flex;
			display: flex;
			-ms-flex-wrap: wrap;
			-webkit-flex-wrap: wrap;
			flex-wrap: wrap;
			margin: 0;
			padding: 0;
			list-style: none
		}
	
		.uk-grid>* {
			margin: 0
		}
	
		.uk-grid>*> :last-child {
			margin-bottom: 0
		}
	
		.uk-grid {
			margin-left: -30px
		}
	
		.uk-grid>* {
			padding-left: 30px
		}
	
	
		.uk-width-expand {
			width: 1px;
			-ms-flex: 1;
			-webkit-flex: 1;
			flex: 1;
			min-width: 0;
			flex-basis: 1px
		}
	
		.uk-grid-collapse {
			margin-left: 0
		}
	
		.uk-grid-collapse>* {
			padding-left: 0
		}
	
		.uk-grid+.uk-grid-collapse,
		.uk-grid-collapse>.uk-grid-margin {
			margin-top: 0
		}
	
		.uk-grid-small {
			margin-left: -15px
		}
	
		.uk-grid-small>* {
			padding-left: 15px
		}
	
		*+.uk-grid-margin-small,
		.uk-grid+.uk-grid-small,
		.uk-grid-small>.uk-grid-margin {
			margin-top: 15px
		}
	
	
	
		[class*=uk-width] {
			box-sizing: border-box;
		}
	
		#top_nav {
			height: 110px;
			border-bottom: 1px solid #e5e5e5;
			position: fixed;
			top: 0;
			left: 0;
			width: 100%;
			z-index: 1009;
			background:#fff;
		}
	
		#top_nav .shrinking_nav {
			height: 70px;
			display: flex;
			flex-direction: row;
		}
	
		#top_nav,
		.shrinking_nav * {
			transition: all 300ms ease;
		}
		/*LANG NAV*/
	
		#top_nav .shrinking_nav .lang_nav {
			display: flex;
			flex-direction: column;
			flex: 1;
			justify-content: center;
		}
	
		#top_nav .shrinking_nav .lang_nav ul li {
			display: block;
			float: left;
		}
	
		#top_nav .shrinking_nav .lang_nav ul li a {
			text-decoration: none;
			font-size: 13px;
			font-weight: 500;
			color: inherit;
		}
	
		#top_nav .shrinking_nav .lang_nav ul li.active a {
			color: #FF5722;
		}
	
		#top_nav .shrinking_nav .lang_nav ul li a:after {
			content: ' \00a0 / \00a0';
		}
	
		#top_nav .shrinking_nav .lang_nav ul li:last-child a:after {
			content: '';
		}
	
	/*	#top_nav.shrinked .shrinking_nav .lang_nav ul li a:after {
			content: '\00a0 \00a0';
		}*/
		/*BRAND LOGO*/
	
		#top_nav .brand_logo {
			flex: 1;
			min-width: 50%;
			max-height: 70px;
			overflow-y: hidden;
			text-align: center;
		}
	
		#top_nav .brand_logo .large_logo {
			height: 70px;
			line-height: 65px;
		}
	
		#top_nav .brand_logo .large_logo img {
			height: 50px;
			vertical-align: middle;
		}
	
		#top_nav .brand_logo .small_logo {
			height: 40px;
			line-height: 38px;
		}
	
		#top_nav .brand_logo .small_logo img {
			width: 140px;
		}
	
		#top_nav .brand_logo .large_logo img,
		#top_nav .brand_logo .small_logo img {
			display: inline-block;
		}
	
	
		#top_nav.shrinked {
			height: 80px;
		}
	
		#top_nav.shrinked .shrinking_nav,
		#top_nav.shrinked .shrinking_nav .brand_logo,
		#top_nav.shrinked .shrinking_nav .brand_logo * {
			height: 40px;
		}
	
		#top_nav.shrinked .shrinking_nav .brand_logo .large_logo {
			transform: translateY(-70px);
		}
	
		#top_nav.shrinked .shrinking_nav .brand_logo .small_logo {
			transform: translateY(-40px);
		}
	
	   
	
		/*PROMO NAV*/
	
		#top_nav .promo_nav {
			display: flex;
			flex-direction: row;
			flex: 1;
			align-items: center;
			justify-content: flex-end;
		}
	
		#top_nav .promo_nav .subscribe_btn a {
			text-decoration: none;
			display: inline-block;
			background: #2196F3;
			color:#fff;
			font-size: 13px;
			font-weight: bold;
			position: relative;
			border: 2px solid #2196F3;
			text-transform: uppercase;
			border-radius: 4px;
			padding: 6px 6px;
			line-height: 14px;
		}
	
		#top_nav .promo_nav .promo_message {
			margin: 0;
			font-size: 14px;
			font-weight: bold;
			line-height: 18px;
			display: inline-block;
			margin: 0 5px 0px;
			text-transform: uppercase;
			text-align: left;
			vertical-align: bottom;
			display: none;
		}
	
		#top_nav .social_buttons{
			display: flex;
			flex-direction: row;
			align-items: center;
			justify-content: flex-end;
			display: none;
		}
		#top_nav .social_buttons > div > a{
			margin: 0 5px;
		}
		#top_nav .social_buttons > div > a > img{
			height: 14px;
		}
	
	
		#top_nav.shrinked .shrinking_nav .shrinked-hidden{
			display: none;
		}
	
		#top_nav.shrinked .shrinking_nav .social_buttons{
			display: flex;
		}
	
	
	
		/*CATEGORIRES NAV*/
	
		#top_nav .categories_nav {
			height: 40px;
			background:#fff;
			border-top: 1px solid #e5e5e5;
			border-bottom: 1px solid #e5e5e5;
			transition: all 300ms ease;
		}
	
		#top_nav .categories_nav #categories_links {
			overflow: auto;
			white-space: nowrap;
		}
	
		#top_nav .categories_nav #categories_links li {
			display: inline-block;
			text-align: center;
			transition: color 300ms ease;
		}
	
		#top_nav .categories_nav #categories_links li:hover a{
			color: #FF5722;
		}
	
	
	
		#top_nav .categories_nav a {
			display: inline;
			text-decoration: none;
			height: 40px;
			line-height: 40px;
			padding: 0px 8px;
			font-size: 13px;
			font-weight: 500;
			color: #424242;
			transition: all 300ms ease;
		}
	
		#top_nav .categories_nav #categories_links li.active a{
			color: #FF5722;
		}
	
		#top_nav .categories_nav #categories_links li.active{
			border-bottom: 2px solid #FF5722;
		}
	
	
		 .subscribe_btn_secondary, .sign_in_btn{
