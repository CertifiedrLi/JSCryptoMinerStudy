                                if (result.message == "OK") {
                                    $("#txtEditGroupName").val(result.name);
                                    $('.manage-folders').css('display', 'inline-block');
                                    //$("#dialog1").dialog("open");
                                }
                                else {
                                    alert("Sistemə daxil olmalısınız")
                                }
                            }
                        });
            });



            var keywords = new Bloodhound({
                datumTokenizer: function (datum) {
                    return Bloodhound.tokenizers.whitespace(datum.value);
                },
                queryTokenizer: Bloodhound.tokenizers.whitespace,
                limit: 100,
                remote: {
                    url: '/az/Home/AutoCompleteNew?search=%QUERY',
                    filter: function (keywords) {
                        // Map the remote source JSON array to a JavaScript object array
                        return $.map(keywords, function (keyword) {
                            if (keyword.additional == '') {
                                return {
                                    label: keyword.title,
                                    value: keyword.title
                                };
                            }
                            else {
                                return {
                                    label: '<span style="color:gray; ">' + keyword.title + '</span>' + '<br/><span style="font-size:14px; font-style:italic;">' + keyword.additional + '</span>',
                                    value: keyword.title
                                };
                            }
                        });
                    }
                }
            });

            keywords.initialize();

            $('input#keywords').typeahead(
                {
                    hint: true,
                    highlight: true,
                    minLength: 3,
                }, {
                    name: 'keywords',
                    displayKey: 'value',
                    source: keywords.ttAdapter(),
                    templates: {
                        suggestion: function (item) { return '<p style="white-space: normal;">' + item.label + '</p>'; }
                    }
                }).on('typeahead:selected', function (obj, datum) {

                    //$('.src input').val(datum.label.replace(/(<([^>]+)>)/ig, ""));
                    //$('input#keywords').attr('data-type', datum.type);

                    $("#frmSearch button").click();

                });

        });

        (function () {
            var link_element = document.createElement("link"),
