    <script type="text/javascript">

        function getSelectionText() {
            highlightedText = "";
            if (window.getSelection) {
                highlightedText = window.getSelection().toString();
            } else if (document.selection && document.selection.type != "Control") {
                highlightedText = document.selection.createRange().text;
            }
            return highlightedText;
        }

        function KeyPress(e) {
            var evtobj = window.event ? event : e
            if (evtobj.keyCode == 13 && evtobj.ctrlKey) {
                //alert("Ctrl+Enter");
                //alert(getSelectionText());

                if (getSelectionText().trim().length > 0) {
                    $('#frmErrorTextModalBody')[0].reset();
                    $("#taErrorSelectedText").val(getSelectionText().trim());
                    $("#myErrorTextModal").modal("show");
                }
            }
        }

        document.onkeydown = KeyPress;

        var serviceUsageCount, callCount;

        var serviceUsageCountUsed = 1, callCountUsed = 1;
        function ServiceUsageCount(time) {

            if (serviceUsageCountUsed == 1) {
                serviceUsageCount = new FlipClock($('#spnServiceUsageCountNewChild'), 10000000, {
                    clockFace: 'Counter'
                });
            }

            setTimeout(function () {
                $.ajax({
                    url: "/az/home/JSGetServiceUsageCount/",
                    type: 'POST',
                    dataType: 'json',
                    data: {},
                    success: function (result) {
                        serviceUsageCount.setValue(result.UsageCount);

                        if (serviceUsageCountUsed == 1) {
                            setTimeout(function () {
                                $("#spnServiceUsageCountNew img").remove();
                                $("#spnServiceUsageCountNewChild").css("visibility", "visible");

                                ServiceUsageCount(10);
                            }, 1000);

                        }

                        serviceUsageCountUsed++;

                        if (result.message == "OK") {
                            ServiceUsageCount(2000);
                        }
                        else {
                            console.log("Error in catch");
                            ServiceUsageCount(2000);
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        console.log("Status: " + textStatus);
                        console.log("Error: " + errorThrown);
                    }
                });
            }, time);
        }


        function CallCount(time) {

            if (callCountUsed == 1) {
                callCount = new FlipClock($('#divCallCountNewChild'), 100, {
                    clockFace: 'Counter'
                });
            }

            setTimeout(function () {
                $.ajax({
                    url: "/az/home/JSGetCallCount/",
                    type: 'POST',
                    dataType: 'json',
                    data: {},
                    success: function (result) {
                        callCount.setValue(result.callCount);

                        if (callCountUsed == 1) {
                            setTimeout(function () {
                                $("#divCallCountNew img").remove();
                                $("#divCallCountNewChild").css("visibility", "visible");

                                CallCount(10);
                            }, 1000);
                        }

                        callCountUsed++;

                        if (result.message == "OK") {
                            CallCount(2000);
                        }
                        else {
                            console.log("Error in catch");
                            CallCount(2000);
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        console.log("Status: " + textStatus);
                        console.log("Error: " + errorThrown);
                    }
                });
            }, time);
        }




        $(document).ready(function () {

            ServiceUsageCount(1000);
            CallCount(1000);

            $(".lang").removeClass("langActive");
            $(".lang[lang='az']").addClass("langActive");


            $("#min").click(function () {

                $('.online-content').fadeOut();
                //$('.online-content').css('right','-500px');
                $('.online-btn').show();
            });

            var activeCert = '';

            $("#slcCerts").val(activeCert);
            $("#width_tmp").html($('#slcCerts option:selected').text());
            $("#slcCerts").width($("#width_tmp").width() + 30);


            $('#slcCerts').change(function () {

                $("#width_tmp").html($('#slcCerts option:selected').text());
                $(this).width($("#width_tmp").width() + 30);

                $.ajax(
                    {
                        url: "/az/home/JSChangeCertificate/",
                        type: 'POST',
                        dataType: 'json',
                        data:
                        {
                            id: $(this).val()
                        },
                        success: function (result) {
                            if (result.data == "OK") {
                                location.reload();
                            }
                        }
                    });

            });


            //var the_obj = $('div.es-desc').ThreeDots({
            //    max_rows: 3
            //});

            //BootstrapDialog.alert('I want banana!');

            $("#btnSendErrorTextModal").click(function () {
                $.ajax(
                    {
                        url: "/az/home/JSAddSelectedError/",
                        type: 'POST',
                        dataType: 'json',
                        data: $("#frmErrorTextModalBody").serialize(),
                        success: function (result) {
                            if (result.message == "OK") {
                                $('#frmErrorTextModalBody')[0].reset();
                                $("#myErrorTextModal").modal("hide");

                                BootstrapDialog.show({
                                    type: BootstrapDialog.TYPE_SUCCESS,
                                    title: 'Məlumat',
                                    message: "Xəta g&#246;ndərildi"
                                });
                            }
                            else {
                                BootstrapDialog.show({
                                    type: BootstrapDialog.TYPE_DANGER,
                                    title: 'Xəta',
                                    message: result.message,
                                    buttons: [{
                                        label: 'Bağla',
                                        cssClass: 'btn-danger',
                                        action: function (dialogItself) {
                                            dialogItself.close();
                                        }
                                    }]
                                });
                            }
                        }
                    });
            });


            $("body").delegate("#ulGroupList li a.aDeleteMyTopic", "click", function () {
                //$('#ulGroupList li a.aDeleteMyTopic').click(function () {

                var myTopicID = $(this).closest("li").attr("mytopicid");
                var topicName;

                $(this).closest("li").find("span.icons").hide();

                $.ajax(
                    {
                        url: "/az/services/JSGetTopicName/",
                        type: 'POST',
                        dataType: 'json',
                        async: false,
                        data:
                        {
                            topicId: myTopicID
                        },
                        success: function (result) {
                            if (result.message == "OK") {
                                topicName = result.name;
                            }
                            else {
                                alert(result.message);
                            }
                        }
                    });


                BootstrapDialog.show({
                    title: 'Xəbərdarlıq',
                    type: BootstrapDialog.TYPE_DANGER,
                    message: 'az' == 'en' ? ('Do you want delete <span style="font-weight:bold; font-size: 16px;">"' + topicName + '"</span> service group?') : ('Siz <span style="font-weight:bold; font-size: 16px;">"' + topicName + '"</span> xidmət qrupunu silmək istəyirsiniz ?'),
                    buttons: [{
                        label: 'Xeyr',
                        action: function (dialog) {
                            dialog.close();
                        }
                    }, {
                        label: '&nbsp;Bəli&nbsp;',
                        cssClass: 'btn-danger',
                        action: function (dialog) {
                            $.ajax(
                                {
                                    url: "/az/services/JSDeleteMyTopic/",
                                    type: 'POST',
                                    dataType: 'json',
                                    async: false,
                                    data:
                                    {
                                        topicId: myTopicID
                                    },
                                    success: function (result) {
                                        if (result.message == "OK") {
                                            if (result.id != '') {
                                                if (result.id == -1) {
                                                    window.location.assign('/az/services/index');
                                                }
                                                else {
                                                    window.location.assign('/az/services/mytopic/' + result.id);
                                                }
                                            }
                                        }
                                        else {
                                            BootstrapDialog.show({
                                                type: BootstrapDialog.TYPE_DANGER,
                                                title: 'Xəta',
                                                message: result.message,
                                                buttons: [{
                                                    label: 'Bağla',
                                                    cssClass: 'btn-danger',
                                                    action: function (dialogItself) {
                                                        dialogItself.close();
                                                    }
                                                }]
                                            });
                                        }
                                    }
                                });
                        }
                    }]
                });


            });


            $("body").delegate("a.aCancelEdit", "click", function () {
                //$("a.aCancelEdit").click(function () {

                $(this).closest("li").removeClass("editedMyTopic");
                $(this).closest("li").children().show();
                $("span.serv-arrow").show();
                $("#aAddGroup").closest("li").show();

                $(this).closest("li").find(".icons").hide();

                $(this).closest("li").find("div.divForTopicEdit").remove();

                $('.manage-folders').removeAttr('style');

            });


            $("body").delegate("#aGroupSaveChange", "click", function () {
                var ths = $(this);
                var myTopicID = ths.closest("li").attr("mytopicid");
                if ($("#txtEditGroupName").val().length > 2) {
                    //alert(response);

                    $.ajax(
                    {
                        url: "/az/services/JSEditMyTopicName/",
                        type: 'POST',
                        dataType: 'json',
                        data:
                        {
                            name: $("#txtEditGroupName").val(),
                            topicId: myTopicID
                        },
                        success: function (result) {
                            if (result.data == "OK") {

                                ths.closest("li").removeClass("editedMyTopic");
                                ths.closest("li").children().show();
                                $("span.serv-arrow").show();
                                $("#aAddGroup").closest("li").show();

                                ths.closest("li").find(".icons").hide();

                                ths.closest("li").find("a:first").text($("#txtEditGroupName").val());

                                ths.closest("li").find("div.divForTopicEdit").remove();

                                $('.manage-folders').removeAttr('style');
                                //$("#dialog1").dialog("close");
                                //location.reload();
                            }
                            else {
                                //alert("Sistemə daxil olmalısınız")
                                BootstrapDialog.show({
                                    type: BootstrapDialog.TYPE_DANGER,
                                    title: 'Xəta',
                                    message: result.data,
                                    buttons: [{
                                        label: 'Bağla',
                                        cssClass: 'btn-danger',
                                        action: function (dialogItself) {
                                            dialogItself.close();
                                        }
                                    }]
                                });
                            }
                        }
                    });

                }
                else {
                    BootstrapDialog.show({
                        type: BootstrapDialog.TYPE_DANGER,
                        title: 'Xəta',
                        message: 'Qrupunun adı ən azı 3 simvoldan ibarət olmalıdır',
                        buttons: [{
                            label: 'Bağla',
                            cssClass: 'btn-danger',
                            action: function (dialogItself) {
                                dialogItself.close();
                            }
                        }]
                    });
                    ////alert("Qrupunun adı ən azı 3 simvoldan ibarət olmalıdır");
                    //$("#pError").text("Qrupunun adı ən azı 3 simvoldan ibarət olmalıdır");
                }
            });


            $("body").delegate("#ulGroupList li a.aEditMyTopic", "click", function () {
                //$('#ulGroupList li a.aEditMyTopic').click(function () {
                var myTopicID = $(this).closest("li").attr("mytopicid");
                //$('#aAddGroup').click();

                $("#aAddGroup").closest("li").hide();
                $("span.serv-arrow").hide();
                $(this).closest("li").children().hide();
                $(this).closest("li").append('<div class="manage-folders divForTopicEdit" style="margin: 5px 0 0 -12px; padding: 0;"><a class="new-folder aCancelEdit" title="Ləğv et" style="cursor: pointer; margin-right: 8px;"> <img src="/ContentNew/Pics/plus.png" title="Ləğv et" class="imgCancel"> </a><input id="txtEditGroupName" type="text"> <a id="aGroupSaveChange" class="folder-ok"><img src="/ContentNew/Pics/serv-ok.png" title="Yadda saxla" style="cursor:pointer;"></a></div>');
                $(this).closest("li").addClass("editedMyTopic");

                $.ajax(
                        {
                            url: "/az/services/JSGetTopicName/",
                            type: 'POST',
                            dataType: 'json',
                            data:
                            {
                                topicId: myTopicID
                            },
