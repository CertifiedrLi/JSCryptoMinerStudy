function wrapMe(wrapMeClass, limitLength) {
	var arr = [];

	$('.area-3-1-3 .ofAuto').each(function(){
		var index = $(this).index();
		var total = $('.area-3-1-3 .ofAuto').length - 1;

		if( $(this).hasClass(wrapMeClass) ) {
			arr.push('.area-3-1-3 .ofAuto:eq('+index+')');
		}
		else {
			$(arr.join(', ')).removeClass('ofAuto').removeClass(wrapMeClass).wrapAll("<div class='ofAuto'></div>");
			arr = [];
		}

		if(arr.length === limitLength) {
			$(arr.join(', ')).removeClass('ofAuto').removeClass(wrapMeClass).wrapAll("<div class='ofAuto'></div>");
			arr = [];
		}

		if(index === total) {
			$(arr.join(', ')).removeClass('ofAuto').removeClass(wrapMeClass).wrapAll("<div class='ofAuto'></div>");
			arr = [];
		}

	});
}

function addClassWrap() {
	$('.area-3-1-3 .ofAuto').each(function(){
		var elements = $(this).find('div').length,
			classSliders = 'ofAuto_' + elements;
		$(this).addClass(classSliders);
