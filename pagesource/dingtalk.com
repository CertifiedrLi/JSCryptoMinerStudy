<script>
	var timer = null;
	var index = 1;
	var sliderTop = 0;
	var liIndexArr = [];
	var liNum = 0;    //换一组的组数
	//默认显示前面4条li
	$("#floor4-tab-list2 li").each(function(i){
		liIndexArr.push(i);
	})
	$("#floor4-tab-list2 li").each(function(i){
		if(i >= liNum*4 && i < (liNum+1)*4){
			var self = $(this).clone();
			$(".floor4-tab-list").append(self);
		}
	})
	
	//tab切换点击按钮换一批
	$(".floor4-tab-change").on("click",function(){
		liNum++;
		if(liNum >= parseInt(liIndexArr.length/4)){
			liNum = 0;
		}
		$(".floor4-tab-list li").remove();
		$("#floor4-tab-list2 li").each(function(i){
			if(i >= liNum*4 && i < (liNum+1)*4){
				var self = $(this).clone();
				$(".floor4-tab-list").append(self);
			}
		})
		index = 0;
		autoTabStyle();
		tabMouse();
	})
	autoTab();
	tabMouse();
	//tab切换自动切换事件
	function autoTab(){
		timer = setInterval(function(){
			autoTabStyle();
			index++;
			if(index > 3){
				index = 0;
			}
		},5000)
	}
	//tab切换触摸事件
	function tabMouse(){
		$(".floor4-tab-list li .tab-list-txt").each(function(item){
			$(this).on("mouseover",function(){
				clearAutoTab();
				index = item;
				autoTabStyle();
				$(".floor4-tab-list li .tab-list-txt").each(function(item2){
					$(this).next().hide();
				})
				$(this).next().show();
				
			})
			$(this).on("mouseout",function(){
				autoTab();
			})
		})
		$(".floor4-tab-list li .tab-list-content").each(function(item){
			$(this).on("mouseover",function(){
				clearAutoTab();
			})
			$(this).on("mouseout",function(){
				autoTab();
			})
		})
	}
	//tab切换样式改变
	function autoTabStyle(){
		sliderTop = index * 110;
		$(".floor4-tab-list li .tab-list-txt").eq(index).css({"background":"white","border":"solid 1px white"}).parent().siblings().find(".tab-list-txt").css({"background":"#fafafa","border":"solid 1px #eceef3"});
		$(".floor4-tab-list li .tab-list-content").eq(index).show().parent().siblings().find(".tab-list-content").hide();
		$(".floor4-tab-slider").css({"top":sliderTop});
	}
	//清除定时器
	function clearAutoTab(){
		clearInterval(timer);
	}


</script>



