	function numTimer(num,ele){

		var numBit = 0;
		var numTen = 0;
		var floor3Timer = setInterval(function(){
			if(num > 40 && num <= 60){
        numBit = numBit + 2;
      }else if(num > 60) {
        numBit = numBit + 4;
      }else{
        numBit++;
      }

			if(numBit > 9){
				numBit = 0;
				numTen++;
			}
			var nowTxt = numTen.toString() + numBit.toString();
			if(nowTxt >= num){
				clearInterval(floor3Timer);
			}
			ele.text(nowTxt);
		},80)
	}
