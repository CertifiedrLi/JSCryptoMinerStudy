<script>


function show_signup_error_msg(){
	//do nothing :-)
	
}

$(document).ready(function(){
    $(".str_sign").click(function(){
		//alert($(this).find("img").attr("title"));
		console.log("dvdvdvdvdvdv");
		$("#starsign_bottom").val($(this).find("img").attr("title"));
		
        $("#selected_sign").attr("src", "https://static.astrologyanswers.com/site-assets/star-signs/footer_sign/"+$(this).find("img").attr("alt")+".png");
		
		$(".email_subscribe .step-1").hide();
		$(".email_subscribe .step-2").fadeIn();
    });
	$(".step-2 .str_sign").click(function(){
        $(".email_subscribe .step-2").hide();
		$(".email_subscribe .step-1").fadeIn();
    });
	/*$(".btn_submit").click(function(){
        $(".email_subscribe .step-2").hide();
		$(".email_subscribe .step-3").fadeIn();
		
    });*/
	

	$("#signupformsubmit_bottom_new").click(function (e) {
		e.preventDefault();
		var $this = $(this);
		var name 	= $('#name_bottom').val();
		var email 	= $('#email_bottom').val();
		var starsign 	= $('#starsign_bottom').val();
		//var location = $(this).find('.signup_source_bottom').val();
		var location = $('#signup_source_bottom').val();
		if (typeof homePageAjaxExecuted === 'undefined' || typeof homePageAjaxExecuted === 'unknown') {}
		else {
			console.log(homePageAjaxExecuted);
			return false;
		}

		if (trim(name) == '' || name == "") {
			$("#signsectionerror_bottom").html('<div class="alert alert-warning"><strong>Warning!</strong> Please provide your name.</div>');
			show_signup_error_msg();
			return false;
		}

		if(!allLetter(name,"Name"))
		{

			$("#signsectionerror_bottom").html('<div class="alert alert-warning"><strong>Warning!</strong> Name must have alphabet characters only, no spaces, dashes or apostrophes.</div>');
			show_signup_error_msg();
			return false;
		}

		if (trim(email) == '' || email == "") {
			$("#signsectionerror_bottom").html('<div class="alert alert-warning"><strong>Warning!</strong> Please enter your valid Email Address.</div>');
			show_signup_error_msg();
			return false;
		}

		var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
		var address = email;
		if (reg.test(address) == false) {
			$("#signsectionerror_bottom").html('<div class="alert alert-warning"><strong>Warning!</strong> Please enter your valid Email Address.</div>');
			show_signup_error_msg();
			return false;
		}

		if (typeof starsign == 'undefined' || starsign == "") {
			$("#signsectionerror_bottom").html('<div class="alert alert-warning"><strong>Warning!</strong> Please select Your Zodiac Sign.</div>');
			show_signup_error_msg();
			return false;
		}
    
	$('#signupformsubmit_bottom_new').prop('disabled', true);

	//Start  Create lead with valid email
	$.ajax({
			url : "https://astrologyanswers.com/signup_lead_footer/verify_email/",
			type : 'post',
			async	:false,
			data : {
				'email' : email
			},
			success : function (data_out) {
				 if(data_out == "Delivery" || data_out == "Fraud" || data_out == "Invalid" || data_out == "Reputation")
				  {
					  console.log("Entered email is not valid. Please check your email id.");
					  $("#signsectionerror_bottom").html('<div class="alert alert-warning">Entered email is not valid. Please check your email id</div>');
					  $('#signupformsubmit_bottom_new').prop('disabled', false);
					 return false; 
				  }else{
					  $(".email_subscribe .step-2").hide();
						$(".email_subscribe .step-3").fadeIn();
		
					  createLead();
				  }
			}
		});
	//End Create lead with valid email	
	
