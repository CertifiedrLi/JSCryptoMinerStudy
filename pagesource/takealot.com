        pvt.sendTrackingEvent();
    }());

    // For cart start checkout event
    $('div.cart-action a').on('click', function(event) {
        var pvt = new PageViewTracker();
        pvt.setSampleRate(1.0);
        var products = JSON.parse($(event.currentTarget).attr('data-event-data'));
        pvt.addTrackingEvent(new PageViewTrackerPayload('cart', 'start_checkout', {'products': products}));
        pvt.sendTrackingEvent();
    });

    // For cart missed promo click through
    $('table.cart-line-items a.missed-promo-link').on('click', function(event) {
        var pvt = new PageViewTracker();
        pvt.setSampleRate(1.0);
        console.log($(event.currentTarget).attr('data-missed-promo'));
        var promoData = JSON.parse($(event.currentTarget).attr('data-missed-promo'));
        pvt.addTrackingEvent(new PageViewTrackerPayload('cart.missed_deals.bundles', 'click_through', promoData));
        pvt.sendTrackingEvent();
    });

    // For pdp bundles tag click event
    $('body.product-single .promotion-deal-tag').on("click", function(event) {
        var pvt = new PageViewTracker();
        pvt.setSampleRate(1.0);
        var plid = $(event.currentTarget).attr('data-productline');
        pvt.addTrackingEvent(new PageViewTrackerPayload('product_details.bundle_advert', 'click_through', {'product_line_id': plid}));
        pvt.sendTrackingEvent();
    });

    // For pdp bundles tab click event
    $('body.product-single li.bundle-tag > a').on("click", function(event) {
        if (!event.isTrigger) {
            var pvt = new PageViewTracker();
            pvt.setSampleRate(1.0);
            var plid = $(event.currentTarget).attr('data-productline');
            pvt.addTrackingEvent(new PageViewTrackerPayload('product_details.bundle_tab', 'click_through', {'product_line_id': plid}));
            pvt.sendTrackingEvent();
        }
    });

    $(function(){
        $(".image-click-metric").on("click", function(evt) {
            var pvt = new PageViewTracker();
            var action = 'click_through';
            pvt.setSampleRate(1);
            var context = evt.target.parentElement.getAttribute('data-context');
            var postData = {
                'other': {
                    'reference': evt.target.alt,
                    'image': evt.target.src,
                    'target': evt.target.parentElement.href
                }
            };
            pvt.addTrackingEvent(new PageViewTrackerPayload(context, action, postData));
            pvt.sendTrackingEvent();
        });

                if(typeof(tal) != "undefined" && typeof(tal.UserTracking) != "undefined") {
            setTimeout(
                function() {
                    $("#merger-banner").on("click", function(e) { tal.UserTracking.sendEvent({ed: "{&quot;c&quot;:&quot;home.merger_banner&quot;,&quot;a&quot;:&quot;click_through&quot;}"}) });
                },
                3000
            );
        }

    });

    function serverTime() {
         return window.apiServerTime ? new Date(window.apiServerTime.format("MMM D, YYYY HH:mm:ss Z")) : new Date();
    }

    /**
