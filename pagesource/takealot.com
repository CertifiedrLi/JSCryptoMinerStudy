<script type="text/javascript">
$(document).ready(function() {
    if (window.tal) {
        tal.minicart.init();
    }

    $.ajax({
        url: "https://www.takealot.com/ajax/header.php?callback=?",
        dataType: 'jsonp',
        type: 'GET',
        success: function(data) {
            var quote_count = 0;
            var wishlist_count = 0;
            var cart_count = 0;
            var customer_name = '';
            var customer_email = '';
            var is_logged_in = false;
            var flash_messages = '';
            var autoFillEmail = true;
            var dataLayer = window.dataLayer || [];

            if (typeof window.autoFillEmail !== "undefined") {
                autoFillEmail = window.autoFillEmail;
            }

            if (typeof data.quote_count !== "undefined") {
                quote_count = data.quote_count;
            }
            if (typeof data.wishlist_count !== "undefined") {
                wishlist_count = data.wishlist_count;
            }
            if (typeof data.cart_count !== "undefined") {
                cart_count = data.cart_count;
            }
            if (typeof data.customer_name !== "undefined") {
                customer_name = data.customer_name;
                dataLayer.push({'firstName': data.customer_name});
            }
            if (typeof data.customer_email !== "undefined") {
                customer_email = data.customer_email;
            }
            if (typeof data.is_logged_in !== "undefined") {
                is_logged_in = data.is_logged_in;
                dataLayer.push({"isLoggedIn": !! data.is_logged_in});
            }
            if (typeof data.flash_messages !== "undefined") {
                flash_messages = data.flash_messages;
            }
            if (data && data.hashed_customer_email) {
                dataLayer.push({"hashedEmail": data.hashed_customer_email});
            }
            if (data && data.customer_id) {
                dataLayer.push({"userId": data.customer_id});
            }

            // Set up tal.customer global
            tal.customer = {};

            $('.top-link-quote').html('<span class="count qt-cnt">' + quote_count + '</span>').closest("li").toggleClass("hidden", quote_count == 0);
            $('.top-link-wish').html('<span class="count wsh-cnt">' + wishlist_count + '</span>');
            $('.top-link-cart').html('<span class="count crt-cnt">' + cart_count + '</span>');
            if (customer_name.length > 0 && is_logged_in) {
                $('#welcome-name').html('Hi ' + customer_name +  '<a href="https://secure.takealot.com/account/forget_me" class="small notMe">(that\'s not me)</a>').addClass('sep-r');
                $('#register-link').hide();
                $('#footer-name').html('Hello ' + customer_name + ':');
                $('.autofill-name').val(customer_name);
            }
            if (is_logged_in) {
                $('#login-logout').html('<a href="https://secure.takealot.com/account/logout" rel="nofollow">Logout</a>');
                $('#register-link').hide();
            }
            if (customer_email.length > 0 && autoFillEmail) {
                tal.customer.email = customer_email;
                $('input[name=email]').val(customer_email);
            }
            if (flash_messages.length > 0) {
                $("div.content").prepend(flash_messages);
            }
            // Emit event to notify any potential listeners that we have customer data stored in tal.customer
            try {
                var customerEvent = document.createEvent('Event');
                customerEvent.initEvent('receive-customer', true, true);
                document.querySelector('body').dispatchEvent(customerEvent);
            } catch(e) {
                // Don't throw exception here - not needed
            }
        }
    })
});
</script>

