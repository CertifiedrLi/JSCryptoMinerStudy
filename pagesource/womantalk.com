<script type="text/javascript">
  var product = 'womantalk'
  var imagePath = 'https://s3-ap-southeast-1.amazonaws.com/album.bunda/portal.image/'
  var jsonPath = 'https://s3-ap-southeast-1.amazonaws.com/album.bunda/kaskusnetworks.json'

  var new_obj = {};

  var staticPortal = [
    {
      text: 'Viral',
      url: 'http://viral.kincir.com/?utm_campaign=GDP&utm_source=womantalk.com&utm_medium=channel',
      image: imagePath+'Viral'+'.png'
    },
    {
      text: 'Bobotoh',
      url: 'http://bobotoh.id/?utm_campaign=GDP&utm_source=womantalk.com&utm_medium=channel',
      image: imagePath+'Bobotoh'+'.png'
    }
  ]
  $.getJSON( jsonPath, function() {

  })
  .done(function(data){
    generatePortal(data)
  })
  .fail(function(){

  })
  .always(function(){

  });
  var socmed = [
    {
      name: 'facebook',
      url: '//facebook.com/womantalkdotcom'
    },
    {
      name: 'twitter',
      url: '//twitter.com/womantalk_com'
    },
    {
      name: 'youtube',
      url: '//youtube.com/channel/UCTc_UjCpsh6WFnYsK0SRsoA'
