    }
  ]
  function generatePortal(json){
    var portalItems = [];
    var wtPos = 0;
    $.each(json.links, function(index, val){
        portalItems.push(val)
      // if(val.text !== 'Beritagar' && val.text !== 'IDNtimes' && val.text !== 'Dailysocial' && val.text !== 'Popbela'){
      //   portalItems.push(val)
      //   if(val.text === 'Womantalk') {
      //     wtPos = portalItems.length-1;
      //     portalItems.splice(0, 0, portalItems.splice(wtPos, 1)[0]);
      //   }
      // }
    })
    $.each(staticPortal, function(index, val){
        portalItems.push(val)
    })
    $.each(portalItems, function(index, val) {
      var appendText = ''
      var appendTextMobile = ''
      var liStyle = ''
      if(index != portalItems.length-1) {
        liStyle+= 'border-bottom: 1px solid #dedede;'
      }
      appendText+= '<li style="'+liStyle+'" class="portal-images">'
        appendText+= '<a href="'+val.url+'" target="_blank" title="">'
          appendText+= '<img alt="'+val.text+'" src="'+val.image+'">'
        appendText+= '</a>'
      appendText+= '</li>'

      $('#channel-ul').append(appendText);
      //for mobile
      appendTextMobile += '<li ><a href="'+val.url+'" class="menu-burger-item" style="color:#4c4c4c!important; ">'+val.text+'</a></li>'
      $('#burger-channel').append(appendTextMobile);

    });
  }
  $.each(socmed, function(index, val) {
    var appendText = ''
    var liStyle = 'display: inline;padding-left: 10px;'
    if(index == socmed.length-1) {
      liStyle+= 'padding-right: 15px;'
    }
    var containerName = 'portal-'+val.name+'-container';
    var bwName = 'portal-'+val.name+'-bw'
    var rgbName = 'portal-'+val.name+'-rgb'
    appendText+= '<li id="'+containerName+'" style="'+liStyle+'">'
      appendText+= '<a href="'+val.url+'" target="_blank" title="">';
        appendText+= '<img id="'+bwName+'" class="" src="'+imagePath+val.name+'-bw.svg" style="height: 15px;width: 15px;"/>'
        appendText+= '<img id="'+rgbName+'" class="off" src="'+imagePath+val.name+'-rgb.svg" style="height: 15px;width: 15px;"/>'
      appendText+= '</a>'
    appendText+= '</li>';

    $(document).on({
      mouseenter: function () {
          $('#'+bwName).toggleClass('off')
          $('#'+rgbName).toggleClass('off')
      },
      mouseleave: function () {
        $('#'+bwName).toggleClass('off')
        $('#'+rgbName).toggleClass('off')
      }
    }, '#'+containerName);

    $('#socmed-ul').append(appendText);
  });
  function toggleChannelDisplay(param) {
    if(param) {
      if(param == 'enter') {
        $('#channelList').removeClass('off').addClass('on');
      } else { //leave
        $('#channelList').addClass('off').removeClass('on');
      }
    } else {
      $('#channelList').toggleClass('off').toggleClass('on');
    }
  }
  var testMobile = /iPhone|iPad|iPod|Android/i.test(navigator.userAgent);
  $(document).on({
    touchstart: function (event) {
      if($('#channelList').hasClass('on')){
        if($(event.target).closest('#channelList').length) { //klik channel list -> jangan close

        } else if ($(event.target).closest('#menu-portal, #menu-portal-container').length) { //klik jduul -> close
          toggleChannelDisplay()
        }
      } else { //off -> on in
        toggleChannelDisplay()
      }
    },
    mouseenter: function () {
      if(!testMobile){
        toggleChannelDisplay('enter');
      }
    },
    mouseleave: function () {
      if(!testMobile){
        toggleChannelDisplay('leave');
      }
    }
  }, '#menu-portal-container');

  $(document).click(function(event) {
    if(!$(event.target).closest('#menu-portal').length && !$(event.target).closest('#channelList').length) {
      if($('#channelList').hasClass('on')){
        toggleChannelDisplay()
      }
    }
  });

</script>

