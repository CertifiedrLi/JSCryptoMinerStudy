<script type="text/javascript">
    var base_url = "https://www.kompasiana.com/";
    var ajax_loader = "https://www.kompasiana.com/assets/images/loader.svg";
    var isKompasianer = "";
    var notifAjax = false;
    var IFA = "";
    var IFASC = "";

    if(isKompasianer){
        if(IFA != ""){
            var myparam = IFA;
            var from = 'kompasid';
        } else if(IFASC != ""){
            var myparam = IFASC;
            var from = 'social';
        }
    }
</script>
