                            console.error('bildirim izni reddedilmiş, ', Notification.permission);
                        }

                    }

                    function subscriberLogin()
                    {
                        var tokenStr;

                        messaging.getToken().then(function (token) {
                            tokenStr = token;

                            if(tokenStr != "")
                            {
                                document.getElementById("hdnFldToken").value = tokenStr;
                                document.getElementById("aCikis").href += "&token=" + tokenStr;
                            }
                        })
                    }

                    function popupBildirimInsert()
                    {
                        var priority = 0;

                        if (document.getElementById("rdBtnBildirim1").checked)
                        {
                            priority = 1;
                        }
                        if (document.getElementById("rdBtnBildirim2").checked)
                        {
                            priority = 2;
                        }
                        if (document.getElementById("rdBtnBildirim3").checked)
                        {
                            priority = 3;
                        }

                        var sonuc;
                        var tokenStr;

                        messaging.getToken().then(function (token) {
                            tokenStr = token;

                            sonuc = ajax_req("https://www.donanimhaber.com/notificationpopup.aspx?mod=add&memberId=" + 0 + "&priority=" + priority + "&token=" + tokenStr);

                            if (sonuc == "OK")
                            {
                                var notification = new Notification('Bildirim aboneliğiniz tamamlandı!', {
                                    icon: 'https://img.donanimhaber.com/img/bildirimpopuplogo.jpg',
                                    body: 'Bildirimlere abone oldunuz.',
                                });

                                notification.onclick = function () {
                                    window.open('https://www.donanimhaber.com/genel-bildirim-tercihleri');
                                    notification.close();
                                };

                                setCookie("notificationPopupDisabled", "true", 10);

                                document.getElementById("notification-permission").style.display = "none";
                            }
                        });
                    }

                    document.addEventListener("DOMContentLoaded", function (event) 
                    {
                        subscriberLogin();

                        if (Notification.permission != 'granted')
                        {
                            document.getElementById("notification-permission").style.display = "";
                        }
                        if (Notification.permission == 'granted') {
                            document.getElementById("notification-permission").style.display = "none";
                        }

                        var popUpDisabled;
                        var popUp = getCookie("notificationPopupDisabled");

                        if (popUp != null && popUp != "") {
                            popUpDisabled = Boolean(popUp);

                            if (!popUpDisabled) {
                                document.getElementById("notification-permission").style.display = "";
                            }
                            else
                            {
                                document.getElementById("notification-permission").style.display = "none";
                            }
                        }
                        else {
                            if (Notification.permission === 'default') {
                                document.getElementById("notification-permission").style.display = "";
                            }
                        }
                    });

