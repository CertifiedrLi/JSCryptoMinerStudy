                                        //alert(data);
                                        if (data.indexOf("OK") > -1) {
                                            location.reload();
                                        }
                                    }
                                });
                            }
                        };

                        xhr.onerror = function () {
                            console.log('Woops, there was an error making the request.');
                        };
                        xhr.withCredentials = true;
                        xhr.send();
                    }

