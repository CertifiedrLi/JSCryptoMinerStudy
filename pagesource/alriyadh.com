	$( "#singup_form" ).submit(function( event ) {
	 
	  event.preventDefault();
	  
	  $('.singup_err').hide();
	  var $form = $( this ),
	    url = $form.attr( "action" );
	    input = $form.serialize();
	    
	  var posting = $.post( url, input );
	  posting.done(function( data ) {
	    var d = $.parseJSON( data );
	    if( d.err ){
		    var msg = '<br>';
		    
		    if( d.err.term ) msg = '<br>  - ' + d.err.term + msg;
		    if( d.err.password ) msg = '<br>  - ' + d.err.password + msg;
		    if( d.err.email ) msg = '<br>  - ' + d.err.email + msg;
			if( d.err.user ) msg = '<br>  - ' + d.err.user + msg;
			
			if( d.err.user_exists ) msg = '<br>  - ' + d.err.user_exists + msg;
			if( d.err.email_exists ) msg = '<br>  - ' + d.err.email_exists + msg;
			
		    $('.singup_err').show();
		    $('.singup_err .msg').html(d.err.title  +msg);
		    
		    
	    }else{
		    if( d.done ){
			 $form.html('');   
			 $('.singup_done').show();
			 $('.singup_done h4 span').html(d.done);
		    }
	    }
	  });
