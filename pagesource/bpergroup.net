		  function openPouUpPreLogin(url, id){
			state = $('#statepopup').html();
			isAble = false;
			isWarning = false;
			isBlock = false;
			if(state.indexOf(id + 'Attivo') >= 0){
				isAble = true;
				if(state.indexOf(id + 'info') >= 0){
					isWarning = true;
				} else if(state.indexOf(id + 'block') >= 0){
					isBlock = true;
				}
				if($('#titolo_' + id).html() == null || $('#titolo_' + id).html() == ''){
					isAble = false;
				}
			}
			if(!isAble){
				return true;
			} else {
				$('#titlepopup').html($('#titolo_' + id).html());
				$('#titlepopup2').html($('#titolo2_' + id).html());
				$('#contentpopup').html($('#' + id).html());
				$('#preloginpopup').children().each(function( index ) {
					var w = $(window);
					$(this).css('top', Math.abs(((w.height() - $(this).outerHeight()) / 3)));
					$(this).css('left', Math.abs(((w.width() - $(this).outerWidth()) / 2)));
				});
				
				$('#preloginpopup').show();
				if(isWarning){
					$('#prelogin-pulsante-popup').html('Continua');
					$('#prelogin-pulsante-popup').attr('href', url);
					$('#prelogin-pulsante-popup').unbind('click');
				} else if(isBlock){
					$('#prelogin-pulsante-popup').html('OK');
					$('#prelogin-pulsante-popup').click(function() {
						colosePopUpPerelogin();
						return false;
					});
				}
				return false;
			}
		  }
		  function colosePopUpPerelogin(){
			$('#preloginpopup').hide();
		  }
		  
