            }
            
            
            
            else if (pattern.test(Y.Cookie.get("TRID"))) {
                localStorage.setItem("TRID",
                        Y.Cookie.get("TRID"));
            }
        });
    } catch(e) {
        return false;
    }
  })();

})();

</script>

