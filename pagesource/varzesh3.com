<script id="AdvertiseTemplate" type="x-tmpl-mustache">
        {{#Data}}
        <div data-extension="{{FileType}}" class="{{Class}}">
            {{#Provider}}
            <a class="provider-badge-container"
               data-provider="{{Provider}}"
               href="{{ProviderUrl}}"
               target="_blank"
               rel="nofollow noopener">
                <div class="inner">
                    <span class="badge initial"></span>
                    <span class="badge hover"></span>
                </div>
            </a>
            {{/Provider}}

            {{#LoadInIframe}}
            <iframe frameborder="0" src="http://static01.farakav.varzesh3.tamasha.com/frame?path={{FilePath}}&url={{Url}}&width={{Width}}&height={{Height}}" width="{{Width}}" height="{{Height}}"
                    scrolling="no" style="border: 1px solid #fff;box-shadow: 1px 1px 2px #bbb;margin: 0;padding: 0;"></iframe>
            {{/LoadInIframe}}
            {{^LoadInIframe}}
            <a href="{{Url}}" target="_blank" rel="nofollow noopener">
                <img width="{{Width}}" {{#showHeight}}={{#showHeight}} height="{{Height}}" {{/showHeight}}={{/showHeight}} alt="" src="{{FilePath}}" />
            </a>
            {{/LoadInIframe}}
        </div>
        {{/Data}}
    </script>
    <!-- Search Result Template -->
