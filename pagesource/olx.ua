        document.addEventListener('threatMetrixPush', function (e) {

            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for(var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }

            var token = getCookie('threat_matrix_token');
            var xhr = new XMLHttpRequest();
            var url = "/ajax/myaccount/threatmetrix/";
            xhr.open("POST", url, true);
            xhr.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
            var params = 'eventType=' + e.detail.event_type + '&actionType=' + e.detail.action_type + '&sessionId=' + token;
            xhr.send(params);
        }, false);

    </script>


