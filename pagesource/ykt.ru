<script type="text/javascript">
    function open_popup(category_id) {
        var catalog = document.createElement('script'); catalog.type = 'text/javascript'; catalog.async = true;
        catalog.src = '//catalog.ykt.ru/jsmenu2/menu_categories_' + category_id + '.utf8.js?nocache=' + (new Date()).getTime();
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(catalog, s);

        $('#popup').css('margin-top', -$('#popup').outerHeight()/2)
        $('#popup').fadeIn();
        $('#overlay').fadeIn();
    }

    $(document).on('mousedown', function(event) {
        if ($(event.target).parents('.dropdown_t').length === 0) {
            $('.dropdown_t').slideUp();
        }
    });

    $(document).ready(function() {
        $('.dropdown_opener').click(function() {
            var catalog = document.createElement('script'); catalog.type = 'text/javascript'; catalog.async = true;
            catalog.src = '//catalog.ykt.ru/jsmenu2/menu_categories_' + $(this).attr('rel') + '.utf8.js?nocache=' + (new Date()).getTime();
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(catalog, s);
            $(this).next().slideDown();
            return false;
        });
        $(document).on('click', '.dropdown_t_close', function(event){
            $('.dropdown_t').each(function() {
                $(this).slideUp();
            });
            return false;
        });

        $("#news-tabs, #afisha-tabs, #forum-tabs").tab();

        var $benzinTabs = $('.informer-tabs-benzin');
        var $benzinItems = $('.informer-item-benzin');
        var ind = Math.round(Math.random() * ($benzinTabs.length - 1));
        $($benzinTabs[ind]).show();
        $($benzinItems[ind]).show();

        $(".informer-tabs li").click(function() {
            $(".informer-tabs li").removeClass("active");
            $(this).addClass("active");
            var paneId = $(this).data("pane");

            switch (paneId) {
                case '#informer-exchange':
                    $(".informer .exchange").show();
                    $(".informer .link").hide();
                    break;
                case '#informer-benzin':
                    $(".informer .exchange").hide();
                    $(".informer .link").show();
                    break;
            }
            $(".informer .informer-item").hide();
            $(paneId).show();
        });
        $(document).on('click', '#popup .close', function() {
            $('#popup').fadeOut();
            $('#overlay').fadeOut();

            return false;
        });

        //Show three random ads under news in mobile
        var $ads = $.extend([], $('.mobile-news-advert'));
        var len = $ads.length;
        for (var i = 0; i < 3 && i < len; i++) {
            var ind = Math.round(Math.random() * ($ads.length - 1));
            $($ads[ind]).show();
            $ads.splice(ind, 1);
        }

        // Show four random ads in Medium block in mobile
        $ads = $('.mobile .adgen_tr438').hide();
        $ads.filter(function () {return $(this).html() === ""}).remove();
        $ads = $.extend([], $($ads.selector));
        len = $ads.length;
        for (i = 0; i < 4 && i < len; i++) {
            ind = Math.round(Math.random() * ($ads.length - 1));
            $($ads[ind]).find('.img').css({'padding': '4px 0', 'vertical-align': 'middle'});
            $($ads[ind]).find('.text').css({'padding': '0 0 0 10px', 'vertical-align': 'middle'});
            $($ads[ind]).show();
            $ads.splice(ind, 1);
        }

        // Shuffle ads on desktop
        $('.news_top--bold').shuffle();
        $('.news_top--regular').shuffle();
    });
</script>

