<script type="text/javascript">
$(document).ready(function() {
	var carousel = $('#ul-carousel');
	var carouselInner = carousel.find('.carousel-inner');
	var aspectRatio = 1220 / 470;
	var cropTopRatio = 1220 / 108;
	
    $('.navbar-blue .navbar-collapse').on('show.bs.collapse', function(e) {
        $('.navbar-blue .navbar-collapse').not(this).collapse('hide');
    });
    carousel.carousel({
    	interval: 3000,
    	cycle: true
    });

    function calCarouselSize(size) {
    	if (size < 768) {
    		carouselInner.css('margin-top', 0);
        	carousel.css('height', 'auto');
        	carousel.find('.item').css('height', '100%');
        	carousel.find('.carousel-caption').css('bottom', 0);
        } else {
        	var resizeHeight = carousel.width() / aspectRatio;
        	var offset = carousel.width() / cropTopRatio;
        	carousel.height(resizeHeight);
        	carouselInner.css('margin-top', -offset);
        	carousel.find('.carousel-caption').css('bottom', offset);
        }
    }
    
    $(window).resize(function() {
    	calCarouselSize($(this).width());
    });
    calCarouselSize($(window).width());
});
</script></div>
