        function checkRefreshAds(isMovingDown)
        {
            $.each(YD.ads.slots, function(key, ad_slot)
            {
                if(checkAdOnScreen(ad_slot, isMovingDown))
                {
                    if (ad_slot['interval_id'] == "none")
