                jQuery(".homePageSlider").parents(".bx-wrapper").css("margin-left","-"+jQuery("#floaterContainer").offset().left-8+"px");
            }
        }, 300);
    }

    function floatHeader()
    {
       var myWidth = window.innerWidth || document.documentElement.clientWidth;
       var myHeight = window.innerHeight || document.documentElement.clientHeight;

       if( Math.min(myWidth, myHeight) >= 750 )
       {
           if(jQuery(window).scrollTop() > jQuery(".floating-nav").height())
           {
                jQuery(".floating-header").css({"position": "fixed", "top": "0px"});
           }
           else
           {
                jQuery(".floating-header").css({"position": "static", "top": "auto"});
           }
       }
    }

