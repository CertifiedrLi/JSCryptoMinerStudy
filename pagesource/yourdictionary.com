                            AdBridg.display(ad_slot['id']);
                            ad_slot['displayed'] = "yes";
                        }
                        ad_slot['interval_id'] = ensureRefreshEnabled(ad_slot, isMovingDown);
                    }
                }
                else
                {
                    if (ad_slot['interval_id'] != "none")
                    {
                        clearInterval(ad_slot['interval_id']);
                        ad_slot['interval_id'] = "none";
                    }
                }
            });
        }

        function checkAdOnScreen(ad_slot, isMovingDown)
        {
            if ($('#' + ad_slot['id']).length == 0) return false;
            var offscreen_threshold = 500;
            if (!has_scrolled)
            {
                offscreen_threshold = 0;
            }
            var docViewTop = $(window).scrollTop();
            var docViewBottom = docViewTop + $(window).height();
            var elemTop = $('#' + ad_slot['id']).offset().top;
            var elemBottom = elemTop + $('#' + ad_slot['id']).height();
            if(isMovingDown)
            {
                docViewBottom += offscreen_threshold;
            }
            else
            {
                docViewTop -= offscreen_threshold;
            }
            if ((elemTop >= docViewTop && elemTop <= docViewBottom) || (elemBottom >= docViewTop && elemTop <= docViewBottom))
            {
                return true;
            }
            return false;
        }

        REFRESH_INTERVAL = 25000;
        function ensureRefreshEnabled(ad_slot, isMovingDown)
        {
            if (ad_slot['interval_id'] == "none")
            {
                return setInterval(function(){
                    if (ad_slot['display_count'] < 50)
                    {
                        AdBridg.refresh([ad_slot['ad']]);
                        ad_slot['display_count']++;
                    }
                }, REFRESH_INTERVAL);
            }
            else
            {
                return ad_slot['interval_id'];
