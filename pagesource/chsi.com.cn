<script type="text/javascript">
function checkvCode(){
    var validationCode = document.getElementById("vcode").value;
    validationCode = validationCode.replace(/[ ]/g,""); // 去掉前后的空格
    if(validationCode == "" || validationCode == "请输入在线验证码"){
        alert("在线验证码不能为空！");
        document.getElementById("vcode").focus();
        return false;
    }else{
        if(!isNumber_Ex(validationCode,12)){
            alert("在线验证码应该是12位数字，请仔细填写！");
            document.getElementById("vcode").focus();
            return false;
        }
        document.getElementById("getXueLi").action = "/xlcx/bg.do?vcode=" + validationCode + "&srcid=index";
        return true;
    }
}
document.getElementById("getXueLi").onsubmit = function(){return checkvCode();}
eventFocusAndBlur("vcode","请输入在线验证码");
</script>
