<script>
    $('.bus_col_nav_con').css({
        width:'1200px'
    })
   function buscalcWidth() {
           var lg = $('.bus_per_fir_title li').length;
           $('.bus_per_fir_title li').css({
               width:parseInt($('.bus_per_fir_title').outerWidth()/lg),
               textAlign:'center'
           })
           $('.bus_lenovo_pc').each(function(index){

               $(this).children('li').css({
                   width:parseInt($(this).outerWidth()/$(this).children('li').length),
                   borderLeft:'1px solid #d6d6d6',
                   boxSizing:'border-box',
                   paddingLeft:'30px'
               })
               if(index==0){
                   $(this).show();
                   $('.bus_per_fir_title li').eq(0).addClass('bus_cur_tri');
                   $('.bus_per_fir_title li').eq(0).find('p').css('color','#585757');
               }else{
                   $(this).hide()
               }
           })
       }
       var busnum = 0 ;
       function busshownav (){

           var busshowTimer = setInterval(function(){
               if(busnum>=$('.bus_per_fir_title li').length){
                   clearInterval(busshowTimer)
               }
               $('.bus_per_fir_title li').eq(busnum).find('a').addClass('bus_on');
               busnum++;
           },100)
       }
           $('.bus_per_home_pro').mouseenter(function(){
              buscalcWidth();
              clearTimeout(per_timer);
              bus_timer = setTimeout(function(){
                  busshownav();
                  $('.bus_per_triangle').removeClass('bus_per_down').addClass('bus_per_up');
                  $('.bus_big_back').css("height",$('.bus_col_nav_con').outerHeight()).stop(true,true).slideDown(300);
                  $('.bus_col_nav_con').stop(true,true).slideDown(300);
                  $('.bus_row_border').stop(true,true).slideDown(300);
              },300)
          })
           $('.bus_per_home_pro').stop(true,true).mouseleave(function(){
               clearTimeout(per_timer);
               clearTimeout(bus_timer);
               $('.bus_per_triangle').removeClass('bus_per_up').addClass('bus_per_down');
               $('.bus_col_nav_con').css('display','none');
               $('.bus_big_back').css('display','none');
               $('.bus_row_border').css('display','none');
           })
           $('.bus_col_nav_con').stop(true,true).mouseleave(function(){
              $('.bus_per_triangle').removeClass('bus_per_up').addClass('bus_per_down');
              $('.bus_col_nav_con').css('display','none');
              $('.bus_big_back').css('display','none');
              $('.bus_row_border').css('display','none');
           })
           $('.bus_per_fir_title').on('mouseenter','li',function(e){
               var eve = e || window.event;
               try{  // 使用 try...catch 语句避免浏览器出现错误提示
                  eve.preventDefault();  // 非 IE 浏览器
               }catch(e){
                  eve.returnValue = false;  // IE8.0 及其以下版本
               }
               $(this).find('.bus_deepcolor').show();
               $(this).siblings('li').find('.bus_deepcolor').hide();
               $(this).find('.bus_lightcolor').hide();
               $(this).find('p').css('color','#585757');
               $(this).addClass('bus_cur_tri');
               $(this).siblings('li').removeClass('bus_cur_tri');
               $(this).siblings('li').find('p').css('color','#898989');
               $(this).siblings('li').find('.bus_lightcolor').show();

               var _this = $(this);
               $('.bus_lenovo_pc').each(function(){
                   if($(this).attr('tabindex')==_this.attr('tab')){
                       $(this).show();
                       $(this).siblings('.bus_lenovo_pc').hide()
                   }
               })
               $('.bus_big_back').css("height",$('.bus_col_nav_con').outerHeight())
           })
</script>


