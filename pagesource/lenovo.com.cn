<script>
	//头部悬浮
	$(window).on('scroll',fn1);
	function fn1(){
		var scrollTop = $(window).scrollTop();
		$('.per_triangle').removeClass('per_up').addClass('per_down');
		$('.col_nav_con').css('display','none');
		$('.big_back').css('display','none');
		$('.row_border').css('display','none');
		$('.bus_per_triangle').removeClass('bus_per_up').addClass('bus_per_down');
		$('.bus_col_nav_con').css('display','none');
		$('.bus_big_back').css('display','none');
		$('.bus_row_border').css('display','none');
		if (scrollTop > 300) {
			var scrollLeft = $(document).scrollLeft();
			$('#logo').hide();
			$('#small_logo').show();
			$('.col_nav_title').css('color','#000');
			$('.bus_col_nav_title').css('color','#000');
			$('.top_header').addClass('backwhite');
			if($('.top_header').hasClass('midbacks')){
				$('.top_header').removeClass('midbacks');
			}
			if($('.top_header').hasClass('midback')){
				$('.top_header').removeClass('midback');
			}
			$('.login').css('background','url(//p3.lefile.cn/product/adminweb/2017/11/27/c327a216-364e-4553-b9b1-12fb2c124917.png) -38px -96px no-repeat');
			$('.home_cart').css('background','url(//p3.lefile.cn/product/adminweb/2017/11/27/c327a216-364e-4553-b9b1-12fb2c124917.png) -112px -76px no-repeat');
			$('.header-main').css('color','#000');
			$('.nav_box ul li>a').css('color','#000');
			$(".search_box  .search_input_bg").addClass("activiry");
			$(".search_box input").addClass("flow");
			$('#header').css('left',-scrollLeft)
		}else if(scrollTop<300&&scrollTop>0){
			if($('.top_header').hasClass('backwhite')){
				$('.top_header').addClass('midbacks');
			}else{
				$('.top_header').addClass('midback')
			}
			$('.top_header').removeClass('backwhite');
		}else if(scrollTop == 0) {
			$('#logo').show();
            $('#small_logo').hide();
            $('.col_nav_title').css('color','#fff');
            $('.top_header').removeClass('backwhite');
            if($('.top_header').hasClass('midbacks')){
				$('.top_header').removeClass('midbacks');
			}
			if($('.top_header').hasClass('midback')){
				$('.top_header').removeClass('midback');
			}
            $('.bus_col_nav_title').css('color','#fff');
			$('.login').css('background','url(//p3.lefile.cn/product/adminweb/2017/11/27/c327a216-364e-4553-b9b1-12fb2c124917.png) -10px -97px no-repeat');
			$('.home_cart').css('background','url(//p3.lefile.cn/product/adminweb/2017/11/27/c327a216-364e-4553-b9b1-12fb2c124917.png) -71px -76px no-repeat');
			$('.header-main').css('color','#fff');
			$('.nav_box ul li>a').css('color','#fff');
			$(".search_box  .search_input_bg").removeClass("activiry");
			$(".search_box input").removeClass("flow");
		}
	}
	// 商城快报
	function AutoScroll(obj) {
		$(obj).find("ul:first").animate({
			marginTop : "-23px"
		}, 500, function() {
			$(this).css({
				marginTop : "0px"
			}).find("li:first").appendTo(this);
		});
	}
	var timerNews = null;
	timerNews = setInterval(function() {
		AutoScroll('#shop_news');
	}, 2000);

	$('#shop_news').hover(function() {
		clearInterval(timerNews);
	}, function() {
		timerNews = setInterval(function() {
			AutoScroll('#shop_news');
		}, 2000);
	});
</script>






