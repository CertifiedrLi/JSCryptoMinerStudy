<script>
    $('.col_nav_con').css({
        width:'1200px'
    })
   function calcWidth() {
           var lg = $('.per_fir_title li').length;
           $('.per_fir_title li').css({
               width:parseInt($('.per_fir_title').outerWidth()/lg),
               textAlign:'center'
           })
           $('.lenovo_pc').each(function(index){

               $(this).children('li').css({
                   width:parseInt($(this).outerWidth()/$(this).children('li').length),
                   borderLeft:'1px solid #d6d6d6',
                   boxSizing:'border-box',
                   paddingLeft:'30px'
               })
               if(index==0){
                   $(this).show();
                   $('.per_fir_title li').eq(0).addClass('cur_tri');
                   $('.per_fir_title li').eq(0).find('p').css('color','#585757');
               }else{
                   $(this).hide()
               }

           })
       }
       var num = 0 ;
       var per_timer,bus_timer;
       function shownav (){

           var showTimer = setInterval(function(){
               if(num>=$('.per_fir_title li').length){
                   clearInterval(showTimer)
               }
               $('.per_fir_title li').eq(num).find('a').addClass('on');
               num++;
           },100)

       }
           $('.per_home_pro').stop(true,true).mouseenter(function(){
               calcWidth();
               clearTimeout(bus_timer);
               per_timer = setTimeout(function(){
                  shownav();
                  $('.per_triangle').removeClass('per_down').addClass('per_up');
                  $('.big_back').css("height",$('.col_nav_con').outerHeight()).stop(true,true).slideDown(300);
                  $('.col_nav_con').stop(true,true).slideDown(300);

                  $('.row_border').slideDown(300);
              },300)
           })
           $('.per_home_pro').stop(true,true).mouseleave(function(){
               clearTimeout(per_timer);
               clearTimeout(bus_timer);
               $('.per_triangle').removeClass('per_up').addClass('per_down');
               $('.col_nav_con').css('display','none');
               $('.big_back').css('display','none');
               $('.row_border').css('display','none');
               $('.bus_per_triangle').removeClass('bus_per_up').addClass('bus_per_down');
           })

           $('.col_nav_con').mouseleave(function(){
                $('.per_triangle').removeClass('per_up').addClass('per_down');
                $('.col_nav_con').css('display','none');
                $('.big_back').css('display','none');
                $('.row_border').css('display','none');
           })
           $('.per_fir_title').on('mouseenter','li',function(e){
               var eve = e || window.event;
               try{  // 使用 try...catch 语句避免浏览器出现错误提示
                   eve.preventDefault();  // 非 IE 浏览器
               }catch(e){
                   eve.returnValue = false;  // IE8.0 及其以下版本
               }
               $(this).find('.deepcolor').show();
               $(this).siblings('li').find('.deepcolor').hide();
               $(this).find('.lightcolor').hide();
               $(this).find('p').css('color','#585757');
               $(this).addClass('cur_tri');
               $(this).siblings('li').removeClass('cur_tri');

               $(this).siblings('li').find('p').css('color','#898989');
               $(this).siblings('li').find('.lightcolor').show();
               var _this = $(this);
               $('.lenovo_pc').each(function(){
                   if($(this).attr('tabindex')==_this.attr('tab')){
                       $(this).show();
                       $(this).siblings('.lenovo_pc').hide()
                   }
               })
               $('.big_back').css("height",$('.col_nav_con').outerHeight())
           })
</script>

