(function(){
			var lunlis = $('.neckFocusimg li');
			//创建轮播显示圆点
			for(var i = 0; i < lunlis.length; i++) {
				var oLi = $('<li nums=' + i + '></li>');
				if(i == 0) {
					oLi.addClass('cur');
				}
				$('.neckFocustab').append(oLi);
			}
			var nums = 0;
			var lunbo_timers = lunboTimersFn();

			function lunboTimersFn() {
				return window.setInterval(function() {
					luneventFn();
				}, 3000);
			}

			function luneventFn() {
				if(nums < lunlis.length) {
					var _this = lunlis.eq(nums);
					_this.fadeIn(500);
					_this.siblings().fadeOut(500);
					actives(nums);
					nums++;
				} else {
					nums == lunlis.length ? actives(0) : actives(nums);
					lunlis.eq(lunlis.length - 1).fadeOut(500);
					lunlis.eq(0).fadeIn(500);

					nums = 0;
				}
			}

			$('.neck_index').hover(function() {
				$('.neckBtn_l').fadeIn(100);
				$('.neckBtn_r').fadeIn(100);
			}, function() {
				$('.neckBtn_l').fadeOut(100);
				$('.neckBtn_r').fadeOut(100);
			});

			//hover
			$('.neckFocusimg').hover(function() {
				window.clearInterval(lunbo_timers);
			}, function() {
				lunbo_timers = lunboTimersFn();
			})
			//btnLeft
			$('.neckBtn_l').on('click', function() {
				lazyBanners();
				window.clearInterval(lunbo_timers);
				for(var i = 0; i < lunlis.length; i++) {
					var _attr = lunlis.eq(i).css('display');
					if(_attr == "list-item") {
						nums = i;
						continue;
					}
				}
				nums--;
				if(nums >= 0) {
					var _this = lunlis.eq(nums);
					_this.fadeIn(500);
