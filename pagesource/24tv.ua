<script>
    $(function() {
        $.ajax({
            type: "GET",
            url: "https://auto.24tv.ua/api/getLastNews",
            data: {
                'count': 4
            },
            quietMillis: 50,
            success: function(newsList) {

                if(newsList && $.trim(newsList) !== "") {
                    $.each(newsList, function (key, item) {

                        let today = "сьогодні";
                        let lang = "uk";

                        let newsPublishDate = getDateFormat(item.publishDate, lang, today);

                        let imageHref = ($('<a/>', {title: item.name, href: item.newsUrl, target: '_blank'}));
                        let imageDiv = $('<div />', {class: 'image'}).append($('<img />', {class: 'image', src: item.photoPath }));

                        let titleNews = ($('<div />', {class: 'title'}).text(item.name)).append($('<span />', {class: 'time-date'}).text(newsPublishDate) );
                        imageHref.append(imageDiv);
                        imageHref.append(titleNews);

                        let newsCard = $('<div />', {class: 'news-card'}).append(imageHref);
                        let info = $('<div />', {class: 'info'})
                                .append(($('<a/>', {title: item.tagName, href: item.tagUrl, target: '_blank'}))
                                        .append($('<span />', {class: 'program'}).text(item.tagName)));
                        newsCard.append(info);

                        $('.auto24-news-block').append($('<div />', {class: 'col-3'}).append(newsCard));
                    });
                    $(".auto24-news").removeClass('hidden');
                }
            },
            error: function() {
                console.info("Error! server auto24 doesn't respond(api/getLastNews) ");
            },
            contentType: "application/x-www-form-urlencoded; charset=UTF-8",
            dataType:"json"
        });

    });

    function getDateFormat(date, lang, today) {
        let monthsTranslate = ["січня","лютого","березня","квітня","травня","червня","липня","серпня","вересня","жовтня","листопада","грудня"];
        if( lang && lang == "ru" ) {
            monthsTranslate = ["января","февраля","марта","апреля","мая","июня","июля","августа","сентября","октября","ноября","декабря"];
        };

        let publishDate = new Date(date);
        let dateNow = new Date();

        if(publishDate.format('yyyyMMdd') ==  dateNow.format('yyyyMMdd')){
           return today+", "+publishDate.format('HH:mm');
        }

        if(publishDate.format('yyyyMMdd') !==  dateNow.format('yyyyMMdd')){
            return publishDate.format("dd")+" "+monthsTranslate[publishDate.getMonth()]+", "+publishDate.format('HH:mm');
        }
    }
</script>
