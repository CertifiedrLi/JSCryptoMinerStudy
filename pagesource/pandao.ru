<script type="text/javascript">
    

    // App-banner
    let app_closed = readCookie('app-closed');
    let loader_wrap = $('#loader-button');

    if (!app_closed) {
        let os = getMobileOperatingSystem();
        let app_banner = $('#app-banner');
        let app_title;
        let app_type;

        if (os === 'iOS') {
            app_type = 'app-iphone';
            app_title = 'Pandao для iPhone';
        } else if (os === 'Android') {
            app_type = 'app-android';
            app_title = 'Pandao для Android';
        }

        if (app_type) {
            app_banner.find('.app-banner-content').addClass(app_type);
            app_banner.find('.app-banner-title').html(app_title);
            //app_banner.find('.app-banner-link').attr('href', app_link);
            $('body').css('overflow','hidden');
            let swiper = new Swipe('#app-banner');
            swiper.onUp(function() {
                app_banner.animate({
                    top: '-100%'
                }, 500, function() {
                    $('body').css('overflow','auto');
                    createCookie('app-closed', 1, 7);
                });
            });
            swiper.run();
            app_banner.show();
        }

        $('.app-skip-button').click(function(){
            app_banner.animate({
                top: '-100%'
            }, 500, function() {
                $('body').css('overflow','auto');
                createCookie('app-closed', 1, 2);
            });
        });
    }

    let $page_num = 1;
    let $page_sort = 'sort';
    let $sort_filter = $('select#filter_option');
    let $qid = '98c8a0b20098714c';
    let $seed = '107978';
    let $loader_offset = 1500;
    let $loader_obj = $('#loader');
    let loader_top = $loader_obj.offset().top - $loader_offset;
    let isMobile = mobilecheck();

    let $filters = {
        'page': $page_num,
        'sort': $page_sort
    };

    if (!isMobile) {
        $sort_filter.select2({
            minimumResultsForSearch: -1,
            width: '164px'
        });
    }

    $sort_filter.on('change', function () {
        $filters = {
            'page': 0,
            'sort': this.value
        };
        load_ajax($filters);
        $loader_obj.show();
    });

    $('#filter_category').change(function(){
        window.location.href = $(this).val();
    });

    fn_init_mobile_filters();

    $(document).on('click', '#loader', function(){
        load_ajax($filters);
        return false;
    });

    function load_ajax(filters)
    {
        loader_top = loader_top + $loader_offset;
        $loader_obj.hide();
        loader_wrap.show();

        $.when($.ajax({
            url:'/ajax/main',
            type: "POST",
            dataType: "json",
            data: {
                "page": filters['page'],
                "sort": filters['sort'],
                "qid": $qid,
                "seed": $seed
            },
            async: true,
            success: function (data)
            {
                if (data.qs) {
                    if (data.qs.qid) {
                        $qid = data.qs.qid;
                    }
                    if (data.qs.seed) {
                        $seed = data.qs.seed;
                    }
                }

                if (data.output === 'end') {
                    $loader_obj.hide();
                    if (filters['page'] === 0) {
                        $('div#product-wrapper').html('Товары по Вашему запросу не найдены.');
                    }
                } else {
                    remove_empty_blocks();
                    if (filters['page'] > 0) {
                        $('div#product-wrapper').append(data.output);
                    } else {
                        $('div#product-wrapper').html(data.output);
                    }
                }
                filters['page']++;
            }
        }).done(function() {
            loader_top = $loader_obj.offset().top - $loader_offset;
            change_url();
            loader_wrap.hide();
            $loader_obj.show();
        }));
    }

    function change_url()
    {
        let url_params = {};

        if ($filters['sort']) {
            if ($filters['sort'] !== 'sort') {
                url_params['sort'] = $filters['sort'];
            }
        }

        /*if ($page_num > 1) {
            url_params['page'] = $page_num;
        }*/

        let params_str = $.param(url_params);

        let url = window.location.pathname + '?' + params_str;

        if (!params_str) {
            url = window.location.pathname;
        }

        window.history.pushState('', document.title, url);

        return true;
    }

    $(document).ready(function () {
        let $menu = $('.wrapper-line-menu');
        let $wrapper_menu = $('.wrapper-menu-tablet-desktop');
        let menu_top = $menu.offset().top;
        let filters_mob = $('.filters-mob');
        let prod_wrapper = $('#product-wrapper');

        $(window).scroll(function (event) {
            let y = $(this).scrollTop();

            if (!isMobile) {
                if (y >= menu_top + 4) {
                    $menu.css({position: 'fixed', top: 0, width: '100%', 'z-index': 10});
                    $wrapper_menu.css({position: 'fixed', top: $menu.height() + 1, width: '100%', 'z-index': 10});
                } else {
                    $menu.css({position: 'relative'});
                    $wrapper_menu.css({position: 'absolute', top: 'auto'});
                }
            } else {
                if (y > prod_wrapper.offset().top - $(window).height() + 50 && y < prod_wrapper.offset().top + prod_wrapper.height() - $(window).height()) {
                    filters_mob.show();
                } else {
                    filters_mob.hide();
                }
            }

            /*if ($loader_obj.is(":visible")) {
                if (y >= loader_top) {
                    load_ajax($filters);
                }
            }*/

        });
    });

    $(document).on('click', '.product-item', function(){
        let el = $(this);

        dataLayer.push({
            'ecommerce': {
                'click': {
                    'actionField': {'list': 'main_page'},
                    'products': [{
                        'name': '' + el.data('name') + '',
                        'id': '' + el.data('id') + '',
                        'price': '' + el.data('price') + '',
                        'category': 'Главная страница',
                        'variant': '' + el.data('variant') + '',
                        'position': el.index('.product-item') + 1
                    }]
                }
            },
            'event': 'gtm-ee-event',
            'gtm-ee-event-category': 'Enhanced Ecommerce',
            'gtm-ee-event-action': 'Product Clicks',
            'gtm-ee-event-non-interaction': 'False',
        });
        return true;
    });
</script>
</body>
</html>