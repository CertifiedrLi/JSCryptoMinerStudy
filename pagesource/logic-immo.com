<script type="text/javascript">
    sas.cmd.push(function () {
        var prefix = 'sas_';
        var blocks = document.getElementsByClassName('sas_block');

        for (i = 0; i < blocks.length; i++) {
            var block = blocks.item(i);
            var blockId = (block.id).substring(prefix.length);

            var renderId = block.dataset.render;
            if (renderId !== undefined) {
                // skip blocks already rendered
                continue;
            }

            sas.cmd.push(function () {
                sas.render(blockId);
            });
        }
    });
</script></body>
</html>