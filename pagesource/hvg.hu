<div class="cimlap-videok panelgap" data-scroll-event-name="ScrollToVideo">
	<!-- Main -->
	<article class="cimlap-videok-article big">
		<div class="video">
			<script type="text/javascript" src="https://content.jwplatform.com/libraries/Hp0mRubH.js"></script>
			<div id="videobox" align="center"></div>
		</div>
	</article>
	<script src="/Content/default/js-notbundled/jwplayervastloader.js"></script>
<script type="text/javascript">

	var relatedTimer;
	var timerInterval = 10;

	var vast = new Vast(false, timerInterval);
	vast.changeTimestamp(jwplayer.defaults);

	var player = jwplayer('videobox');

	player.setup(
	{
		playlist: "https://cdn.jwplayer.com/v2/playlists/1NXBQzPf",
		localization: {
			related: "Ajánlott videók",
			playlist: "További videók",
		    nextUp: "Következő videó"
		},
	    sharing: false
	});

	
	var previousItemIndex = 0;
	var relatedTimer;

	player.on('play', function (event) {

		window.clearTimeout(relatedTimer)

		var i = this.getPlaylistIndex();

		if (i != previousItemIndex) {
			var playList = this.getPlaylist();
			updateItemList(i, playList);
			previousItemIndex = i;
		}
	});

	registerAutoPlayNextVideo();

	function playThis(i)
	{
		player.playlistItem(i);
	}

	function updateItemList(s, p) {
		var playListHTML = "";

		for (var i = (s+1); i < (s+3); i++) {

			var videoItem = {
				PlaylistIndex: i,
				title: p[i].title,
				imageUrl: p[i].image,
			};

			playListHTML += Mustache.to_html('<div class="column-5" data-lazy-item data-lazy-control="video-list"><article class="cimlap-videok-article"><a class="img" onclick="playThis({{PlaylistIndex}});"><img src="{{imageUrl}}" alt="{{title}}"></a><div class="bg"></div><div class="content"><div class="content-inner"><h1><a onclick="playThis({{PlaylistIndex}});">{{title}}</a></h1></div></div><div class="video_icon"></div></article></div>', videoItem);
		}

		document.getElementById("playlist").innerHTML = playListHTML + '<div class="actions"><a href="/video" class="L-button on_dark">További videók</a></div>';
	}

