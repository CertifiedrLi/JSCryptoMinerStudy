                                    //alertify.success("अपने महत्वपूर्ण सूचना देख ली है ");
                                }
                            });
                            return false;
                        }
                    }
                    else {
                    }
                }
            });
        }
        function showcirculars(fw) {

            if (fw == 1 & swicir1 == 0) {
                dvCircular = $("#dvcircular1");
                swicir1 = 1;
                LoadCircular(1);
            }
            if (fw == 2 & swicir2 == 0) {
                dvCircular = $("#dvcircular2");
                swicir2 = 1;
                LoadCircular(2);
            }
            if (fw == 3 & swicir3 == 0) {
                dvCircular = $("#dvcircular3");
                swicir3 = 1;
                LoadCircular(3);
            }
            if (fw == 4 & swicir4 == 0) {
