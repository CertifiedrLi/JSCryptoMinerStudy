        function reset() {
            $("#toggleCSS").attr("href", "http://mpsc.mp.nic.in/gacdn/com/alert-Messages/themes/alertify.default.css");
            alertify.set({
                labels: {
                    ok: "OK",
                    cancel: "Cancel"
                },
                delay: 5000,
                buttonReverse: false,
                buttonFocus: "ok"
            });
        }
        function NoticeLoad() {
            $.fx.speeds._default = 1000;
            $.ajax({
                url: "/Administration/Handlers/Notices.ashx",
                cache: false,
                success: function (a) {
                    if (a != "NOMSG") {
                        var wsee = $.cookie('epnoticeseen');
                        if (wsee == 'yes') {

                        }
                        else {
                            var date = new Date();
                            date.setTime(date.getTime() + (5 * 60 * 1000));
                            $.cookie('epnoticeseen', 'yes', { expires: date });
                            reset();
                            alertify.set({ delay: 1000000 });
                            //alertify.alert("<span class='alert-success btn-large'>Important Announcements</span><br /><br />" + a);
                            alertify.alert("<span class='alert-success btn-large'>महत्वपूर्ण सूचना</span><br /><br />" + a, function (e) {
