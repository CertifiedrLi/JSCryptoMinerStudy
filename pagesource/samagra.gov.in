        function LoadCircular(p) {
            dvCircular.empty();
            dvCircular.addClass("loading");
            dvCircular.load("/KnowledgeSharing/Handlers/GetCirculars.ashx?fw=" + p, function () {
                dvCircular.removeClass("loading");
            });
        }

        function VedioDetail(BID) {
            dvCalculatScholarships.modal('show');
            var emda = new Object();
            emda.ID = BID;
            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: "Public/WebServices/PublicData.asmx/VideoDetail",

                data: JSON.stringify(emda),
                dataType: "json",
                success: function (data) {
                    var emda = JSON.parse(data.d);
                    if (emda != null) {

                        $.each(emda.rows, function (i, brd) {

                            __binfo.html('').append('<div>Title: <b>' + brd[2] + '</b>&nbsp Video Sub-Category: <b>' + brd[3] + '</b>&nbsp Uploaded Date: <b>' + brd[5] + '</b>&nbsp Total Views: <b>' + brd[1] + '</b></div><div><iframe width="700" height="350" src="' + brd[0] + '" frameborder="0" allowfullscreen></iframe></div>');
