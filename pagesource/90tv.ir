<script type="text/html" class="leagueschedule-list">
    <table class="league-schedule table table-striped table-condensed table-hover">
        <tbody>
        <% var col = 4 %>
         <% var match_id = [] %>
         <% for (var key in data) { %>

            <% if (key != "data") { %>
                <% if( typeof data['data']['videos'] !== 'undefined') { %>
                    <% if (data['data']['videos'] != '') { %>
                        <% col = 5 %>
                    <% } %>
                <% } %>
                <tr>
                    <th colspan="<%= col %>" class="text-center"><%= key %></th>
                </tr>
            <% } %>

            <% if (key != "data") { %>

                <% for (var i = 0;i < data[key] . length;i++) { %>
                    <tr>
                            <%  match_id[i] = data[key][i].id %>
                            <% if( typeof data['data']['videos'] !== 'undefined') { %>
                                <% if (data['data']['videos'] != '') { %>
                                    <td class="text-left" width="35px">
                                        <% _.forEach(data['data']['videos'], function(value, key){ %>
                                            <% if( value.competition_id == match_id[i] ) { %>
                                                   <%
                                                    if( value.title != '' ) {
                                                        value.title = value.title.replace(new RegExp(' ', 'g'), '-');
                                                    }
                                                    %>
                                                   <%  if( value.show_detail ) { %>
                                                        <a class="pull-right" href="/competition/<%=match_id[i]  %>/<%=value.title %>" target="_blank"><i class="glyphicon glyphicon-info-sign"></i></a>
                                                   <%  } %>
                                                    <%  if( value.show_video ) { %>
                                                        <a class="pull-right u-textGreen" href="/video/<%=value.video %>/<%=value.name %>" target="_blank"><i class="glyphicon glyphicon-play"></i></a>
                                                    <%  } %>
                                            <% } %>
                                        <% }); %>
                                    </td>
                                <% } %>
                            <% } %>
                        <td class="text-left"><%= data[key][i] . home %></td>
                        <td class="result text-center">
                            <% if(data[key][i] . result.length > 0) { %>
                                <%= persianAllNumbers(data[key][i] . result) %>
                            <% } else { %>
                                -
                            <% } %>
                        </td>
                        <td class="text-right"><%= data[key][i] . away %></td>
                        <td class="time-pin text-left">
                            <% if(data[key][i] . status == 'post') { %>
                                تعویق
                            <% } else {%>
                                <%= persianAllNumbers(data[key][i] . time) %>
                            <% } %>
                        </td>
                    </tr>

                <% } %>
            <% } %>
        <% } %>
        </tbody>
    </table>
</script>
