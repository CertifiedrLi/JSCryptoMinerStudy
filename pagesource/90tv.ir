</script>
<script type="text/html" class="leagueprogramgroup-list">
    <% var col_group = 5 %>
    <% var program_id = [] %>
    <table class="league-table league-schedule table table-striped table-condensed table-hover">

        <% for (var k in data.program){ %>
            <% if( data.videos != '' ) { %>
                <% col_group = 6 %>
            <% } %>
        <tr>
            <th colspan="<%=col_group %>" class="text-center"><%= k %></th>
        </tr>
        <% var games = data.program[k]; %>
            <% for (var game in games){ %>
            <tr>
                <% if( data.videos != '' ) { %>
                    <td class="text-left" width="35px">
                        <%  program_id[k] = games[game].id  %>
                        <% _.forEach(data.videos, function(value, key){ %>
                            <% if( value.competition_id == program_id[k] ) { %>
                                    <%
                                        if( value.title != '' ) {
                                            value.title = value.title.replace(new RegExp(' ', 'g'), '-');
                                        }
                                    %>
                                    <%  if( value.show_detail ) { %>
                                        <a class="pull-right" href="/competition/<%=program_id[k]  %>/<%=value.title %>" target="_blank"><i class="glyphicon glyphicon-info-sign"></i></a>
                                    <% } %>
                                    <%  if( value.show_video ) { %>
                                        <a class="pull-right u-textGreen" href="/video/<%=value.video %>/<%=value.name %>" target="_blank"><i class="glyphicon glyphicon-play"></i></a>
                                    <% } %>
                            <% } %>
                        <% }); %>
                    </td>
                <% } %>
                <td class="text-left"><%= games[game].home %></td>
                <td class="result text-center"><%= persianAllNumbers(games[game].result) %></td>
                <td class="text-right"><%= games[game].away %></td>
                <td class="text-left time-pin">
                    <% if(games[game].status == 'post') { %>
                    تعویق
                    <% } else { %>
                        <%= persianAllNumbers(games[game].time) %>
                    <% } %>
                </td>
            </tr>
            <% } %>
        <% } %>
    </table>

    <table class="league-table table table-condensed table-hover bottom-ranking">
        <tr class="head-ranking">
            <th></th>
            <th class="text-center">تیم</th>
            <th>بازی‌ها</th>
            <th>امتیاز</th>
        </tr>
        <% for (var k in data.ranking.ranking){ %>
        <tr>
            <td><%= persianAllNumbers(parseInt(k)+1) %> </td>
            <td>
                <%= data.ranking.ranking[k].name %>
            </td>
            <td>
                <%= persianAllNumbers(data.ranking.ranking[k].plays) %>
            </td>
            <td>
                <%= persianAllNumbers(data.ranking.ranking[k].pts) %>
            </td>
        </tr>
        <% } %>
        <tr>
            <td colspan="4" class="text-center">
                <a href="/fronttournament/ranking/fullview?leaguename=<%= data.ranking.leagueName %>" class="ranking-fullview">مشاهده کامل جدول</a>
            </td>
        </tr>
    </table>

</script>
