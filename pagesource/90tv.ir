        <time class="special">
            <% } else { %>
            <time>
                <% } %>
                <%= persianAllNumbers(data.time) %>
            </time>
            <%

            var iconClass = 'text-default fa';
            var urlPrefix;

            switch(data.type) {
                case 'news':
                    urlPrefix = 'news';
                    break;
                case 'gallery':
                    iconClass += ' fa-camera';
                    urlPrefix = 'gallery';
                    break;
                case 'newspaper':
                    iconClass += ' fa-newspaper-o';
                    urlPrefix = 'newspaper';
                    break;
                case 'video':
                    iconClass += ' fa-video-camera';
                    urlPrefix = 'video';
                    break;
            }
            %>
            <span class="<%= iconClass %>"></span>
            <a href="<%= '/' + urlPrefix + '/' + data.id + '/' + data.name %>" class="detail">
                <%= data.title %>
            </a>
    </li>
</script>
