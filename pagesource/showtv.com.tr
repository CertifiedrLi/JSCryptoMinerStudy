<script>
    function makeStreamingSwipe (day) {
        var data = $('div[data-day="'+day+'"] > .swiper-slide').clone();
        var active = $('div[data-day="'+day+'"] > .swiper-slide.live').index();
        var activeIndex = 0;
        if (active != '-1') {
            activeIndex = active;
        }
        if (data.length > 0) {
            $('.streamingswipe .swiper-wrapper').hide();
            $('.streamingswipe .swiper-wrapper').empty().append(data);
            $('.streamingswipe .swiper-wrapper').show('fast');
            if (typeof streamingswipe != 'undefined') {
                streamingswipe.update();
            } else {
                var streamingswipe = new Swiper('.streamingswipe', {
                    slidesPerView: 4,
                    spaceBetween: 1,
                    autoHeight: true,
                    paginationClickable: true,
                    navigation: {
                        nextEl: '.nextstreamingswipe',
                        prevEl: '.prevstreamingswipe',
