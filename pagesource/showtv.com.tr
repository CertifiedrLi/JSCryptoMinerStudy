        <script>
			$(document).ready(function () {

				var splashCookieSet = getCookie("splashPage");
				var cookieSet = new Array();
                 
				 
					$.getJSON("/index/splashPage", {})
							.done(function (splashData) {
								if (splashData) {

									$.each(JSON.parse(splashData), function (i, item) {
									var splashCount = 0;

									if (splashCookieSet) {
										var splashCookie = JSON.parse(splashCookieSet);
										if (splashCookie[i]) {
											splashCount = parseInt(splashCookie[i].count);
										}
									}

										if (splashCount < 1) {
											var backgroundImg = item.image_background + "?" + item.version;
											var splashUrl = item.url+'?utm_source=splash'; 
											splashCount = splashCount + 1;
											$(".main-modal-a").attr("href", splashUrl);
											$(".main-modal-img-video").css("width","890px");
											$(".main-modal-img-video").css("height","500px");
											$(".main-modal-img-video").css("background-image", "url(" + backgroundImg + ")");

											if (item.splash_type == 1) {
												$(".splashImg").hide();
												$(".splashIframe").hide();

											} else if (item.splash_type == 2) {
												$(".splashImg").attr("src", item.image_front);
												$(".splashIframe").attr("src", "http://www.showtv.com.tr/video/embed/" + item.video_id);
											}	
											$(".main-modal").show();
											cookieSet[i] = {"splashId": item.splash_id, "splashStatus": "2", "count": splashCount};
											setCookie("splashPage", JSON.stringify(cookieSet), 0.1);
											return false;
										} else {
											cookieSet[i] = {"splashId": item.splash_id, "splashStatus": "2", "count": splashCount};
											setCookie("splashPage", JSON.stringify(cookieSet), 0.1);
										}

									});

								}

							});
			    	
				
					
				$.getJSON("/index/headline", {})
							.done(function (headlineData) {
								if (headlineData) {
									var headlineJSON = JSON.parse(headlineData);
									var randomItem = Math.floor(Math.random() * 2) ;

									var headlineId = Object.keys(headlineJSON)[randomItem];
									var headline = headlineJSON[headlineId];
								
									if (headline) {
//										console.log(headline);
												var leftImg =  headline.leftimage + "?" ;
												var rightImg =  headline.rightimage + "?" ;
												var backgroundImg =  headline.background + "?" ;
												$(".leftt img").attr("src", leftImg);
												$(".rightt img").attr("src", rightImg);
												$(".widget-new-promotion").css("background-image",  "url(" + backgroundImg + ")");
												$(".widget-new-promotion a").attr("href", headline.url+'?utm_source=headline');
												$(".centerr .label").text(headline.redline);
												$(".centerr .shorttext").text(headline.shorttext);


									}

								}

				});	

				// MAIN MODAL CLOSE
				$(".main-modal-close").click(function () {
					$(".main-modal").remove();
				});
				function setCookie(cname, cvalue, exdays) {
					var d = new Date();
					d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
					var expires = "expires=" + d.toUTCString();
					document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
				}

				function getCookie(cname) {
					var name = cname + "=";
					var ca = document.cookie.split(';');
					for (var i = 0; i < ca.length; i++) {
						var c = ca[i];
						while (c.charAt(0) == ' ') {
							c = c.substring(1);
						}
						if (c.indexOf(name) == 0) {
							return c.substring(name.length, c.length);
						}
					}
					return "";
				}

			});
			var SHOWTV_URL = 'http://www.showtv.com.tr';
