                        },
                        767: {
                            slidesPerView: 1,
                        },
                        425: {
                            slidesPerView: 1,
                        }
                    }
                });
            }
            streamingswipe.slideTo(activeIndex);
        }
    }
    $(document).ready(function () {
        var today = '2018-03-17';
        makeStreamingSwipe(today);
        $('.streaming-dropdown a').click(function (event) {
            $(this).closest('.dropdown.white').find('.d-title').text($(this).text());
            event.preventDefault();
            var day = $(this).data('day');
            makeStreamingSwipe(day);
        });
    });

</script>
