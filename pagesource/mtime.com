        document.getElementById("dkek").innerHTML = '<video id="videod" src="http://vfx.mtime.cn/Video/2018/03/09/flv/180309152915171998.mp4" autoplay height="550" width="1920"></video>';
    }

    function setCookieMethod(name, value, time) {
        var strsec = getsec(time);
        var exp = new Date();
        exp.setTime(exp.getTime() + strsec * 1);
        document.cookie = name + "=" + escape(value) + ";expires=" + exp.toGMTString();
    }

    function getsec(str) {
        var str1 = str.substring(1, str.length) * 1;
        var str2 = str.substring(0, 1);
        if (str2 == "s") {
            return str1 * 1000;
        }
        else if (str2 == "h") {
            return str1 * 60 * 60 * 1000;
        }
        else if (str2 == "d") {
            return str1 * 24 * 60 * 60 * 1000;
        }
    }

    function getCookieMethod(name) {
        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");

        if (arr = document.cookie.match(reg))

            return (arr[2]);
        else
            return null;
    }
    function load() {
        var lastShowFlashTime = getCookieMethod("xiaohuangren");
        if (lastShowFlashTime) {
            isCookie = true;
        }
    }
    function browserRedirect() {
        var sUserAgent = navigator.userAgent.toLowerCase();
        var bIsIpad = sUserAgent.match(/ipad/i) == "ipad";
        var bIsIphone = sUserAgent.match(/iphone/i) == "iphone";
        var bIsIpod = sUserAgent.match(/iphone/i) == "ipod";
        var bIsAndroid = sUserAgent.match(/android/i) == "android";
        var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";

        if (bIsIpad || bIsIphone || bIsIpod || bIsAndroid || bIsWM) {
            isMobile = true;
        }
    }

    if (document.getElementById("videod")) {
        var videoD = document.getElementById("videod");
        if (videoD.ended) {
            document.getElementById("dkek").innerHTML = '';
            setCookieMethod("xiaohuangren", "1", "h2");
        }
        videoD.addEventListener("ended", function () {
            document.getElementById("dkek").innerHTML = '';
            setCookieMethod("xiaohuangren", "1", "h2");
        })
    }

</script>
