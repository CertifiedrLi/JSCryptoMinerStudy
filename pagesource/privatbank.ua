            <script defer="defer">
    (function(window) {
        'use strict';
        const RATES_KEY = 'pb_deposit_rates';
        const RATES_SET_TIME_KEY = 'pb_deposit_rates_set_time';
        var sessStorage = window.sessionStorage;
        var rates = JSON.parse(sessStorage.getItem(RATES_KEY)),
            ratesSetTime = sessStorage.getItem(RATES_SET_TIME_KEY);

        window.onload = function() {
            if(rates === null || ratesSetTime === null || !_allRatesExists(rates.data) || _ratesMayBeExpired(ratesSetTime)) {
                $.ajax({
                    url: '/ajax/pb/get-deposit-rates',
                    success: function(resp) {

                        resp = JSON.parse(resp);
                        if(resp && resp.errCode === '' && resp.state === 'r' && _allRatesExists(resp.data)) {
                            sessStorage.setItem(RATES_KEY, JSON.stringify(resp));
                            sessStorage.setItem(RATES_SET_TIME_KEY, new Date());

                            _fillRates(resp);


                        }
                    }
                });
            } else {
                _fillRates(rates);
            }
        };

        function _fillRates(resp) {
            rates = resp.data;

            $('#uah_rate').text(rates.max_uah);
            $('#usd_rate').text(rates.max_usd);
            $('#eur_rate').text(rates.max_eur);

            $('#rates_container').removeClass('hidden');
        }

        function _ratesMayBeExpired(utc) {
            var date = new Date();
            var oldDate = new Date(utc);

            return date.getDate() !== oldDate.getDate();
        }

        function _allRatesExists(rates) {
            return rates.max_uah != false && rates.max_usd != false && rates.max_eur != false;
        }
    })(window);
</script>
    

        








