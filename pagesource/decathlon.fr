jQuery(document).ready(function () {
     setInterval(function () {
        moveRight();
    }, 4000);
   
  var slideCount = jQuery('#sliderrappel ul li').length;
  var slideWidth = jQuery('#sliderrappel ul li').width();
  var slideHeight = jQuery('#sliderrappel ul li').height();
  var sliderrappelUlWidth = slideCount * slideWidth;
  
  jQuery('#sliderrappel').css({ width: slideWidth, height: slideHeight });
  
  jQuery('#sliderrappel ul').css({ width: sliderrappelUlWidth, marginLeft: - slideWidth });
  
  jQuery('#sliderrappel ul li:last-child').prependTo('#sliderrappel ul');

    function moveLeft() {
        jQuery('#sliderrappel ul').animate({
            left: + slideWidth
        }, 600, function () {
            jQuery('#sliderrappel ul li:last-child').prependTo('#sliderrappel ul');
            jQuery('#sliderrappel ul').css('left', '');
        });
    };

    function moveRight() {
        jQuery('#sliderrappel ul').animate({
            left: - slideWidth
        }, 600, function () {
            jQuery('#sliderrappel ul li:first-child').appendTo('#sliderrappel ul');
            jQuery('#sliderrappel ul').css('left', '');
        });
    };

    jQuery('a.control_prev').click(function () {
        moveLeft();
    });

    jQuery('a.control_next').click(function () {
        moveRight();
    });

    jQuery('#sliderrappel').toggle('slow');

});    
}
