    <span id='yes'>Fabulous</span>
    <span id='no'></span>
    <span id='primary_button'>#yes</span>
</script>

<script id="ApplyGiftCompleteDialogContent" type="text/html">
  <div>
    <div id='header'><h2 class="centered">You're promoted!</h2></div>
    <div id='content'>
      <p class="centered">
        Your promo code has been applied and you're all set to use PicMonkey Premium, gratis. Now, go blow the doors off photography as we know it.
      </p>
    </div>
    <div id='button'>Righteo!</div>
</script>

<script id="ApplyGiftSupremiumCompleteDialogContent" type="text/html">
  <div>
    <div id='header'><h2 class="centered">Promo code applied</h2></div>
    <div id='content'>
      <p class="centered">
        Enjoy your free Supremium! We'll pop you back to your regular membership on {{expiration_date}}, when it expires.
      </p>
    </div>
    <div id='button'>Noice!</div>
</script>

