<script id="YesNoDialogTemplate" type="text/html">
  <div class="modal-dialog modal-dialog-manage"><div class="modal-content yes-no-content">
    <div class="modal-header">
      <div id='title'>Title goes here</div>
    </div>
    <div class="modal-body">
      <p id='message'>Message goes here</p>
      <div id='buttons'>
        <button type='button' id='yes' class='btn btn-manage yes'>Yes</button>
        <button type='button' id='no' class='btn btn-manage no'>No</button>
      </div>
    </div>
  </div></div>
</script>

<script id="EmailTakenErrorDialogContent" type="text/html">
  <div>
    <span id='title'>Email taken</span>
    <span id='message'>
    That email is already taken!</span>
    <span id='yes'>Okay</span>
    <span id='no'></span>
    <span id='primary_button'>#yes</span>
  </div>
</script>

<script id="MonkeyLoveDialogTemplate" type="text/html">
  <div class="modal-dialog"><div class="modal-content">
    <div class="modal-header">
        <div id='close_window' class='close_button white_gray back'></div>
        <div class='monkey_love'></div>
        {{& header}}
    </div>
    <div class="modal-body">
      {{& content}}
      <button id='button' type='button' class='btn btn-primary back' style='display:block;margin-left:auto;margin-right:auto'>{{& button}}</button>
    </div>
  </div></div>
</script>

<script id="PlanChangedContentTemplate" type="text/html">
  <div>
    <span id='title'>All done!</span>
    <span id='message'>
      {{#yearly}}
      We've changed your subscription from monthly to annual.
      {{/yearly}}
      {{^yearly}}
      We've changed your subscription from annual to monthly.
      {{/yearly}}
