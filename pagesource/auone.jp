                                                                                                                          <li class="js-news__tab-item news__tab-item" data-tab="6" data-ga-action="Function" data-ga-label="コネタタブ">コネタ</li>
                                                                          </ul>
                                    <div class="js-news__tab--active-marker news__tab--active-marker" style="width: 127px; left: 10px; opacity: 1;"></div>
                                </div>
                            </div>
                        </div>
                                        

                    
                                                                                                                        <div class="js-news__genre news__genre" style="display:block;">
                                                            
                                <section class="js-lifetools lifetools">
    <div class="lifetools__items">

        
        <div class="js-weather weather">
            
            <a href="/setting/services/" class="js-analytics" data-ga-action="Daily" data-ga-label="天気フィラー">
                <i class="lifetools__sprite lifetools__sprite--icon01"></i>
                <span>天気<i class="lifetools__sprite lifetools__sprite--icon04"></i></span>
            </a>
            
            <script type="text/template" class="js-template">
                <a href="http://weather.auone.jp/area/pref?prefecture_code=<%= model.get('pref_code') %>&subdiv1_code=<%= model.get('area_code') %>" class="js-weather__link weather__link js-analytics" data-ga-action="Daily" data-ga-label="天気">
                    <p class="lifetools__personal--icon"><% if (isValid === true) { %><img width="30" height="20" class="js-weather__img" src="<%= model.get('weather_today') %>"><% } %></p>
                    <div class="lifetools__personal">
                        <p class="js-weather__area weather__area"><%= model.get('area_name') %></p>
                        <dl class="weather__data">
                            <dt class="weather__label">最高気温</dt>
                            <dd class="js-weather__maxtemp weather__maxtemp"><%= model.get('high_temper_today') %>℃</dd>
                            <dt class="weather__label">降水確率</dt>
                            <dd class="js-weather__precipitation weather__precipitation"><%= model.get('rain_probab_today') %>%</dd>
                        </dl>
                    </div>
                </a>
            </script>

        </div>
        


        
        <div class="js-fortune fortune">
            <script type="text/template" class="js-template">
                <a href="<%= model.get('link') %>" class="js-fortune__link fortune__link js-analytics" data-ga-action="Daily" data-ga-label="占い">
                    <p class="lifetools__personal--icon"><i class="js-fortune__sprite fortune__sprite fortune__sprite--icon<%= model.get('astro_code') %>"></i></p>
                    <div class="lifetools__personal">
                        <p class="js-fortune__rank fortune__rank"><%= model.get('rank') %>位</p>
                        <p class="js-fortune__label fortune__label"><%= model.get('astro_name') %></p>
                    </div>
                </a>
            </script>
            
            <a href="/setting/services/" class="fortune__link js-analytics" data-ga-action="Daily" data-ga-label="占いフィラー">
                <i class="lifetools__sprite lifetools__sprite--icon02"></i>
                <span>占い<i class="lifetools__sprite lifetools__sprite--icon04"></i></span>
            </a>
            
        </div>
        
    </div>
</section>




