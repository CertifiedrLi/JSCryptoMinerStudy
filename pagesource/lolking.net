<script>
var fixedPos = 74;
function adjustFixedAdClasses() {
    $('#rightcol').height("");
	if ($('#leftcol').outerHeight() != $('#rightcol').outerHeight() && $('#leftcol').outerHeight() > $('#rightcol').outerHeight()) $('#rightcol').height($('#leftcol').outerHeight() - 10);
    var fixedColHeight = $('#fixed-column').outerHeight(true);
    if ($('.overview-footer').length > 0) {
    	var maxDistance = $('.overview-footer').offset().top - 10;
    	var fixedBottom = "ad-fixed-bottom champion";
    } else if ($('#bottom-leaderboard').length > 0) {
    	var maxDistance = $('#bottom-leaderboard').offset().top - 10;
    	var fixedBottom = "ad-fixed-bottom";
    } else {
        var maxDistance = $('footer').offset().top - 10;
        var fixedBottom = "ad-fixed-bottom";
    }
    //  1.
    //      you scrolled past the top of the fixed ad
    //      the gap between the top of the footer and the bottom of the last widget is bigger than the fixed ad's height
    //      the bottom of the fixed ad is above the footer
    //
    //  2.
    //      the bottom of the fixed ad is below or equal to the footer
    //      the gap between the top of the footer and the bottom of the last widget is bigger than the fixed ad's height
    //      the fixed ad is already fixed
    if ($(window).scrollTop() >= $('#rightcol').offset().top-fixedPos && (maxDistance-$('#fixed-column').offset().top) > fixedColHeight && $('#fixed-column').offset().top + $('#fixed-column').outerHeight(true) < maxDistance) {
        $("#fixed-column").addClass("ad-fixed");
        $("#fixed-column").removeClass(fixedBottom);
    } else if ($('#fixed-column').offset().top + $('#fixed-column').outerHeight(true) >= maxDistance && (maxDistance-$('#rightcol').offset().top) > fixedColHeight && $("#fixed-column").hasClass('ad-fixed')) {
        $("#fixed-column").removeClass("ad-fixed");
        $("#fixed-column").addClass(fixedBottom);
    } else if ($(window).scrollTop() <= $('#fixed-column').offset().top-fixedPos && $('#fixed-column').hasClass(fixedBottom)) {
        $("#fixed-column").removeClass(fixedBottom);
        $("#fixed-column").addClass("ad-fixed");
    } else {
        $("#fixed-column").removeClass("ad-fixed");
    }
}

$( document ).ready(function() {
    adjustFixedAdClasses();
});


$(window).scroll(function(){
    adjustFixedAdClasses();
});

</script>
