      if (data.length == 0) $("#myCarousel").hide();
      for (var i in data) {
        var news = data[i];
        var url = "";
        var blurb = "";
        var subtitle = "";
        var variables = news.content.split('<p>').join('');
        variables = variables.split('</p>\n').join('=');
        variables = variables.split('</p>').join('');
        variables = variables.split('\n').join('');
        var pairs = variables.split('=');

        for(var j = 0; j < pairs.length; j+=2){
          if(pairs[j] =="url"){
            url = pairs[j+1];
          }
          else if(pairs[j] =="subtitle"){
            subtitle = pairs[j+1];
          }
          else if(pairs[j] =="blurb"){
            blurb = pairs[j+1];
          }
        }


        var active = '';
        var itemActive = '';
        if (i == 0) {
          active = ' class="active"';
          itemActive = ' active';
        }
        var html = $('<li data-target="#myCarousel" data-slide-to="' + i + '"' + active + '></li>');
        $("#carousel-indicators").append(html);
        $("#carousel-inner").append('<a href="' + (news.meta && news.meta.url ? news.meta.url : url) + '" id="item' + i + '" class="item' + itemActive + '"></a>');
        var item = $("#item" + i);
        item.append('<div class="news-category">' + subtitle + '</div>');
        item.append('<div class="gradient"></div>');
        item.append($('<div class="img-back" style="background-image:url(' + news.featured_image + ')" />'));
        item.append('<a href="' + url + '"><span class="header1">' + news.title + '</span></a>');
        item.append('<a href="' + url + '"><p>' + blurb + '</p></a>');
      }
    });
  });

