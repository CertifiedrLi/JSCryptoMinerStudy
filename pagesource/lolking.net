                    url: "//loldata.services.zam.com/v1/skin?pageSize=100&sort=-audit.created.date",
                    cache: true
                })
                .done(function(data) {
                  $.ajax({
                      url: "//loldata.services.zam.com/v1/champion?fields=champion_id,name&sort=name",
                      cache: true
                  })
                  .done(function(data2) {
                    var newSkins = [];
                    var index = 0;
                    for (var i in data) {
                      if(data[i].media){
                        for(var j in data2){
                          if(data2[j].champion_id == data[i].champion_id){
                            data[i].champion = data2[j];
                          }
                        }
                        newSkins.push(data[i]);
                        index++;
                        if(index == 4) break;
                      }
                    }

                    newSkins = newSkins.reverse();

                    for(var i in newSkins){
                      var skin = newSkins[i];
                      var uri = "";
                      for (var j in skin.media) {
                        if (skin.media[j].type == 'SKIN_VIGNETTE_IMAGE_NO_FADE') uri = skin.media[j].url;
                      }
                      var newElement = $('<li><a href="/models?champion=' + skin.champion_id + '&skin=' + skin.num +'">' + (skin.name.length > 15 ? skin.name.substring(0, 15) + '...' : skin.name) + '</a></li>');
                      newElement.insertAfter($("#model-text"));

                      var cl = '';
                      if(i == 3) cl = " hidden-xs hidden-sm hidden-md";
                      if(i == 2) cl = " hidden-xs hidden-sm";

                      $("#model-vignette").append('<div class="col-xs-6 col-md-4 col-lg-3' + cl + '">' +
                        '<a href="/models?champion=' + skin.champion_id + '&skin=' + skin.num + '" class="card-nav-img card-anim-lift">' +
                          '<div class="card-img" style="background-image:url(' + uri + ');"></div>' +
                          '<div class="card-info">' +
                            '<small>' + skin.name.replace(skin.champion.name, '').trim() + '</small>' + skin.champion.name +
                          '</div>' +
                          '<span class="card-info-extra"><i class="icon-riot-fist ng-scope"></i>' + skin.rp_cost + '</span>' +
                        '</a>' +
                      '</div>');
                    }
                  });
