                        url: '/plugins/',
                        target: '_self'
                    });
                }, 2000);

                var d = new Date();
                d.setTime(d.getTime() + (7 * 24 * 60 * 60 * 1000));
                var expires = "expires=" + d.toUTCString();
                document.cookie = "MetaStats-Contribute" + "=JoinUs;" + expires + ";path=/";
            }

            $(document).on("mousemove", '.card-list-item', function (event) {
                var left = event.pageX - $(this).offsetParent().offset().left + 15;
                var top = event.pageY - $(this).offsetParent().offset().top - 100;

                $(this).find('#card-image-hover').css({top: top, left: left}).show();
            });

            $(document).on("mouseout", '.card-list-item', function (event) {
                $(this).find('#card-image-hover').hide();
            });

        });

        function getCookieValue(a) {
            var b = document.cookie.match('(^|;)\\s*' + a + '\\s*=\\s*([^;]+)');
            return b ? b.pop() : '';
        }

        
        function getTopDeck(deckCount, deckid) {
            var deckid = (typeof deckid !== 'undefined') ? deckid : "";

            $("#top-archetype-deck").html("");
            $('#myModal').modal('show');
            TopDeckSpinner = new Spinner(opts).spin(document.getElementById('top-archetype-deck'));

            $.ajax({
                url: "/topearchetypedeck.php?deckcount=" + deckCount + "&archetype=" + archetype + "&deckid=" + deckid,
                dataType: "text",
                async: true,
                contentType: "text/html; charset=utf-8",
                timeout: 10000
            }).done(function (responseData) {
                //console.log(responseData);
                TopDeckSpinner.stop();
                $("#top-archetype-deck").html(responseData);

            }).fail(function (err, ee, msg) {
                console.log("Error: " + msg);
                $("#top-archetype-deck").html("<h3>Error Fetching Data... Try again<h3>");
            });
        }

