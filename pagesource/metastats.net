        function getGameStats() {

            var spinner = new Spinner(opts).spin(document.getElementById('region-graph'));
            var gameformatSpinner = new Spinner(opts).spin(document.getElementById('gameformat-graph'));
            var rankedgamesSpinner = new Spinner(opts).spin(document.getElementById('rankedgames-graph'));

            $.ajax({
                url: "/metastats/regiongraph.php",
                dataType: "json",
                async: true,
                timeout: 15000
            }).done(function (jsonData) {
                //console.log(jsonData);
                spinner.stop();

                var data = new google.visualization.DataTable(jsonData);
                //console.log(data);
                var options = {
                    title: 'Hearthstone Games by Region'
                };

                var chart = new google.visualization.PieChart(document.getElementById('region-graph'));
                chart.draw(data, options);
            }).fail(function (err, ee, msg) {
                console.log("Error: " + msg);
                $("#region-graph").html("<h3>Error Fetching Data... Try again<h3>");

            });

            $.ajax({
                url: "/metastats/gameformat.php",
                dataType: "json",
                async: true,
                timeout: 15000
            }).done(function (jsonData) {
                //console.log(jsonData);
                gameformatSpinner.stop();

                var data = new google.visualization.DataTable(jsonData);
                //console.log(data);
                var options = {
                    title: 'Hearthstone Games by Format'
                };

                var chart = new google.visualization.PieChart(document.getElementById('gameformat-graph'));
                chart.draw(data, options);
            }).fail(function (err, ee, msg) {
                console.log("Error: " + msg);
                $("#gameformat-graph").html("<h3>Error Fetching Data... Try again<h3>");

            });

            $.ajax({
                url: "/metastats/rankedgames.php?region=all&range=all",
                dataType: "json",
                async: true,
                timeout: 15000
            }).done(function (jsonData) {
                //console.log(jsonData);
                rankedgamesSpinner.stop();

                var data = new google.visualization.DataTable(jsonData);
                //console.log(data);
                var options = {
                    focusTarget: 'category',
                    title: 'Hearthstone Ranked Games (Standard)',
                    hAxis: {
                        title: 'Rank'
                    },
                    vAxis: {
                        title: '% of Total Games',
                        format: '#\'%\'',
                        viewWindow: {
                            min: 0
                        }
                    },
                    legend: {position: 'none'}
                };

                var chart = new google.visualization.ColumnChart(document.getElementById('rankedgames-graph'));
                chart.draw(data, options);
            }).fail(function (err, ee, msg) {
                console.log("Error: " + msg);
                $("#rankedgames-graph").html("<h3>Error Fetching Data... Try again<h3>");
            });
        }

