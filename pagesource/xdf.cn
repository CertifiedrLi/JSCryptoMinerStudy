	<script type="text/javascript">
            /*显示购物车里数字*/
            var cartHtml = myGetCookie("Xdf.WebPay.V4.Cart");
            var cartArr = "";
            var cart = 0;
            if(cartHtml != undefined && cartHtml != "" && cartHtml != null){
                cartArr = cartHtml.toString().split("|");
                cart = cartArr.length;
                var num=0;
                var Price =0;
                for(var i=0;i<cart;i++){
                    cartInfo = cartArr[i].split(",");
                    Price += parseInt(cartInfo[4]); 
                    $('.u-price').html("￥"+Price);
                    $('#kInfo').append("<li style='width:300px;'><a target='_blank' href='http://souke.xdf.cn/ClassDirect.aspx?sid="+ cartInfo[2] +"&code="+ cartInfo[0]+"'>"+ decodeURIComponent(cartInfo[1]) + "-" + cartInfo[0] +"</a></li>");
                    num++;
                }
            }
            if(cart >0){
                $(".J-nub").show().html(cart);
                $("#gnum").html(cart);
            }else{
                $(".J-nub").hide().html("");
                $("#gnum").html("");
            }
            $('.Gnum').html("共<i>"+ cart +"</i>个班级");
            /*判断登录状态，是否显示绑定学员号*/
            var nickCookie = myGetCookie("U2NickName");
            if (nickCookie != null && nickCookie != "null" && nickCookie!="") //必须判断nick != "null"
            {
                $(".J-dlzc").hide();
                $(".J-bdxx").show();    
            }

            /*判断当在第一屏的时候，回到顶部按钮的时候隐藏*/
            $(window).scroll(function(){
                if($(window).scrollTop()>100){ //垂直滚动条offset
                    $(".totop").show();
                }else{
                    $(".totop").hide();
                }
            }); 
        </script>
