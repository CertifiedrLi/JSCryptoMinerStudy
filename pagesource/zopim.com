      lang = 'ja';
    } else {
      lang = host;
    }

    changeLang(lang);

    var loadWidth = $(window).width();

    if (loadWidth < 700) {
      $('.nav-pills').on('click',function(){
       $('#header_lower').toggle();
      })
    }

    // detect touch devices
    if ('ontouchstart' in window || navigator.maxTouchPoints) {
      document.body.className += ' touch';
    } else {
      document.body.className += ' no-touch';
    }

    webutils.storeCampaign(); // store campaign code in cookie for attribution
  });

  function changeLang(lang) {
    $('#language').val(lang);

    $('#language_menu li').removeClass('active');
    $('#language_menu #' + lang).addClass('active');

    $('html')[0].lang = lang;

    $('a[href*="dashboard.zopim.com"], a[href*="account.zopim.com"]').each( function() {
      var href = $(this).attr('href');

      if (href.match('lang=')) {
        href = href.replace(/lang=[a-z-.]+/i, 'lang=' + lang)
      } else {
        href += (href.match('\\?') ? '&' : '?') + 'lang=' + lang;
      }

      $(this).attr('href', href);
    });

    /* set widget language as well */
    $zopim(function() {
      $zopim.livechat.setLanguage(lang);
    });
  }
</script>

