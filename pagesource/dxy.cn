    function showRqtjAttentions() {
        var ipts = document.getElementsByName("rzjg");
        var iptVals = "";
        for (var i = 0; i < ipts.length; i++) {
            var ipt = ipts[i];
            iptVals += ipt.value + ",";
        }

        var ipts = document.getElementsByName("rqtj");
        for (var i = 0; i < ipts.length; i++) {
            var ipt = ipts[i];
            iptVals += ipt.value + ",";
            if (i < ipts.length)
                iptVals += ","
        }

        AjaxSocialUtilForArticle.showAttentionUsers(userName, iptVals, function (data) {
            var ipts = document.getElementsByName("rzjg");
            for (var i = 0; i < ipts.length; i++) {
                var ipt = ipts[i];
                if (ipt.value) {
                    var span = ipt.parentNode.getElementsByTagName("span")[0];
                    var isAttention = data["attenion_" + ipt.value];
                    var isPic = data["showpic_" + ipt.value].replace("48", "128");
