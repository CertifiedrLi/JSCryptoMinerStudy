			if(event.size[0] == 0 && event.size[1] == 0) {
				$('#' + event.slot.getSlotId().getDomId()).parents('.article-adv-box').removeClass('type2').addClass('type-fluid');
				return;
			}
		}


		/**************************** MOBILE SLOTS ****************************/

		// fanplayer
		if(event.slot.getAdUnitPath() == "/1010633/fpmobile_fplayer")
		{
			if(event.isEmpty || event.size[0] != 1 || event.size[1] != 1) { // il fanplayer è 1x1
