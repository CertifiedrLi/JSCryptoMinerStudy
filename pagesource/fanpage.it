			if(event.isEmpty) {
				return;
			}

			if (event.size[0] > 450 || (event.size[0] == 0 && event.size[1] == 0)) {
				$('#div-banner-8').css({
					'float': '',
					'margin-left': 0
				});
				return;
			}
		}

		// text 2
		if(event.slot.getAdUnitPath() == "/1010633/fp300x250text2") {

			if(event.isEmpty) {
				return;
			}

