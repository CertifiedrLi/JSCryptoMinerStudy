  )("https://api.tinypass.com/xbuilder/experience/load?aid=4rqMYyzEyD");

  /**
   * Sets cookies for registration form in order to send conversion to Piano
   *
   * @param user
   * @param trackingId
   */
  function pianoSetTrackingIdForRegistration(user, trackingId) {
    var d = new Date();
    d.setTime(d.getTime() + 60 * 60 * 24 * 30 * 1000);
    document.cookie = "piano_registration_tracking_id=" + trackingId + "; expires=" + d.toUTCString() + "; path=/";
    document.cookie = "piano_registration_info=nameFirst:" + user.firstName + "|nameLast:" + user.lastName + "|email:" + user.email + "; expires=" + d.toUTCString() + "; path=/";
  }

  /**
   * Sends user information to Clickshare for registration
   *
   * @param email
   * @param firstName
   * @param lastName
   * @param country
   */
  function pianoRegisterUser(user, redirectUrl) {

    var newsletters = ((user.country != 'CAN') ? '&selection.promoteSubscriptions=Y&selection.promoteEvents=Y&selection.promoteWebinars=Y&selection.promoteEditorial=Y&selection.dailyAlerts=Y&selection.breakingNewsAlerts=Y' : '');

    window.location =
      'https://' + get_clickshare_domain() + '/clickshare/addAccountReg.do' +
        '?addAcct.email=' + user.email +
        '&addAcct.nameFirst=' + user.firstName +
        '&addAcct.nameLast=' + user.lastName +
        newsletters + 
        '&submit=1' +
        '&allowContinue=true' +
        '&CSTargetURL=' + redirectUrl;
  }

  /**
   * If user has registered via paywall form and returned to the site, verify and send conversion information to Piano
   */
  function pianoSaveConversionIfRegistered() {
    function getCookie(name) {
      var value = "; " + document.cookie;
      var parts = value.split("; " + name + "=");
      if (parts.length == 2) return parts.pop().split(";").shift();
    }

    function deleteCookie(name) {
      document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;path=/';
    }

    var pianoTrackingId = getCookie('piano_registration_tracking_id');
    var csDemoCookie = getCookie('CSDemos');

    if (typeof pianoTrackingId !== 'undefined' && typeof csDemoCookie !== 'undefined') {
      var pianoUserInfo = getCookie('piano_registration_info').split('|');
      var csUserInfo = csDemoCookie.replace('"', '').split('|');

      if (pianoUserInfo.nameFirst == csUserInfo.nameFirst &&
          pianoUserInfo.nameLast  == csUserInfo.nameLast &&
          pianoUserInfo.email     == csUserInfo.email) {

        $.ajax({
          type: 'post',
          url: '/api/piano/register_conversion',
          data: {
            trackingId: encodeURIComponent(pianoTrackingId)
          }
        });

         deleteCookie('piano_registration_tracking_id');
         deleteCookie('piano_registration_info');
      }
    }
  }

  /**
   * Function to send data upstream to Piano template
   *
   * @param iframeId - Element id of the Piano iframe
   * @param success - true/false. Status of the message
   * @param message - Error message text
   * @param object - Additional data, like the the field that triggered the error
   */
  function pianoSendPostMessage(iframeId, success, message, object) {
    var iframe = jQuery('#' + iframeId);

    if (iframe.length) {
      iframe[0].contentWindow.postMessage({
        piano: {
          success: success,
          message: message,
          object: object
        }
      }, '*');
    }
  }

  /**
   * Verify email is valid
   *
   * @param email
   * @returns {boolean}
   */
  function isValidEmail(email) {
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
  }

  pianoSaveConversionIfRegistered();

