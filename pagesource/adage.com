  /**
   * On submission of registration paywall form, send values to Clickshare for user registration
   */
  tp.push(['addHandler', 'customEvent', function (event, b, c, d) {
    if (event.eventName = 'adage-reg-paywall') {
      var params = JSON.parse(event.params.params);
      var form = (event.params.form != '') ? JSON.parse(event.params.form) : '';

      if (form == '') {
        pianoSendPostMessage(params.iframeId, false, 'All fields are required', 'email');
        return;
      }

      var user = {
        firstName: $.trim(form.subscribeName),
        lastName: $.trim(form.subscribeLastName),
        email: $.trim(form.subscribeEmail),
        country: $.trim(form.subscribeCountry)
      };

      if (user.firstName == '' || user.lastName == '' || user.email == '') {
        pianoSendPostMessage(params.iframeId, false, 'All fields are required', '');
      } else if (! isValidEmail(user.email)) {
        pianoSendPostMessage(params.iframeId, false, 'You have entered an invalid email', 'email');
      } else {
        pianoSetTrackingIdForRegistration(user, params.trackingId);
        pianoRegisterUser(user, params.url);
      }
    }
  }]);

