<script type='text/javascript'>
    $(function () {
        var configPubJson = {
            slotNames: {
                oopSlotName: 'OutOfPage',
                mastheadSlotName: 'BTFhorz',
                mrecSlotName: 'BTFvert',
                leaderboardSlotName: 'BTFhorz'
            },
            adUnitPaths: {
                oopAdUnitPath: '/4196/CORREIODAMANHA_WEBSITE/',
                mastheadAdUnitPath: '/4196/CORREIODAMANHA_MASTHEAD/',
                mrecAdUnitPath: '/4196/CORREIODAMANHA_WEBSITE/',
                leaderboardAdUnitPath: '/4196/CORREIODAMANHA_MASTHEAD/',
                pubTestSuffix: 'HOMEPAGE'
            },
            slotSizes: {
                mastheadSizes: [[1240, 600],[1240, 220],[970, 250],[728, 90]],
                mrecSizes: [[300, 600], [300, 250]],
                leaderboardSizes: [[728, 90]],
            }
        };
        COF.pubApi.configPubs(configPubJson);

        googletag.cmd.push(function () {
            var _mastheadCheckSizes = googletag.sizeMapping()
                .addSize([0, 0], [[300, 100], [320, 50], [320, 100], [320, 200]])
                .addSize([768, 200], [728, 90])
                .addSize([1190, 200], [[728, 90], [970, 250], [1240, 220], [1240, 600]])
                .build();

            COF.pubApi.configMastheadCheckSizes(_mastheadCheckSizes);
        });


        var kwpag = [];
        kwpag.push('homepage');

        if (typeof (keywordForPub) !== 'undefined') {
            if (keywordForPub != '') {
                var keys = keywordForPub.split(',');
                var i;
                for (i = 0; i < keys.length; ++i) {
                    if (keys[i] != '') {
                        var keyToAdd = keys[i].trim();
                        kwpag.push(keyToAdd);
                    }
                }
            }
        }


        var totalCount = 0;
        function adsRenderEnded(adsEvent) {
            totalCount++;
            var slotElementID = adsEvent.slot.getSlotElementId();

            if (totalCount == $('div[id*="div-gpt-ad"]').length) {
                if ($('.mainPub') != null && $('.mainPub').length > 0 && $($('.mainPub')[0]).height() > 300) {
                    if ($('maiscm_LD').length != 0 || window.location.pathname != '/') {
                        $('.noticiasGrow').css({
                            'height': '350px'
                        });
                    }
                    else {
                        $('.noticiasGrow').css({
                            'height': '681px'
                        });
                    }
                }
                else {
                    if ($('maiscm_LD').length != 0 || window.location.pathname != '/') {
                        $('.noticiasGrow').css({
                            'height': '681px'
                        });
                    }
                    else {
                        $('.noticiasGrow').css({
                            'height': '1200px'
                        });
                    }
                }
            }

            //quando há masthead, MREC em baixo de últimas.

            if (!adsEvent.isEmpty) {
                if (in_version == "pc" && slotElementID.indexOf("horzATF") > -1 && $(".pubMasthead").height() > 0) {
                    //HP
                    if ($(".mrecToMove").length > 0 && $("#ultimasHp_move").length > 0 && $("#lateralCanal").length < 1) {
                        $("#ultimasHp_move").insertBefore($(".mrecToMove"));
                        $(".mrecToMove").insertBefore($("#mrecContainer"));
                    }
                    else if ($("#lateralCanal").length > 0 && in_version == "pc" && slotElementID.indexOf("horzATF") > -1 && $(".pubMasthead").height() > 0) {
                        //canal
                        $("#lateralCanal").css("margin-top", "15px");
                        $(".mainPub.mrecPrincipal").appendTo("#lateralCanal");
                    }
                }
            }

            //validacao masthead
            if (!adsEvent.isEmpty) {
                if (slotElementID.indexOf('div-gpt-ad-1469112105721-0') > -1) {
                    if ($('#div-gpt-ad-1469112105721-0').length > 0 && $('#div-gpt-ad-1469112105721-0').is(":visible")) {
                        $('body').addClass('mastheadVisible');
                    }
                }
            }

            //Show/Hide pub tag
            if (slotElementID.indexOf('BTFvert') > -1 || slotElementID.indexOf('vertATF') > -1) {
                if ($('#' + slotElementID).siblings('p').text() != 'pub')
                    $('#' + slotElementID).before('<p class="pubLabel">pub</p>');
            }

            //validacao para carregar o script do optinmonster
            if ((adsEvent.isEmpty && slotElementID == 'OutOfPage') || (adsEvent.isEmpty && slotElementID.indexOf('horzATF') > -1)) {
                indexOptin++;
                if (indexOptin == 2 && typeof loadOptinMonster === 'function' && loadOptinMonster() !== undefined) {
                    loadOptinMonster();
                }
            }
        }

        var indexOptin = 0;
        googletag.cmd.push(function () {
            googletag.pubads()
                .setTargeting('end', [window.location.href.match(/([^\?]*)\/*$/)[0]])
                .setTargeting('kw', kwpag);

            googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                adsRenderEnded(event);
            });


            googletag.pubads().enableSingleRequest();
            googletag.pubads().enableAsyncRendering();
            googletag.pubads().collapseEmptyDivs(true, true);
            googletag.pubads().setCentering(true);
            googletag.companionAds().setRefreshUnfilledSlots(true);
            googletag.pubads().disableInitialLoad();

            googletag.enableServices();

            //OOP
                
            COF.pubApi.renderOOP($('#div-gpt-ad-1480001045829-0'));
            

            //MastHead
                
                    COF.pubApi.renderMasthead($('#div-gpt-ad-1469112105721-0'), '', 'horzATF', 'ATF', false);
                
            
            //Mrec
                 
                     if (in_version == 'pc') {
                         if ($('#div-gpt-ad-1379692092145-0').length > 0) {
                             COF.pubApi.renderMrec($('#div-gpt-ad-1379692092145-0'), '', 'vertATF', 'ATF', false);
                         }
                         else if ($('#div-gpt-ad-1379692092145-0-manchete').length > 0) {
                             COF.pubApi.renderMrec($('#div-gpt-ad-1379692092145-0-manchete'), '', 'vertATF', 'ATF', false);
                         }
                     }
                     else if (in_version == 'tablet') {
                         COF.pubApi.renderMrec($('#div-gpt-ad-1379692092145-0-tablet'), '', 'vertATF', 'ATF', false);
                     }
                     else {
                         COF.pubApi.renderMrec($('#div-gpt-ad-1379692092145-0-mobile'), '', 'vertATF', 'ATF', false);
                 }
                



             googletag.pubads().refresh();
             });
    });

</script>







