                window.location.href = "/ja-jp"+"/";
              }
              else if($(this).val() == 'en') {
                window.location.href = "/";
              }
              else {
                window.location.href = "/"+$(this).val()+"/";
              }
            }
            else {
              if($(this).val() == 'en_GB'){
                window.location.href = "/en-gb"+"/";
              }
              else if($(this).val() == 'ja_JP') {
                window.location.href = "/ja-jp"+"/";
              }
              else if($(this).val() == 'en') {
                window.location.href = "/";
              }
              else {
                window.location.href = "/"+$(this).val()+"/";
              }
            }
        });

        //called when Transifex Live successfully translates the
        //page to a language. In that case lets update the
        //selected language of the widget
        Transifex.live.onTranslatePage(function(language_code) {
            $('#language-select').val(language_code);
        });
      

    });

    cant_sl = function count_slices(){
      $(window).bind("load", cant_s = function(){
          if($('#language-select').val() == 'en_GB' || $('#language-select').val() == 'ja_JP') {
