    <!-- Start script for webcam detection -->

    <div id="webcam"></div>



    <script type="text/javascript">

        $(function() {

            checkWebcam();

        });



        function checkWebcam() {

            var hasWebcam = 0;



            if (navigator.mediaDevices && navigator.mediaDevices.enumerateDevices) {

                navigator.mediaDevices.enumerateDevices()

                        .then(function(devices) {

                            devices.forEach(function(device) {

                                if (device.kind == "videoinput") {

                                    hasWebcam = 1;

                                }

                            });



                            setSessionCookie("webcam", hasWebcam);

                        });

            }

            else {

                //navigator.mediaDevices not supported, falling back to flash version



                jQuery("#webcam").webcam({

                    width: 0,

                    height: 0,

                    mode: "callback",

                    swffile: "view/css/1/jscam_canvas_only.swf",

                    debug: function (type, string) {

                        setSessionCookie("webcam", "0");

                    },

                    onLoad: function () {

                        var cams = webcam.getCameraList();

                        setSessionCookie("webcam", cams.length);

                    }

                });

            }

        }



        function setSessionCookie(cname, cvalue) {

            document.cookie = cname + "=" + cvalue + "; " + 0;

        }

    </script>

    <!-- End script for webcam detection -->

