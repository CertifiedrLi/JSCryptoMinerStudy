				$("#search_mobile").blur();
			}
		}
	});
	$(document).on('keyup',function(event) {
		if(event.keyCode==13) { 	
			$('#gsc-i-id1').val('');
			if( $('#search').val()!='' ) {
				var searchKey = $('#search').val();                            
			} else {
				var searchKey = $('#search_mobile').val();
			}
			if( searchKey!='' ) { 
				$('#gsc-i-id1').val( searchKey );
				$('button.gsc-search-button').click();
			}
		}
	});
});
