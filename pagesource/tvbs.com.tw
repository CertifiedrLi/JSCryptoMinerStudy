		if( $(this).val()!='' ) {
			$('#gsc-i-id1').val( $(this).val() );
			$('#search_button,#search_button_mobile').click(function(){ $('button.gsc-search-button').click(); }); 
		}
	});								
	$("body").on('keyup','#search_mobile',function(){
		if( $(this).val()!='' ) { 		
			$('#gsc-i-id1').val( $(this).val() );
