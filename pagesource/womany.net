    articleDat.enName = $selInterest.data('name');
  	articleDat.zhName = $selInterest.text();
    articleDat.intID = intID;

    articleDat.articles.forEach(function(ele, index){
      if (ele.published_at)
        ele.published_at_str = $.timeago(new Date(ele.published_at));
    });

    var $list = $(wigTemplate(articleDat));
    $block.find('.inner_container').replaceWith($list);
    var $titles = $list.find('h2');
    adjustTitleLen($titles);

    var $leftArrow = $list.find('button.left');
    var $rightArrow = $list.find('button.right');

    if (window.ga){
      $leftArrow.click(function(){
        ga('send', 'event', 'website-tracking', wgName, 'toggle-article-left');
      });
      $rightArrow.click(function(){
        ga('send', 'event', 'website-tracking', wgName, 'toggle-article-right');
      });
    }

    function checkSlickNadImp(slick){
      var $nad = slick.$slides.filter('.article-block[aria-hidden=false][data-nad]');
      if (!$nad.length) return;

      var nadID = $nad.attr('data-nad');
      $.post('//api.womany.net/nads/imps/' + nadID);
      if (window.ga) ga('send', 'event', 'website-tracking', 'native-ad', 'imps-' + nadID);
    }

    $list.find('.articles')
      .on('init', function(event, slick, direction){
        if (nadEnabled) checkSlickNadImp(slick);
        dfd.resolve();
      })
      .on('afterChange', function(event, slick, currentSlide){
        if (nadEnabled) checkSlickNadImp(slick);
      })
      .slick({
        // draggable: false,
        prevArrow: $leftArrow,
        nextArrow: $rightArrow,
        speed: 600,
        mobileFirst: true,
        variableWidth: true,
        // infinite: false,

        slidesToShow: 2,
        slidesToScroll: 2,

        responsive: [
        {
          breakpoint: 960,
          settings: {
            slidesToShow: 3,
            slidesToScroll: 3
          }
        },
        {
          breakpoint: 1500,
          settings: {
            slidesToShow: 4,
            slidesToScroll: 4
          }
        }]
      })
      .on('swipe', function(ev, slick, direction){
        if (window.ga) ga('send', 'event', 'website-tracking', wgName, 'swipe/drag-' + direction);
      });
  });

  return dfd.promise();
}

