	function changeImageCookie(imageSrc) {
		var imageCookie = readFromCookie("jsonImageSrc");
		if(imageCookie && imageCookie.indexOf("12345")>0) {
			return;
		}
		var newImageSrc = imageSrc.replace("pages=123","v=12345");
		var d = new Date();
		d.setTime(d.getTime() + (5*60*1000));
		var expires = "expires="+d.toUTCString();
		document.cookie = "jsonImageSrc" + "=" + newImageSrc + "; " + expires;
	}
	
	function readFromCookie(name) {
		var json="";
		name = name+"=";
    	var ca = document.cookie.split(';');
    	for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) {
            	return c.substring(name.length,c.length);
            };
    	}
		return json;
	}
