	function saveCookie(json) {
		var cookieIsThere = readFromCookie("jsonUserName");
		if(cookieIsThere){return;}
        var d = new Date();
        d.setTime(d.getTime() + (5*60*1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = "jsonUserName" + "=" + json.loginStatus.userName + "; " + expires;
