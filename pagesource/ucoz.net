		'afterLoad': function(anchorLink, index){
			lastSection = (index == 8);
			var mobile = $(window).width() < 992;
			if(index == 1){
				$('.nav-button a').addClass('dark');
				$('#fp-nav ul').addClass('black');
			}
			if(index == 2){
				$('#slider-pad').removeClass('fadeOutDown')
					.addClass('fadeInUp');
				$('.nav-button a').addClass('dark');
				$('#fp-nav ul').addClass('gray');
			}
			if(index == 3){
				$('#business-slider').addClass('fadeInLeft')
					.removeClass('fadeOutLeft');
				$('#business-info').addClass('fadeInRight')
					.removeClass('fadeOutRight');
				$('.nav-button a').addClass('light');
				$('#fp-nav ul').addClass('white');
			}
			if(index == 4){
				$('#slider-web-shops-box').removeClass('fadeOutRight')
					.addClass('fadeInRight');
				$('#info-web-shops').addClass('fadeInLeft')
					.removeClass('fadeOutLeft');
				$('.nav-button a').addClass('light');
				$('#fp-nav ul').addClass('white');
			}
			if(index == 5){
				$('.uscl-animate-left').addClass('fadeInRight')
					.removeClass('fadeOutRight');
				$('.uscl-animate-right').addClass('fadeInLeft')
					.removeClass('fadeOutLeft');
			}
			if(index == 6){
				$('#info-my-sites').addClass('fadeInLeft')
					.removeClass('fadeOutLeft');
				$('#image-my-sites').addClass('fadeInRight')
					.removeClass('fadeOutRight');
				$('.nav-button a').addClass('dark');
				$('#fp-nav ul').addClass('gray');
			}
			if(index == 7){
				$('#carousel-personal-page').addClass('fadeInLeft')
					.removeClass('fadeOutLeft');
				$('#info-personal-page').addClass('fadeInRight')
					.removeClass('fadeOutRight');
				$('.nav-button a').addClass('light');
				$('#fp-nav ul').addClass('white');
			}
			if(index == 8){
				$('#info-prof-templates').addClass('zoomIn')
					.removeClass('zoomOut');
				$('#fp-nav ul').addClass('white');
				$('.nav-button a').addClass('dark');
			}
			if(index == 9){
				$('#image-uran-browser').addClass('fadeInLeft')
					.removeClass('fadeOutLeft');
				$('#info-uran-browser').addClass('fadeInRight')
					.removeClass('fadeOutRight');
				$('.nav-button a').addClass('light');
				$('#fp-nav ul').addClass('white');
				$('#image-uran-browser .viewport').removeClass('short')
					.addClass('tall');
			}
		},
		'onLeave': function(index, nextIndex, direction){
			$('#fp-nav ul').removeClass();
			$('.nav-button a').removeClass();
			if(index == 2){
				$('.switcher').hasClass('active-point') && toggleSlider();
				$('#slider-pad').removeClass('fadeInUp')
					.addClass('fade-in');
				var trans = $('#slider-pad').addClass('fadeOutDown');
				setTimeout(function() {
					trans.removeClass('fadeOutDown');
				}, 1000);
			}
			if(index == 3){
				$('#business-info').removeClass('fadeInRight')
					.addClass('fadeOutRight');
				$('#business-slider').removeClass('fadeInLeft')
					.addClass('fadeOutLeft');
			}
			if(index == 4){
				$('#slider-web-shops-box').removeClass('fadeInRight')
					.addClass('fadeOutRight');
				$('#info-web-shops').removeClass('fadeInLeft')
					.addClass('fadeOutLeft');
				$('#slider-plate').removeClass('zoomIn');
			}
			if(index == 5){
				$('.uscl-animate-left').removeClass('fadeInRight')
					.addClass('fadeOutRight');
				$('.uscl-animate-right').removeClass('fadeInLeft')
					.addClass('fadeOutLeft');

			}
			if(index == 6){
				$('#info-my-sites').removeClass('fadeInLeft')
					.addClass('fadeOutLeft');
				$('#image-my-sites').removeClass('fadeInRight')
					.addClass('fadeOutRight');
			}
			if(index == 7){
				$('#carousel-personal-page').removeClass('fadeInLeft')
					.addClass('fadeOutLeft');
				$('#info-personal-page').removeClass('fadeInRight')
					.addClass('fadeOutRight');
			}
			if(index == 8){
				$('#info-prof-templates').removeClass('zoomIn')
					.addClass('zoomOut');
				$('#image-uran-browser .viewport').removeClass('short')
					.addClass('tall');

			}
			if(index == 9){
				$('#image-uran-browser').removeClass('fadeInLeft')
					.addClass('fadeOutLeft');
				$('#info-uran-browser').removeClass('fadeInRight')
					.addClass('fadeOutRight');
			}
		}
	});

	$('#slider-pad .switcher').click(function() {
		toggleSlider();
	});

	$('.nav-button .menu-btn').click(function() {
		$('#main-nav a').toggleClass('bounceInRight');
		return false;
	});

});
