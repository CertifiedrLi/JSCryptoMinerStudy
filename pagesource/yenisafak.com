                        });
                        $il.find('.user-menu .info img').attr('src', response.Image).attr('alt', response.Name);
                    }
                    else {
                        $il.find('.button .avatar').css({ 'background-image': "url('/images/avatar.png')" });
                        $il.find('.user-menu .info img').attr('src', '/images/avatar.png');
                    }
                    $il.removeClass("hide");
                    $al.addClass("hide");
                    $ll.addClass("hide");
                } else {
                    $al.find('> a').attr('href', PiriId.LoginUrl);
                    $il.addClass("hide");
                    $al.removeClass("hide");
                    $ll.addClass("hide");
                }

                $(".drop-down").each(function () {
                    var dropWidth = $(this).next().find(".drop-content.right").width() - 30;
                    $(this).next().find(".drop-content.right").css("left", -dropWidth);
                });
            },
            error: function (xhr, ajaxOptions, thrownError) {
                console.log(xhr.status + ": " + thrownError);
            }
