    <script type="text/javascript">

        /*********************************
        *取得Login 資料與打卡
        **********************************/
        function status(data) {
            if (data.login == 'false') {
                $('#HomeCheck').css('display', '');
                $('#HomeCheckDone').css('display', 'none');

                document.getElementById('login_box').innerHTML =
                '<a href="https://account.ytower.com.tw/member/register.jsp" title="註冊"><span class="Ytimg btn_reg pull-right"></span></a>' +
                '<a href="https://account.ytower.com.tw/oauth2/1/login.jsp?client_id=6044f195f39980658edd8a4be66b22ae6f983b12&redirect_uri=http%3A%2F%2Fwww.ytower.com.tw" title="登入">' +
                '<span class="Ytimg btn_login pull-right"></span></a>' +
                '<span id="HomeCheck" class="Ytimg btn_noncheck pull-right"></span>'
            } else {
                document.getElementById('login_box').innerHTML =
                '<div class="uid"><a href="https://account.ytower.com.tw/member/login.jsp">' + data.mail + '</a></div>' +
                '<a href="javascript:void(0)" title="登出" id="Logout"><span class="Ytimg btn_logout pull-right" onclick="logout()"></span></a>' +
                //'<a href="http://recipe.ytower.com.tw/Channel/MyFavorites" title=我的收藏簿"><span class="Ytimg btn_myfavorite pull-right"></span></a>' +
                //'<span id="HomeCheck" class="Ytimg btn_noncheck pull-right"></span>' +
                '<span id="HomeCheckDone" class="Ytimg btn_checked pull-right" style="display:none;"></span>'
                loginCheck(data.mail);
            }

            /*********************************
            *每日打卡點擊動作
            **********************************/
            $('#HomeCheck').click(function () {
                if (!$(this).attr('data-email')) {
                    alert('請先登入！');
                    location.href = 'https://account.ytower.com.tw/oauth2/1/login.jsp?client_id=6044f195f39980658edd8a4be66b22ae6f983b12&redirect_uri=http%3A%2F%2Fwww.ytower.com.tw';
                } else {
                    $.getScript('http://recipe.ytower.com.tw/Widget/GetHomeCheckBouns?callback=homebouns&email=' + encodeURIComponent($(this).attr('data-email')) + '&_=' + (new Date()).getTime());
                }
            });

        }


        /*********************************
        *登入打卡確認
        **********************************/
        function loginCheck(email) {
            $('#HomeCheck').attr('data-email', email);
            $.getScript('https://account.ytower.com.tw/quest/1/status.jsp?quest_id=9a9e434f3ab1b83f5310f40823696a955a9bdb36&_=' + (new Date()).getTime());
        }


        /*********************************
        *打卡圖片顯示確認
        **********************************/
        function homebouns(result) {
            if (result.ok) {
                $('#HomeCheck').css('display', 'none');
                $('#HomeCheckDone').css('display', '');
                if (result.js) eval(result.js);
            } else {
                alert(result.msg);
            }
        }


        /*********************************
        *打卡圖片顯示確認
        **********************************/
        function quest(result) {
            if (result && result.error == "none" && result.quest_id == "9a9e434f3ab1b83f5310f40823696a955a9bdb36") {
                if (result.left < 1) {
                    $('#HomeCheck').css('display', 'none');
                    $('#HomeCheckDone').css('display', '');
                } else {
                    $('#HomeCheck').css('display', '');
                    $('#HomeCheckDone').css('display', 'none');
                }
            }
        }


        //登出確認
        function logout() {
            $(document.body).append('<iframe id=\"LogoutFrame\" style=\"display:none;\" />');
            $('#LogoutFrame').attr('src', 'https://account.ytower.com.tw/json/logout.jsp?_=' + (new Date()).getTime());
            $('#LogoutFrame').load(function () {
                parent.location.reload();;
            });
        }

        //切換Slider
        $(document).ready(function () {
            $("#slider").easySlider({
                auto: true,
                continuous: true,
                numeric: true,
                speed: 500,
                pause: 3000
            });
        });

        //切換TAB
        $(document).ready(function () {
            $("#tabs").tabs({
                active: 0
            });
        });

        //新聞輪播,頁尾資料,註冊資料
        $(document).ready(function () {
            $.getScript('https://account.ytower.com.tw/oauth2/1/status.jsp?host=www.ytower.com.tw&_=' + (new Date()).getTime());

            //$.getScript('http://www.ytower.com.tw/Scripts/footer_nav.js?_=' + (new Date()).getTime());
            //$.getScript('http://www.ytower.com.tw/news_marquee.js?_=' + (new Date()).getTime(), function () {
            //    $('#news li').css('display', 'block');
            //    $('#news').cycle({
            //        fx: 'scrollDown',
            //        delay: -2000
            //    });
            //});
            $('#news li').css('display', 'block');
            $('#news').cycle({
                fx: 'scrollDown',
                delay: -2000
            });
        });
