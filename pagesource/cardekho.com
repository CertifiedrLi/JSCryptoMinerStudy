<script type="text/javascript">
	addPageScript(function() {
  var positionAds='';
  if (!isEmpty(positionAds)){
  if(positionAds=='First Position'){
    $('.recommendedtabs').find('.active').removeClass('active');
    $('.recommendedtabs').find('#addrecommendedtabid').addClass('active');
    $('#recommended').find('#Upcoming').addClass('disnone');
  }
  else{
  	$('#recommended').find('#addrecommendedtabdesc').addClass('disnone');
  }}
});
	//Lazyload bg image, needed lazyload v1.0.3
	addPageScript(function() {
        
		$.gsp_lazyload.bgSelectors({ 
			'.cardekhohome': 'https://img.gaadicdn.com/images/newcarimg/cardekhohome-1.png',
			'.brandlogos': 'https://img.gaadicdn.com/images/newcarimg/brandsprite-5.png' 
		});
		
		$('footer .topfooter').on('gse_onlazyload', function(){
			$('style#lazyload-footer-bg').detach();
		});

	$('.usedcarfrthr li').click(function(){
    	$('.usedcarfrthr li.active').removeClass('active');
    	$(this).addClass('active');
  	});
  	bydisp("usedcarpop");
	
	  //****************** Interested News Tab js *******************//
      $('.intnewstab li').click(function(){
        $('.intnewstab li.active').removeClass('active');
        $(this).addClass('active');
      });
      newsdisp("intrstExpert");

});

//******** By Prce-&-City Tab js *********//
function bydisp(divIdToDisplay){
  $('div.divblock2').hide();
  $('div#'+divIdToDisplay+'.divblock2').fadeIn("slow");
}

function newsdisp(divIdToDisplay){
      $('div.divblock1').hide();
      $('div#'+divIdToDisplay+'.divblock1').fadeIn("slow");
    }
