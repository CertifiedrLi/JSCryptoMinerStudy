    function trimNewsText(text, length) {
        text = $.trim(text);
        length = typeof a !== 'undefined' ? length : 13;

        var words = text.split(" ");
        if (words.length > length) {
            return words.splice(0, length).join(" ").concat('...');
        } else {
            return text;
        }
    }
	

