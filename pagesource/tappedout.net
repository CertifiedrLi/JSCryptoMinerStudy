<script type='text/x-jquery-tmpl' id="broadcastModal-broadcastResult">
    {{if !results.length}}
        <p>
        {{if last_broadcast}}
            Last broadcast ended ${last_broadcast} ago. Start one!
        {{else}}
            Nobody currently broadcasting. Start one!
        {{/if}}
        </p>
    {{/if}}
    <div class="panel panel-default">
    {{each(i, bc) results}}
        <div class="panel-heading" role="tab" id="broadcastModal-broadcastResults-${bc.id}">
            <h4 class="panel-title">
                {{html bc.user.display_name}}
                {{if bc.venue != null}}
                - ${bc.venue.name}, ${bc.venue.city}
                {{/if}}

                {{if authenticated}}
                <a class="btn btn-success btn-xs {{if bc.joined}}hide{{/if}}" id="join-${bc.id}" href="javascript:void()">Join</a>
                <a class="btn btn-danger btn-xs {{if !bc.joined}}hide{{/if}}" id="leave-${bc.id}" href="javascript:void()">Leave</a>
                {{/if}}
            </h4>
        </div>
        <div id="collapse${bc.id}" class="" aria-labelledby="heading${bc.id}">
            <div class="panel-body broadcast-panel-body">
                {{html bc.description_parsed}}
                <p><small>
                    ${bc.created} | ${bc.views} views | <a data-toggle="collapse" href="#followers${bc.id}">${bc.followers.length} joined</a>
                </small></p>
                <p class="collapse" id="followers${id}">
                {{each bc.followers}}
                    {{html $value.display_name }}
                {{/each}}
                </p>
            </div>
        </div>
    {{/each}}
    </div>
</script>
<script type='text/x-jquery-tmpl' id="broadcastModal-eventResult">
    {{if !results.length}}
        <p>No upcoming events near you</p>
    {{/if}}
    <div class="panel panel-default">
    {{each(i, e) results}}
        <div class="panel-heading" role="tab" id="broadcastModal-eventResult-${e.id}">
            <h4 class="panel-title">
                <a href="${e.venue.local_url}">${e.venue.name}</a> - ${e.date}
                {{if authenticated}}
                <a class="btn btn-success btn-xs {{if e.venue.joined}}hide{{/if}}" id="join-${e.id}" href="javascript:void()">Join</a>
                <a class="btn btn-danger btn-xs {{if !e.venue.joined}}hide{{/if}}" id="leave-${e.id}" href="javascript:void()">Leave</a>
                {{/if}}
            </h4>
        </div>
        <div id="collapse${e.id}" class="" aria-labelledby="heading${e.id}">
            <div class="panel-body broadcast-panel-body">
                {{html e.description_parsed}}
                <p><small>
                    <a data-toggle="collapse" href="#followers${e.id}">${e.venue.followers.length} joined</a>
                </small></p>
                <p class="collapse" id="followers${id}">
                {{each e.venue.followers}}
                    {{html $value.display_name }}
                {{/each}}
                </p>
            </div>
        </div>
    {{/each}}
    </div>
</script>

                
                
                
                
