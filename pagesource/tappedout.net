<script type="text/javascript">
	function reset_clicker(){
		$("#latest-bg").removeClass("tone-3");
		$("#top-bg").removeClass("tone-3");
		$("#featured-bg").removeClass("tone-3");
		$("#help-bg").removeClass("tone-3");
		$("#friends-bg").removeClass("tone-3");
        $("#help").hide();
        $("#friends").hide();
		$("#latest").hide();
		$("#top").hide();
		$("#featured").hide();
	}
	$(window).load(function(){
		$(".clicker").click(function(e){
			reset_clicker();
			var id = $(this).attr("id").split("-")[0];
			$("#" + id + "-bg").addClass("tone-3");
			$("#" + id).show();
			e.preventDefault();
		});
	});
</script>

<script type="text/javascript">
    function setupCycleSocket(){
        var socket = io('http://static.tappedout.net', {path: '/decksockets'});
        socket.on('decks.front', function (data) {
            var selector = "#latest #" + $(data.html).attr("id");

            if ($(selector).length) {
                //if deck already in list, remove
                $(selector).remove();
            } else if ($("#latest .deck-row-lg").length >= 13) {
                //if list is full remove last
                $("#latest .deck-row-lg").last().remove();
            }
            $(data.html).prependTo("#latest");
            $(selector).hide().fadeIn("slow");

        });
    }
    setTimeout(setupCycleSocket, 10000);
</script>


