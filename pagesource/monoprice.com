<script>
    function linkTrack(linkName) {
        _satellite.track(linkName);
    }
    function searchFilter() {
        digitalData.eventData = {
            searchFilter: digitalData.page.pageInfo.onsiteSearchFilterType | digitalData.page.pageInfo.onsiteSearchFilter
        }
        linkTrack('searchFilter');
    }
    function onsiteSearchPagination(onsiteSearchPagination) {
        digitalData.eventData = {
            onsiteSearchPagination: onsiteSearchPagination
        }
        linkTrack('seachPagination');
