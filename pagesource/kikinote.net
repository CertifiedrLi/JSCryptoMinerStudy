<script type="text/javascript">
    $(document).ready(function() {
        //計算最新文章滑動頁面的資訊
        var width = $('.index-new-slider').width();
        var count = 0;
        
        // 點點的click
        $('.controller-dot').click(function() {
            count = $('.controller-dot').index(this);
            $('.active').removeClass('active');
            $(this).addClass('active');
            indexNewSliderPosition();//移動到當頁
        });
        
        //initial => 排列最原始
        $('.index-new-slider').find('.index-new-slider-item').each(function(key) {
            var length = (parseInt(key)-count)*width;
            $(this).css('left',length);
        }); 
        
        //移動到點擊的頁面
        function indexNewSliderPosition () {
            var slider = -(count*width);
            $('.index-new-slider-wrapper').animate({'left':slider});
        }
    });
</script>

		
