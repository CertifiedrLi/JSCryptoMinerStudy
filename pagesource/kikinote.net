            subMenu.css("display", "block");
            subMenu.stop().animate({
              'bottom':(-subHeigh)
            },200);
        },function() {
            $(this).find('.menu-content').stop().animate({
                'bottom':'20px'
            },200, function() {
                $(this).css("display", "none");
            });
        });
        
        $('#user-triggle').click(function() {
            $('#user-info').toggle();
        });
        
        //this is for menu logo show sub web logo
        $('#subweb-trigger , .subweb-wrapper').hover(function() {
            $('.subweb-wrapper').toggle();
        });
        
        //search
        //open and close
        $('#search-button').on('click mouseover',function() {
            $('.search').toggle();
        });
        
        //deal with enter
        $("#search").keypress(function(e) {
            var searchText = $('#search').val();
            if(e.which == 13) {//enter鍵
                searchInfo(searchText);
            }
        });
    });
    
    function searchInfo(searchText) {
        var form = $("<form action='"+"//kikinote.net/s/"+encodeURIComponent(searchText)+"?q="+encodeURIComponent(searchText)+"' method='POST'  target='_blank'>" + 
                    "<input type='hidden' id = 'searchToken' name='"+"D6nikW8jPatv7pSa" +"'value='"+ "WohqYTOtrUZDMyX" +"'/>" +
                    "</form>");
        // HTML標準規定如果form表單沒有被添加到document裡，那麼form表單提交將會被終止。
        // 解決方法就是把form表單添加到document後再提交
        $(document.body).append(form);
        form.submit();
    }
</script>		
