<script type="text/javascript">
$(function(){
    $('.bxslider').bxSlider({
      mode: 'fade',
      auto: true,
      pagerCustom: '#bx-pager',
      captions: true,
      onSliderLoad: function(){
          $("#focus_left_photo").css("visibility",  "visible");
        }
    });
