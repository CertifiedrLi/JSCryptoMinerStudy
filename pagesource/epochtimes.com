<script>

var file_path = 'http://www.epochtimes.com/assets/uploads';
function loadTabContent (tab) {
    var cid = $(tab).find("a").attr("cid");
    var slug_id = $(tab).find("a").attr("slugid");
    var url = file_path + "/segments/"+ encoding+"/"+cid+".html";
    $("#"+slug_id).html("<img src='http://www.epochtimes.com/assets/themes/djy/images/loading.gif' />");
  $.get(url,function(data){
    $("#"+slug_id).html(data);
  });
}
function loadAccordionContent (accordion) {
    var cid = $(accordion).attr("cid");
    var slug_id = $(accordion).attr("slugid");
    var url = file_path + "/segments/"+ encoding+"/"+cid+".html";
    $("#"+slug_id).html("<img src='http://www.epochtimes.com/assets/themes/djy/images/loading.gif' />");
  $.get(url,function(data){
    $("#"+slug_id).html(data);
  });
}
function changeTabBackground (tab) {
  $(tab).siblings().find("a.hori").removeClass("sprite-star_navi1");
  $(tab).siblings().find("a.hori").removeClass("sprite-star_navi2");
  $(tab).siblings().find("a.hori").addClass("sprite-star_navi2");
  $(tab).find("a.hori").removeClass("sprite-star_navi2");
  $(tab).find("a.hori").addClass("sprite-star_navi1");
}
$(document).foundation({
    tab: {
        callback : function (tab) {
            loadTabContent(tab);
            changeTabBackground(tab);
            }
    },
    accordion: {
        callback : function (accordion) {
          console.log(accordion);
          loadAccordionContent(accordion);
            }
    }
});
</script>
