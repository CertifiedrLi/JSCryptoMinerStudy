        <script>
            $(document).ready(function(){
                var switchInt;
                //头部banner切换
                $('.linkhaitao-topbanner .index-btn span').click(function(event) {
                    var page = parseInt($(this).attr('page'));
                    $('.linkhaitao-topbanner ul li.active').removeClass('active');
                    $('.linkhaitao-topbanner ul li').eq(page - 1).addClass('active');
                    $(this).addClass('active').siblings().removeClass('active');
                    clearInterval(switchInt);
                    switchInt = setInterval(switch_banner,6000)
                });
                function switch_banner(){
                    var page = parseInt($('.linkhaitao-topbanner .index-btn span.active').attr('page'))%4 + 1;
                    $('.linkhaitao-topbanner ul li.active').removeClass('active');
                    $('.linkhaitao-topbanner ul li').eq(page - 1).addClass('active');
                    $('.linkhaitao-topbanner .index-btn span').eq(page - 1).addClass('active').siblings().removeClass('active');
                }
                switchInt = setInterval(switch_banner,6000)
                //监听标题切换事件
                $('.title-tab h3').bind('click',function(){
                    $(this).addClass('active').siblings().removeClass('active');
                    $('.linkhaitao-guanggao-list ul').eq($(this).index()).css('display','block');
                    $('.linkhaitao-guanggao-list ul').eq($(this).index()).siblings().css('display','none');
                });

                //监听图片组左右滑动事件
                function imgtab(){
                    var showWidth = $('.slider-wrapper').width(); //获取框架内容的宽度，即每次移动的大小度
                    var len = Math.ceil($('.slider-wrapper .slider-list').find('li').length/5);  //到一共有几组版面
                    var page = 1;      //初始化page变量

                    //点击下一页按钮
                    $('.next').bind('click',function(){

                        if(!$('.slider-list').is(':animated')){			//当没有执行动画时
                            if(page == len){//已经到最后一个版面了，如果再向后，必须跳转到第一个版面
                                $('.slider-list').animate({left:'0px'},'slow')   //通过改变left值，跳转到第一个版面
                                page=1;
                            }else{
                                console.log(showWidth);
                                $('.slider-list').animate({left:'-='+showWidth},'slow');
                                page++;
                            }
                        }
                        return false;
                    });
                    //点击上一页按钮
                    $('.prev').bind('click',function(){
                        if(!$('.slider-list').is(':animated')){
                            if(page == 1){ //到第一版面的时
                                $('.slider-list').animate({left:'-'+showWidth*(len-1)},'slow');
                                page=len;

                            }else{
                                $('.slider-list').animate({left:'+='+showWidth},'slow')
                                page--;
                            }
                        }
                        return false;
                    });
                }
                imgtab();
            });
        </script>