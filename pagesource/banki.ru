	<script>
		banki.utils.domReady.push(function () {
			require(['jquery', 'inViewport'], function($, inViewport) {
				var $filter = $('#js-all-banks-filter-typeahead');

				inViewport($filter, function() {
					require(['bloodhound', 'typeahead'], function(Bloodhound) {
						var prepareRequestSettings = function(settings) {
							settings.type = 'POST';
							settings.contentType = 'application/json; charset=UTF-8';
							settings.data = JSON.stringify({
								'jsonrpc': '2.0',
								'method': 'bankInfo/getBankList',
								'params': {},
								'id': 'test'
							});

							return settings;
						};
						var transformResponse = function(response) {
							var result = [];

							if (response && response.result && response.result.data) {
								response.result.data.forEach(function (item) {
									result.push({
										_id: item.bank_id,
										name: item.bank_name.replace('«', '&laquo; ').replace('»', ' &raquo;'),
										value: item.bank_name,
										licence: item.licence
									});
								});
							}

							return result;
						};
						var engine = new Bloodhound({
							datumTokenizer: Bloodhound.tokenizers.obj.whitespace(['name', 'licence']),
							queryTokenizer: Bloodhound.tokenizers.whitespace,
							prefetch: {
								url: '/api/',
								cache: false,
								prepare: prepareRequestSettings,
								transform: transformResponse
							}
						});
						var typeaheadSettings = {
							hint: true,
							highlight: true,
							minLength: 1
						};
						var datumSettings = {
							source: engine,
							display: 'value',
							limit: 10,
							templates: {
								suggestion: function (data) {
									return '<div>' + data.name + ' <span class="font-size-small color-gray-blue">(' + data.licence + ')</span></div>';
								}
							}
						};

						$filter.typeahead(typeaheadSettings, datumSettings);

						$filter.on('typeahead:select typeahead:autocomplete', function(event, suggestion) {
							document.location.href = '/banks/bank/?ID=' + suggestion._id;
						});
					});
				});
			});
		});
	</script>
