  function check(form) {
    if ((form == 'login' && $("#login-form").valid()) || (form != 'login' && $("#register-form").valid())){
      var password = '', key = "290c3c5d812a4ba7ce33adf09598a462", encrypted = '', confirm_password = '';
      if (form === 'login') {
        password = $("#user_password").val();
        encrypted = GibberishAES.enc(password, key);
        $("#user_password").val(encrypted);
      }
      else {
        password = $("#sign_up_password").val();
        encrypted = GibberishAES.enc(password, key);
        $("#sign_up_password").val(encrypted);
        // confirm_password = $("#sign_up_confirm_password").val();
        // encrypted = GibberishAES.enc(confirm_password, key);
        $("#sign_up_confirm_password").val(encrypted);
      }

      document.getElementById('user_password').value = encrypted;
      $('#login-model').block({
        message: '<h4><i class="fa fa-spinner fa-spin" style="font-size:60px;color:red"></i></h4>',
        css: {border: '1px solid '}
      });

      return true;
    }
  }

  function apply_coupon() {
    $("#coupon-id").val($("#input_hidden").val())
    if ($('#coupon-id').val() === '') {
      $('#invalid_coupon').html('Please select any one offer.');
      $('#invalid_coupon').css('color', 'red');
      return false;
    }

    // if ($('#current_user_id').val() === '') {
    if(false) { 
      $(".msg-box").slideDown();
      return false;
    }
    else {
      $("#wallet_debit").val(0);
      $('#ry-wallet').removeAttr('checked');
      $("#pay-walletmoney").hide();
      $("#pay-bus").show();
      $('#payment-selection').block({
        message: '<h4><i class="fa fa-spinner fa-spin" style="font-size:60px;color:red"></i></h4>',
        css: {border: '1px solid '}
      });
      var data = {
        coupon_code: $('#coupon-id').val(),
        invoice_id: $('#invoice_id').val(),
        ecomm_type: $('#ecomm_type').val(),
        user_id: $('#user_id').val()
      };

      $.ajax({
        type: 'get',
        url: '/check-coupon-validity',
        data: data,
        dataType: 'json',
        success: function(response) {
          // online_discount = parseFloat($("#food_online_discount").val())
          $("#valid-coupon").data('validcoupon',response.discount_coupon_details) 
          if (response.success === true && response.is_valid === true) {
            $('.form-clear').removeClass('hidden');
            $('#invalid_coupon').html(response.msg + ' ' + response.discount_coupon_details.coupon_cashback_text);
            $('#invalid_coupon').css('color', 'green');
            $("#net-payable").html('&#8377; ' + response.discount_coupon_details.total_amount);
            $("#pay-bus").html('PAY &#8377; ' + response.discount_coupon_details.total_amount);
            if ($('#ecomm_type').val() != '0'){
              $("#total-fare").html('&#8377; ' + response.discount_coupon_details.amount_before_discount);
            }
            //  dic = parseFloat(response.discount_coupon_details.total_discount)+online_discount
            // $("#discount").html('&#8377; ' +dic ) ;
            $("#coupon_code").val(response.discount_coupon_details.coupon_code);
            $("#orderAmount").val(response.discount_coupon_details.total_amount);
            var vendor_discount = parseFloat("");
            if (vendor_discount){
              if (response.discount_coupon_details.coupon_type == 10 ) {
                $("#discounted_amount").val(vendor_discount);
              }else{
                $("#discounted_amount").val(parseFloat(response.discount_coupon_details.total_discount) + vendor_discount);
              }
             } else { 
                if (response.discount_coupon_details.coupon_type == 10 ) {
                  $("#discounted_amount").val(0);
                }else{
                  $("#discounted_amount").val(response.discount_coupon_details.total_discount);
                }
              }
              if(response.discount_coupon_details.payment_type==0){
                $('.cod-hide').addClass('disable-div')
                $(".not-allow").text("The coupon is only applicable for online payment")
                $('input.ry-online').first().prop('checked',true)
              }else{
                $('.cod-hide').removeClass('disable-div');
                $(".not-allow").empty()
                // $('input.ry-cod').prop('checked',true)
              }
              if(response.discount_coupon_details.payment_type==1){
               $('input.ry-cod').prop('checked',true)
              }
             $(".discount-lavel").css("display","block");
              updatePayment()
            $(".payment-button").show();
            alertify.success(response.msg + ' ' + response.discount_coupon_details.coupon_cashback_text);
            closePopup()
            // var orderAmount = parseFloat($("#orderAmount").val());
            // var money = parseFloat("");
            // if (money < orderAmount ){
            //   if ($("#ry-wallet").prop('checked')){
            //     var amount = orderAmount - money;
            //     $("#wallet_debit").val(money);
            //   }else{
            //     var amount = orderAmount + money;
            //     $("#wallet_debit").val(0);  
            //   }
            //   $("#orderAmount").val(amount);  
            //   $("#net-payable").text(amount);
            //   $("#pay-bus").text("PAY ₹ " + amount);
            // } else {
            //   if ($("#ry-wallet").prop('checked')){
            //     $("#pay-walletmoney").show();
            //     $("#wallet_debit").val(orderAmount);
            //     $("#net-payable").html(0);
            //     $('input:radio').removeAttr('checked');
            //     $("#pay-walletmoney").text("Pay by wallet ₹ " + orderAmount);
            //     $("#pay-bus").hide();
            //   } else {
            //     $('input:radio:first').attr('checked', "checked");
            //     $("#pay-walletmoney").hide();
            //     $("#net-payable").html(orderAmount);
            //     $("#wallet_debit").val(0);
            //     // $("#pay-walletmoney").text("Pay by wallet " + orderAmount);
            //     $("#pay-bus").show();
            //   }
            // }  

          }
          else {
            alertify.error(response.msg);
            $('#invalid_coupon').html(response.msg);
            $('#invalid_coupon').css('color', 'red');
            $("#coupon_code").val(0);
            $(".discount-lavel").css("display","none");
            $("#discounted_amount").val(0);
          }
          $('#payment-selection').unblock();
        },
        error: function(error_res) {
          $('#payment-selection').unblock();
        }

      });

    }
  }

  function send_otp() {
    $("#register-form").validate();
    if ($("#register-form").valid()){
      $('#otp-collapse-block').hide();
      $("#otp-collapse").collapse('show');
      var data = {
        mobile_number: $('#user_mobile_number').val(),
        email: $('#register-form #user_email').val(),
        source: $("#register-source").val()
      };
      $('#login-model').block({
        message: '<h4><i class="fa fa-spinner fa-spin" style="font-size:60px;color:red"></i></h4>',
        css: {border: '1px solid '}
      });
      $.ajax({
        type: 'get',
        url: '/send-otp',
        data: data,
        dataType: 'json',
        success: function(response) {
          //if (response.success === true) {

          $('#login_result').css('color', '');
          $('#login_result').css('color', 'green');
          $('#otp_field').show();
          $('#login_result').show();
          $('#login_result').html(response.message);
          $('#login-model').unblock();
          //}        
        },
        error: function(response) {
          console.log('in error');
          $('#login-model').unblock();
          $('#otp-collapse-block').toggle();
          $("#otp-collapse").collapse('toggle');
        }
      });
    }
  }

  function login_block_ui() {
    if (form != 'login' && $("#forgot-password-form").valid())
    $('#login-model').block({
