          if( bNext ){
            marginLeft -= liWidth;
          } else {
            marginLeft += liWidth;
          }
          if( 0 >= marginLeft && listWidth >= (Math.abs(marginLeft) + panelWidth) ){
            friendLinkListScroll = true;
            $friend_link_list.stop().animate({ marginLeft: marginLeft }, function(){
              friendLinkListScroll = false;
            });
          }
        }
        $friend_link_scroll.on('click', function(e){
          e.preventDefault();
          fFriendListScroll($(this).hasClass('next'));
        });

        yp.sub('page/bodyclass/change', function(){
          $friend_link_list.stop().css({ marginLeft: 0 });
        });
