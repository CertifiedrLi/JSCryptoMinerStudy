        $friend_link_list.width( liWidth * $friend_link_list.find('.js-friend-link-item').length );

        var friendLinkListScroll = false;
        function fFriendListScroll(bNext){
          var marginLeft = parseInt($friend_link_list.css('marginLeft'))
            , panelWidth = $friend_link_list_panel.width()
            , listWidth = $friend_link_list.width();

          if( friendLinkListScroll ){
            return false;
