		<script>
		
			$(document).ready(function() {
				// funzionalita  hover search - freia - RIL_440
				$("input#ricerca").on('input', function() {
					ricercaAutocomplete("#formRicerca", '#bglist_search', 'list_search:not(.hp_assist)');
				});
					
				
				$("input#ricercaHpAssistenza").on('input', function() {
					ricercaAutocomplete("#formRicercaHpAssistenza", '#bglist_search_assist', 'list_search.hp_assist');
				});
					
				
				//tasti su e giu nell'autocomplete list
			    $('.desktop #ricerca, .desktop #ricercaHpAssistenza').keydown(function (e) { //not actually the down key though :-)
			    	
			    	if($(this).attr('id') == 'ricerca'){
			    		var overlay = '#bglist_search';
			    	}
			    	else{
			    		var overlay = '#bglist_search_assist';
			    	}
			    	
			        if (e.keyCode == 40 || e.keyCode == 38) {
	
	
			            if ($('.list_search.active > a').hasClass('listActive')) { // do any links have this class?
			                var selected = $(this).parent().find('a.listActive'); // if the do, they are the 'selected' link
			                selected.removeClass('listActive'); // this cleans up the one that we move from
			                var htmlString = $(this).parent().find(".list_search.active a.listActive").html();
	
			                $(this).keypress(function (event) {
			                    if (event.keyCode == 13) {
			                        selected.removeClass('listActive');
			                        $(this).parent().find('.list_search').removeClass('active');
			                        $(overlay).removeClass("active");
			                    }
			                });
	
	
			                if (40 == e.keyCode) { // going down the list
			                    if (selected.next().length == 0) {
			                        // if there isn't another list item, highlight the first
			                    	$(this).parent().find('.list_search.active > a:first').addClass('listActive');
			                        $(this).val($(".list_search.active a.listActive").html());
			                    } else {
			                        // add the class to the next item
			                        selected.next().addClass('listActive');
			                        $(this).val($(".list_search.active a.listActive").html());
			                    }
			                } else { // going up the list
			                    if (selected.prev().length == 0) {
			                        // add the class to the last item if you have gone to the top of the list
			                    	$(this).parent().find(".list_search.active > a:last").addClass("listActive");
			                        $(this).val($(".list_search.active a.listActive").html());
			                    } else {
			                        // add the class to the next one up the list
			                        selected.prev().addClass('listActive');
			                        $(this).val($(".list_search.active a.listActive").html());
			                    }
			                }
			            } else {
			                // if none were initially selected, select the first one
			            	$(this).parent().find(".list_search.active > a:first").addClass("listActive");
			                $(this).val($(".list_search.active a.listActive").html());
			            }
			        }
			    });
			});//chiusura $(document).ready..
		
		
			
		function ricercaAutocomplete(selettore, bgList, listSearch)
		{
			//variabile di input per la droplet di autocomplete
			var minCharacter = "3";
			var maxAutoCompleteLength = "8";
			var maxAutoSuggestLength = "5";
			var autoCompleteId ="2617933015";
			var searchableDimensionId ="3336767012";
			var suggestDimensionId ="1233144398";
			var targetDomain1 ="1169148416";
			var targetDomain2 ="1560031064";
			
			var textAutocomplete=$(selettore + " .ricerca").val().trim(); //testo inserito nella barra di ricerca
			var nCharacter = textAutocomplete.length;
			var textSuggest; //testo della prima keyword di autocomplete ottenuta
			var list_search_html="";
			
			//si attiva solo se il testo inserito nella barra di ricerca maggiore del paratetro dell' XM
			if (nCharacter >= minCharacter) {
				
				//chiamata ajax alla droplet TIAutocompleteDroplet per ottenere l'elenco delle AutocompleteKeyword
				var autoCompleteKeywords;
				$.ajax({
					type: "POST",
			  		url:"/cartridges/TIBoxDiRicerca/TIAutocomplete.jsp",
			  		//async: false,
			  		data:{text : "*"+textAutocomplete+"*", maxAutoCompleteLength : maxAutoCompleteLength, 
			  			autoCompleteId : autoCompleteId, searchableDimensionId: searchableDimensionId,
			  			targetDomain1 : targetDomain1,
			  			targetDomain2 : targetDomain2
			 			},        		
			  		success:function(result) {
			  			//lista degli autocomplete
			  			if(result.trim())
			  			{
			  				autoCompleteKeywords=JSON.parse(result);
			  				
			  				if (autoCompleteKeywords && autoCompleteKeywords.length>0)
							{
								list_search_html=creaListaAutocomplete(autoCompleteKeywords);
								textSuggest=autoCompleteKeywords[0].titolo;
							}
							
							//se viene visualizzato almeno una keyword, effettuo l'interrogazione alla suggestionDroplet
							if(textSuggest)
							{
								var autoSuggestKeywords;
								$.ajax({ 
									type: "POST",
							  		url:"/cartridges/TIBoxDiRicerca/TISuggestion.jsp",
							  		//async: false,
							  		data:{text : "*"+textSuggest+"*", 
							  			maxAutoSuggestLength : maxAutoSuggestLength, 
							  			suggestDimensionId: suggestDimensionId,
							  			targetDomain1 : targetDomain1,
							  			targetDomain2 : targetDomain2
							  		},        		
							  		success:function(result) {
							  			//lista dei suggerimenti
							  			if(result.trim())
							  			{
							  				autoSuggestKeywords=JSON.parse(result);
							  				
							  				if (autoSuggestKeywords && autoSuggestKeywords.length>0)
											{
												list_search_html+=creaListaSuggest(autoSuggestKeywords);
												
											}
							  				
							  			}
							  			//se ci sono risultati nella ricerca, mostro la lista
						  				$("."+listSearch).html(list_search_html);
				 						$("."+listSearch).addClass("active");
				 						$(bgList).addClass("active");
				 					}
							  	}); //chiusura $.ajax({ 
							}//chiusura if if(textSuggest)
							else
							{
								//se non ci sono risultati nella ricerca, nascondo e svuoto la lista
								$("."+listSearch).empty();
								$("."+listSearch).removeClass("active");
								$(bgList).removeClass("active");
							}
			  			}//chiusura if(result.trim())
			  			else
			  			{
			  				//se non ci sono risultati nella ricerca, nascondo e svuoto la lista
							$("."+listSearch).empty();
							$("."+listSearch).removeClass("active");
							$(bgList).removeClass("active");
			  			}
			  		}//chiudura function
			  	}); //chiusura $.ajax
				
			}//chiusura if principale
				else {
				//se il numero di caratteri inseriti  troppo basso, nascondo e svuoto la lista
				$("."+listSearch).empty();
				$("."+listSearch).removeClass("active");
				$(bgList).removeClass("active");
				}
			$("*").click(function() {
				$("."+listSearch).empty();
				$("."+listSearch).removeClass("active");
				$(bgList).removeClass("active");
			});
		}//chiusura funzione ricerca
	    
		function creaListaAutocomplete(autoCompleteKeywords)
		{
			var autocompleteHtml="";
			for (i = 0; i < autoCompleteKeywords.length; i++) {
				autocompleteHtml+= "<a href='"+autoCompleteKeywords[i].url;
				if($('#inputIdAssistenza').length>0){
                    autocompleteHtml+= "&N="+$('#inputIdAssistenza').val();
				}
				autocompleteHtml+= "'>"+autoCompleteKeywords[i].titolo+"</a>" ;
			}
			return autocompleteHtml;
		}
		
		function creaListaSuggest(autoSuggestKeywords)
		{
			var autosuggestHtml="<div class='box-offerte'>";
			autosuggestHtml+="<h5>Suggeriti per te</h5>";
			for (i = 0; i < autoSuggestKeywords.length; i++) {
				if(autoSuggestKeywords[i].url != ''){
					autosuggestHtml+= "<a href='"+autoSuggestKeywords[i].url+"'>";
				}
