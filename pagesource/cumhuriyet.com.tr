	<script type="text/javascript">
		
		function init() {
			
			$('html.lte-ie8 #haber-galerisi .text.shadowed').each(function() {
				var t = $(this),
					html = t.html();
				$('<span></span>')
					.addClass('text shadow-box')
					.html(html)
					.insertBefore(t);
			});
			
			$('#video-haber .info b').limitLines(4);
			$('#haber-dikey-4lu a').each(function() {
				var b = $(this).find('b').limitLines(3),
					n = 4 - b.lines();
				b.next().limitLines(n);
			});
			$('#haber-5li a .title span').limitLines(2);
			$('#yazarlar .article a').limitLines(2);
			$('#kucuk-bolumler li .info').each(function() {
				var b = $(this).find('b').limitLines(2),
					n = 3 - b.lines();
				b.next().limitLines(n);
			});
			$('#haber-galerisi .text').each(function() {
				var p = $(this).parent(),
					n = p.is('.small') ? 2 : (p.is('.big') ? 4 : 3);
				$(this).limitLines(n);
			});
			$('#most-news .tabbed-news').data('lines',1)
				.find('.lists > ul.active').addClass('checked').find('a .text').limitLines(1);
			$('#mid-tabbed-newslist .tabbed-news').data('lines',4)
				.find('.lists > ul.active').addClass('checked').find('a .text').limitLines(4);
			$('.tabbed-news .sectnav > li:not(.digest)').on('click',function() {
				$(this).addClass('active').siblings(':not(.digest)').removeClass('active');
				var act = $(this).closest('.tabbed-news').find('.lists > ul').removeClass('active').eq($(this).index()).addClass('active');
				if (!act.is('.checked')) act.addClass('checked').find('a .text').limitLines(act.closest('.tabbed-news').data('lines'));
			});
			$('#news-grid-gallery li').each(function(i,e) {
				var title = $(e).find('.title'),
					sum = $(e).find('.summary')
				;
				if ($(e).is('.large')) {
					var tl = title.limitLines(4).lines();
					sum.limitLines(Math.min(4, 9 - tl));
				} else if ($(e).is('.medium')) {
					var tl = title.limitLines(4).lines();
					sum.limitLines(6 - tl);
				} else {
					title.limitLines(4);
				}
			});
			$('#hor-gallery3 a').each(function() {
				var a = $(this).find('.title');
				a.limitLines(2,null,this);
			});
			$('#doubleColNews .colNews a.news').each(function() {
				$(this).find('.title').limitLines(3,null,this);
			});
			
			$('.wide-sect-news-area li a .title').limitLines(5);
			$('a.imgNews .title').each(function() {
				$(this).limitLines(3,null,$(this).closest('a'));
			});
			
			(function() {

				var hg = $('#hor-gallery3'),
					wr = hg.find('.wrapper'),
					lx = hg.find('.links'),
					li = lx.children(),
					
					arrP = hg.find('.arrow > a.prev'),
					arrN = hg.find('.arrow > a.next'),
					
					curSlide = 1,
					maxSlide = Math.ceil(li.length / 3),
					
					mH = lx.children(':visible:first-child').margin().horizontal
				;
				
				if (li.length < 4) return;
				
				hg
				.find('.arrow').show()
					.find('> .prev').addClass('disabled')
					.end()
				.end()
				.on('click','.arrow > a:not(.disabled)',function() {
					
					if (lx.is(':animated')) return;
					if ($(this).is('.next') && curSlide == maxSlide) return;
					if ($(this).is('.prev') && curSlide == 1) return;
					
					curSlide += $(this).is('.next') ? 1 : -1;
					lx.animate({
						marginLeft: wr.width() * (curSlide-1) * -1 + (curSlide-1) * -mH
					},300,function() {
						if (curSlide == maxSlide) arrN.addClass('disabled');
						else arrN.removeClass('disabled');
						if (curSlide == 1) arrP.addClass('disabled');
						else arrP.removeClass('disabled');
					});
				});
				
			})();
			
			makeSlider('#main-slider');
			
			
			
			
		}
	
	</script>
