	function rotateRank(index) {

		setTimeout(function(){

			lastRotateIndex = index;

			if (!bRotate) {
				bRotateStop = true;
				return;
			}

			$('#rankList ul').animate({
				top: -$('#rankList li').height() * index
			}, 500, function() {
				if (index == $('#rankList li').length -1) {
					$('#rankList ul').css('top', '0px');
					index = 0;
				}
				rotateRank(index + 1);
			});
		}, 2000);
	}

    rotateRank(1);

	$("#rankList").click(function(){
		bRotateStop = false;
		bRotate = false;
		$("#rankLayer").fadeIn();
	});

	$("#rankLayer").hover(function(){}, function(){
		$("#rankLayer").fadeOut();
		bRotate = true;
		if(bRotateStop) {
			bRotateStop = false;
			rotateRank(lastRotateIndex);
