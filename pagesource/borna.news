    function twittSubscribe() {
        if (Clients.session === false) {
            window.setTimeout(function(){
                twittSubscribe();
            }, 2000);
            return false;
        }
        Clients.session.subscribe("/twitt/" + masterConfig.lang, function (topic, event) {
			$twittLi = $('<li/>').addClass('pt8 pb8');
				$twittLi.append(
					$('<span/>').addClass('twitt-number defloat')
				);
				$twittLi.append(
					$('<span/>').addClass('time-icon block icon float')
				);
				$twittLi.append(
					$('<time/>').addClass('block float').text(window.currentTime.getHours() + ':' + window.currentTime.getMinutes())
				);
				$twittLi.append(
					'<div class="clear"></div>'
				);$twittLi.append(
					$('<p/>').text(event.title)
				);

				$('ul.livetweet').prepend($twittLi);
				$('ul.livetweet li').each(function() {
					$('.twitt-number', $(this)).text($(this).index() + 1);
				});
        });
        return true;
    }

</script>
