    function CompleteStartDate(myStartDate)
    {
        var myDate = myStartDate;
        var myValidatePattern = /^(\d{4})[0-1]{1}(\d{1})([0-3]{1})(\d{1})$/;
        var myValidatePatternForMonth = /^(\d{4})(\/)(\d{1})(\/)/;
        var myValidatePatternForDay = /^(\d{4})(\/)(\d{2})(\/)(\d{1})$/;

        // 20170501  -> 2017/05/01
        if (myStartDate.match(myValidatePattern) != null)
        {
            myDate = myStartDate.substring(0, 4) + "/" + myStartDate.substring(4, 6) + "/" + myStartDate.substring(6, 8);
            $('#txtStartDate').val(myDate);
        }
            // 2017/5/5 -> 2017/05/05
        else if (myStartDate.indexOf('/') != -1 && (myStartDate.length >= 8) && myStartDate.length <= 9 && myStartDate.lastIndexOf('/') != myStartDate.indexOf('/'))
        {
            if (myStartDate.match(myValidatePatternForMonth) != null)
            {
                var mySlicePosition = myStartDate.indexOf('/') + 1;
                var myMonth = myStartDate.substring(myStartDate.indexOf('/') + 1, myStartDate.lastIndexOf('/'));
                myDate = [myStartDate.slice(0, mySlicePosition), '0' + myMonth, myStartDate.slice(mySlicePosition + 1)].join('');
                myStartDate = myDate;
            }
            if (myStartDate.match(myValidatePatternForDay) != null)
            {
                var mySlice2Position = myStartDate.lastIndexOf('/');
                myDay = myStartDate.substring(myStartDate.lastIndexOf('/') + 1, myStartDate.length);
                myDate = [myStartDate.slice(0, mySlice2Position + 1), '0' + myDay].join('');
            }
            $('#txtStartDate').val(myDate);
        }
        return myDate;
    }

    function validateDateFormat(myDateVal)
    {
        var myErrBool = false;
        if (myDateVal == null || typeof (myDateVal) != 'string')
        {
            alert('輸入日期有誤');
            $('#txtStartDate').val('');
            return;
        }
        var validatePattern = /^(\d{4})(\/|-)(\d{2})(\/|-)(\d{2})$/;

        myDateValues = myDateVal.match(validatePattern);

        if (myDateValues == null)
        {
            alert('輸入日期有誤');
            $('#txtStartDate').val('');
            return;
        }

        var myYear = myDateValues[1];
        myMonth = myDateValues[3];
        myDay = myDateValues[5];

        if (myMonth < 1 || myMonth > 12)
            myErrBool = true;
        else if (myDay < 1 || myDay > 31)
            myErrBool = true;
        else if ((myMonth == 4 || myMonth == 6 || myMonth == 9 || myMonth == 11) && myDay == 31)
            myErrBool = true;
        else if (myMonth == 2)
        {
            var isleap = (myYear % 4 == 0 && (myYear % 100 != 0 || myYear % 400 == 0));
            if (myDay > 29 || (myDay == 29 && !isleap))
                myErrBool = true;
        }
        if (myErrBool)
        {
            alert('輸入日期有誤');
            $('#txtStartDate').val('');
        }
        return;
    }

