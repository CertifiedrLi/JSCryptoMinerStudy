            var left = $iframe.position().left + $input.offset().left;
            var calendar = $('#calendar');
            var minDate = search_mindate; // 最小日期預設明天
            var maxDate = 330; // 最大日期預設330天
            var top = $iframe.position().top + $input.offset().top + $input.height() + 5;
            var defaultDate = new Date();
            try
            {
                defaultDate = $.datepicker.parseDate('yy/mm/dd', $input.val());
            }
            catch (e)
            {
            }

            $input.addClass('datepicker');

            calendar.datepicker('destroy');
            calendar.css({ 'top': top, 'left': left }).datepicker({
                dateFormat: 'yy/mm/dd',
                onSelect: function (dateText, inst)
                {
                    $input.val(dateText).trigger('change');
                    closeCalendar();
                },
                minDate: minDate,
                maxDate: maxDate,
                changeMonth: true,
                changeYear: true,
                numberOfMonths: 2
            }).datepicker('refresh').appendTo($iframe.parent()).fadeIn(200);

            // 月曆不在可視範圍內，滑動到月曆的位置
            if (!isElementInViewport(calendar))
            {
                $('html, body').animate({
                    scrollTop: calendar.offset().top - $(window).height() + calendar.height() + 5
                }, 200);
            }
            // 建立事件偵測點擊 calendar 以外的地方關閉 calendar
            $(document).on('click.calendar mousedown.calendar', function (e)
            {
                var _target = $(e.target), _selector = "calendar";
                if (_target.attr('id') !== _selector &&
                    _target.closest("#" + _selector).length <= 0 &&
                    !_target.hasClass('ui-datepicker-next') &&
                    _target.closest('.ui-datepicker-next').length == 0 &&
                    !_target.hasClass('ui-datepicker-prev') &&
                    _target.closest('.ui-datepicker-prev').length == 0)
                {
                    closeCalendar();
                }
            });
        }
        /**
         * 關閉月曆
         */
        function closeCalendar()
        {
            $('#calendar').hide();
            // 移除關閉事件
            $(document).off('click.calendar mousedown.calendar');
        }
        /**
         * 判斷 element 是否顯示在可視範圍內
         */
        function isElementInViewport(el)
        {
            //special bonus for those using jQuery
            if (typeof jQuery === "function" && el instanceof jQuery)
            {
                el = el[0];
            }
            var rect = el.getBoundingClientRect();
            return (rect.top >= 0 && rect.left >= 0 && rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) && rect.right <= (window.innerWidth || document.documentElement.clientWidth));
        }


