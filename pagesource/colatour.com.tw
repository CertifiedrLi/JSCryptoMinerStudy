    <!--  -->
    <div id="Div_Type2"></div>
    <script>
        $(function ()
        {
			//$('input').placeholder({customClass:'my-placeholder'});
			add_placeholder('txtDestination', '輸入城市/飯店中英文名稱');

            $("#Div_Type2").css('top', top);
            $("#Div_Type2").css('z-index', '999');
            var list;
            ///Cola_NoBackUp/COLA/A03P_Package/Project/pkg_empty.jpg
            //$.getJSON('Search.js', function (data) {
            $.getJSON('/Cola_NoBackUp/B2C/C10P_Package/Search.js', function (data)
            {
                list = data

                //排序城市優先順序
                list.Citys.sort(function (a, b)
                {
                    return a.Sort_No > b.Sort_No ? 1 : -1;
                });
            });

            var myBackground = '',
                myContent = '',
                Sys = {},
                ua = navigator.userAgent.toLowerCase(),
                s;
            //Hadouken
            (s = ua.match(/rv:([\d.]+)\) like gecko/)) ? Sys.ie = s[1] :
                (s = ua.match(/msie ([\d.]+)/)) ? Sys.ie = s[1] :
                    (s = ua.match(/firefox\/([\d.]+)/)) ? Sys.firefox = s[1] :
                        (s = ua.match(/chrome\/([\d.]+)/)) ? Sys.chrome = s[1] :
                            (s = ua.match(/opera.([\d.]+)/)) ? Sys.opera = s[1] :
                                (s = ua.match(/version\/([\d.]+).*safari/)) ? Sys.safari = s[1] : 0;

            if (Sys.ie) { myBackground = 'background'; myContent = 'innerText' }
            if (Sys.firefox) { myBackground = 'background-color'; myContent = 'innerHTML' }
            if (Sys.chrome) { myBackground = 'background'; myContent = 'innerText' }
            if (myBackground == '') { myBackground = 'background'; myContent = 'innerText' }


            var myChangeMark = false;
            $('#txtDestination').on('input', function (event)
            {
                //關鍵字
                var keyword = this.value.trim().toUpperCase();

                /*如果沒有任何文字*/
                if (keyword != '')
                {
                    SearchList(list, keyword);
                } else
                {
                    $('#Div_Type2')[0].innerHTML = '';
                    $('#Div_Type2')[0].style.display = 'none';
                    $('.destination_popup_box').show()
                }
            });

            $('#txtDestination').on('keydown', function (event)
            {
                if (event.keyCode == 13)
                {
                    event.preventDefault();
                    var myli = $('#Div_Type2 .text_search_popup2_box').find('li');
                    myli.each(function (k, v)
                    {
                        if (v.className == 'DefaultSelected')
                        {
                            $('.formstyle.sel_w2.txttypeform.locationchose')[0].value = v.children[0].children[0][myContent];
                            $('#hidDestination')[0].value = v.id;
                            $(".destination_popup_box").hide();
                            $(".text_search_popup2_box").hide();
                            myChangeMark = true;
                        }
                    });
                }
            })

            $('#txtDestination').on('keyup', function (event)
            {
                if (event.keyCode == 40)
                {
                    var _old, _new;
                    var myli = $('#Div_Type2 .text_search_popup2_box').find('li');
                    for (var i = 0; i < myli.length; i++)
                    {
                        if (myli[i].className == 'DefaultSelected' && myli[i].className != 'remarks')
                        {
                            _old = i;
                            _new = i + 1;
                            if (myli.length - 1 == i) return;
                        }
                    }

                    if (myli.length != 0 && $(myli[_new])[0].className != 'remarks')
                    {
                        $(myli[_old]).removeClass('DefaultSelected');
                        $(myli[_new]).addClass('DefaultSelected');
                    }
                }

                if (event.keyCode == 38)
                {
                    var _old, _new;
                    var myli = $('#Div_Type2 .text_search_popup2_box').find('li');
                    for (var i = 0; i < myli.length; i++)
                    {
                        if (myli[i].className == 'DefaultSelected')
                        {
                            _old = i;
                            _new = i - 1;

                            if (_new == -1) return;
                        }
                    }
                    if (myli.length != 0)
                    {
                        $(myli[_old]).removeClass('DefaultSelected');
                        $(myli[_new]).addClass('DefaultSelected');
                    }
                }
            })

            $('#txtDestination').focus(function ()
            {
                //需要top的定位點
                $('.maincolumn').css('position', 'relative');

                var headHeight = $('.cola-header').height();
                var txtHeight = $(this).height();
                var pos = $(this).offset();
                var top = pos.top - headHeight + txtHeight - 5 + "px";
                $(".destination_popup_box").css('top', top);
                $(".destination_popup_box").css('z-index', '999');

                myChangeMark = false;
                $('.destination_popup_box').show();
            });

            $('#txtDestination').focusout(function ()
            {
                if ($('#hidDestination')[0].value == '')
                {
                    this.value = ''
                }
            });

            $('#txtDestination').on('focusin', function ()
            {
                //console.log("Saving value " + $(this).val());
                $(this).data('val', $(this).val());
            }).on('change', function (e)
            {
                if (myChangeMark == true) { return; }

                var prev = $(this).data('val');
                var current = $(this).val();
                //console.log("Prev value " + prev);
                //console.log("New value " + current);
                if (prev != current && prev != '')
                {
                    this.value = '';
                    $('#hidDestination')[0].value = '';
                }
            });

            //搜尋Button
            $('#pkg_personal .search_btn').on('click', function ()
            {
                var totalPax = parseInt($('#selectAdt').val()) + parseInt($('#selectChd').val())
                if (totalPax > 9)
                {
                    alert("行程預訂人數最多8位(大人+小孩)，若您的人數超過8位，請分別預訂。");
                    return false;
                }

                if (window.location.href.indexOf('Portal') < 0)
                {
                    ga('send', 'event', '搜尋框', 'click', '國外自由行_大首頁搜尋');
                }
                else
                {
                    var myEntry = window.location.href.split('C000_Portal/')[1];
                    if (myEntry.indexOf('HP03') > -1)
                    {
                        ga('send', 'event', '搜尋框', 'click', '國外自由行_自由行館搜尋');
                    }
                    else if (myEntry.indexOf('HP12') > -1)
                    {
                        ga('send', 'event', '搜尋框', 'click', '台中出發自由行_搜尋');
                    }
                    else if (myEntry.indexOf('HP07') > -1)
                    {
                        ga('send', 'event', '搜尋框', 'click', '高雄出發自由行_搜尋');
                    }
                }

                RedirectToAction();

                return false;
            })

            $('#pkg_tour .search_btn').on('click', function ()
            {
                if (window.location.href.indexOf('Portal') < 0)
                {
                    ga('send', 'event', '搜尋框', 'click', '團體自由行_大首頁搜尋');
                }
                else
                {
                    var myEntry = window.location.href.split('C000_Portal/')[1];
                    if (myEntry.indexOf('HP03') > -1)
                    {
                        ga('send', 'event', '搜尋框', 'click', '團體自由行_自由行館搜尋');
                    }
                    else if (myEntry.indexOf('HP12') > -1)
                    {
                        ga('send', 'event', '搜尋框', 'click', '台中出發團體自由行_搜尋');
                    }
                    else if (myEntry.indexOf('HP07') > -1)
                    {
                        ga('send', 'event', '搜尋框', 'click', '高雄出發團體自由行_搜尋');
                    }
                }

                Query_Package();

                return false;
            })

        });

        function SearchList(list, keyword)
        {
            $('.destination_popup_box').hide()

            var mySearchTag = '';
            mySearchTag += "<div class='text_search_popup2_box'>";

            //城市
            var myCityBlock = ''
            var myli = ''; //
            var myCityMark = [];
            var myCityCount = 0; //記錄城市數量
            myCityBlock += "<div class='popup2_wrap'>";
            myCityBlock += "<div class='popup2_label'><p>城市</p><img src='/C10P_Package/search/images/arrow_bg_1.png'></div>";
            myCityBlock += "<ul class='text_search_list'>";
            for (var i = 0; i < list.Citys.length; i++)
            {
                if (list.Citys[i].Country_Chinese_Name.indexOf(keyword) != -1 ||
					list.Citys[i].City_Chinese_Name.indexOf(keyword) != -1 ||
					list.Citys[i].International_English_Name.toUpperCase().replace(' ', '').indexOf(keyword) != -1 ||
					list.Citys[i].International_English_Name.toUpperCase().indexOf(keyword) != -1 ||
					list.Citys[i].Destination_Name.indexOf(keyword) != -1 ||
					list.Citys[i].City_Code.toUpperCase().indexOf(keyword) != -1)
				{
                    if (myCityCount <= 3)
                    {
                        var mark = myCityMark.some(function (e)
                        {
                            return e == list.Citys[i].City_Code
                        });

                        if (mark == false)
                        {
                            myCityMark.push(list.Citys[i].City_Code)
                            myli += "<li id='" + list.Citys[i].City_Code + "' onclick='getCityValue(this)' ><a href='javascript:void(0)'><span>" + list.Citys[i].City_Chinese_Name + "</span>，" + list.Citys[i].Country_Chinese_Name + "</a></li>";
                            myCityCount++;
                        }
                    } else
                    {
                        myHotelli += "<div style='padding-left:8px'><a style='color:#999'>搜尋城市眾多，請輸入更多關鍵字縮小範圍</a></div>";
                    }
                }
            }
            //如城市數量不為0
            if (myCityCount != 0)
            {

                myCityBlock += myli + "</ul>";
            }
            myCityBlock += "</div>"

            //飯店
            var myHotelBlock = '';
            var myHotelli = '';
            var myArrayHotel = [];
            var myArrayAirHotel = [];
            var myHotelCount = 0;
            myHotelBlock += "<div class='popup2_wrap'>";
            myHotelBlock += "<div class='popup2_label'><p>飯店</p><img src='/C10P_Package/search/images/arrow_bg_1.png'></div>";
            myHotelBlock += "<ul class='text_search_list'>";

            for (var i = 0; i < list.Hotels.length; i++)
            {
                myArrayHotel[0] = list.Hotels[i].Hotel_Name.toUpperCase();
                var myEveryHotel = myArrayHotel.every(function (_event, _index, _array)
                {
                    return keyword.split(' ').every(function (_event2, index2, _array2)
                    {
                        return _event.toUpperCase().indexOf(_event2.toUpperCase()) > -1;
                    })
                });
                if (myEveryHotel == true)
                {
                    if (myHotelCount <= 3)
                    {
                        myHotelli += "<li id='" + list.Hotels[i].Hotel_No + "' onclick='getCityValue(this)' ><a href='javascript:void(0)'><span>"
                            + list.Hotels[i].Hotel_Name + "</span>，" + list.Hotels[i].Chinese_Name + "，" + list.Hotels[i].Country_Chinese_Name + "</a></li>";
                        myHotelCount++;
                    }
                }

