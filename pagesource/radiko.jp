<!-- DMP DAC AudienceOne -->
<script type="text/javascript">
(function(){
  var oid = '0a44b7362c5d7b1f';
  var interval = 300; // seconds
  var timer;
  try{
    var params = {
      getArea : function(){
        return window.areaId || "";
      },
      getStation : function(){
        return (window.programInfo) ? window.programInfo.stationId || "" : "";
      },
      getProgram : function(){
        return (window.programInfo) ? encodeURIComponent(window.programInfo.title) || "" : "";
      },
      isPlaying : function(){
        return (typeof player != "undefined" && player.isPlaying) ?  player.isPlaying() : !1;
      }
    }
    var url_base = ('https:' == document.location.protocol ? 'https' : 'http')+'://penta.a.one.impact-ad.jp/dc?';
    var sendRequest = function(){
      var collect_params = [];
      var collect_url = "http://radiko.jp/";
      if(params.getArea()){
        collect_params.push(params.getArea());
        if(params.getStation()){
          collect_params.push(params.getStation());
          if(params.getProgram()){
            collect_params.push(params.getProgram());
          }
        }
      }
      var collect_queryparam = "?playing=";
      collect_queryparam += params.isPlaying() ? "ON" : "OFF";
      collect_queryparam += (typeof uid3 != "undefined") ? "&radiko_id="+uid3() : "";
      collect_params.push(collect_queryparam);
      collect_url += collect_params.join("/");
      var aone_params = [];
      aone_params.push("oid=" + oid);
      aone_params.push("src=" + encodeURIComponent(collect_url));
      var img=document.createElement('img');
      img.setAttribute('width', '1');
      img.setAttribute('height', '1');
      img.setAttribute('style', 'display:none');
      img.src=url_base + aone_params.join("&");
      var b = document.getElementsByTagName('script')[0];
      b.parentNode.insertBefore(img, b);
    };
    if ("complete" == document.readyState){
        sendRequest();
    }else{
        if(window.addEventListener) {
            window.addEventListener("load", function(){sendRequest();}, false);
        }else if (window.attachEvent) {
            window.attachEvent("onload", function(){sendRequest();});
        }else{
            window.onload = function(){sendRequest();};
        }
    }
    timer = setInterval(function(){sendRequest()}, interval * 1000);
  }catch(e){clearInterval(timer)}
})();
</script>
<!-- /DMP DAC AudienceOne -->

