<!-- People Driven DMP -->
<script type="text/javascript">
        var __pParams = __pParams || [];

        var sendPeopleDrivenDmp = function() {
          try{
                // 最後に再生した番組の情報  最後に再生した番組が無い場合は送らない
                var title, ft, to, stationId, stationName;
                if (typeof window.programInfo != "undefined") {
                        title   = window.programInfo.title;
                        ft              = window.programInfo.ft;
                        to              = window.programInfo.to;
                        stationId = window.programInfo.stationId;
                        stationName = window.programInfo.stationName;
                }
                // 会員フラグ　＆　エリアフリー権限フラグ
                var areafree, premium;
                if (typeof $.Radiko.login_status != "undefined") {
                        areafree        = $.Radiko.login_status.areafree.toString();
                        premium         = $.Radiko.login_status.premium.toString();
                }
                // タイムフリー聴取フラグ  最後に再生した番組が無い場合は送らない
                var isLive;
                if (typeof player != "undefined" && title){
                        isLive = (!player.isLive() || player.chasing()).toString();
                }
                __pParams.push({
                        client_id : 5,
                        c_1: ft,
                        c_2: to,
                        c_3: stationName,
                        c_4: stationId,
                        c_5: title,
                        c_6: window.areaId || "",
                        c_7: premium,
                        c_8: uid3(),
                        c_9: isLive,
                        c_10: areafree
                });
                // 実際の送信処理は電通作成のJSタグ無いで行われる
                $("body").append('<script type="text/javascript" src="//cdn.d2-apps.net/js/tr.js" async>\<\/script>');
              } catch(e){
                clearInterval(PeopleDrivenDmpSetIngervlID)
              }
        };
        var PeopleDrivenDmpSetIngervlID = setInterval(sendPeopleDrivenDmp ,300 * 1000);

</script>
<!-- /People Driven DMP -->
