    <script type='text/javascript'>

        var keywordByType = 'homepage';

        var masthead_Sizes = [[1349, 220], [970, 250], [728, 90]];
        var leaderBoard_Sizes = [[728, 90]];
        var mrec_Sizes = [[300, 600], [300, 250]];
        var areaname = "HOMEPAGE";

        if (in_version == "tablet") {
            masthead_Sizes = [728, 90];
        }
        else if (in_version == "mobile") {
            masthead_Sizes = [[300, 100], [320, 100]];
        }

        $(function () {
            var configPubJson = {
                slotNames: {
                    oopSlotName: 'OutOfPage',
                    mastheadSlotName: 'BTFhorz',
                    mrecSlotName: 'BTFvert',
                    leaderboardSlotName: 'BTFhorz'
                },
                adUnitPaths: {
                    oopAdUnitPath: '/4196/RECORD_WEBSITE/',
                    mastheadAdUnitPath: '/4196/RECORD_MASTHEAD/',
                    mrecAdUnitPath: '/4196/RECORD_WEBSITE/',
                    leaderboardAdUnitPath: '/4196/RECORD_MASTHEAD/',
                    pubTestSuffix: areaname
                    //pubTestSuffix: 'HOMEPAGE_TESTE'                  
                },
                slotSizes: {
                    mastheadSizes: masthead_Sizes,
                    mrecSizes: mrec_Sizes,
                    leaderboardSizes: leaderBoard_Sizes,
                }
            };
            COF.pubApi.configPubs(configPubJson);

            googletag.cmd.push(function () {
                var _mastheadCheckSizes = googletag.sizeMapping()
                    .addSize([0, 0], [[300, 100], [320, 100]])
                    .addSize([768, 200], [728, 90])
                    .addSize([1190, 200], [[728, 90],[970, 250], [1349, 220]])
                    .build();

                COF.pubApi.configMastheadCheckSizes(_mastheadCheckSizes);
            });

            var kwpag = [];
            kwpag.push(keywordByType);

            if (typeof (keywordForPub) !== 'undefined') {
                if (keywordForPub != '') {
                    var keys = keywordForPub.split(',');
                    var i;
                    for (i = 0; i < keys.length; ++i) {
                        if (keys[i] != '') {
                            var keyToAdd = keys[i].trim();
                            kwpag.push(keyToAdd);
                        }
                    }
                }
            }

            if (areaname == "PREMIUM" || $("#hdnIsPremium").val() == "1") {

                    kwpag.push('assinaturadigital');
            }

            var totalCount = 0;
            var indexOptin = 0;
            function adsRenderEnded(adsEvent) {
                totalCount++;
                var slotElementID = adsEvent.slot.getSlotElementId();

                //validacao para carregar o script do optinmonster
                if ((adsEvent.isEmpty && slotElementID == 'OutOfPage') || (adsEvent.isEmpty && slotElementID.indexOf('horzATF') > -1)) {
                    indexOptin++;
                    if (indexOptin == 2 && typeof loadOptinMonster === 'function') {
                        loadOptinMonster();
                    }
                }

            }

            googletag.cmd.push(function () {
                googletag.pubads()
                    .setTargeting('end', [window.location.href.match(/([^\?]*)\/*$/)[0]])
                    .setTargeting('kw', kwpag);

                googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                    adsRenderEnded(event);
                });

                googletag.pubads().enableSingleRequest();
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().collapseEmptyDivs(true, true);
                googletag.pubads().setCentering(true);
                googletag.companionAds().setRefreshUnfilledSlots(true);
                googletag.pubads().disableInitialLoad();

                googletag.enableServices();

                //PUB
                var loc = window.top.location.pathname;

                if (in_version == "pc") {

                    //OOP
                    if (location.pathname == "/" || location.pathname == "/default.aspx") {

                        COF.pubApi.renderOOP($('#div-gpt-ad-1382715432914-0-oop'));   //masterpge ATF HP
                    }
                    else if (loc.indexOf("/epaper.html") == -1 && loc.indexOf("/livecenter.aspx") == -1 && loc.indexOf("/jogos_em_direto.aspx") == -1 && loc.indexOf("/jogos_estatisticas.aspx") == -1 && loc.indexOf("/euro2016_ext.aspx") == -1 && loc.indexOf("/politica_cookies.aspx") == -1 && loc.indexOf("/jogos.aspx") == -1 && loc.indexOf("/sabermais/") == -1 && loc.indexOf("subscrever.aspx") == -1 && loc.indexOf("/e-de-homem") == -1) {

                        COF.pubApi.renderOOP($('#div-gpt-ad-1379692478589-0-oop'));   //masterpge ATF
                    }                 

                    //Mrec
                    if (loc.indexOf("multimedia") > -1 || loc.indexOf("iniciativas") > -1 || loc.indexOf("record-social") > -1) {

                        COF.pubApi.renderMrec($('#div-gpt-ad-1379692478589-0'), '', 'vertATF', 'ATF', false);                   

                    } else {
                        if (loc.indexOf("/euro2016_ext.aspx") == -1 && loc.indexOf("/resultados-e-classificacoes/jogos-em-direto.html") == -1 && loc.indexOf("/jogos_em_direto.aspx.aspx") == -1 && loc.indexOf("/jogos_estatisticas.aspx") == -1) {

                            COF.pubApi.renderMrec($('#div-gpt-ad-1379692478589-0'), '', 'vertATF', 'ATF', false);                           

                        }

                        //MastHead
                        if (location.pathname == "/" || location.pathname == "/default.aspx" || loc.indexOf("/detalhe/") > -1) {

                            //MastHead Topo e detalhe
                            COF.pubApi.renderMasthead($('#div-gpt-ad-1458147793283-0-intro'), '', 'horzATF', 'ATF', false);                        

                        }
                    }

                } else if (in_version == "tablet") {

                        //Detalhe tem pub unica amovivel..
                        if (loc.indexOf("/detalhe/") > -1) {

                            COF.pubApi.renderMrec($('#div-gpt-ad-1379692478589-0'), '', 'vertATF', 'ATF', false);   

                        }
                        
                        if (loc.indexOf("/euro2016_ext.aspx") == -1) {

                                if ($('#div-gpt-ad-1379692478589-0-tablet').length > 0)
                                {
                                    COF.pubApi.renderMrec($('#div-gpt-ad-1379692478589-0-tablet'), '', 'vertATF', 'ATF', false);
                                }
                                    
                                COF.pubApi.renderOOP($('#div-gpt-ad-1379692478589-0-oop')); //masterpge ATF
                        }
                
                        if (location.pathname == "/" || location.pathname == "/default.aspx" || loc.indexOf("/detalhe/") > -1) {

                                //MastHead Topo
                                COF.pubApi.renderMasthead($('#div-gpt-ad-1458147793283-0-intro'), '', 'horzATF', 'ATF', false); //Masterpage ATF
                        }
                                          
                } else if (in_version == "mobile") {

                    //Detalhe tem pub unica amovivel..
                    if (loc.indexOf("/detalhe/") > -1) {

                        COF.pubApi.renderMrec($('#div-gpt-ad-1379692478589-0'), '', 'vertATF', 'ATF', false);   

                    }

                    if (loc.indexOf("/euro2016_ext.aspx") == -1) {

                        if ($('#div-gpt-ad-1379692478589-0-tablet').length > 0) {
                            COF.pubApi.renderMrec($('#div-gpt-ad-1379692478589-0-tablet'), '', 'vertATF', 'ATF', false);
                        }

                        COF.pubApi.renderOOP($('#div-gpt-ad-1379692478589-0-oop')); //masterpge ATF
                    }

                    if (location.pathname == "/" || location.pathname == "/default.aspx") {
                        //MastHead Mobile
                        COF.pubApi.renderMasthead($('#div-gpt-ad-1452530240421-0-masthead-mobile'), '', 'horzATF', 'ATF', false);  
                    }
                }

                googletag.pubads().refresh();
            });
        });

    </script>

    

    

