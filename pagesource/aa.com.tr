<script>
    var timerBreaking;
    var breakingStrLen = 0;

    $.breakingInit = function(){
        if (!document.hidden) {
            $.get('https://aa.com.tr/tr/BreakingNewsAjax',{},function(data){
                if(data==""){
                    breakingStrLen = 0;
                    $('.BreakingNewsDiv').hide();
                }else{

                    if(breakingStrLen != data.length){
                        breakingStrLen = data.length;
                        $("#bn1").remove();
                        $(".BreakingNewsDiv").html($('#breakingCopy').html());
                        $('#breakingNewsId').html(data);

                        $.breakingLoad();

                        $("#bn1").breakingNews({

                            autoplay: true,
                            timer: 8000,
                            color: "red"
                        });
                        $('#bn1>div>h2').css({display:'inline-block'});
                        $('.bn-title').css({width:'auto'});
                        $('.BreakingNewsDiv').show();
                    }
                }

            });
        }
    }

    $.breakingLoad = function(){
        var breakingCharLimit = 130;

        $(".bn-title").click(function () {
            if ($(window).width() < 767) {
                if ($("#breakingNewsId").css("display") == "none") {
                    $("#breaking-close").children().removeClass("fa-plus")
                    $("#breaking-close").children().addClass("fa-times")
                }
                else {
                    $("#breaking-close").children().removeClass("fa-times")
                    $("#breaking-close").children().addClass("fa-plus")
                }
                $("#breakingNewsId").slideToggle();
            }
        });
        $.each($("#breakingNewsId").find("a"), function (k, v) {
            if ($(v).text().length >breakingCharLimit){
                if ($(window).width() > 767) {
                    $(this).css("font-size", "14px");
                    $(v).parent().css("line-height","17px")
                }
            }
        })
    }

    $(window).focus(function() {
        $.breakingInit();
    });


    $(document).ready(function () {
        $.breakingInit();
        var timerBreaking = setInterval(function(){ $.breakingInit() }, 5000);
    })

</script>
