<script type="text/javascript" language="javascript">
$(function(){
	var scrolls=new globle_scroll('.index_forit .list','','','.index_forit .ol',12000);
	scrolls.gos();
	var scrolls=new globle_scroll('.index_latest .box > ul','','','',12000);
	scrolls.gos();
	var sc=function(){
		if($(window).scrollTop()>450){
			$('.float_livechat').show();
		}else{
			$('.float_livechat').hide();
		}
	}
	$(window).scroll(function(){//totop滚动
		sc();
	});
	//for banner
	var banner_resize=function(){
		var banner_resize_fun=function(){
			var banner_h=$(window).height();
			if(banner_h<720){
				banner_h=720;
			}else if(banner_h>1080){
				banner_h=1080;
			}
			$('.banner .banner_ul').css({maxHeight:banner_h});
		}
		if(ispc){
			$(window).resize(function(){
				banner_resize_fun();
			});
			banner_resize_fun();
		}
	}
	banner_resize();
	//other webs
	var banner_position=function(){
		var url=document.URL;
		url=url.match(/banner\=.*/);
		if(url!=null){
			url=url[0];
			if(/\&/.test(url)){
				url=url.split('&');
				url=url[0];
			}
			url=url.replace('banner=','');
			$('.banner_ol span:eq('+url+')').click();
		}
	}
	banner_position();
	//index--scroll-to-second
	var index_scroll=function(){
		$('.banner .mouse').click(function(){
			var top=$('.index_dofor').offset().top;
			$('body,html').animate({ scrollTop:top},600);
		});
		//mouse up-down
		var direct=0; 
		var scrollFunc=function(e){ 
			e=e || window.event;
			if(e.wheelDelta){//IE/Opera/Chrome 
				if(e.wheelDelta>0){
					direct=1;
				}else{
					direct=-1;
				}
			}else if(e.detail){//Firefox 
				if(e.detail>0){
					direct=1;
				}else{
					direct=-1;
				}
			}
		} 
		if(document.addEventListener){
			document.addEventListener('DOMMouseScroll',scrollFunc,false); 
		} 
		window.onmousewheel=document.onmousewheel=scrollFunc;
		//mouse up-down -end
		var num=0;
		$(document).bind('mousewheel',function(event){
			//mouse scroll
			if(direct<0){
				var self=$(this);
				if(num==0){
					num=1;
					var top=$('.index_dofor').offset().top-55;
					if(top>$(document).scrollTop()){
						$('body,html').animate({ scrollTop:top},600,function(){
							self.unbind('mousewheel');
						});
					}else{
						self.unbind('mousewheel');
					}
				}
				return false;
			}
			//mouse scroll-end
		});
	}
	if(ispc){
		index_scroll();
	}
	//nav float
	var nav_float=function(){
		var scroll_show=true;
		var once=true;
		$(document).scroll(function(){
			var header_box=$('.header_box');
			var height=header_box.height();
			var top=$('.header_box').height();
			if(top<$(window).scrollTop()){
				scroll_show=true;
			}else{
				scroll_show=false;
			}
			if(scroll_show){
				if(once){
					once=false;
					header_box.removeAttr('style').addClass('current');
					var st=window.setTimeout(function(){
						header_box.css({transition:'400ms'}).addClass('fixed');
					},1);
				}
			}else{
				if(!once){
					once=true;
					header_box.removeClass('fixed');
					var st=window.setTimeout(function(){
						header_box.removeClass('current');
					},400);
				}
			}
		})
	}
	nav_float();
	//resize
	var dom=$('.index_map .idmap');
	jigsaw_puzzle_resize=function(){
		var width=$(window).width()/1200;
		if(width<1){
			dom.css({fontSize:40*width+'px'});
		}else{
			dom.removeAttr('style');
		}
	}
	jigsaw_puzzle_resize();
	$(window).resize(function(){
		jigsaw_puzzle_resize();
	});
})
</script>
