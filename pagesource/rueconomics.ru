    <script>
    $('#elections-slider').lightSlider({
        item:20,
        loop:true,
        slideMargin: 16,
        //autoWidth: false,
        autoWidth: true,
        slideMove:2,
        mode: 'slide',
        useCSS: true,
        easing: 'linear',
        speed: 300,
        pager: false,
        gallery: false,
        slideEndAnimation: true,
        controls: true,
        prevHtml: '<div class="triangle-left"></div>',
        nextHtml: '<div class="triangle-right"></div>',

    });

    // window.scrollVal = 0;
    // jQuery(window).on('scroll', function(event){
    //     window.scrollVal = 0;
    //     window.scrollVal = jQuery(window).scrollTop();
    //
    // });

    /***************************************************/
    var keys = {37: 1, 38: 1, 39: 1, 40: 1};
    function preventDefault(e) {
        e = e || window.event;
        if (e.preventDefault)
            e.preventDefault();
        e.returnValue = false;
    }
    function preventDefaultForScrollKeys(e) {
        if (keys[e.keyCode]) {
            preventDefault(e);
            return false;
        }
    }
    function disableScroll() {
        if (window.addEventListener) // older FF
            window.addEventListener('DOMMouseScroll', preventDefault, false);
        window.onwheel = preventDefault; // modern standard
        window.onmousewheel = document.onmousewheel = preventDefault; // older browsers, IE
        window.ontouchmove  = preventDefault; // mobile
        document.onkeydown  = preventDefaultForScrollKeys;
    }
    function enableScroll() {
        if (window.removeEventListener)
            window.removeEventListener('DOMMouseScroll', preventDefault, false);
        window.onmousewheel = document.onmousewheel = null;
        window.onwheel = null;
        window.ontouchmove = null;
        document.onkeydown = null;
    }
    /***************************************************/
    
    function tagListTrim() {
        var outer_width = jQuery('.elections-box__tags').width();
        var inner_width = 0;
        jQuery('.elections-box__tags li').each(function(){
            inner_width = inner_width + jQuery(this).width() + 8;
            if (inner_width < outer_width){
                jQuery(this).addClass('approved');
            }
        });
        jQuery('.elections-box__tags li').not('.approved').remove();
    }

    var box_open = false;
    jQuery('#elections-box-control, #modal-shadow').on('click', function(event){
        event.preventDefault();

        jQuery('.elections-bar__short-link').blur();

        jQuery('#elections-box').slideToggle(300, function () {

            if (jQuery('#elections-box').is(':visible') && !box_open){
                box_open = true;
                tagListTrim();
            }

            var button_text = '';
            if (jQuery('#elections-box').is(':visible')){
                button_text = 'Скрыть';
                jQuery('#modal-shadow').addClass('active');
                disableScroll();
                jQuery('#elections-box-control').addClass('sub-header-open');
                jQuery('.elections-bar__full-link').hide();

            }
            else{
                button_text = 'Подробнее';
                jQuery('#modal-shadow').removeClass('active');
                enableScroll();
                //jQuery('.l-sticky-header').removeClass('menu-holder');
                jQuery('#elections-box-control').removeClass('sub-header-open');
                jQuery('.elections-bar__full-link').show();
            }
            jQuery('.elections-bar__short-link').html(button_text);
        });


    });
</script>

