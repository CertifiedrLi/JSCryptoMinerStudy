/**
 * Adds an iref to a URL on mousedown
 *
 * @param   element atag    Anchor tag to replce the href for
 * @param   string  iref    iref code to add to the URL
 * @return  bool
 *
 */
DN.iref = function(atag, iref) {

    if (this.tagName && this.tagName == "A") {
        // this is a bound event that is expected
        // to automatically attach an iref
        atag = this;
        iref = "";
    }

    if (atag.tagName == "A") {

        var url = atag.href;
        var parts = null;

        var hasAttr = false;

        if (DN.empty(iref)) {
            iref = atag.getAttribute("data-iref");
            if (iref) {
                // add page type and device target to the iref
                if (DN.pageData && DN.pageData.type) {
                    iref += "-" + DN.pageData.type;
                }
                if (typeof deviceTarget !== "undefined") {
                    iref += "-" + deviceTarget.toLowerCase();
                }
                hasAttr = true;
            }
        }

        if (DN.empty(iref)) {
            return;
        }

        iref = iref.toLowerCase();
        iref = iref.replace(/[^a-z0-9\-_]+/, "_");

        var sep = "&";

        if (url.search(/\?/) < 0) {
            sep = "?";
        } else if (url.search(/,/) >= 0 || url.search(/\=/) === -1) {
            sep = ",";
        }

        if (url.search(/\#/) !== -1) {
            parts = url.split("#");
            url = parts[0];
        }

        url += sep + "iref=" + encodeURIComponent(iref);

        if (parts !== null) {
            parts[0] = url;
            url = parts.join("#");
        }

        atag.href = url;
        atag.onmousedown="";

        if (hasAttr) {
            atag.setAttribute("data-iref", "");
        }

    }

    return iref;
};

