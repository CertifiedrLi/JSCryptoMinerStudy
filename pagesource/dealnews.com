    var cookiesEnabled = (navigator.cookiesEnabled) ? true : false;
    if (typeof navigator.cookiesEnabled == "undefined" && !cookiesEnabled) {
        document.cookie = "testcookie";
        cookiesEnabled  = (document.cookie.indexOf("testcookie") != -1) ? true : false;
    }
    return (cookiesEnabled);
};

/**
 * Sets a cookie
 *
 * @param {string} name    The cookie name
 * @param {string} value   The value to be stored in the cookie
 * @param {object} options Optional settings for the cookie.
 *                          expires Either a Date object or number of days
 *                          path    Defaults to /
 *                          domain  Defaults to current hostname
 * @return  void
 *
 */
DN.setCookie = function(name, value, options) {

    var cookieValue;

    cookieValue = name+"="+escape(value);

    if (DN.isset(options)) {

        if (DN.isset(options["expires"])) {
            var expires;
            if (parseInt(+options["expires"],10)  && !isNaN(parseInt(options["expires"]))) {
                expires=new Date();
                expires.setDate(expires.getDate()+options["expires"]);
            } else {
                expires = options["expires"];
            }
            cookieValue += "; expires=" + expires.toUTCString();
        }

        if (DN.isset(options["path"])) {
            cookieValue += "; path="+escape(options["path"]);
        } else {
            cookieValue += "; path=/";
        }

        if (DN.isset(options["domain"])) {
            cookieValue += "; domain="+escape(options["domain"]);
        } else {
            var hostParts = window.location.hostname.split(/[.\-]/);

            if (hostParts.indexOf('secure') > -1 || !isProd) {
                cookieValue += "; domain="+window.location.hostname;
            } else {
                cookieValue += "; domain=."+hostParts[hostParts.length-2]+"."+hostParts[hostParts.length-1];
            }
        }

    }

    document.cookie = cookieValue;

};

/**
 * Gets a cookie if it exists. Returns null if it does not
 *
 * @param   {string}  name The cookie name
 * @return  mixed
 *
 */
DN.getCookie = function(name) {
    var dc     = document.cookie;
    var prefix = name+'=';
    var begin  = dc.indexOf('; '+prefix);

    if (begin == -1) {
        begin = dc.indexOf(prefix);

        if (begin !== 0) {
            return null;
        }
    } else {
        begin += 2;
    }
    var end = document.cookie.indexOf(';', begin);

    if (end == -1) {
        end = dc.length;
    }

    return unescape(dc.substring(begin+prefix.length, end));

};

/**
 * Checks for and returns values from the query string. Returns null
 * if the query string does not contain a parameter of this name.
 *
 * @param   {string}  varName     Query string parameter to get
 * @return  mixed
 *
 */
DN.getQuery = function(varName) {

    if (typeof DN.getQuery.params == 'undefined') {

        DN.getQuery.params = {};

        qs = location.search.substring(1, location.search.length);

        if (qs.length > 0) {

            qs = qs.replace(/\+/g, ' ');

            var args = qs.split('&');

            for (var i = 0; i < args.length; i++) {
                var value;
                var pair = args[i].split('=');
                var name = unescape(pair[0]);

                if (pair.length == 2) {
                    value = unescape(pair[1]);
                } else {
                    value = name;
                }

                DN.getQuery.params[name] = value;
            }
        }
    }

    if (typeof DN.getQuery.params[varName] != "undefined") {
        return DN.getQuery.params[varName];
    } else {
        return null;
    }

};

/**
 * Parses a query string, returning an object of keys and values.
 *
 * @param  {string}   query   A query string. If none is passed, the current
 *                          query string is used.
 *
 * @return object           An object containing keys and values. You can pass
 *                          the object to buildQuery to get the string back.
 */
DN.parseQuery = function(query) {

    if (typeof query == "undefined") {
        var query = window.location.search.substring(1);
    }

    var vars = query.split('&');
    var ob   = {};

    for (var i = 0; i < vars.length; i++) {

        if (vars[i] == "") {
            continue;
        }

        var pair                        = vars[i].split('=');
        ob[decodeURIComponent(pair[0])] = decodeURIComponent(pair[1]);
    }

    return ob;
};

/**
 * Builds a query string from an object
 *
 * @param  {object} params object of key value pairs to make a query string
 *
 * @return string
 */
DN.buildQuery = function(params) {
    var qstr = [];

    for (var param in params) {
        qstr.push(encodeURIComponent(param)+"="+encodeURIComponent(params[param]));
    }

    return qstr.join("&");
};

