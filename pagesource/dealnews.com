                 if (region.left < document.documentElement.clientWidth / 3) {
                    attachToCorner = "bl";
                    elCorner = "tl";
                 } else {
                    attachToCorner = "br";
                    elCorner = "tr";
                 }

            } else {
                if (!attachToCorner) {
                    attachToCorner = "br";
                }
                if (!elCorner) {
                    elCorner = "tr";
                }
            }

            var newLeft = region[attachToCorner]["x"];
            var newTop = region[attachToCorner]["y"];

            switch (elCorner) {
                case "tr":
                    newLeft = newLeft - elRegion.width;
                    break;
                case "bl":
                    newTop = newTop - elRegion.height;
                    break;
                case "br":
                    newTop = newTop - elRegion.height;
                    newLeft = newLeft - elRegion.width;
                    break;
                case "tl":
                    // do nothing
                    break;
            }

            var isAttached = true;

            if (keepOnScreen) {
                if (newLeft < 0) {
                    newLeft = 0;
                    isAttached = false;
                }

                if (newTop < 0) {
                    newTop = 0;
                    isAttached = false;
                }

                var container = {};

                if (DN.getStyle(attachElement, "position") != "fixed") {
                    container = {
                        width: document.documentElement.clientWidth  + scrollLeft,
                        height: document.documentElement.clientHeight + scrollTop
                    };
                } else {
                    container = {
                        width: window.innerWidth,
                        height: window.innerHeight
                    };
                }

                if (newLeft + elRegion.width > container.width) {
                    newLeft = container.width - elRegion.width;
                    isAttached = false;
                }

                if (newTop + elRegion.height > container.height) {
                    newTop = container.height - elRegion.height;
                    isAttached = false;
                }
            }

            attachElement.style.left = newLeft + "px";
            attachElement.style.top = newTop + "px";
            attachElement.style.bottom = "auto";
            attachElement.style.right = "auto";

            return isAttached;
        }
    }
};

/**
 * Attempts to find an elements z-index.
 *
 * @param el
 *
 */
DN.getZIndex = function (el) {
    var position = DN.getStyle(el, 'position');
    var zIndex = DN.getStyle(el, 'z-index');
    while ((position == "static" || position == "inherit" || isNaN(parseInt(zIndex, 10))) && el != document.body) {
        el = el.parentNode;
        position = DN.getStyle(el, 'position');
        zIndex = DN.getStyle(el, 'z-index');
    }

    return zIndex;
};

