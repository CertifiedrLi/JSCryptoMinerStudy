        $('div.menusub[data-for="'+this_sub+'"] li').removeClass('opacity05');
        if(cur_visible_submenu){
            $('div.menusub[data-for="'+this_sub+'"]').show();
        }
        else{
            $('div.menusub[data-for="'+this_sub+'"]').fadeIn();
        }
        cur_visible_submenu = this_sub;
    });
    $('.root_node').not('.has_sub').mouseover(function(){
        hide_all_sub();
    });
    $('.menusub li').mouseover(function(){
        $('.menusub li').removeClass('opacity05').not(this).addClass('opacity05');
    });
    $('.wrapper, .header, .content').mouseover(function(){
        $('.menusub li').removeClass('opacity05');
        if(cur_visible_submenu)
        {
            cur_visible_submenu = 0;
            hide_all_sub();
        }
    });
});
</script>
<script>
    var first_topbar_run = 1;
    function ajaxReplaceTopBars(topIndex)
    {
        classNameTop = $('.header_top_holder a')[1];
        $.getJSON('/posts/get?limit=8&param=8388',function(lastestPosts){
            if('posts' in lastestPosts)
            {
                lastestPosts = lastestPosts.posts;
                if('data' in lastestPosts)
                {
                    lastestPosts = lastestPosts.data;
                    for(key in lastestPosts)
                    {
                        if(lastestPosts.hasOwnProperty(key))
                        {
                            curpost = lastestPosts[key];
                            if(!$('.header_top_holder a#a'+curpost.id).is('a'))
                            {
                                curtime = curpost.published_at.match(/ (\d\d\:\d\d)\:/i);
                                if(curtime)
                                {
                                    $(classNameTop).fadeOut("slow",function(){
                                        $(this).remove();
                                        $('.header_top_holder').prepend('<a id="a'+curpost.id+'" class="hidetop" onclick="yaCounter26041173.reachGoal(\'hot_news\'); return true;" href="/'+curpost.id+'-'+curpost.slug+'">'+curtime[1] + '&nbsp' + curpost.title+'<span class="backlines"></span></a>');
                                        $('.header_top_holder a.hidetop').fadeIn("slow").removeClass('hidetop').attr('style','display:block');
                                        if(first_topbar_run)
                                        {
                                            first_topbar_run = 0;
                                            window.setTimeout(function(){
                                                ajaxReplaceTopBars(1);
                                            },17000);
                                        }else{
                                            window.setTimeout(function(){
                                                ajaxReplaceTopBars(1);
                                            },20000);
                                        }
                                    });
                                    return true;
                                }
                            }
                        }
                    }
                }
            }
        });
    }
    $(document).ready(function() {
        
                $('.header_top_holder .hidetop').each(function(i,v){
            if(i == 1 || i == 3)
            {
                $(v).addClass('secondary');
            }
            if(i < 2)
            {
                $(v).removeClass('hidetop');
            }
        });
        window.setTimeout(function(){
            ajaxReplaceTopBars(1);
        },30000);
    });
    $('.loop').click(function(){
        hide_all_sub();
        $(".header_form").fadeToggle();
        $(".header_share").fadeOut();
        $('.share').removeClass('close');
        $(this).toggleClass('close');
        // $('.main_logo').fadeOut();
        $('.search_field').focus();

        if( $(this).hasClass('close'))
        {
            $('.main_logo').fadeOut();
            $('li.root_node').fadeOut();
        }
        else
        {
            $('.main_logo').fadeIn();
            $('li.root_node').fadeIn();
        }
    });

    $('.share').click(function(){
        hide_all_sub();
        $(".header_form").fadeOut();
        $(".header_share").fadeToggle();
        // $('.main_logo').fadeOut();
        $('.loop').removeClass('close');
        $(this).toggleClass('close');

        if( $(this).hasClass('close'))
        {
            $('.main_logo').fadeOut();
            $('li.root_node').fadeOut();
        }
        else
        {
            $('.main_logo').fadeIn();
            $('li.root_node').fadeIn();
        }
    });

    $(document).click(function(event) {
        if ($(event.target).closest('.loop, .header_form').length) return;
        if ($(event.target).closest('.share, .header_share').length) return;
        if ($(event.target).closest('#regions_switch_inner a').length) return;
        $('.main_logo').fadeIn();
        $('.header_form').fadeOut();
        $('.header_share').fadeOut();
        $('.loop').removeClass('close');
        $('.share').removeClass('close');
        $('li.root_node').fadeIn();
        hide_all_sub();
        $('#regions_switch_inner').removeClass('full-open');
    });
</script>

