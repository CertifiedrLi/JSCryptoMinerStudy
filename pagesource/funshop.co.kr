    <script type="text/javascript">
        $(function () {
            
            $('.thumb').lazyload({ threshold: 1000, effect : "fadeIn" });
            $('.communiytlazy').lazyload({ threshold: 1000, effect : "fadeIn" });

            $("#image_list_1").MainrollOver({nWidth: 134,nHeight: 89,nCount: 4,isActType: "none",nOrderNo: 1,nDelay: 10000,isBtnType: "li",isShuffle: "Y"});
            $("#image_list_2").MainrollOver({nWidth: 134,nHeight: 89,nCount: 4,isActType: "none",nOrderNo: 1,nDelay: 10000,isBtnType: "li",isShuffle: "Y"});
            $("#image_list_3").MainrollOver({ nWidth: 134, nHeight: 89, nCount: 4, isActType: "none", nOrderNo: 1, nDelay: 10000, isBtnType: "li", isShuffle: "Y" });
            /* keyword length */
            var leftWidth = $("img.left_imgnvk").width();
            var rightWidth = $("img.right_imgnvk").width();
            var widCut = ($("#center_nvk > span").width() / 2);

            var centerMar;
            var centerMargin;
            if ($("#center_nvk").length > 0) {
                centerMar = $("#center_nvk").css("margin-left");
                centerMargin = centerMar.replace("px", "");
            }

            $("img.left_imgnvk").attr("width", leftWidth - widCut);
            $("#center_nvk").css("margin-left", centerMargin - widCut);
            $("img.right_imgnvk").attr("width", rightWidth - widCut);
            

            function numberCounter(target_frame, target_number) {
                this.count = 0; this.diff = 0;
                this.target_count = parseInt(target_number);
                this.target_frame = document.getElementById(target_frame);
                this.timer = null;
                this.counter();
            };

            numberCounter.prototype.counter = function () {
                var self = this;
                this.diff = this.target_count - this.count;

                if (this.diff > 0) {
                    self.count += Math.ceil(this.diff / 3);
                }

                this.target_frame.innerHTML = this.formatNumber(this.count);

                if (this.count < this.target_count) {
                    this.timer = setTimeout(function () { self.counter(); }, 2);
                } else {
                    clearTimeout(this.timer);
                }
            };

            numberCounter.prototype.formatNumber = function (num) {
                num += '';
                x = num.split('.');
                x1 = x[0];
                x2 = x.length > 1 ? '.' + x[1] : '';
                var rgx = /(\d+)(\d{3})/;
                while (rgx.test(x1)) {
                    x1 = x1.replace(rgx, '$1' + ',' + '$2');
                }
                return x1 + x2;
            };

            var lastps = 0;
            $("input[name='sbcount']").each(function () {
                
                var position = $(this).attr("data");
                var splPosition = position.split(",");
                var divPos = $(this).attr("pos");
                var nData = $(this).attr("ndata");
                var nColor = $(this).attr("color");
                var nSize = $(this).attr("size");

                if (nColor == undefined || nColor == "") {
                    nColor = "ffffff";
                }

                if (nSize == undefined || nSize == "") {
                    nSize = "32";
                }

                if (lastps <= divPos) {

                
                    var strDivision = ''
                    strDivision += '<div name="aaa" id="pb' + nData + divPos + '" class="scount" style="position:absolute; left:' + splPosition[0] + 'px; top:' + parseInt(splPosition[1]) + 'px;z-index:1000000; color:#' + nColor + ';font-size:' + nSize + 'px">';
                    strDivision += '';
                    strDivision += '<div>';

                    $("#sbcount" + divPos).append(strDivision);

                    var requestData = {
                        pNo: nData,
                    };

                    $.ajax({
                        type: "post",
                        url: "/Home/GetBData",
                        contentType: "application/json",
                        data: JSON.stringify(requestData),
                        success: function (result) {
                            new numberCounter("pb"+nData + divPos, result.BuyCount);
                        }

                    });
                    lastps = divPos;
                }
                
            })



            if (!Cookies.hasItem("BannerCookies")) {

                //메일수신하는지 파악후 배너를 보여줌
                var isLogin = "False";
                if (isLogin == "True") {

                    var requestData = {
                        customerNo: "0"
                    };

                    $.ajax({
                        type: "post",
                        url: "/Home/SelectMailChacek",
                        contentType: "application/json",
                        data: JSON.stringify(requestData),
                        success: function (result) {
                            if (!result) {
                                $("#mailBanner").hide();
                            } else {
                                $("#mailBanner").show();
                            }
                        },
                        error: function (xhr, textStatus, errorThrown) {
                            toastr.error(xhr.responseText);
                        }
                    });
                }
            }



            $("#images_cookies").bind("click", function () {
                Cookies.setItem('BannerCookies', 'clickBanner', moment().add('d', 120).toString(), '/');
            })


        });
    </script>
