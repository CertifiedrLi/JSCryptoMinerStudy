    <script type="text/javascript">
        var Funshop = null;
        var title = "";
        var pubDate = "";
        window.onload = function () {
            if (!Funshop) {
                Funshop = new _funshop('https://funshop.akamaized.net', 'https://www.funshop.co.kr', 'http://www.raksik.com', 'http://urban.funshop.co.kr');
            }


                    
            Funshop.RenderAsideBar();
            checkSide();
            




        };

        $(function () {
            var liContain = 'True';
            if (liContain=="False") {
                $("#Message-slider-btn").hide();
            }
            
            var $layerPopupObj = $('.moveicondiv');
            var top = (($(window).height() - 42) / 2);
            $layerPopupObj.css({ 'top': top, 'position': 'absolute', 'left': '-17' });
            checkSide();

            $(".btncancle-choice").bind("click", function () {
                $("#pop3").hide();
            });

            $("#offlineSale").hide();
            $.ajax({
                type: "post",
                async: false,
                url: "/Shared/SelectRss",
                contentType: "application/json",
                data: "",
                success: function (result) {
                    if (result == "" || result == null) {
                        $("#offlineSale").hide();
                    } else {
                        $("#offlineText").text(result.Title);
                        $("#offlineSale").show();
                        title = result.Title;
                        pubDate = result.Created;

                    }
                }
            });

            $("#menuUrban").on("mouseenter", function () { var d = new Date(); $(this).attr("src", "https://funshop.akamaized.net/www/common/header/urban_ani.gif?" + d); });
            $("#menuUrban").on("mouseleave", function () { $(this).attr("src", "https://funshop.akamaized.net/www/common/header/urban_ani.png"); });

            $("#menuBook").on("mouseenter", function () { var d = new Date(); $(this).attr("src", "https://funshop.akamaized.net/www/common/header/book_ani.gif?" + d); });
            $("#menuBook").on("mouseleave", function () { $(this).attr("src", "https://funshop.akamaized.net/www/common/header/book_ani.png"); });

            $("#menuGear").on("mouseenter", function () { var d = new Date(); $(this).attr("src", "https://funshop.akamaized.net/www/common/header/gear_ani.gif?" + d); });
            $("#menuGear").on("mouseleave", function () { $(this).attr("src", "https://funshop.akamaized.net/www/common/header/gear_ani.png"); });

            $("#menuToy").on("mouseenter", function () { var d = new Date(); $(this).attr("src", "https://funshop.akamaized.net/www/common/header/toy_ani.gif?" + d); });
            $("#menuToy").on("mouseleave", function () { $(this).attr("src", "https://funshop.akamaized.net/www/common/header/toy_ani.png"); });

            $("#menuFood").on("mouseenter", function () { var d = new Date(); $(this).attr("src", "https://funshop.akamaized.net/www/common/header/food_ani.gif?" + d); });
            $("#menuFood").on("mouseleave", function () { $(this).attr("src", "https://funshop.akamaized.net/www/common/header/food_ani.png"); });

            

            var Messageslider = $('.Message-slides').bxSlider({
                mode: 'horizontal',
                captions: false,
                randomStart: false,
                adaptiveHeight: true,
                pager: false,
                controls: false,
                auto: true,
                pause: 5000
            });

            $('#Message-slider-next').click(function () {
                Messageslider.goToNextSlide();
                return false;
            });

            $('#Message-slider-prev').click(function () {
                Messageslider.goToPrevSlide();
                return false;
            });


            var CategoryTagslider = $('.CategoryTag-slides').bxSlider({
                mode: 'horizontal',
                captions: false,
                randomStart: false,
                adaptiveHeight: true,
                pager: false,
                controls: false,
                slideWidth: 80,
                minSlides: 1,
                maxSlides: 12,
                moveSlides: 2,
                auto: false,
                infiniteLoop: false,
                hideControlOnEnd: true

            });


            $('#CategoryTag-slider-next').click(function () {
                CategoryTagslider.goToNextSlide();
                var slide_count = CategoryTagslider.getSlideCount();
                var slide_curr = CategoryTagslider.getCurrentSlide();
                if (slide_count - 12 == slide_curr) {
                    //흐리게하기
                    $("#right_arrow").css("opacity", "0.5");
                    $('#CategoryTag-slider-next').css("cursor", "default");
                } else {
                    $("#right_arrow").css("opacity", "1");
                    $('#CategoryTag-slider-next').css("cursor", "pointer");
                }
                $("#left_arrow").css("opacity", "1");
                $('#CategoryTag-slider-prev').css("cursor", "pointer");
                return false;
            });

            $('#CategoryTag-slider-prev').click(function () {
                CategoryTagslider.goToPrevSlide();
                var slide_curr = CategoryTagslider.getCurrentSlide();
                if (slide_curr == 0) {
                    //흐리게하기
                    $("#left_arrow").css("opacity", "0.5");
                    $('#CategoryTag-slider-prev').css("cursor", "default");
                } else {
                    $("#left_arrow").css("opacity", "1");
                    $('#CategoryTag-slider-prev').css("cursor", "pointer");
                }
                $("#right_arrow").css("opacity", "1");
                $('#CategoryTag-slider-next').css("cursor", "pointer");
                return false;
            });
        });


    </script>
