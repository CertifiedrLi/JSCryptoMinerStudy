<script type="text/javascript">
    $(function () {

        var indexDataLoad = false;

        $(window).scroll(function () {

            var lastScroll = 0;

            var st = $(this).scrollTop();
            var scrollPos = $(document).scrollTop();


            //var p = $("#image_list_3");
            var p = $("#h3Recommend");
            var offset = p.offset();


            if (st > lastScroll) {

                if (scrollPos > offset.top - 4000) {
                    if (!indexDataLoad) {
                        Common.ToggleAjaxLoad(true);
                        indexDataLoad = true;
                        GetIndexData();
                    }
                }
            }

            lastScroll = st;

        });


        


        var GetIndexData = function () {


            $.ajax({
                type: "post",
                url: "/Home/GetIndexData",
                //data: JSON.stringify(requestData),
                contentType: "application/json",
                success: function (result) {


                    $("#tmplRecommend").tmpl(result.RecommendedItems, { dataArrayIndex: function (item) { return $.inArray(item, result.RecommendedItems); } }).appendTo($("#ulRecommend"));
                    $("#tmplRestocked").tmpl(result.RestockedItems, { dataArrayIndex: function (item) { return $.inArray(item, result.RestockedItems); } }).appendTo($("#ulRestocked"));
                    //$("#tmplRecent").tmpl(result.RecentItems, { dataArrayIndex: function (item) { return $.inArray(item, result.RecentItems); } }).appendTo($("#divRecent"));


                    var resultStr = "";

                    if (result.RecentItems.length > 0) {

                        $.each(result.RecentItems, function (key1, val1) {
                            if ((key1 + 1) % 4 == 1) {
                                resultStr = resultStr + "<div class='list-row'>";
                                resultStr = resultStr + "<ul class='prd-list-type01 prd-list-type02 clear-obj'>";
                            }
                            resultStr = resultStr + "<li>";
                            resultStr = resultStr + "<a href='/goods/detail/" + val1.no_ + "?t=hr'>";
                            //resultStr = resultStr + "key1 : " +  key1;
                            resultStr = resultStr + "<img src=\"https://funshop.akamaized.net/products/" + Common.LeadingZero(val1.no_, 10) + "/vs_image298.jpg\" class=\"thumb search-thumb\" />";
                            resultStr = resultStr + "<span class=\"information\">";
                            resultStr = resultStr + "<em class=\"spot\">";
                            resultStr = resultStr + "<span style=\"white-space:nowrap\">";
                            resultStr = resultStr + val1.name.replace(/ /g, "</span> <span style='white-space:nowrap'>");
                            resultStr = resultStr + "</span>";
                            resultStr = resultStr + "</em>";

                            if (val1.PriceX > 0) {
                                if (val1.PriceX == val1.Price || (val1.IsHiddenPrice == 1 && !val1.isLogin)) {
                                    resultStr = resultStr + "<del></del>";
                                    resultStr = resultStr + "<strong class=\"price no-discount-best\">";
                                    resultStr = resultStr + numeral(val1.PriceX).format('0,0') + "원";
                                    resultStr = resultStr + "</strong>";
                                } else {
                                    resultStr = resultStr + "<del>";
                                    resultStr = resultStr + numeral(val1.PriceX).format('0,0') + "원";
                                    resultStr = resultStr + "</del>";
                                    resultStr = resultStr + "<strong class=\"price\">";
