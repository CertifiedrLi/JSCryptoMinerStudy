<script type="text/javascript">
    $(window).load(function () {
        var currentLiIndex = -1;
        var listItemsInSearch;
        var listsLength;
        var rightdiv;

        $(".SearchProductName").autocomplete("https://store.tcgplayer.com/handler/autocomplete.ashx", {
            extraParams: {
                r: function () { return Math.floor(Math.random() * 165468787); },
                g: function () { return ""; },
                ns: 'true',
                useCache: 'true'
            },
            cacheLength: 0,
            scrollHeight: 850,
            selectFirst: false,
            delay: 300,
            minChars: 1,
            matchContains: 1,
            width: "",
            highlight: false,
            formatItem: function (data, i, n, value) {
                $('.ac_results').html(value);   // this manually places the autocomplete results in ac_results but outside the dynamically generated list elements
                currentLiIndex = -1;
                listItemsInSearch = $('.autocomplete-results').find(".autocomplete-results__result");
                listsLength = listItemsInSearch.length;
                rightdiv = $(".autocomplete-results__groups .autocomplete-results__group-result:first");
                //return value;  // commented out return (how results are normally returned to autocomplete.js) because we already manually wrote the results above
            },
            //end formatItem
            formatResult: function (data, value) {
                return $(".site-search__input").val();
            }
        }).click(function () {
            currentLiIndex = -1;
            $(listItemsInSearch).removeClass("ac_over");
        }).keydown(function (event) {
            if (event.which == 40) {
                currentLiIndex++;
                if (currentLiIndex >= listsLength) {
                    currentLiIndex = 0;
                }
                $(listItemsInSearch).removeClass("ac_over");
                $(listItemsInSearch).eq(currentLiIndex).addClass("ac_over");

            }
            else if (event.which == 38) {
                currentLiIndex -= 1;
                if (currentLiIndex < 0) {
                    currentLiIndex = listsLength - 1;
                }
                $(listItemsInSearch).removeClass("ac_over");
                $(listItemsInSearch).eq(currentLiIndex).addClass("ac_over");
            }
            else if (event.which == 37 || event.which == 39) {
                if (currentLiIndex !== -1) {
                    if ($(".autocomplete-results__groups").has($(listItemsInSearch).eq(currentLiIndex)).length > 0) {
                        currentLiIndex = 0;
                    } else {
                        currentLiIndex = $(listItemsInSearch).index(rightdiv);
                    }
                    $(listItemsInSearch).removeClass("ac_over");
                    $(listItemsInSearch).eq(currentLiIndex).addClass("ac_over");
                    return false;
                }
            }
            else if (event.which == 13) {
                if (currentLiIndex == -1) {
                    window.location = 'https://store.tcgplayer.com/productcatalog/product/show?newSearch=false&ProductType=All&IsProductNameExact=false&ProductName=' + encodeURIComponent($(".site-search__input").val());
                } else {
                    $(this).val($(listItemsInSearch).eq(currentLiIndex).text()).trigger('blur');
                    $(listItemsInSearch).eq(currentLiIndex).children(":first").trigger("click");
                }
            }
            else if (event.which == 9) {
                if (currentLiIndex != -1) {
                    $(this).val($(listItemsInSearch).eq(currentLiIndex).text());
                    $('.site-search__button').focus();
                }
            };
        });

        $(".btn-search").click(function () {
            if (currentLiIndex == -1) {
                window.location = 'https://store.tcgplayer.com/productcatalog/product/show?newSearch=false&ProductType=All&IsProductNameExact=false&ProductName=' + encodeURIComponent($(".site-search__input").val());

            } else {
                $(listItemsInSearch).eq(currentLiIndex).children(":first").trigger("click");
            }
        });


        $(".site-search__input--mobile").keydown(function (event) {
            if (event.which == 13) {
                if (currentLiIndex == -1) {
                    window.location = 'https://store.tcgplayer.com/productcatalog/product/show?newSearch=false&ProductType=All&IsProductNameExact=false&ProductName=' + encodeURIComponent($(".site-search__input--mobile").val());
                } else {
                    $(listItemsInSearch).eq(currentLiIndex).children(":first").trigger("click");
                }
            };
        });
        $(".site-search__button--mobile").click(function () {
            if (currentLiIndex == -1) {
                window.location = 'https://store.tcgplayer.com/productcatalog/product/show?newSearch=false&ProductType=All&IsProductNameExact=false&ProductName=' + encodeURIComponent($(".site-search__input--mobile").val());
                
            } else {
                $(listItemsInSearch).eq(currentLiIndex).children(":first").trigger("click");
            }
        });


        $(document).on("mouseenter", ".ac_results li", function () {
            currentLiIndex = $(listItemsInSearch).index($(this));
            $(listItemsInSearch).removeClass("ac_over");
            $(listItemsInSearch).eq(currentLiIndex).addClass("ac_over");
        });


        //end document ready
    });
</script>


    
    


