            jQuery.urlParam = function (name) {
                var results = new RegExp('[\\?&amp;]' + name + '=([^&amp;#]*)').exec(window.location.href);
                if (results == null) {
                    return 0;
                }
                return results[1] || 0;
            }
            
            if (jQuery.urlParam('hiddenUsername') == 'true') {
                jQuery("#login-user").attr('autocomplete', 'OFF').val('');
            }
