		<script type="text/javascript">
			jQuery('form.confirm-password-form').live('submit', function(e){
				e.preventDefault();
				var $s_form = $(this);
				var data = {};

				data['loginData.username'] = '';
				data['loginData.password'] = $s_form.find('input[name="confirmPassword"]').val();
				data['loginData.rememberMe'] = true;

				jQuery.ajax({
					url: jQuery('.login-ajax-url').attr('href'),
					cache: false,
					type: 'post',
                    headers: {
                        'X-CSRF-Token':$("meta[name='_csrf']").attr("content")
                    },
					data: data,
					success: function(response) {
						var result = $.parseJSON(response);
						if( result.result === 'ok' ){
							$s_form.find('a.close').triggerHandler('click');
							jQuery(document).trigger('realLoginVerified');
						} else {
							$s_form.find('p.error-msg').html(result.errorDescription).show();
						}
					}
				});
			});

			jQuery(document).delegate('button.logon-security-submit', 'click', function(e){
				e.preventDefault();
				$(this).closest('form').submit();
			});
		</script>

			

