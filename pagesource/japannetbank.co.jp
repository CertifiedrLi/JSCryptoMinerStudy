var changeFlg=true;
var autoChangeBnr = function() {
	changeFlg = false;
	var currentDisp = $("#topBnr .on").index("#topBnr li");
	var nextDisp = 0;

	if(currentDisp < $("#topBnr li").length-1){
		nextDisp = currentDisp+1;
	}
	$("#topBnr li").removeClass("on").removeAttr("style");
	$("#topBnr li").eq(nextDisp).fadeIn(800,function(){
		$(this).addClass("on");
		changeFlg = true;
		if(chkTrackUnsen(getTopBnrSrc())){
			_satellite.track("tb_imp");
		}
	});
	$("#topBnrNavi li").removeClass("on").eq(nextDisp).addClass("on");
}
/* ドロワー */
function drawer(){
	var drwMove = $("#drawerContainer").outerWidth();
	var drwLeft = $("#pageContainer").offset().left;
	if(drwLeft>0){//close
		$("body").removeClass("on");
		$("#pageContainer, #headContainer").animate({left:0}, 300, "linear");
	}else{//open
			$("#pageContainer, #headContainer").animate({left: drwMove}, 300, "linear", function(){
			$("body").addClass("on");
			$(window).scrollTop(0,0);
		});
	}
}

