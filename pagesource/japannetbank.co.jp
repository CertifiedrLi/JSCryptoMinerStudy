{ easeOutExpo: function (x, t, b, c, d) {
	return (t==d) ? b+c : c * (-Math.pow(2, -10 * t/d) + 1) + b;
	}
});

$(function(){

	if(chkTrackUnsen(getTopBnrSrc())){
		_satellite.track("tb_imp");
	}

	$("#infoTab li").bind("click",function(){
		$(this).addClass("on").siblings("li").removeClass("on");
		$("#boxInformation .toList,#boxInformation .MaintenanceNone").css("display","none");
		$("#boxInformation dl").css("display","none").eq($(this).index("#infoTab li")).css("display","block")
		.next(".toList,.MaintenanceNone").css("display","block");
	});
	//TOPバナー 自動切替
	timer = setInterval(autoChangeBnr, interval);
	$("#topBnrNavi li").bind("click",function(){
		if(changeFlg){
			clearInterval(timer);
			$("#topBnr li").stop();
			var nextDisp;
			$("#topBnrNavi li").removeClass("on");
			$(this).addClass("on");
		
			nextDisp = $(this).index("#topBnrNavi li");
			$("#topBnr li").removeClass("on").removeAttr("style");
			$("#topBnr li").eq(nextDisp).fadeIn(800,function(){
				$(this).addClass("on");
				_satellite.track("tb_imp");
			});
			timer = setInterval(autoChangeBnr, interval);
		}
	});
	$("#toLeft,#toRight").bind("click",function(){
		if(changeFlg){
			clearInterval(timer);
			$("#topBnr li").stop();
			var nextDisp;
			nextDisp = $("#topBnrNavi .on").index("#topBnrNavi li");
			if(this.id =="toRight"){
				//if(nextDisp <2){
				if(nextDisp <3){
					nextDisp++;
				}else{
					nextDisp =0;
				}
			}else{
				if(nextDisp !=0){
					nextDisp--;
				}else{
					//nextDisp =2;
					nextDisp =3;
				}
			}
			$("#topBnrNavi li").removeClass("on").eq(nextDisp).addClass("on");
			$("#topBnr li").removeClass("on").removeAttr("style");
			$("#topBnr li").eq(nextDisp).fadeIn(800,function(){
				$(this).addClass("on");
				_satellite.track("tb_imp");	
			});				

			timer = setInterval(autoChangeBnr, interval);
		}
	});

	//infoTip
	$("#boxEntrance>.fR, #directLogin, #infoTip").hover(
		function(){
			$("#infoTip").addClass("on");
		},
		function(){
			$("#infoTip").removeClass("on");
		}
	);
	$("#infoClose").bind("click",function(event){
		$("#infoTip").removeClass("on");
		event.stopPropagation() ;
	});

	//ページTOPスクロール
	$(window).bind("scroll",function(){
		if($(window).scrollTop()>200){
			$("#toPageTop").css("display","block");
		}else{
			$("#toPageTop").css("display","none");
		}
	});
