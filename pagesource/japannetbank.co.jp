function chkTrackUnsen(key){
	if(key in flgTrackUnsent){
		return flgTrackUnsent[key];
	}else{
		flgTrackUnsent[key] = false;
		return true;
	}
}
function getTopBnrSrc(){
	return $("#topBnr li.on img").attr("src");
