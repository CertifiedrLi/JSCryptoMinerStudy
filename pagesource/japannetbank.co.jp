					}
				}
			}
		},
		error: function(){
		}
	});


//	特定ブラウザへのお知らせ表示	
	var ua = window.navigator.userAgent ? window.navigator.userAgent : '';
	if(showUnsupportedInfo(ua)){
		$("#topWrapper").addClass("on");
	}

	yServiceSetCk();
});


var jUserClass =  "1410_top_no_attrs";
if(userPrefs.length != 0) {
	if(Math.random() > 0.5) {
		jUserClass = "1410_top_targeting";
	} else {
		jUserClass = "1410_top_not_targeting";
	}
}
	
	
function initBannerSequence() {
	var showBanners = null;
	
	try {
		showBanners = createBanners();
	} catch(e) {
		showBanners = createDefaultBanners();
	}
	
	displayBanners(showBanners);
	
	// バナー定義用HTML削除
	removeUnusedElements();
}

function removeUnusedElements(){
	if(new RegisteredServiceRepository().contains("login")) {
		$("#visitorBnr").find("img").not(":visible").each(function() {
			$(this).removeAttr("src");
		});
	} else {
		$("#memberBnr").find("img").not(":visible").each(function() {
			$(this).removeAttr("src");
		});
	}
	
	$("#visitorBnr").detach();
	$("#memberBnr").detach();
}

function displayBanners(showBanners) {
	// 一番目のバナーを初期表示対象にする
	if(showBanners.length > 0) {
		showBanners[0].addClass("on");
	}
	
	// 定義用バナーの削除　＆ バナーの書き出し
	var bannerContainer = $("#topBnr");
	bannerContainer.empty();
	
	for(var i = 0; i < showBanners.length; i++) {
		bannerContainer.append(showBanners[i]);
	}
	
	bannerContainer.show();
}

function createDefaultBanners() {
	var showBanners = [];
	var topbanners = $("#memberBnr > li");
	topbanners.each(function() {
		$(this).removeClass("on");
		showBanners.push($(this));	
	});
	
	return showBanners;
}

function selectTargetingBanners() {
	if(new RegisteredServiceRepository().contains("login")) {
		return $("#memberBnr > li");
	} else {
		return $("#visitorBnr > li");
	}
}

function createBanners() {
	// 属性に応じてバナー取得
	var topbanners = selectTargetingBanners();
	
	// バナーの表示順決定
	var targetingBanners = [];
	var normalBanners = [];
	
	topbanners.each(function() {
		$(this).removeClass("on");
		if( isTargetingBanner($(this)) ) {
			targetingBanners.push($(this));
		} else {
			normalBanners.push($(this));
		}
	});
	
	var showBanners = targetingBanners.concat(normalBanners);
	
	
	// 検証
	//if(showBanners.length != 3) {
	if(showBanners.length != 4) {
		showBanners = createDefaultBanners(topbanners);
	}
	
	return showBanners;
}

function isTargetingBanner(banner) {
	if(jUserClass != "1410_top_targeting") {
		return false;
	}
	
	if(userPrefs == null) {
		return false;
	}
	
	
	for(var i = 0; i < userPrefs.length; i++) {
		if(banner.hasClass("bs_" + userPrefs[i])) {
			return true;
		}
	}
	
	
	return false;
}

function showUnsupportedInfo(ua){
	var targetUA = new Array(/Windows 95/, /Windows 98/, /Windows 3\.1/, /Windows NT 4\./, /Windows NT 5\./, /WindowsNT 5\.1;/, /Android 1\./, /Android 2\.0/, /Android 2\.1/, /Android 2\.2/, /Mac OS X 10\.4/, /Mac OS X 10_4/);

	if(ua){
		for(var i=0; i<targetUA.length; i++){
			if(ua.search(targetUA[i])>-1){
				return true;
			}
		}
	}
	return false;
}

function yServiceSetCk(){
	var url   = location.href;
	var queryStr  = url.split("?");
	if(queryStr.length <= 1) return;
	
	var params   = queryStr[1].split("&");
	var paramArray = {};
	for ( i = 0; i < params.length; i++ ) {
		var param = params[i].split("=");
		if(param.length == 2) {
			paramArray[param[0]] = param[1];
		}
	}

	if (paramArray["ys"] == 1) {
		var sikibetuId = "2016000006";
		setCkNoDay("yServiceSikibetuId", sikibetuId);
		setCkNoDay("yServiceShohinKbn", "NBG12013G11001");
		setCkNoDay("yServiceParams", "__type=0003,__sid=00000,__uid=0000,__fid=NBGAF120,__gid=0000,B_ID=1,SikibetuId=" + sikibetuId);
	}
}

function setCkNoDay(key, val){
	var domain = ".japannetbank.co.jp";
	var path = "/";
	document.cookie = key + "=" + encodeURIComponent(val) + "; domain=" + domain + "; path=" + path;
}

