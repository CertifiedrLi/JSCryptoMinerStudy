<script charset="utf-8" type="text/javascript">
    RR.jsonCallback = function () {
        // Place your rendering logic here. Actual code varies depending on your website implementation.
        console.dir(RR.data.JSON.placements);

        function bannersReplace() {
            /** CRIA ARRAY DE FERENCIA PARA OS FUTUROS BANNERS **/

            var itens = RR.data.JSON.placements;

            var banners = {
                'home_page.content_hero1': {'img': '', 'url': ''},
                'home_page.content_hero2': {'img': '', 'url': ''},
                'home_page.content_hero3': {'img': '', 'url': ''},
                'home_page.content_hero4': {'img': '', 'url': ''},
                'home_page.content_hero5': {'img': '', 'url': ''},
                'home_page.content_hero6': {'img': '', 'url': ''},
                'home_page.content_hero7': {'img': '', 'url': ''},
                'home_page.content_hero8': {'img': '', 'url': ''},
                'home_page.content_hero9': {'img': '', 'url': ''},
                'home_page.content_mini_topo1': {'img': '', 'url': ''},
                'home_page.content_mini_topo2': {'img': '', 'url': ''},
                'home_page.content_mini_mid1': {'img': '', 'url': ''},
                'home_page.content_mini_mid2': {'img': '', 'url': ''},
                'home_page.content_mini_bottom1': {'img': '', 'url': ''},
                'home_page.content_mini_bottom2': {'img': '', 'url': ''}
            };


            /** MONTA A ARRAY COM OS NOVOS BANNERS **/
            jQuery.each(itens, function (index, value) {
                var valItem = value.name;
                if (valItem in banners) {
                    banners[value.name]['img'] = value.content.imageURL;
                    banners[value.name]['url'] = value.content.targetURL;
                }
            });

            /** EACH BANNER PRINCIPAL NA HOME **/
            var i = 1;

            /** FAZ O REPLACE DOS BANNERS **/
            jQuery("#banners-home .owl-item").each(function () {
                if (banners['home_page.content_hero' + i] != '') {
                    jQuery(this).find("img").attr("src", banners['home_page.content_hero' + i]['img']);
                    jQuery(this).find("a").attr("href", banners['home_page.content_hero' + i]['url']);
                    i++;
                }
            });

            // REPLACE BANNER MINI TOPO
            if (banners['home_page.content_mini_topo1'] != '') {
                jQuery(".banner-mini-topo1").attr("src", banners['home_page.content_mini_topo1']['img']);
                jQuery(".banner-mini-topo1").parent().attr("href", banners['home_page.content_mini_topo1']['url']);
            }

            if (banners['home_page.content_mini_topo2'] != '') {
                jQuery(".banner-mini-topo2").attr("src", banners['home_page.content_mini_topo2']['img']);
                jQuery(".banner-mini-topo2").parent().attr("href", banners['home_page.content_mini_topo2']['url']);
            }

            // REPLACE BANNER MINI MIDDLE
            if (banners['home_page.content_mini_mid1'] != '') {
                jQuery(".banner-mini-mid1").attr("src", banners['home_page.content_mini_mid1']['img']);
                jQuery(".banner-mini-mid1").parent().attr("href", banners['home_page.content_mini_mid1']['url']);

            }
            if (banners['home_page.content_mini_mid2'] != '') {
                jQuery(".banner-mini-mid2").attr("src", banners['home_page.content_mini_mid2']['img']);
                jQuery(".banner-mini-mid2").parent().attr("href", banners['home_page.content_mini_mid2']['url']);
            }

            // REPLACE BANNER MINI BOTTOM
            if (banners['home_page.content_mini_bottom1'] != '') {
                jQuery(".banner-mini-bottom1").attr("src", banners['home_page.content_mini_bottom1']['img']);
                jQuery(".banner-mini-bottom1").parent().attr("href", banners['home_page.content_mini_bottom1']['url']);
            }

            if (banners['home_page.content_mini_bottom2'] != '') {
                jQuery(".banner-mini-bottom2").attr("src", banners['home_page.content_mini_bottom2']['img']);
                jQuery(".banner-mini-bottom2").parent().attr("href", banners['home_page.content_mini_bottom2']['url']);
            }
        }

        /* FUNCTION QUE REALIZA O REPLACE DE PRODUTOS */
        function productReplace(data) {
            var dataProducts = {};
            var i = 1;
            /* EACH DE PRODUTOS NA HOME */
            jQuery.each(data, function (index, value) {
                if (value.items) {
                    var idProducts = {};
                    for (var j = 0; j < value.items.length; j++) {
                        idProducts[j] = value.items[j].id;
                    }
                    dataProducts[i] = idProducts;
                    /* CHAMADA AJAX QUE ENVIA O ARRAY DE PRODUTOS, NOME DO BLOCO E TITULO DO BLOCO */
                    jQuery.ajax({
                        url: "http://www.drogaraia.com.br/rrelevance/instrument/productHomeReplace/",
                        type: "POST",
                        data: 'products=' + JSON.stringify(idProducts) + '&blockName=' + 'listhome' + i + '&blockMessage=' + value.message,
                        success: function (data) {
                            /* RECEBE O HTML DA LIST-HOME COM E FAZ O REPLACE DOS BANNERS */
                            jQuery(jQuery.parseJSON(data).container).replaceWith(jQuery.parseJSON(data).html);
                            bannersReplace();
                        }
                    }).error(function (data) {
                        console.log("error: rrelevance product replace");
                    });
                    i++;
                }
            });
        }

        //productReplace(RR.data.JSON.placements);
    };
</script>


