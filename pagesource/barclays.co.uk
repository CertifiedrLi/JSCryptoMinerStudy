        function getCookie(name) {
            var value = "; " + document.cookie;
            var parts = value.split("; " + name + "=");
            if (parts.length == 2) return parts.pop().split(";").shift();
        }

        var items = 'Personal'.split(',');
        var pageDepth = items.length;
        var ccpCookieValues = 'undefined' !== typeof getCookie('CCP') ? JSON.parse(decodeURIComponent(getCookie('CCP'))).publicuser : {cat2:'on',cat3:'on',cat4:'on'};
        var newReturning = 'undefined' !== typeof getCookie('CCP_OTM') ? "returning" : "new";

        var digitalData = {
            page: {
                pageInfo: {
                    pageName: items.join(':'),
                    cmsPageName: 'personal',
                    cmsPath: '\/content\/barclaysuk\/en\/personal',
