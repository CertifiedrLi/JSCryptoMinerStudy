                    pageVersion: "",
                    impressions: "",
                    buildVersion: "",
                    applicationId: "",
                    applicationType: "",

                    // W3C Data Layer recommendation
                    timeStamp: new Date(),
                    destinationURL: document.location,
                    referringURL: document.referrer,
                    breadCrumbs: items
                },
                searchInfo: {}
            },
            user: {
                ccp: {
