<script type="text/javascript">
function video(video,img,title){
	$(".overlay,#flvBox").show();
	document.getElementById('videoTitle').innerHTML=title;
	var so = new SWFObject("263/images/video/player.swf","myCuPlayer","640","480","9","#000000");
	so.addParam("allowfullscreen","true");
	so.addParam("allowscriptaccess","always");
	so.addParam("wmode","opaque");
	so.addParam("quality","high");
	so.addParam("salign","lt");
	so.addVariable("JcScpCuePointInfo",""); //提示点信息
	so.addVariable("JcScpCuePointTime",""); //提示点秒数值
	//播放器设置文件-----------------------------
	so.addVariable("ShowJcScpAFront","no"); 
	so.addVariable("ShowJcScpAVideo","no"); 
	so.addVariable("ShowJcScpAPause","no"); 
	so.addVariable("ShowJcScpACorner","no"); 
	so.addVariable("ShowJcScpAEnd","no"); 
	so.addVariable("ShowJcScpAMoveText","no"); 
	//视频文件及略缩图--------------------------
	so.addVariable("JcScpAutoPlay","no"); //是否自动播放
	so.addVariable("JcScpVideoPath",video); //视频文件地址
	so.addVariable("JcScpImg",img);//视频缩略图
	//-----------------------------------------
	so.addVariable("JcScpSharetitle",title); 
	so.write("videoCon");
}
$("#flvBox>a.try_close").click(function(){
	$("#videoCon").html("");
	$(".overlay,#flvBox").hide();
	})
