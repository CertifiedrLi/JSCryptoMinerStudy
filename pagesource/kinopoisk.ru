                        $("#hide_adv, #top_superscreen").show();
                        $("#top_3banners").hide();
                    }

                    // Support of 250 height banners
                    var inner = $('#superscreen_code_inner');
                    var wrap = $('.superscreen_wrap');
                    var wrapList = wrap.find('.list');

                    if (inner.height() < 250) {
                        wrap.css('height', 267);
                        wrapList.css('top', 250);
                    }
                }

                function onSuperbannerFailed() {
                    $("#hide_adv, #top_superscreen").hide();
                    $("#top_3banners").show();
                }

