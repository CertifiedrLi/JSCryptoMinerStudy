<script language="javascript"><!--It needs JQuery adn Cookies.js -->

function HTML5GeoIPLocate(){
	if (navigator.geolocation) {
			navigator.geolocation.getCurrentPosition(function(position) {
			HTML5geoIPFound(position.coords.latitude, position.coords.longitude, position.coords.accuracy);
		}, function(err) { HTML5geoIPError(err) }, {enableHighAccuracy:false, timeout:10000, maximumAge:60*60*1000} );
	}
	else {
 
	}
}

function HTML5geoIPError(err){ 
  //alert('Erro localizando: '+err.code);
}

function HTML5geoIPFound(mylat, mylng, accuracy) {
	mylat = (Math.round(mylat*1000000)/1000000); //arredondar 6 casas
	mylng = (Math.round(mylng*1000000)/1000000); //arredondar 6 casas
	accuracy = Math.round(accuracy);
	createCookie('user_lat', mylat, 1);
	createCookie('user_lng', mylng, 1);
	createCookie('pos_accu', accuracy, 1);
	$.ajax({
		type: 'GET',
		dataType: "json",
		url: "https://maps.googleapis.com/maps/api/geocode/json?latlng="+mylat+","+mylng+"&sensor=false",
		data: {},
		success: function(data) {
			var city, state, countryCode, countryName;
			$.each( data['results'],function(i, val) {
				$.each( val['address_components'],function(i, val) {
					if (val['types'] == "locality,political") city = val['long_name']; 
					if (val['types'] == "administrative_area_level_1,political") state = val['short_name'];
					if (val['types'] == "country,political") { countryCode = val['short_name']; countryName = val['long_name']; }
				});
			});
			createCookie('user_city', city, 1);
			createCookie('user_state', state, 1);
			createCookie('user_countryCode', countryCode, 1);		
			createCookie('user_countryName', countryName, 1);	
//			console.log('Success: '+countryName+'-'+city);
			$.ajax({
				type: 'GET',
				dataType: "text",
				url: 'http://www.localizaip.com.br/databank/ipinfo.php?ip=this&city='+city+'&state='+state+'&country='+countryCode+'&lat='+mylat+'&lng='+mylng+'&accu='+accuracy
			});
		},
		error: function () { /*console.log('HTML5 City locate error');*/ } 
	}); 
}

HTML5GeoIPLocate();
</script><!--inline-->


