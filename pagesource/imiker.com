var user;
$(document).ready(function(){
    var url ="https://i.imiker.com/index.php?guanwang_login&type=gw_json";
    //var url ="http://i.xuexi-local.com/index.php?guanwang_login&type=gw_json";
    jQuery.ajax({
        url: url,
        xhrFields: {
            withCredentials:true
        },
        timeout : 5000,
        dataType:"json",
        async: false,
        crossDomain:true,
        success: function (data) {
            user=data;
            if(data.is_login){
                $(".avatar img").attr("src",data.headerimgurl);
                $(".username").text(data.username);
                $(".login_before").hide();
                $(".logined").show();
            }else{
                $(".login_before").show();
                $(".logined").hide();
            }
        },
        complete : function(XMLHttpRequest,status){
            if(status=='timeout'){
                $(".login_before").show();
                $(".logined").hide();
            }
        },
        error: function (textStatus) {
            $(".login_before").show();
            $(".logined").hide();
        },
    });

    //登陆状态弹出二级菜单
    $(".information").hover(function(){
        $(".name").css('color','#53A5EE');
        $(this).find(".qqserver-warp").show();
    },function(){
        $(".name").css('color','#333333');
        $(this).find(".qqserver-warp").hide();
    });
    $(document).on("click", ".logout", function() {
            $.getScript("https://i.imiker.com/index.php?guanwang_logout").done(function() {
                $(".login_before").show();
                $(".logined").hide();
            }).fail(function() {
                window.location.reload();
            })
     })  
});

</script>

