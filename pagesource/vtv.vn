            var timeStamp = Math.floor(Date.now());
            $(".maxacnhan").html('<img style="width:57px;height:27px;" src="' + appSettings.ajaxDomain + '/captchauser.htm?t=' + timeStamp + '" title="click để lấy mã khác" onclick="DiemNhanForm.initCaptcha();"/>');
        },
        GuiDiDiemNhan: function() {
            var me = this;
            var txtTenDiemNhan = $("#txtTenDiemNhan").val();
            var txtEmailDiemNhan = $("#txtEmailDiemNhan").val();;
            var txtNoiDungDiemNhan = $("#txtNoiDungDiemNhan").val();

            if (txtTenDiemNhan == "") {
                alert('Không để trống họ tên!');
                return false;
            }
            if (!validateEmail(txtEmailDiemNhan)) {
                alert('Điền đúng dạng email!');
                return false;
            }
            if (txtNoiDungDiemNhan == "" || txtNoiDungDiemNhan == '') {
                alert('Không để trống nội dung!');
                return false;
            }
            //var maxacnhan = $('#txtMaXacNhan').val();
            //if (maxacnhan != me.numTotalN1AndN2.toString()) {
            //    alert('Gõ sai mã xác nhận');
            //    $(".maxacnhan").focus();
            //    return false;
            //}

            //getCookie("Nickname");
            //setCookie("Nickname", name, new Date(new Date().getTime() + 60000 * 60 * 24));
            var nameCookie = "DiemNhan_Home_Vtv";
            if (getCookie(nameCookie)) {
                alert('Bạn đã gửi rồi, bạn có thể gửi lại sau 15p!');
            } else {
                $.ajax({
                    url: appSettings.ajaxDomain + "/aj-y-kien-diem-nhan.htm",
                    data: { 'name': txtTenDiemNhan, 'email': txtEmailDiemNhan, 'content': txtNoiDungDiemNhan, 'captcha': $('#txtMaXacNhan').val() },
                    // data: { 'type': 1, 'commentid': commentid, 'newsid': newsId },
                    xhrFields: {
                        withCredentials: true
                    },
                    method: "post",
                    success: function(data) {
                        if (data == "1") {
                            alert("Bạn đã gửi thành công, xin cảm ơn bạn!");
                            setCookie(nameCookie, "1", new Date(new Date().getTime() + 1000 * 60 * 15));
                            $(".fromguidiemnhan").slideUp();
                        }
                        else if (data == "-2") {
                            alert('Có lỗi! Vui lòng báo cho ban quản trị, cám ơn!');
                        }
                        else if (data == "-1") {
                            alert('Mã xác nhận không đúng!');
                        }
                        $("#txtMaXacNhan").val('');
                        DiemNhanForm.initCaptcha();
                    }
                });
            }
            return true;
        },
        ShowOrHideBoxDiemNhan: function() {
            var me = this;
            if (me.checkBoxDiemNhan == 0) {
                //show
                me.initCaptcha();
                $(".fromguidiemnhan").slideDown();
                me.checkBoxDiemNhan = 1;
                $(".diemnhan_box .video_hot .title_box .iconshow").addClass("active");
            } else {
                //hide
                $(".fromguidiemnhan").slideUp();
                me.checkBoxDiemNhan = 0;
                $(".diemnhan_box .video_hot .title_box .iconshow").removeClass("active");
            }
        }
    };
    (runinit = window.runinit || []).push(function() {
        
       
        DiemNhanForm.init();
    });
