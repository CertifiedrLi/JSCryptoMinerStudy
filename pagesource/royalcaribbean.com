	<section class="rcl_component">
		<script>
			var cruisingTo = 'cruisingToMultiSelect';
			var data = "";
			var cruiseSearchInit = function() {
				$glit(document).ready(function () {

					var titleCaps = function (title) {
						var lowercaseValue = title.toLowerCase();
						return lowercaseValue.replace(/ [a-z]([a-z])$| [a-z]([a-z] )|[\( ]([a-z])|^([a-z])/g, function (v) {
							return v.toUpperCase();
						});
					};

					function daysInMonth(year,month) {
						return new Date(year, month, 0).getDate();
					}

					// function to retrieve the cookie value for environment (set from the backebd)
					function getEnvironmentCookieValueByname (cookieName) {
						var value = "; " + document.cookie;
        				var parts = value.split('; ' + cookieName + '=');
        				return parts.pop().split(';').shift();
					}

					// function to save the filterOptions in this component to the localStorage
					function saveFilterOptions (minDate, maxDate, leavingFromOptions, cruisingToOptions) {
						var objectToSave = {
							leavingFrom: leavingFromOptions,
							minDate: minDate !== '' ? decodeURIComponent(minDate) : null,
							maxDate: maxDate !== '' ? decodeURIComponent(maxDate) : null,
							sailingTo: cruisingToOptions
						};
						var localStorageKeyname = 'browseID';
						var envName = getEnvironmentCookieValueByname('env');
						if (envName && 'new' !== envName) {
							localStorageKeyname += '-' + envName;
						}
						try {
							localStorage.setItem(localStorageKeyname, JSON.stringify(objectToSave));
						} catch(e) {
							console.log('Storage| set data to the localStorage', e);
						}
						
					}

					// submit forms
					$glit('#find-a-cruise').click(function () {
						var queryPath = '/cruises';
						var actionMethod = "POST";
						var cruisingToString = $glit('#cruisingTo :checked').map(function () {
							return this.value;
						}).get().join(',');

						var spitQueryValuesCruisingTo = cruisingToString.split(",");
						var prefix = '?';
						var finalSubmitCruisingToString = "";
						var cruisingToArray = [];

						for (var index = 0; index < spitQueryValuesCruisingTo.length; index++) {
							if(spitQueryValuesCruisingTo[index]) {
								var cruisintToValue = 'destinationRegionCode_' + spitQueryValuesCruisingTo[index];
								cruisingToArray.push(cruisintToValue);
								finalSubmitCruisingToString += prefix + cruisintToValue + "=true";
								prefix = "&";
							}
						}

						var leavingFromString = $glit('#cruisingFrom :checked').map(function () {
							return this.value;
						}).get().join(',');

						var spitQueryValuesLeavingFrom = leavingFromString.split(",");
						var finalSubmitLeavingFromString = "";
						var leavinFromArray = [];

						for (var index2 = 0; index2 < spitQueryValuesLeavingFrom.length; index2++) {
							if(spitQueryValuesLeavingFrom[index2]) {
								var departureValue = 'departureCode_' + spitQueryValuesLeavingFrom[index2];
								leavinFromArray.push(departureValue);
								finalSubmitLeavingFromString += prefix + departureValue + "=true";
								prefix = "&";
							}
						}

						var beginDataRange = $glit('.beginDate .date').attr('data-src');
						var endDateRange = $glit('.endDate .date').attr('data-src');
						var endDateRangeSplit = endDateRange.split('-');
						var beginDataRangeSplit =  beginDataRange.split('-');
						var maxDays = 31;
						var minDays = '01';
						var minDate = "";
						var maxDate = "";

						if(endDateRangeSplit[0] && endDateRangeSplit[1]) {
							maxDays = daysInMonth(endDateRangeSplit[0], endDateRangeSplit[1]);
						}

						var startDateQueryString = "";
						var endDateQueryString = "";

						if(beginDataRangeSplit[1] && beginDataRangeSplit[1] != undefined) {
							minDate = beginDataRangeSplit[1] + encodeURIComponent('/') + minDays + encodeURIComponent('/') + beginDataRangeSplit[0];
							startDateQueryString = prefix + 'dates_minDate=' + minDate;
						}

						if(endDateRangeSplit[1] && endDateRangeSplit[1] != undefined) {
							maxDate = endDateRangeSplit[1] + encodeURIComponent('/') + maxDays + encodeURIComponent('/') + endDateRangeSplit[0];
							endDateQueryString = '&dates_maxDate=' + maxDate;
						}

						saveFilterOptions(minDate, maxDate, leavinFromArray, cruisingToArray);
						
						const domainWuc = getCurrentWuc();
						if (isNonProd()){
							setCookie("wuc_nonprod", domainWuc);
						} else {
							setCookie("wuc", domainWuc);
						}
						
						queryPath += finalSubmitCruisingToString + finalSubmitLeavingFromString + startDateQueryString + endDateQueryString;
						var actionDomain = '';
						
						var newAction = actionDomain + queryPath;

						if(newAction.indexOf('http') == 0 || newAction.indexOf('www') == 0) {
							window.location.replace(newAction);
						} else {
							window.location.replace(window.location.protocol + '//' + window.location.host + newAction);
						}
					});


					executeAjaxForCruiseSearch = function() {

						$glit('#cruisingTo').removeClass('active');
						$glit('#cruisingFrom').removeClass('active');

						var cruisingToString = $glit('#cruisingTo :checked').map(function () {
							return this.value;
						}).get().join(',');

						if (!cruisingToString) {
							cruisingToString = "-1";
						}

						var finalQueryString = "?cruisingTo=" + cruisingToString;

						var leavingFromString = $glit('#cruisingFrom :checked').map(function () {
							return this.value;
						}).get().join(',');

						if (!leavingFromString) {
							leavingFromString = "-1";
						}

						finalQueryString = finalQueryString + "&leavingFrom=" + leavingFromString;

						// get date values
						var beginDataRange = $glit('.beginDate .date').attr('data-src');
						var endDateRange = $glit('.endDate .date').attr('data-src');
						var endDateRangeSplit = endDateRange.split('-');
						var beginDataRangeSplit = beginDataRange.split('-');
						var maxDays = 31;

						if (endDateRangeSplit[0] && endDateRangeSplit[1]) {
							maxDays = daysInMonth(endDateRangeSplit[1], endDateRangeSplit[0]);
						}

						var startDateQueryString = "";
						var endDateQueryString = "";


						if(typeof beginDataRangeSplit[1] != "undefined" && typeof beginDataRangeSplit[0] != "undefined") {
							startDateQueryString = '&startDate=' + beginDataRangeSplit[1] + encodeURIComponent('/') + '01' + encodeURIComponent('/') + beginDataRangeSplit[0];
						}

						if(typeof endDateRangeSplit[1] != "undefined" && typeof endDateRangeSplit[0] != "undefined") {
							endDateQueryString = '&endDate=' + endDateRangeSplit[1] + encodeURIComponent('/') + maxDays + encodeURIComponent('/') + endDateRangeSplit[0];
						}

						var domainWuc = getCurrentWuc();
						var currentWucCode = "";
						if(domainWuc != null && typeof domainWuc != "undefined") {
							currentWucCode = "&wuc=" + domainWuc;
						}

						finalQueryString += startDateQueryString + endDateQueryString + currentWucCode;

						var beginDateSelected = false;
						var endDateSelected = false;

						if(beginDataRange) {
							beginDateSelected = true;
						}

						if(endDateRange) {
							endDateSelected = true;
						}

						// update all the forms
						$glit.ajax({
							type: 'GET',
							dataType: "json",
							async: false,
							headers: {"Accept": "application/json"},
							url: '/ajax/findACruise' + finalQueryString,
							success: function (data) {
								var cruisingToMultiSelect = data.cruisingToMultiSelect.options;

								// clear out the old html
								$glit('#cruisingTo').empty();

								var outPutHtml = "";
								for (var i = 0; i < cruisingToMultiSelect.length; i++) {
									var label = cruisingToMultiSelect[i]['label'];
									var cruisingToValue = cruisingToMultiSelect[i]['value'];
									var isSelected = cruisingToMultiSelect[i]['selected'];
									var isDisabled = cruisingToMultiSelect[i]['disabled'];
									var cruisingToDisableOutput = "";
									var cruisingToSelectOutput = "";

									if (isSelected) {
										isDisabled = false;
										cruisingToSelectOutput = ' checked';
									}

									if (isDisabled) {
										cruisingToDisableOutput = " disabled ";
									}

									var myLabelForId = "1myCheckbox" + i;
									var lbl = '<label for=' + '"' + myLabelForId + '"' + '>' + CruiseSearchI18n.translate(label) + '</label>';
									var inputHtmlEndLabel = "";
									inputHtmlEndLabel = '<input id="' + myLabelForId + '" ' + cruisingToDisableOutput + cruisingToSelectOutput + ' name="cruisingTo" value="' + cruisingToValue + '" type="checkbox">';
									if (cruisingToValue != "-1") {
										outPutHtml += '<div>' + inputHtmlEndLabel + lbl + '</div>';
									}
								}

								var cruisingToPhoneHtml = ' <div class="show-for-phone"><p class="back-button text-center"><a href="#" class="close less"> <strong>Select a destination</strong></a></p> </div>';
								var cruisingToTabletHtml = '<div class="show-for-desktop show-for-tablet"><p class="text-right"><a href="#" class="close">Close&nbsp;<span>&times;</span></a></p></div>';
								var cruisingPhoneButtontHtml ='<div class="show-for-phone"><p class="continue-button hide"><a href="#" class="close more">Continue</a></p></div>';

								$glit('#cruisingTo').append(cruisingToPhoneHtml);
								$glit('#cruisingTo').append('<section class="active">' + outPutHtml + '</section>');
								$glit('#cruisingTo').append(cruisingToTabletHtml);
								$glit('#cruisingTo').append(cruisingPhoneButtontHtml);
								var leavingFromMultiSelect = data.leavingFromMultiSelect.groups;

								// CRUISING FROM
								$glit('#cruisingFrom').empty();
								var groupLabelHtml = "";
								var sectionHtml = "";
								var LeavingFromID = $glit('#cruisingFrom');
								var LeavingFromRegionID = $glit('#cruisingFrom');
								var regionStartTag = '<ul class="region">';
								var combinedSectionInputCode = "";

								for (var k = 0; k < leavingFromMultiSelect.length; k++) {
									var groupLabel = leavingFromMultiSelect[k]['groupLabel'];
									var groupOptions = leavingFromMultiSelect[k]['options'];

									var classActive = "";

									if(k == 1) {
										classActive = "class=active";
									}

									groupLabelHtml = '<li><a href="#region' + k + '">' + CruiseSearchI18n.translate(groupLabel) + '</a></li>';

									if (groupLabel) {
										regionStartTag += groupLabelHtml;
									}

									sectionHtml = '<section ' + classActive + ' id="region' + k + '">';
									var sectionInputHtml = "";

									for (var j = 0; j < groupOptions.length; j++) {
										var leavingLabel = groupOptions[j]['label'];
										var leavingValue = groupOptions[j]['value'];
										var isGroupSelected = groupOptions[j]['selected'];
										var isGroupDisabled = groupOptions[j]['disabled'];
										var disabledHtmlOutPut = "";
										var selectedHtmlOutPut = "";

										if (isGroupSelected) {
											isGroupDisabled = false;
											selectedHtmlOutPut = 'checked';
										}

										if (isGroupDisabled) {
											disabledHtmlOutPut = "disabled";
										}

										var myLeavingLabelForId = "myCheckbox" + j + k;
										var leavingLabelHtml = '<label for=' + '"' + myLeavingLabelForId + '"' + '>' + CruiseSearchI18n.translate(titleCaps(leavingLabel)) + '</label>';

										if(leavingValue != "-1") {
											var inputHtml = '<input id="' + myLeavingLabelForId + '" ' + selectedHtmlOutPut + ' ' + disabledHtmlOutPut + ' name="cruisingFrom" value="' + leavingValue + '" type="checkbox">';
										}

										sectionInputHtml +=  '<div>' + inputHtml + leavingLabelHtml + '</div>';
									}
									var endSectionHtml = '</section>';
									combinedSectionInputCode += sectionHtml + sectionInputHtml + endSectionHtml;
								}
								LeavingFromRegionID.append(regionStartTag + '</ul>');
								var cruisingFromCloseButton = '<div class="show-for-phone"><p class="back-button text-center"><a href="#" class="close less"> <strong>Select a departure</strong></a></p></div>';
								var cruisingFromTabletButton = '<div class="show-for-desktop show-for-tablet"><p class="text-right"><a href="#" class="close">Close&nbsp;<span>&times;</span></a></p></div>';
								var cruisingFromPhoneButton = '<div class="show-for-phone"><p class="continue-button hide"><a href="#" class="close more">Continue</a></p></div>'
								LeavingFromID.append(cruisingFromCloseButton + combinedSectionInputCode + cruisingFromTabletButton + cruisingFromPhoneButton);
								$glit('.datepicker').empty();
								var cruiseSearchMonths = data.months;
								var beginDateFound = false;
								var currentYear;
								var previousYear = "";
								var currentMonth;
								var monthCount = 1;
								var beginDate = "";
								var beginMonth = "";
								var beginYear = "";
								var endDate = "";
								var endMonth = "";
								var endYear = "";
								var CalendarHtml = "";
								var monthNames = [" ",
									"Jan",
									"Feb",
									"Mar",
									"Apr",
									"May",
									"Jun",
									"Jul",
									"Aug",
									"Sep",
									"Oct",
									"Nov",
									"Dec"
								];

								for (var ii = 0; ii < cruiseSearchMonths.length; ii++) {
									var yearMonthValue = cruiseSearchMonths[ii]['value'];
									var yearMonth = cruiseSearchMonths[ii]['value'].split("-");

									currentYear = yearMonth[0];
									currentMonth = yearMonth[1];
									var selected = cruiseSearchMonths[ii]['selected'];
									var disabled = cruiseSearchMonths[ii]['disabled'];

									if (!beginDate && !disabled) {
										beginMonth = currentMonth;
										beginYear = currentYear;
										beginDate = monthNames[parseInt(currentMonth, 10)] + ' ' + currentYear;
									}

									if (currentYear != previousYear) {

										if (previousYear) {
											CalendarHtml += '</div></div>';
										}
										CalendarHtml += '<div class="slide"><div class="year">' + currentYear + '</div>';
										CalendarHtml += '<div class="table">';
									}

									if (monthCount == 1) {
										CalendarHtml += '<div class="tr">';
									}

									CalendarHtml += '<div class="td ';

									if (!disabled && !beginDateFound && !beginDateSelected) {
										beginDateFound = true;
										CalendarHtml += 'beginTime selected ';
										beginMonth = currentMonth;
										beginYear = currentYear;
										beginDate = monthNames[parseInt(currentMonth, 10)] + ' ' + currentYear;
									}

									if (disabled) {
										CalendarHtml += ' disabled';
									}

									CalendarHtml += '" data-date="' + currentYear + '-' + currentMonth + '">';
									CalendarHtml += monthNames[parseInt(currentMonth, 10)];
									CalendarHtml += '</div>';

									if (monthCount % 4 == 0) {
										CalendarHtml += '</div>';
										monthCount = 1;
									} else {
										monthCount++;
									}
									previousYear = currentYear;
								}

								CalendarHtml += '</div></div>';
								$glit('.datepicker').append(CalendarHtml);
							},
							error: function (errorThrown) {
								console.log("ERROR : " + errorThrown);
							}
					});
					};
				});
			};
			if(typeof mainInitArray != "undefined") {
				mainInitArray.push(cruiseSearchInit);
			}
		</script>
