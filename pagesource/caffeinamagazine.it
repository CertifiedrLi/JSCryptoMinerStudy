		article .thecontent a, article .thecontent a:hover {
    color: #1ea1fc !important;
}
::selection {
    background: #1ea1fc;
}

.navigation.mobile-menu-wrapper,#header{ background-color: #fff !important; }
a#pull{ background-color: #ccc !important; }
.navigation ul li{ float:none; display:inline-block; }
.navigation ul li a{ padding: 5px 5px; font-weight: 500 !important; font-size: 12px !important; }
header .widget_search{ margin: 34px 0 32px; }
.big-posts .latestPost:before{
	background: linear-gradient(to bottom, rgba(0,0,0,0) 0%,rgba(0,0,0,0.9) 100%);
}
.tickercontainer{
	height:30px;
}


/*#header .container{ background-color: #999 !important; }*/
.logo-wrap{ float:none; text-align:center; }
#header h1{ text-align: center; float: none; }
#logo a{ float:none; }
#primary-navigation{ float:none; text-align:center;margin-bottom:20px; }
#primary-navigation .navigation{ float:none;display:inline-block;text-align:center; }
#primary-navigation li a{ color: #252525 !important; }

.pea_cook_wrapper.pea_cook_topcenter {
    background: rgba(86,86,86,0.95) !important;
    top: 0 !important;
    left: 0 !important;
    margin: 0 !important;
    width: 100% !important;
    padding: 10px 0;
}

@media only screen 
and (min-width : 1000px)  {
.big-posts{ width:1000px; margin:0 auto; }
.big-posts .big-1 header{ padding:4%; }
.latestPost-categories ul li a{ border-bottom: 1px solid #d9d9d9; padding:22px 30px 11px 30px; }
.latestPost-categories{ border:none; }
.upper-navigation{ width:1000px; margin:0 auto;float:none;clear:both;border-bottom: 1px solid #e5e5e5; }
.latestPost-categories ul li:first-child{ border-left:1px solid #d9d9d9; }
.latestPost-categories ul li:last-child{ border-right:1px solid #d9d9d9; }
}

@media only screen 
and (max-width : 870px){ 
.navigation ul li{ line-height: 10px; } 
.navigation .toggle-caret{ color:#000 !important; }
#primary-navigation .sub-menu li:hover{ background-color:rgba(0, 0, 0, 0.2) !important; }
}


@media only screen 
and (max-width: 480px){
.navigation.mobile-menu-wrapper{
    width: 250px !important;
}
/*#primary-navigation{
    text-align:center;
    float:none !important;
}*/
a#pull {
    /*display: inline-block !important;*/
    width: 150px !important;
    margin-top: 20px;
    padding: 10px !important;
}
a#pull:after{
    right: 10px !important;
    top: 11px !important;
}
}
			
