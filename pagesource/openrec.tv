                <script type="text/javascript">
                if (!String.prototype.startsWith) {
                    String.prototype.startsWith = function(searchString, position) {
                        position = position || 0;
                        return this.indexOf(searchString, position) === position;
                        };
                }
                if (!String.prototype.endsWith) {
                    String.prototype.endsWith = function(searchString, position) {
                        var subjectString = this.toString();
                        if (typeof position !== 'number' || !isFinite(position) || Math.floor(position) !== position || position > subjectString.length) {
                            position = subjectString.length;
                        }
                        position -= searchString.length;
                        var lastIndex = subjectString.indexOf(searchString, position);
                        return lastIndex !== -1 && lastIndex === position;
                    };
                }

                $(function() {
                    var AdModal = function() {
                        var adModal = $('#ad-modal-container').remodal({ hashTracking: false });
                        var adVideo = videojs('admodal-video');
                        var adObject = null;
                        adVideo.on('error', function(err) {
                            return false;
                        });

                        // ad object setting
                        function bindAdObject(adObject) {
                            var videoEl = $('.admodal-video');
                            var imgEl = $('.admodal-substitute-image');
                            function changeMainContent(videoVisiblity) {
                                if (videoVisiblity) {
                                    videoEl.css('display', 'block');
                                    imgEl.css('display', 'none');
                                    adVideo.load();
                                    adVideo.play();
                                } else {
                                    videoEl.css('display', 'none');
                                    imgEl.css('display', 'block');
                                    adVideo.pause();
                                }
                            }
                            if (adObject.videoUrl.endsWith('.mp4') || adObject.videoUrl.endsWith('.MP4')) {
                                adVideo.src({ type: 'video/mp4', src: adObject.videoUrl});
                                if (adObject.currentTime) {
                                    adVideo.currentTime(adObject.currentTime);
                                }
                                changeMainContent(true);
                            } else {
                                $('.admodal-substitute-image').attr('src', adObject.substituteImage);
                                changeMainContent(false);
                            }
                            if (adObject.icon) {
                                $('.admodal-icon').css('display', 'block').children('img').attr('src', adObject.icon);
                            } else {
                                $('.admodal-icon').css('display', 'none');
                            }
                            $('.admodal-advertiser').text(adObject.advertiser);
                            $('.admodal-title').text(adObject.title);
                            $('.admodal-body').children('span').text(adObject.body);
                            $('.admodal-action-link').text(adObject.callToAction);
                        }

                        function callAction() {
                            if (adObject.clickUrl) {
                                adVideo.pause();
                                window.open(adObject.clickUrl);
                            }
                        }

                        $('.admodal-action-button').click(function() { callAction(); });

                        $(window).on('message', function(e) {
                            var d = e.originalEvent.data;
                            var adVideo = videojs('admodal-video');

                            if (e.originalEvent.origin !== 'http://tpc.googlesyndication.com' && e.originalEvent.origin !== 'https://tpc.googlesyndication.com') {
                                return;
                            } else if (!d.startsWith('Web/video/popup_movie')) {
                                return;
                            } else {
                                adObject = JSON.parse(d.slice(22));
                                bindAdObject(adObject);
                                adModal.open();
                            }
                        });

                        // fix font
                        var fixBodyText = function () {
                            if (!adObject) return;
                            var maxWidth = $('.admodal-body').children('span').text(adObject.body).width();
                            var maxHeight = 29;

                            function setWidth() {
                                var el = $('.admodal-body').children('span');
                                var h = el.get(0).offsetHeight;
                                if (h > maxHeight) {
                                    var txt = el.text();
                                    if (!txt.endsWith('…')) {
                                        txt = txt + '…';
                                    }
                                    txt = txt.substr(0, txt.length - 2) + '…';
                                    el.text(txt);
                                    setWidth();
                                }
                            }

                            setWidth();
                        }

                        // setting video size
                        var fixAdModalSize = function() {
                            var containerEl = $('.ad-modal-container');
                            var contentEl = $('.admodal-content');
                            var detailEl = $('.admodal-details');
                            var h = contentEl.width() * 9.0 / 16.0;
                            contentEl.height(h);
                            detailEl.height(containerEl.height() - h);
                        }

                        // set fixing event
                        $(window).resize(fixAdModalSize);
                        $(window).resize(fixBodyText);
                        $(document).on('closing', '.remodal', function() {
                            adVideo.pause();
                        }).on('opening', '.remodal', function () {
                            if (!adObject) {
                                adModal.close();
                            } else {
                                fixAdModalSize();
                                fixBodyText();
                            }
                        });
                    };

                    var adModal = new AdModal();
                });
                </script>
