    data-image=         "{{if movie_live && movie_live.offline_thumbnail}}{{:movie_live.offline_thumbnail}}{{else}}{{:thumbnail_url}}{{/if}}"
    data-file=          ""
    data-sourcefile=    ""
  {{else}}
    data-image=         "{{:thumbnail_url}}"
    data-file=          "{{:video_play_m3u8}}"
    data-sourcefile=    "{{if video_play_source_m3u8}}{{:video_play_source_m3u8}}{{/if}}"
  {{/if}}
  data-userid=          "{{:user_id}}"
  data-username=        "{{:user_name}}"
  data-usericon=        "{{:user_icon}}"
  data-view=            "{{:views}}"
  data-liveview=        "{{:live_views}}"
  data-type=            "{{if live_flg && movie_type == 8}}LIVE{{else}}MOVIE{{/if}}"
  data-game=            "{{:game_title}}"
  data-gameimage=       "{{:game_image}}"
  data-sec=             "{{:play_time}}"
  data-start-time=      "{{:start_time}}"
  data-id=              "{{:#index+1}}"
  data-movieid=         "{{:movie_id}}"
  data-onairstatus=     "{{if movie_live && movie_live.onair_status}}{{:movie_live.onair_status}}{{/if}}"
  data-link=            "https://www.openrec.tv/{{if live_flg}}live/{{else}}movie/{{/if}}{{:identify_id}}"
  data-profileurl=      "https://www.openrec.tv/user/{{if user_key}}{{:user_key}}{{else}}{{:recxuser_identify_id}}{{/if}}"
  data-movieurl=        "https://www.openrec.tv/{{if live_flg}}live/{{else}}movie/{{/if}}{{:identify_id}}"
  data-gameurl=         "https://www.openrec.tv/game/{{:game_identify_id}}"
  data-is-special=      "{{if special && special.trailer_url}}1{{else}}0{{/if}}"
  data-special-trailer= "{{if special && special.trailer_url}}{{:special.trailer_url}}{{/if}}"
  data-special-mark=    '{{if special }}<span class="u-specialmark">SP</span>{{/if}}'>
  >&nbsp;</div>
  {{/range}}
</script>
<script id="template-yell-user-grid" type="text/x-jsrender">
  <a href={{:liveUrl}} class="link--hover">
    <div class="yell-user-grid">
      <img class="yell-user-grid__thumb" src="{{:iconUrl}}">
      <div class="yell-user-grid__detail">
        <div class="yell-user-grid__detail__name{{if isOfficial}} official-icon--after{{/if}}">
          <span class="ellipsis-text">
            {{html:name}}
          </span>
        </div>
        <div class="yell-user-grid__detail__points">
            {{:totalYells}}Yell        </div>
      </div>
    </div>
  </a>
</script>        
        <div class="js-obj__topLoadingBar c-obj__topLoadingBar"></div>
    </body>
</html>