  <script type="x/template" id="slider-banner-template">
    <div class="slider-banner">
      <transition-group
        tag="ul"
        @before-enter="beforeEnter"
        @enter="enter"
        @leave="leave"
        :css="false"
      >
        <li
          v-for="(image, index) in images"
          :key="index"
          v-if="currentPosition === index"
        >
          <a
            :href="image.url"
            class="slider-banner-link"
          >
            <img
              :id="'banner-image-' + currentPosition"
              :src="image.image_src"
              :alt="image.title"
              @mouseover="imageMouseOverHandler"
              @mouseout="imageMouseOutHandler"
            />
          </a>
        </li>
      </transition-group>

      <ul class="slider-image-cache">
        <li
          v-for="(image, index) in images"
          :key="index"
        >
          <img :src="image.image_src" />
          <img :src="image.image_bg_src" />
        </li>
      </ul>
    </div>
  </script>

  <script type="x/template" id="slider-navigation-template">
    <div class="slider-navigation">
      <ul>
        <li
          v-for="(image, index) in images"
          :key="index"
          :href="image.url"
          :class="{ active: currentPosition === index }"
          @mouseover="barMouseOverHandler(index)"
          @mouseout="barMouseOutHandler"
          class="position-bar"
        >
          <a @click.prevent></a>
        </li>
      </ul>
    </div>
  </script>

  <script type="x/template" id="slider-bg-template">
    <div class="slider-bg">
      <transition-group
        tag="ul"
        @before-enter="beforeEnter"
        @enter="enter"
        @leave="leave"
        :css="false"
      >
        <li
          v-for="(image, index) in images"
          :key="index"
          v-if="currentPosition === index"
        >
          <img
            :src="image.image_bg_src"
          />
        </li>
      </transition-group>
    </div>
  </script>

