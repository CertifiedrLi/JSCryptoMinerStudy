    $(function() {
        // ajax call
        var competitionSurveyAjaxCall = function(){
            $.ajax({
                url:'https://www.acunn.com/Ajax/competition_survey?ts='+Date.now(),
                dataType:'json',
                xhrFields: {
                    withCredentials: false
                }
            }).then(function(d,s,xhr){
                if (s == 'success') {
                    if (d.survey == true) {
                        var $fullNameAndSong = d.competitor.first_name + ' '+d.competitor.last_name + ' - ' + d.competitor.song;
                        $('.survey-box-wrapper .competitorCover').attr('src', d.competitor.cover)
