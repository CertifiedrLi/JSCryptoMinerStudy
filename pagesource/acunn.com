        var elStatus = $this.attr("data-like-status");
        var elId = $this.attr("data-competitor-id");
        //$(".like-btn-box .btn").attr('disabled',true);

        $.ajax({
            type: "POST",
            url: 'https://www.acunn.com/Ajax/competition_survey/vote/',
            data: 'value='+elStatus+'&id='+elId,
            success: function (response) {
                if (response.error) {
                    $('.like-status-text').text(response.message).show();
                } else {
                    localStorage.setItem("survey_status_" + elId, elId);
                    $(".like-btn-box").hide();
                    $('.survey-box-like-info').hide();
                    $('.like-status-text').text('Oyunuz alınmıştır.').show();
                }
            }
        });

    });
});
