                        if(localStorage){
                            if(localStorage.getItem("survey_status_" + d.competitor.id)){
                                $(".like-btn-box").hide();
                                $('.survey-box-like-info').hide();
                                $('.like-status-text').text('Bu yarışmacı için oy kullandınız.').show();
