<script>
  window.dataLayer = window.dataLayer || [];
  
  (function(jQuery, $) {
    $(function() {
      function determineSecureHostname(){
        if (location.hostname == 'wistia.tech')
          return 'secure.wistia.tech';
        else if (location.hostname == 'wistia.st')
          return 'secure.wistia.st';
        else
          return 'secure.wistia.com';
      }
  
      function overwriteSecureNavAnchors(){
        var base = window.location.protocol + '//' + determineSecureHostname();
        $('.w-global-nav.show a[data-secure-host=true]').each(function(_, link) {
          $(link).attr('href', base + $(link).attr('href'));
        });
      }
  
      overwriteSecureNavAnchors();
  
      var navMappings = {
        product: ['/product', '/platform', '/competitors'],
        solutions: ['/solutions'],
        learn: ['/hub', '/library', '/blog', '/community'],
        company: ['/about', '/jobs', '/agencies', '/use-cases'],
        support: ['/support', '/doc'],
        pricing: ['/pricing']
      };
  
      function determineSection() {
        var explicitSection = location.href.match(/header_section=([^\&]+)/);
        if (explicitSection) {
          return explicitSection[1];
        }
  
        if (location.hostname === 'status.wistia.com') {
          return 'support';
        }
  
        for (var section in navMappings) {
          var paths = navMappings[section];
          for (var i = 0; i < paths.length; i++) {
            if (location.pathname.indexOf(paths[i]) === 0) {
              return section;
            }
          }
        }
        // Nothing matched? Show the Product section.
        return 'product';
      }
  
      var $section = $('.w-global-nav__level2[data-section=' + determineSection() + ']');
      $section
        .addClass('w-global-nav__current-section')
        .addClass('w-global-nav__visible-section');
  
      // When you click the hamburger, show or hide the menu.
      $('.w-global-nav__toggle-hamburger').click(function(e) {
        e.preventDefault();
        var $nav = $('.w-global-nav');
        var $navMenu = $('.w-global-nav__menu');
        if ($nav.is('.w-global-nav--expanded')) {
          $('body, html').css({ overflow: '', height: '' });
          $nav.removeClass('w-global-nav--expanded');
        } else {
          $('body, html').css({ overflow: 'hidden', height: '100%' });
          $nav.addClass('w-global-nav--expanded');
        }
      });
  
  
      window.W_determineLoggedIn = function(data){
        $('.w-global-nav').toggleClass('w-global-nav--logged-in', data["logged_in"]);
        dataLayer.push({"logged_into_wistia": data["logged_in"], 'event': 'logged_in_status_updated'});
      }
  
      var determineSessionState = function() {
        var verify_endpoint = window.location.protocol + '//' + determineSecureHostname() + '/session/verify';
        jQuery.ajax({
          url: verify_endpoint,
          dataType: 'jsonp',
          jsonpCallback: 'W_determineLoggedIn'
        });
      };
  
  
      determineSessionState();
  
      var setLogoutUrl = function() {
        var url = window.location.protocol + '//' + determineSecureHostname() + '/logout';
        $('.w-global-nav__logout').attr('href', url);
      };
  
      setLogoutUrl();
    });
  }(jQuery, jQuery));
</script>
