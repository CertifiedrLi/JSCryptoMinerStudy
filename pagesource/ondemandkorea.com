<script>
$(document).ready(function() {
    updateSearchWord();
    var s = getSearchField();
    s.on('keyup',function(e) {
      if(s.val().trim().length === 0 && (e.which == 8 || e.which == 46)) {
        updateSearchWord();
      }
    });
    s.on('keydown', function (e) {
      updateSearchWord();
    });
    $("#searchreset").on('click', function() {
      s.val('');
      updateSearchWord();
    });
});

function processSearchResult(search_word, json_obj) {
    var html = '<div class="searchResultTxt" style="padding-left:10px;">';
    search_word_template = '&quot;<strong style="letter-spacing:0.05em;">{search_word}</strong>&quot;'.replace('{search_word}',search_word);
    html += 'Search Results for {search_word_template}'.replace('{search_word_template}', search_word_template);
    html += '</div><div class="searchResultContainer">'
    var programs = json_obj;
    if(programs && programs.length > 0) {
      for(i=0; i<programs.length; i++) {
          var program = programs[i];
          if(i % 5 == 0)
              html += "<div class='search_result_row'>";
          var tile;
          if(i % 5 == 4)
            tile = "<div class='search_result_entry last'>";
          else
            tile = "<div class='search_result_entry'>";
          tile += "<a href='/" + program.slug + ".html' title='" + program.title + "'>";
          tile += "<img src='" + program.image_url + "'>";
          tile += "<div class='ep_title'><b>" + program.title.replace(/\\/, "") +"</b></div>";
          tile += "<div class='ep_date ep_tag_text'>" + program.tags +"</div>";
          tile += "</a></div>";
          html += tile;
          if(i % 5 == 4)
              html += "</div>";
      }
      if(i % 5 != 4)
          html += "</div>";
    } else {
      html += "</div>"
      html += "<div class='searchResultMissing'>";
      html += "No matching videos found. Please try again with different search terms.";
      html += "</div>";
    }
    $("div#search_result").html(html);
    showSearchResult();
}

function showSearchResult() {
    $("div#search_result").css("height", $("body").css("height"));
    $("div#search_result").show(0);
    $("div#search_result").animate({
        opacity: 1
    }, 100, function() {
        $("div#search_result").show(0);
        $("div#search_result").css("opactity", 1);
    });
}

function hideSearchResult() {
    $("div#search_result").animate({
        opacity: 0,
    }, 100, function() {
        $("div#search_result").hide(0);
        $("div#search_result").css("opactity", 0);
    });
}

function showSearchIndicator() {
  $("#searchindicator").animate({
      opacity: 1
  }, 300, function() {
      $("#searchindicator").css("opacity", 1);
  });
  $("#searchsubmit").animate({
      opacity: 0
  }, 300, function() {
      $("#searchsubmit").css("opacity", 0);
  });
}

function hideSearchIndicator() {
  $("#searchindicator").animate({
      opacity: 0
  }, 300, function() {
      $("#searchindicator").css("opacity", 0);
  });
  $("#searchsubmit").animate({
      opacity: 1
  }, 300, function() {
      $("#searchsubmit").css("opacity", 1);
  });
}

function showSearchReset() {
  $("#searchreset").show();
}

function hideSearchReset() {
  $("#searchreset").hide();
}

function getSearchField() {
    var s;
    if($("#s").length == 1)
      s = $("#s");
    if($("#sq").length == 1)
      s = $("#sq");
    return s;
}

var last_keyup_timeout_flag = 0;
var last_searched_word = '';
function updateSearchWord() {
    var search_word = getSearchField().val().trim();
    if (search_word == "") {
      hideSearchReset();
    } else {
      showSearchReset();
    }
    var current_keyup_timeout_flag = window.setTimeout(function() {
        if(current_keyup_timeout_flag == last_keyup_timeout_flag)
        {
            //console.log("fire!");
            var s = getSearchField();
            var search_word = s.val().trim();
            if (search_word == "") {
                hideSearchResult();
                hideSearchIndicator();
            } else {
              if(last_searched_word != search_word) {
                last_searched_word = search_word;
                post_data = {
                  'csrfmiddlewaretoken': $('input[name="csrfmiddlewaretoken"]').val()
                };
                showSearchIndicator();
                $.ajax({
                  url: '/includes15/search?q='+encodeURI(search_word),
                  data: post_data,
                  s: search_word,
                  method: "POST"
                }).done(function(jsonObj) {
                    var s = getSearchField();
                    if(this.s == s.val().trim()) {
                        hideSearchIndicator();
                        if(jsonObj && $.isArray(jsonObj.data.programs))
                            processSearchResult(this.s, jsonObj.data.programs);
                    }
                }).fail(function() {
                  var s = getSearchField();
                  var search_word = s.val().trim();
                  search_word_template = '&quot;<strong style="letter-spacing:0.05em;">{search_word}</strong>&quot;'.replace('{search_word}',search_word);
                  var html = '<div style="height:50px;background-color:#464646;font-size:15px;color:#fff;line-height:50px;padding-left:10px;margin-bottom:24px;">';
                  html += 'Search Results for {search_word_template}'.replace('{search_word_template}', search_word_template);
                  html += '</div>';
                  html += "<div class='search_result_row' style='font-size:20px; width:980px; height:200px; display:table-cell; vertical-align:middle; text-align:center;'>";
                  html += 'Oops... a server error occurred. Please try again.';
                  html += '</div>';
                  $("div#search_result").html(html);
                  showSearchResult();
                  hideSearchIndicator();
                });
              } else {
                showSearchResult();
              }
            }
        }
    }, 80);
    last_keyup_timeout_flag = current_keyup_timeout_flag;
}
    </script>
