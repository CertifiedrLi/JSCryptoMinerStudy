<script>
// Facebook authentication stuff
$(document).ready(function() {
  $.ajaxSetup({
    cache: true
  });
  $.getScript('//connect.facebook.net/en_US/sdk.js', function() {
    FB.init({
      appId: '309479342402963',
      version: 'v2.7'
    });
    // $('#loginbutton,#feedbutton').removeAttr('disabled');
    FB.getLoginStatus(updateStatusCallback);
    $('.btn-facebook-login').click(function() {
      g_fb_login = true;
      FB.login(function(response) {
        updateStatusCallback(response);
      }, {
        scope: 'email,public_profile'
      })
    });

    function updateStatusCallback(response) {
      var is_facebook_connected = 0;
      if (response.status === 'connected') {
        is_facebook_connected = 1;
      }
      setCookie('is_facebook_connected', is_facebook_connected);

      if (response.status === 'connected' && g_fb_login) {
        var csrftoken = Cookies.get('csrftoken');
        $.ajaxSetup({
          beforeSend: function(xhr, settings) {
            if (!csrfSafeMethod(settings.type) && !this.crossDomain) {
              xhr.setRequestHeader("X-CSRFToken", csrftoken);
            }
          }
        });
        $.ajax("/includes15/facebook-registration", {
          data: response.authResponse,
          method: 'POST',
          success: function(data) {
            if(data.next && (typeof(PLAYER_LOGIN_BUTTON_CLICKED) !== 'undefined' && PLAYER_LOGIN_BUTTON_CLICKED != true)){
              window.location.href=data.next;
            }else{
              window.location.reload();
            }

            if (typeof(PLAYER_LOGIN_BUTTON_CLICKED) !== 'undefined') {
              PLAYER_LOGIN_BUTTON_CLICKED = false;
            }
          }
        });
      }
    }
  });
  var g_fb_login = false;

  function csrfSafeMethod(method) {
    // these HTTP methods do not require CSRF protection
    return (/^(GET|HEAD|OPTIONS|TRACE)$/.test(method));
  }
});

