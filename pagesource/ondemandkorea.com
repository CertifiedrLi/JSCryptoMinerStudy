  <script>
    var selected_lang = 'en';
    if(selected_lang == "ko") {
      selected_lang = "kr";
    }

    function render_episodes(widget,episodes,popular) {
      popular = typeof popular !== 'undefined' ? popular : false;
      var entries = [];
      var pattern = RegExp("^(([^:/?#]+):)?(//([^/?#]*))?([^?#]*)(\\?([^#]*))?(#(.*))?");

      $.each(episodes,function(index,val){
        var entry_hot = $('<div/>',{
          'class': 'entry'
        });

        var poster = $('<img/>',{
          'src': val.poster
        });

        if (popular) {
          var anchor = $('<a/>',{
            'href' : val.url,
            'title' : val['title'][selected_lang],
            'class' : 'bi-popular'
          });
          var ep_title = $('<div/>',{
            'class': 'ep_title',
            'text': val['title_'+selected_lang],
            'css': {
              'font-weight' : 'bold'
            }
          });
        } else {
          var anchor = $('<a/>',{
            'href' : val.url,
            'title' : val['title'][selected_lang],
            'class' : 'bi-newest'
          });
          var ep_title = $('<div/>',{
            'class': 'ep_title',
            'text': val['title'][selected_lang],
            'css': {
              'font-weight' : 'bold'
            }
          });
        }

        var subtitle_span = $('<span/>',{}).css('float','right');

        $.each(val.subtitle,function(i,v) {
          var css = undefined;
          if(i == 0) {
            css = {'border': 'none','padding-left':'3px','padding-bottom':'1px','height':'13px','width':'auto'}
          } else {
            css = {'border-top':'none','border-right':'none','border-bottom':'none','border-left':'1px solid #dddddd','padding-left':'3px', 'padding-bottom':'1px', 'height':'13px', 'width':'auto','margin-left':'3px'}
          }

          var subtitle = $('<img/>',{
            'src': 'https://sp.ondemandkorea.com/images/cc_'+v+'.png'
          }).css(css);

          subtitle_span.append(subtitle);
        });

        var ep_date = $('<div/>',{
          'class': 'ep_date'
        });

        if(index%5 != 4) {
          $(ep_date).css('width','82%');
        }

        var plus_only = {};
        if (val.plusOnly == "1") {
          plus_only = $('<img/>',{
            'src': "https://sp.ondemandkorea.com/images/content/ic_odkplus.gif"
          }).css({'width':'58px','height':'14px','border':'none','position':'absolute'});
        }

        if (val.onAirDate) {
          ep_date.append(val.onAirDate + subtitle_span[0].outerHTML);
        } else {
          ep_date.append(val.onairdate + subtitle_span[0].outerHTML);
        }

        $(anchor).append(plus_only);
        $(anchor).append(poster);
        $(anchor).append(ep_title);
        $(anchor).append(ep_date);
        $(entry_hot).append(anchor);

        entries.push(entry_hot);
      });

      var items = [];

      for(var i = 0; i < entries.length/5; i++) {
        var item = $('<div/>',{
          'class': 'item'
        });
        items.push(item);
      }

      $.each(entries,function(index,val) {
        val[0].setAttribute('data-index', index);
        val[0].setAttribute('data-category', widget);
        var matches =  val.children('a').attr('href').match(pattern);
        var slug = matches[5];
        val[0].setAttribute('data-slug', slug);

        if(index % 5 == 4) {
          $(val).addClass('last');
        }
        $(items[Math.floor(index/5)]).append(val);
      });

      $.each(items,function(index,val) {
        $('.'+widget+' > .slider').append(val);
      });

      $('.iosSlider.'+widget).iosSlider({
        snapToChildren: true,
        desktopClickDrag: true,
        keyboardControls: true,
        infiniteSlider: true,
        navNextSelector: $('.next.'+widget),
        navPrevSelector: $('.prev.'+widget)
      });
    }

    $(document).ready(function() {
      $.getJSON("/includes15/categories/main_widgets.json" + window.location.search, function(json) {
        
          
        
          
          if(typeof json.drama !== "undefined" && json.drama.length > 0) {
            render_episodes('drama', json.drama);
          }
          
        
          
          if(typeof json.variety !== "undefined" && json.variety.length > 0) {
            render_episodes('variety', json.variety);
          }
          
        
          
          if(typeof json.news !== "undefined" && json.news.length > 0) {
            render_episodes('news', json.news);
          }
          
        
          
          if(typeof json.doc !== "undefined" && json.doc.length > 0) {
            render_episodes('doc', json.doc);
          }
          
        
          
          if(typeof json.movies !== "undefined" && json.movies.length > 0) {
            render_episodes('movies', json.movies);
          }
          
        
          
          if(typeof json.kids !== "undefined" && json.kids.length > 0) {
            render_episodes('kids', json.kids);
          }
          
        
          
          if(typeof json.life !== "undefined" && json.life.length > 0) {
            render_episodes('life', json.life);
          }
          
        
          
        
      });
    });
  </script>
