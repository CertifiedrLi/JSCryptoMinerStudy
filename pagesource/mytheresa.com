                    return results;
                }

                var suggestTemplate = new Template('<div class="suggest-wrapper#{noProducts}"><div class="further-suggestions overlay-col">#{searchTerm}#{brand}#{category}</div>#{productName}<div class="markuparrow-suggest"></div></div>');
