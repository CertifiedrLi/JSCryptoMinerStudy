            var wishlistButton = $('#button-add-to-wishlist');
            if (wishlist.inWishlist(sku) && wishlistButton.length) {
                wishlistButton.text(wishlistButton.data('added-text')).wrapInner('<span><span></span></span>');
            }
        }

        function toggleMoveLink() {
            var moveButtons = $('#shopping-cart-table .btn-wishlist');
            moveButtons.each(function () {
                var btn = $(this), data = btn.data('wishlist') || {};
                if (data.hasOwnProperty('sku') && wishlist.inWishlist(data.sku)) {
                    btn.hide();
                } else {
                    btn.fadeIn();
                }
            });
        }

        var cookie = Mage.Cookies.get('mzpersistent_cart');
        var hash = (cookie && $.parseJSON(cookie).hash) ? $.parseJSON(cookie).hash : '';

        try{
            document.cookie = "mzpersistent_cart=" + "; domain=.mytheresa.com; path=/; expires=Thu, 01-Jan-70 00:00:01 GMT";
        } catch(e){}

        var reference = (cookie && $.parseJSON(cookie).customer_reference) ? $.parseJSON(cookie).customer_reference : '';
        wishlist = initWishlist(hash, reference);


        toggleMoveLink();

        
        /* Wishlist size overlay */
        wishlistHearts = $(wishlist.getOption('addtoWishlistItem'));
        $(document).on('click', wishlist.getOption('addtoWishlistItem'), function (e) {
            e.stopPropagation();
            wishlistHearts.filter('.active').not(this).each(function () {
                this.removeClassName('active');
                $(this).siblings('.wishlist').removeClass('visible').fadeOut('fast');
            });

            this.addClassName('active');
            $(this).siblings('.wishlist').addClass('visible').fadeIn(1000);
        });
        $(document).on('click', function (e) {
            if ($(e.target).closest('.wishlist').length) return;
            wishlistHearts.filter('.active').each(function () {
                this.removeClassName('active');
                $(this).siblings('.wishlist').removeClass('visible').fadeOut('fast');
            });
        });
    });
</script>
