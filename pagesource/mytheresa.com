<script type="text/javascript">
    (function (channelCountries, countries) {
        var pattern = /\|([A-Z]{2})(?:$|\|)/, cookie = String(Mage.Cookies.get('myth_country'));
        if (pattern.test(cookie)) countries.push(pattern.exec(cookie)[1]);
        for (var i = 0; i < countries.length; i++) {
            if (channelCountries.hasOwnProperty(countries[i])) {
                $('l10n-country').update(channelCountries[countries[i]]);
                break;
            }
        }
    })({"DE":"Germany"}, ["DE"]);

    (function($){
        var flyoutTriggerSelector = '#meta_customer_flyout_trigger';
        var flyoutLinkSelector = '#myaccount';
        var isNotTouch = $('html').hasClass('no-touch');
        var isOffCanvasOpened = $('body').hasClass('off-canvas');

        function checkState() {
            isNotTouch = $('html').hasClass('no-touch');
            isOffCanvasOpened = $('body').hasClass('off-canvas');
        }

        function showFlyout(){
            checkState();

            if(isNotTouch && !isOffCanvasOpened)
                $(flyoutTriggerSelector).find('div#header-account').addClass('skip-active');
        }

        function hideFlyout(e){
            if(isNotTouch && !isOffCanvasOpened && !$(e.target).is('input'))
                $(flyoutTriggerSelector).find('div#header-account').removeClass('skip-active');
        }

        function followLink(){
            if(isNotTouch && !isOffCanvasOpened)
                window.location.href = $(this).attr('href');
        }

        /**
         * detect OS type and insert into html class
         */
        function detectingOperatingSystem() {
            var operationSystemName = "Unknown OS",
                osTypes = ["Win", "Mac", "X11", "Linux"];

            osTypes.find(
                function(entry){
                    if (navigator.appVersion.indexOf(entry) != -1) {
                        operationSystemName = entry;
                        return true;
                    }
                }
            );
            jQuery('body').parent().addClass(operationSystemName);
        }

        detectingOperatingSystem();

        $(document).on('mouseenter', flyoutTriggerSelector, showFlyout);

        $(document).on('mouseleave', flyoutTriggerSelector, hideFlyout);

        $(flyoutTriggerSelector).on('click', flyoutLinkSelector, followLink);
    }(jQuery))
</script>
