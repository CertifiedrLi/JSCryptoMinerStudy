            MonkeyBroker.defineSlot({slot: 'medrec', sizes: ["300x250"], el: target});
            MonkeyBroker.registerSlotCallback(unit.type, WAS.updateSlotSize);
            MonkeyBroker.adPlacement({slot: 'medrec', sizes: ["300x250"], el: target});
        }
    } else if (WAS.getProvider() === 'DFP') {
                WAS.create(unit.type, target);
    }
})();
