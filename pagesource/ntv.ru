<script>
  var BNRS = {};
BNRS.show = 1;
function closeFullscreen(){
	$("#fullscreenbanner").hide();
	$(".adfox__banner_fullscreen").hide();
	$("#adfox_container").html('');
}
function closeTimer(s){
	if(s>0){
		s=s-1;
		$("#fullscreenCloseTimer").html(s);
		setTimeout(function(){closeTimer(s);}, 1000);
		return true;
	}else{
		$("#fullscreenbanner").hide();
		closeFullscreen();
		return false;
	}
}
function isLangscape(){
	return window.innerHeight<window.innerWidth?true:false;
}
function drawFullScreen(p){
	if(p.type=='image_smartphone'){

		if(isLangscape()){ 
			$("#adfox_container img").attr("src",p.images.landscape);
		}else{ 
			$("#adfox_container img").attr("src",p.images.portrait);
		}
	}else if(p.type=='html5'){
		$("#adfox_container a").html('<iframe src="'+p.url+'"></iframe>');
	}
	if(p.pixel){
		var i = new Image();
		i.src=p.pixel;
	}
	$("#adfox_container a").attr("href",p.reference);
	setTimeout(function(){$("#fullscreenbanner").show();}, 2000);
	//setTimeout(function(){$(".adfox__close").show();}, p.showCloseButton+2000);
	$(".adfox__close").show();
	$("#fullscreenbanner").show();
	closeTimer(p.closeTimer);
}

