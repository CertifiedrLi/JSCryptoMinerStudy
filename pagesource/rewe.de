<script type="template/underscore" id="mf-thumbnail-view-template">
    <div class="mf-pages">
        <@ pages.forEach(function(page) {@>
        <div class="mf-page">
            <@ page.images.forEach(function(image) {@>
            <img src="<@=image.thumb@>"/>
            <@ });@>
            <div class="mf-page-highlight">
                <span>Seite <@= page.images.map(function(image) {return image.no+1;}).join("-") @></span>
            </div>
        </div>
        <@ });@>
    </div>
</script>

<script type="template/underscore" id="mf-navigation-view-template">
    <a class="mf-prev">Vorherige Seite</a>
    <a class="mf-next">Nächste Seite</a>
</script>

<script type="template/underscore" id="mf-pageflip-view-template">
    <@ pages.forEach(function(page, i) {@>
    <@ page.images.forEach(function(image) {@>
    <div class="<@=(i==0 ? 'cover' : 'page')@>" data-background-file="<@=image.desktop@>">
    </div>
    <@ });@>
    <@ });@>
</script>

<script type="template/underscore" id="mf-desktop-view-template">
    <div id="pageflip" class="mf-pageflip-view"></div>
    <div class="mf-thumbnail-view"></div>
    <div class="mf-navigation-view"></div>
    <div class="mf-controls mf-controls-top">
        <a class="mf-link-close" data-role="close-modal">REWE Prospekt schließen</a>
    </div>
    <div class="mf-controls mf-controls-bottom">
        <a class="mf-link-zoom-in">Vergrößern</a>
        <a class="mf-link-zoom-out">Verkleinern</a>
        <a class="mf-link-enter-fullscreen">Vollbild</a>
        <a class="mf-link-leave-fullscreen">Zurück</a>
    </div>
</script>

<script type="template/underscore" id="mf-page-number-view-template">
    <div class="mf-page-number-center">
        <a class="mf-prev">Vorherige Seite</a>
        <div class="mf-page-number">
            <span class="mf-current-page"></span> / <span class="mf-total-pages"><@=pages.length@></span>
        </div>
        <a class="mf-next">Nächste Seite</a>
    </div>
</script>

<script type="template/underscore" id="mf-slider-view-template">
    <div class="mf-pages">
        <@ pages.forEach(function(page) {@>
        <div class="mf-page">
            <@ page.images.forEach(function(image) {@>
            <img src="<@=image.mobile@>"/>
            <@ });@>
        </div>
        <@ });@>
    </div>
</script>

<script type="template/underscore" id="mf-zoom-view-template">
    <img class="mf-panzoom-content mf-zoom-image">
</script>

<script type="template/underscore" id="mf-index-view-template">
    <div class="mf-pages">
        <@ pages.forEach(function(page, no) {@>
        <div class="mf-page" data-page-no="<@=no@>">
            <@ page.images.forEach(function(image) {@>
            <img src="<@=image.mobile@>"/>
            <@ });@>
        </div>
        <@ });@>
    </div>
</script>

<script type="template/underscore" id="mf-mobile-view-template">
    <div class="mf-index-view"></div>
    <div class="mf-slider-view"></div>
    <div class="mf-zoom-view"></div>
    <div class="mf-page-number-view"></div>
    <div class="mf-controls">
