          <style>
        /* Brandon to remove later v */
div#shelf-global{display:block!important;}
div#shelf-secondary{display:none!important}

.mw { color:#000000;font-family:Verdana,Arial,Helvetica;font-weight:bold;font-size:xx-small;text-decoration:none; }
a.mw:link	{color:#000000;font-family:Verdana,Arial,Helvetica;font-weight:bold;font-size:xx-small;text-decoration:none;}
a.mw:visited	{color:#000000;font-family:Verdana,Arial,Helvetica;font-weight:bold;font-size:xx-small;text-decoration:none;}
a.mw:hover	{color:#0000FF;font-family:Verdana,Arial,Helvetica;font-weight:bold;font-size:xx-small;text-decoration:none;}
#logoSpaceContent {
  display:none;
}
div#shelf-global {
    display: none;
}
#middle-top .header {
    margin-bottom: 15px;
}
.hide-slider{
display:none !important;
}
#top #topSlideshow{
max-height:none !important;
}
#middle-center .videoBg-row > .row-flex{padding:0 !important}
.video-background {
background: #000;
top: 0; right: 0; bottom: 0; left: 0;
z-index: -99;
overflow:hidden;
}
.video-foreground,
.video-background iframe {
position: relative;
top: 0;
left: -5%;
width: 110%;
height: 100%;
pointer-events: none;
}
.video-foreground{
height:800px;
}
@media screen and (max-width:665px){
.video-foreground{
height:275px;
background-image:url('https://www.ohio.edu/frontdoor/images/video-placeholder.jpg');
background-size:cover;
background-position:center;
}
}
#vidtop-content {
top: 0;
color: #fff;
}
.no-video{
display:none;
}
.vid-info {
position: absolute;
top: 44%;
left: 13%;
width: 75%;
color: #fff;
padding: 1rem;
font-family: Avenir, Helvetica, sans-serif;
}
.vid-info img{
display:block;
max-width:100%;
}
.wrap-container {
    position: absolute;
    bottom: 10%;
    left: 5%;
    width: calc(100% - 120px);
}
#video-pause{ display:inline-block;vertical-align:top;margin-right:10px;}
#video-pause, #video-captions {
width: 40px;
height: 40px;
display:inline-block;
vertical-align:top;
color:#00694e;
border-radius: 20px;
overflow: hidden;
background: rgba(255, 255, 255, 1);
z-index: 99;
position: relative;
text-decoration:none;
box-shadow: 1px 1px 16px #000;
}
#video-pause:focus, #video-captions:focus{
outline:#85b7d1 solid 2px;
}
#video-pause:after{
content:'\f04c';
font-family:FontAwesome;
font-size:18px;
text-decoration:none;
left: 0;
position:relative;
top: 0;
}
#video-pause.paused:after{
content:'\f04b';
left:2px;
}
#video-pause span, #video-captions span {
position: absolute;
left: 9999px;
}
#video-pause:focus{
border:2px solid #00694e;
background:#fff;
}
.captions-container {
    display: inline-block;
    vertical-align: top;
    position: relative;
    width: 80%;
    text-align: left;
}
#video-captions:after{
content: "\f129";
font-family:FontAwesome;
font-weight: bolder;
font-size: 20px;
font-weight: normal;
top: 9px;
left: 9px;
}
#video-captions.active{
background:#00694e;
color:#fff;
}
#dynamic-captions{
min-height:80px;
max-width: 62%;
position:relative;
display:inline-block;
margin-left:10px;
margin-top:-23px;
padding:15px;
background:#00694e;
z-index:-99999;
}
#dynamic-captions.active{
z-index:999999;
}
#dynamic-captions:before{
content: '';
width: 0;
height: 0;
border-bottom: 15px solid transparent;
border-top: 15px solid transparent;
border-right: 15px solid #00694e;
position: absolute;
left: -14px;
top: 25%;
z-index: 999999;
}
#dynamic-captions .captions-wrap{
max-width:45%;
overflow-x:hidden;
}
#dynamic-captions h1{
position:absolute;
left:-99999px;
}
#dynamic-captions p{
display:block;
width:0;
height:0;
margin:0;
overflow:hidden;
position:relative;
float:left;
transition:all .5s;
color:#fff;
font-size:16px;
}
#dynamic-captions p.showing{
height:auto;
width:auto;
}
#dynamic-captions p a{
color:#fff;
}
/*@media (min-aspect-ratio: 16/9) {
.video-foreground { height: 300%; top: -100%; }
}
@media (max-aspect-ratio: 16/9) {
.video-foreground { width: 300%; left: -100%; }
}*/
@media all and (max-width: 600px) {
.vid-info { width: 50%; padding: .5rem; }
.vid-info h1 { margin-bottom: .2rem; }
}
@media all and (max-width: 500px) {
.vid-info .acronym { display: none; }
}
.top .featuredSlideshow{display:none !important;}
#flipper-60744C7B-5056-A851-83822BC75CBC0762 ul li {
float: left;
list-style: none;
position: absolute;
}
#flipper-60744C7B-5056-A851-83822BC75CBC0762 ul li + li {
display: none;
}
#flipper-60744C7B-5056-A851-83822BC75CBC0762 .flipperButtons span {
cursor: pointer;
}
.imageImg img.hover {
display: none !important;
}
.imageImg img.hover.disp {
display: inline !important;
}
.modInnerContent.col-flex-xs-12.col-flex-sm-9.col-flex-md-9.col-flex-lg-9 {
    max-width: 1170px;
    margin: 0 auto;
}
.header {
    margin: 0 0 5px 0;
    color: #00694e;
    font-weight: normal;
    font-size: 35px;
}
span.lastWord {
    color: #57534b;
    text-transform: uppercase;
    font-weight: bold;
    font-size: 35px;
}
.middle .allNews {
    text-align: right;
    padding-top: 10px;
    position: absolute;
    right: 40px;
    top: 0;
}
#flipper-60744C7B-5056-A851-83822BC75CBC0762 ul li {
    float: left;
    list-style: none;
    position: absolute;
}
.middle .newsFlipper ul li a {
    text-decoration: none;
    font-weight: 700;
    color: #00694e;
}
.middle .newsFlipper > ul {
    margin: 0;
    float: left;
    padding: 5px 0 0 6px;
    width: -moz-calc(100% - 78px);
    width: -webkit-calc(100% - 78px);
    width: calc(100% - 78px);
    position: relative;
    top: -4px;
}
.middle .newsFlipper > a {
    height: 12px;
    width: 12px;
    overflow: hidden;
    display: block;
    float: left;
    line-height: 13px;
    padding-top: 1px;
    font-size: 1.3333333333333333em;
    font-family: FontAwesome;
    font-style: normal;
    color: #00694e;
}
.middle .newsFlipper {
    position: absolute;
    top: 10px;
    left: 225px;
    width: -moz-calc(100% - 367px);
    width: -webkit-calc(100% - 367px);
    width: calc(100% - 367px);
}
.bottom .bottom-left {
    width: 260px;
    float: left;
    padding: 40px 30px 40px 10px;
}
.bottom .socialOhio a {
    text-decoration: none;
}
.bottom .socialOhio {
    float: left;
}
.bottom a.buttonClass, .middle a.buttonClass, .top a.buttonClass {
    display: block;
    float: none;
    background-color: #e3e2e0;
    color: #57534B;
    text-decoration: none;
    padding: 10px 0;
    width: 400px;
    text-align: center;
    border-radius: 5px;
    font-size: 14px;
    margin: 5px 0 30px;
}
.bottom .bottom-right a.buttonClass {
    float: left;
    margin: 30px 0 0 11px;
    width: calc(50% - 11px);
}
.bottom a.buttonClass:focus, .bottom a.buttonClass:hover, .middle a.buttonClass:focus, .middle a.buttonClass:hover, .top a.buttonClass:focus, .top a.buttonClass:hover {
    color: #e3e2e0;
    background-color: #938c86;
}
.eventCalendar .eventCalendar-event {
  width: 33% !important;
}
.eventCalendar-event:hover p, .eventCalendar-event:hover span {
  color: #fff !important;
}
.ouws_pi_image {
    float: left;
    padding: 0 20px 0 15px;
}
.row-flex.start-flex-xs.start-flex-sm.start-flex-md.start-flex-lg {
    clear: both;
    width: 100%;
}
.middle .allNews a, .middle .allNewsLink, a.allNewsLink {
    color: #57534B;
    text-transform: uppercase;
    font-weight: 700;
    text-decoration: none;
    font-size: 16px;
    margin-right: 13px;
}
.middle .allNews a::after, .middle .allNewsLink::after, a.allNewsLink::after, a[title="a.allNewsLink"]::after {
    content: "\f105";
    font-family: FontAwesome;
    padding-left: 8px;
    position: absolute;
}
.row-flex.center-flex-xs.center-flex-sm.center-flex-md.center-flex-lg {
    border-bottom: 16px solid #d3c9c0;
}
.eventCalendar .eventCalendar-event .type_tag {
  width: calc(100% - 25px) !important;
}
.paragraph .paragraph__column {
    padding: 0!important;
}
.row-flex.start-flex-xs.start-flex-sm.start-flex-md.start-flex-lg > div {
    padding: 10px 0 30px 0 !important;
}
#middle .eventCalendar .eventCalendar-event a.bottomLink:focus {
    background: none;
}
.modInnerContent + .row-flex.center-flex-xs.center-flex-sm.center-flex-md.center-flex-lg {
    background: #00694e !important;
    border: 0;
    width: 100%;
    padding: 30px 0;
    margin-top: 30px;
}
.container-flex-fluid > .row-flex + .row-flex > .modInnerContent + .row-flex > .modInnerContent + .row-flex {
    width: 100%;
    max-width: 1170px;
    background: #fff!important;
    border: 0;
    padding: 20px 0 !important;
    margin: 0 auto;
}
.container-flex-fluid > .row-flex + .row-flex > .modInnerContent + .row-flex > .modInnerContent + .row-flex .modInnerContent {
    max-width: 1170px;
    margin-bottom: 21px;
}
.imageImg.hasHover {
    margin-bottom: 15px;
    display: inline-block;
    max-width: 275px;
}
@media (min-width: 1200px)
.page-node-type-modular-page .row {
    padding: 0 !important;
}
@media only screen and (max-width: 1170px) {
  .eventCalendar .eventCalendar-event {
    float: none !important;
    width: 100% !important;
    border: 0 !important;
    margin-bottom: 10px !important;
  }
  .eventCalendar .eventCalendar-event .type_tag {
    width: calc(100% - 15px)!important;
  }
  .ouws_pi_image {
    max-width: 40%;
  }
}
/* Helper Classes */

/* We have forced all content to be within 1170, however, this forces fluid. */
.force-fluid .row-flex.innerRow{
    width:100%;
}

@media only screen and (max-width: 975px) {
  .row-flex.center-flex-xs.center-flex-sm.center-flex-md.center-flex-lg {
    margin: 0;
    padding: 0;
  }
  #dynamic-captions {
    width: 90%;
    max-width: none;
    position: absolute;
    margin: 0;
    bottom: -25px;
    left: 10%;
  }
  .captions-container {
    width: 100%;
  }
  #video-pause, #video-captions {
    display: block;
    margin-bottom: 10px;
  }
}
@media (max-width:767px) {
  .row-flex.start-flex-xs.start-flex-sm.start-flex-md.start-flex-lg {
    padding: 0;
  }
  .row-flex.start-flex-xs.start-flex-sm.start-flex-md.start-flex-lg .paragraph>.paragraph__column {
    padding: 0 25px;
  }
  .bottom .bottom-right a.buttonClass {
    width: 100%;
    margin: 30px 0 0 0;
  }
  .ouws_pi_image {
    float: none;
    width: 100%;
    padding: 0 0 20px 0;
    max-width: 100%;
    text-align: center;
  }
}
@media (max-width:377px) {
  #middle-top h2.header, .allNews {
    display:none;
  }
  .eventCalendar .eventCalendar-event {
    height: 260px;
  }
}
