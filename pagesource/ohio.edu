		  	if (!item.hasClass('show') && (!jQuery('> ul', item).is(':visible') || jQuery('> ul', item).css('opacity') == 0)) {
		  		showMenu_login_show(item);
		  	} else {
				showMenu_login_hide(item);
			}
		}

		// Expecting an li
		function showMenu_login_show(item) {
			var loginUl = jQuery('> ul', item);
			item.addClass('show');
			loginUl.slideToggle('fast').attr('aria-hidden', false);
			jQuery("[role='menuitem']", loginUl).first().focus();
		}

		// Expecting an li
		function showMenu_login_hide(item) {
			var loginUl = jQuery('> ul', item);
			item.removeClass('show');
			loginUl.slideToggle('fast').attr('aria-hidden', false).attr("aria-hidden", true);
		}

