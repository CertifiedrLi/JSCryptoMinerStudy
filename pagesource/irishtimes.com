      },
      surname: {
        required: "Required"
      },
      email : {
        required: "Required",
        email: "Not valid"
      }
    },
    submitHandler: function(form) {
      jq.ajax({
        type: "POST",
        url: jq('#rewarding-times-form').attr('action'),
        data: jq("#rewarding-times-form").serialize(), 
        success: function (data, status) {
          _gaq.push([ '_trackEvent', 'Rewarding Times', 'Rewarding Times Signup' ]);
        },
        complete: function (data, status) {    
          if (data.status != 200) {
            alerttype="alert-error";
            message = data.responseText;
          } else {
            response = jq.parseJSON(data.responseText);
            if (response.result == 'OK') {
              alerttype="success";
              jq("#rewarding-times-form fieldset").hide();
              message="<p class=\"thanks2\">You are now signed up to receive <strong>Rewarding Times</strong> emails. We look forward to sending you exclusive travel, entertainment and lifestyle offers from partners of <strong>The Irish Times</strong>.</p>";
              message+="<p class=\"thanks2\">Thank you for signing up.</p>";
              message+="<p class=\"thanks2\">The <strong>Rewarding Times </strong>team</p>";
              message+='<img height="1" width="1" style="border-style:none;" alt="" src="//www.googleadservices.com/pagead/conversion/1018865686/?value=1&amp;label=A0vvCKLnugcQltDq5QM&amp;guid=ON&amp;script=0"/>';
            } else {
              alerttype = "alert-error";
              message = response.result;
            }
          }
          jq('#alert').addClass("successOverlay");  
          jq('#alert').html('<div id="alertdiv" class="alert '+alerttype+'"><span>'+message+'</span></div>');
        },             
      });
      return false;
    }
  });
});

