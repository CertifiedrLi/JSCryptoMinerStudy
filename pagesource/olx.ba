    $( document ).ready(function() {
        
        $(".admin_select_artikla").click(function() {
            id = $(this).attr('data-id');

            if (isInArray(id , artikli)) {
                artikli.remove(id);
            }
            else {
                artikli.push(id);
            }

        });

        $('.moderacija-artikala-submit').on('click', function(e) {
            if ( artikli.length > 0 ) {
                var wpurl = "https://" + location.host + "/admin/moderacija_artikala?ids=" + artikli.toString();
                $(this).attr('href', wpurl);
            }
            else {
                e.preventDefault();
                console.log('artikli su prazni');
            }
        });

    });


    function isInArray(value, array) {
        return array.indexOf(value) > -1;
    }

    Array.prototype.remove = function() {
        var what, a = arguments, L = a.length, ax;
        while (L && this.length) {
            what = a[--L];
            while ((ax = this.indexOf(what)) !== -1) {
                this.splice(ax, 1);
            }
        }
        return this;
    };
