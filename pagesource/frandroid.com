                        }else{
        //check the cookie havevisitedfrandroid (expire 30min)
        //If cookie created => isFirstPage is 1
        //else isFirstPage is 0
        function getTargetIsFirstPage(nameCookie){
            var isFirstPage = '1';
            //get cookie
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' '){
                    c = c.substring(1);
                }
                if (c.indexOf(nameCookie) == 0) {
                    isFirstPage = '0';
                }
            }
            return isFirstPage;
        }
        var nameCookie = 'havevisitedfrandroid';
        var isFirstPage = getTargetIsFirstPage(nameCookie);
        //set cookie
