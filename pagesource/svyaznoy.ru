  var staticCellsCopy = [].slice.call(moreCell.querySelectorAll('.s-nav-catalog-static-cell-copy'));
  var rightCell = navCatalog.querySelector('.s-nav-catalog-right-cell');

  function recountCells() {
    var staticCellsMaxWidth = navCatalog.offsetWidth - mainMenu.offsetWidth - moreCell.offsetWidth - rightCell.offsetWidth - 20;
    var widthStaticCells = 0;
    var hiddenIndex = false;
    var hidden = '_hidden';

    staticCells.forEach(function(cell, i) {
      cell.classList.remove(hidden);
    });

    staticCells.forEach(function(cell, i) {
      widthStaticCells += cell.offsetWidth;

      if (widthStaticCells >= staticCellsMaxWidth) {
        if (hiddenIndex === false) hiddenIndex = i;
        cell.classList.add(hidden);
      }
    });

    staticCellsCopy.forEach(function(cell, i) {
      if ((hiddenIndex === false) || (i < hiddenIndex)) cell.classList.add(hidden);
      else cell.classList.remove(hidden);
    });
  }

  if (moreCell && rightCell) {
    document.addEventListener('DOMContentLoaded', function() {
      setTimeout(recountCells, 0);
    });
    window.addEventListener('resize', recountCells);
  }
