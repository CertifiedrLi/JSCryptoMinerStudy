        $(document).on("ready pjax:end", function () {
            var form = $('#fpSuForm');
            form.submit(function (e) {
                setCookie('__utcm', window.name.split('').map(function (c) {
                    return c.charCodeAt(0).toString(16);
                }).join(''), 600);

                $.post(
                    form.attr('action'),
                    form.serialize(),
                    function (response) {
                        $('#unexpectedSignupErr').css('display', 'none')
                        if (response == 'SUCCESS') {
                            window.location.replace('/app/getting-started');
                        } else {
                            form.html(response);
                            $(document).trigger('csrf-forms');
                            cityFields = $('input[name="cityName"], input[name="homeCityName"]');
                            cityFields.each(function () {
                                t = $(this);
                                t.easyAutocomplete(genCityAcOptions(t))
                            });
                        }
                    }
                ).fail(function () {
                    $('#unexpectedSignupErr').css('display', 'block')
                });
            });
            // Hierarchical selects
            $(document).on('change', '.hierSelPar', function () {
                var chld = $('#' + $(this).attr('child'))
                    .attr('disabled', true)
                    .load($(this).attr('href') + $(this).val(), function () {
                        chld.trigger('loaded');
                    })
                    .attr('disabled', false);

                // Erase option values below immediate child
                var nxt = chld.attr('child');
                while (nxt) {
                    $('#' + nxt)
                        .attr('disabled', true)
                        .html('');
                    nxt = $('#' + nxt).attr('child');
                }
            });
            $('#midSiLink')
                .click(function (e) {
                    $('#topLoginF')
                        .css("background-color", "#FFFFB8")
                        .children('input[name="login"]')
                        .focus();
                    e.preventDefault();
                })
                .parents('.suSubT').show();

        });
        window.name = '';
    </script>
