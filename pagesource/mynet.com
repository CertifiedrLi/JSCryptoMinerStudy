<script type="text/javascript">
    function krediHesapla(dataAction, dataValue) {
        changeNames(dataValue);
        changeAction(dataAction);
    }

    function changeAction(dataAction) {
        var site = 'http://kredihesaplama.mynet.com/';
        if(dataAction == "" || dataAction == null || dataAction == "undefined")
            dataAction = "konut/karsilastir/";

        jQuery("#dymbx-11 form").attr("action",site + dataAction);
        return true;
    }

    function changeNames(dataValue) {
        if(dataValue == "" || dataValue === null || dataValue == "undefined")
            dataValue = "konut";

        if(dataValue !== "konut")
            jQuery("#simpleLoan_propertyValue").attr("disabled","disabled");

        jQuery("div.krediForm form select#kredivade").attr("name", "simpleLoan_" + dataValue + "[term]");
        jQuery("div.krediForm form input#simpleLoan_calcType").attr("name", "simpleLoan_" + dataValue + "[calcType]");
        jQuery("div.krediForm form input#kredimiktar").attr("name", "simpleLoan_" + dataValue + "[amount]");
        jQuery("div.krediForm form input#simpleLoan_loanType").attr("name", "simpleLoan_" + dataValue + "[loanType]");
        jQuery("div.krediForm form input#simpleLoan_currency").attr("name", "simpleLoan_" + dataValue + "[currency]");
        jQuery("div.krediForm form input#simpleLoan_bankSlug").attr("name", "simpleLoan_" + dataValue + "[bankSlug]");
        jQuery("div.krediForm form input#simpleLoan_noCostsOnly").attr("name", "simpleLoan_" + dataValue + "[noCostOnly]");
        jQuery("div.krediForm form input#simpleLoan_loanType").val(dataValue);
    }

    mynetReady(function () {
        changeNames();
        changeAction();
    });
</script>        </div>

