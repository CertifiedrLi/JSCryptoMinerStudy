             */
            function detectIE() {
                var ua = window.navigator.userAgent;
                var msie = ua.indexOf('MSIE ');

                if (msie > 0) {
                    // IE 10 or older => return version number
                    return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
                }

                var trident = ua.indexOf('Trident/');
                if (trident > 0) {

                    if (bt. sitePrefix === 'OMG'){
                        $('#exit_intent_dialog article').css({ maxWidth: 'auto!important' });
                    }

                    if (!bt.isHcDesign) {
                        $('.popup-content').css({width: '100%'});
                    } else {
                        $(window).load(function () {
                            var $element = $('.sidebar');
                            var $mainWrap = $('.main-wrap');
                            var lastHeight = $element.css('height');

                            $element.css('width', '310px');
                            if ($mainWrap.css('height') < lastHeight){
                                $mainWrap.css('height', lastHeight);
                            }
                            //need listener since sidebar changes after load in single, about is a page of its own.
                            var heightChange = function checkForChanges() {
                                if ($element.css('height') !== lastHeight) {
                                    console.log('height changed, ET to the rescue');
                                    lastHeight = $element.css('height');
                                    if ($mainWrap.css('height') < lastHeight){
                                        $mainWrap.css('height', lastHeight);
                                    }
                                }
                                setTimeout(checkForChanges, 2000);
                            };
                            heightChange();
                        });

                    }
                    $('.wrap > strong').css('width','309px');
                    $('#top-banner').css('display','block');

                    /*normalizing menu items by text size*/

                    var $menu_text = $('.main-menu > .menu-item:not(.more, .css-icon-exit) a');
                    var cutoff = 10.5;

                    $.each($menu_text, function (index, item) {
                        if (item.text.length > cutoff) {
                            var currentSize = parseFloat($(item).css('font-size'));
                            var reduction_by_size = (currentSize * cutoff / item.text.length).toFixed(1);
                            $(item).css({fontSize: reduction_by_size + 'px'});
                        }
                    });

                    $('.archive-image').css('overflow', 'hidden');

                    if (bt.isBtDesign) {
                        $('.three-d-slider').css('display', 'none');
