window.getUrlParByPref = function (prefix){ //get url parameter name value by name prefix
    var sPageURL = window.location.search.substring(1).toLowerCase();
    var sURLVariables = sPageURL.split('&');
    var _return = [];

    for (var i = 0; i < sURLVariables.length; i++) {
        var sParameterName = sURLVariables[i].split('=');

        if (sParameterName[0].substring(0, prefix.length) == prefix){
            _return.push({
                name:sParameterName[0],
                val:sParameterName[1] || false,
                fullStr: sParameterName[0] + '=' + sParameterName[1]
            });
        }
    }
    return _return;
};

function getAllUrlParams() {
    var queryParams = ['pas', 'as', 'as-source', 'shr', 'crf', 'bdk', 'adv_txt', 'adv_img', 'utm_', 'ch'];
    var queryString = location.search.slice(1);
    var queryParamFiltering = function(prev, curr) {
        var kvp = curr.split('=');
        var key = kvp[0];
        var val = decodeURIComponent(kvp[1]);
        var index = queryParams.indexOf(key);
        var isNoUtmParamInUrl = (key.indexOf('utm_') === -1);
        if (kvp.length !== 2 || (index === -1 && isNoUtmParamInUrl)) { return prev; }
        // else
        if (key === 'crf' && val.indexOf('src_') === -1) { val = 'src_' + val; }
        // else
        if (isNoUtmParamInUrl) { queryParams.splice(index, 1); } // if url has `utm_` prefix
        // else
        prev[key] = val;
        return prev;
    };
    if (!queryString.length) { return {}; }
    return queryString.split('&').reduce(queryParamFiltering, {});
}
