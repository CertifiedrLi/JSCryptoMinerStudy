/*
 * store-cookie.js
 * */
bt.cookie = bt.cookie || {};
window.cookie = bt.cookie;
(function () {
    var cookieName = 'btUserData';
    var cookie = bt.cookie;
    cookie.get = function () {
        var currentState = Cookies.getJSON(cookieName);
        currentState = currentState || {};
        return currentState;
    };
    cookie.remove = function () {
        Cookies.remove(cookieName);
    };
    cookie.set = function (key, val) {
        var change = cookie.get();
        change[key] = val;
        Cookies.set(cookieName, change, {expires: 365});
    };
    cookie.increment = function (key) {
        var change = cookie.get(),
            val = change[key];
        if (isNaN(val)) {
            val = 0;
        }

        change[key] = val + 1;

        Cookies.set(cookieName, change);
    };
})();

jQuery(document).ready(function ($) {
    var currentId = bt.postId,
        lastId = cookie.get()["lastpid"];
    if (!lastId || lastId != currentId) {
        cookie.set("lastpid", currentId);
        cookie.increment("views");
    }

    if (!Cookies.get("bt_session")) {
        Cookies.set("bt_session", 1, {expires: 1 / 48});
        cookie.increment("sessions");
    }
});

//module video-iframes.js
/*
 * Video iframes
 * */
window.videoIndex = 0;
window.vimeoloaded = 'not';
window.ytloaded = 'not';

window.updateVideoIframes = function ($elements) {
    "use strict";

    var postToFeed = function () {
        if (
            typeof btabCta == 'object' &&
            typeof btabCta.videoEnd == 'boolean' &&
            btabCta.videoEnd == true
        ) {
            jQuery(btabCta).trigger('videoEnd');
        } else {
            do_custom_popup("share_dialog");
        }
    };

    //return video type or false
    var parseVideoUrl = function (url) {
        if (url != "" && url != undefined) {
            url.match(/(http:|https:|)\/\/(player.|www.)?(vimeo\.com|youtu(be\.com|\.be|be\.googleapis\.com))\/(video\/|embed\/|watch\?v=|v\/)?([A-Za-z0-9._%-]*)(\&\S+)?/);
            if (RegExp.$3.indexOf('youtu') > -1) {
                var type = 'youtube';
            } else if (RegExp.$3.indexOf('vimeo') > -1) {
                var type = 'vimeo';
            } else {
                var type = '';
            }
            return {
                type: type,
                id: RegExp.$6
            };
        }
        return false;
    };


    //Load vimeo urls and events
    var addVimeoEvents = function ($iframes) {
        // Enable the API on each Vimeo video

        var ready = function (playerID) {
            Froogaloop(playerID).addEvent('finish', onFinish);
            //Froogaloop(playerID).api('play');
        };
        var onFinish = function (playerID) {
            postToFeed();
        };


        $iframes.filter('iframe.vimeo_player').each(function (index, element) {
            var old_source = jQuery(element).attr("src");
            //console.log(old_source);
            if (old_source != "" && typeof old_source !== "undefined") {
                var parseStatus = parseVideoUrl(old_source);
                if (parseStatus) {
                    old_source = old_source.split("?")[0];
                    if (parseStatus && parseStatus.type == "vimeo") {
                        var player_id = "vimeo_player_" + window.videoIndex++;
                        var new_source = old_source + "?api=1&player_id=" + player_id;
                        jQuery(element).addClass("vimeo_player");
                    } else if (parseStatus && parseStatus.type == "youtube") {
                        var player_id = "youtube_player_" + window.videoIndex++;
                        var new_source = old_source + "?enablejsapi=1&wmode=transparent";
                        jQuery(element).addClass("youtube_player");
                        jQuery(element).attr("wmode", "transparent");
                    }
                    if (player_id != "" && new_source != "") {
                        jQuery(element).attr("src", new_source);
                        jQuery(element).attr("id", player_id);
                    }
                }
            }
            Froogaloop(this).addEvent('ready', ready);
        });
    };

    var addYoutubeEvents = function ($iframes) {
        //Load youtube urls and events
        var youtubeCallback = function (event) {
            if (event.data == YT.PlayerState.ENDED) {
                postToFeed();
            }
            if ((navigator.userAgent.match(/iPhone|iPod|iPad/i))
                && event.data == YT.PlayerState.PAUSED) {
                postToFeed();
            }
        };

        new YT.ready(function () {
            $iframes.filter('iframe.youtube_player').each(function (index, element) {
                //player id
                var yt_player_id = jQuery(element).attr("id");
                if (yt_player_id != "") {
                    element.ytPlayer = element.ytPlayer || new YT.Player(yt_player_id, {
                            events: {
                                'onStateChange': youtubeCallback
                            }
                        });
                }
            });
        });
    };

    var initialvimeo = vimeoloaded;
    var initialyt = ytloaded;

    $elements = $elements || jQuery('.post-single-content iframe');
    $elements.each(function (index, element) {
        var old_source = jQuery(element).attr("src");
        if ((old_source != null) && (old_source != "")) {
            var parseStatus = parseVideoUrl(old_source);
            old_source = old_source.split("?")[0];
            if (parseStatus && parseStatus.type == "vimeo") {
                var player_id = "vimeo_player_" + window.videoIndex++;
                var new_source = old_source + "?api=1&player_id=" + player_id;
                jQuery(element).addClass("vimeo_player");
                if (vimeoloaded == 'not') {
                    vimeoloaded = 'loading';
                    if (window.location.protocol == "https:")
                        jQuery.getScript('https://secure-a.vimeocdn.com/js/froogaloop2.min.js', function () {
                            window.vimeoloaded = 'loaded';
                            addVimeoEvents($elements)
                        });
                    else
                        jQuery.getScript('http://a.vimeocdn.com/js/froogaloop2.min.js', function () {
                            window.vimeoloaded = 'loaded';
                            addVimeoEvents($elements)
                        });
                }
            } else if (parseStatus && parseStatus.type == "youtube") {
                var player_id = "youtube_player_" + window.videoIndex++;
                old_source = window.location.protocol + old_source.substr(old_source.indexOf(':') + 1);
                var new_source = old_source + "?enablejsapi=1&wmode=transparent&origin=" + window.location.origin;
                jQuery(element).addClass("youtube_player");
                jQuery(element).attr("wmode", "transparent");
                if (ytloaded == 'not') {
                    ytloaded = 'loading';
                    jQuery.getScript('//www.youtube.com/iframe_api', function () {
                        window.ytloaded = 'loaded';
                        addYoutubeEvents($elements);
                    });
                }
            }
            if (player_id != "" && new_source != "") {
                //jQuery(element).attr("src", new_source);
                jQuery(element).attr("id", player_id);
            }
        }
    });
    if ((vimeoloaded == 'loaded') && (initialvimeo == 'loaded'))
        addVimeoEvents($elements);
    if ((ytloaded == 'loaded') && (initialyt == 'loaded'))
        addYoutubeEvents($elements);

    return false;
};
jQuery(document).ready(function () {
    updateVideoIframes();
});

</script>
