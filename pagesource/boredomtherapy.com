//module social-lib.js
var facebookSharePopup = function (shareid, href, pic) {
    if (typeof shareid === 'undefined') {
        shareid = 0;
        if (href){
            href = href.replace('local.','');
            href = href.replace('staging.','');
        }
    }

    href = getAppendedShareNameToAsParamInUrl(href, 'facebook');

    FB.ui({
        method: 'share',
        app_id: window.bt_fb_app_id,
        href: href,
        picture: pic
    }, function (response) {
        jQuery(cta).trigger('fb-share');
        jQuery(bt).trigger('fb-share');
        var  facebook_like_popup_status = api_dialog.facebook.facebook_popup.after_share.is_on;
        //console.log('this is beginstate: ' + facebook_like_popup_status);
        if(facebook_like_popup_status){
            document.body.classList.add("facebook_like_popup");
        }else{
            api_dialog.is_error_js = 1;
        }

    });

    if (bt.isTrackShares) {
        ga('send', 'event', 'ShareChannel', 'Facebook', bt.deviceFullName);
    }

    return true;
};

var twitterPopup = function (href, text) {
    var text = text.replace(/[^a-zA-Z0-9-_ ]/g, '');
    var replace = bt.siteId == 8 ? "_" : "";
    href = getAppendedShareNameToAsParamInUrl(href, 'twitter');
    var twitterName = bt.siteDispalyName.replace(/ /g, replace);
    if (text.length + href.length + ' via ' + twitterName.length > 140) {
        text = text.substring(0, 137 - href.length - ' via @' + twitterName.length) + '...';
    }
    if (bt.isTrackShares) {
        ga('send', 'event', 'ShareChannel', 'Twitter', bt.deviceFullName);
    }
    window.open("http://twitter.com/share?url=" + encodeURIComponent(href) + "&text=" + text + "&via=" + twitterName, "_blank", "toolbar=yes, scrollbars=yes, resizable=yes, top=90, left=400, width=550, height=420");
    return true;
};

var pinterestPopup = function (href, image_src) {
    href = getAppendedShareNameToAsParamInUrl(href, 'pinterest');
    if (bt.isTrackShares) {
        ga('send', 'event', 'ShareChannel', 'Pinterest', bt.deviceFullName);
    }
    window.open('https://www.pinterest.com/pin/create/button/?url=' + encodeURIComponent(href) + '&media=' + encodeURIComponent(image_src) + '', '_blank', 'toolbar=yes, scrollbars=yes, resizable=yes, top=90, left=400, width=550, height=420');
    return true;
};

var singleFacebookPopup = function () {
    facebookSharePopup($(this).attr('data-share-id'), window.location.href);
};
var singlePinterestPopup = function () {
    pinterestPopup(window.location.href, $("meta[property='og:image']").attr("content"));
};
var singleTwitterPopup = function () {
    twitterPopup(window.location.href, document.title);
};

