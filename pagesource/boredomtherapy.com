//module image-share-process.js
/*
 * Image share
 * */
window.share_image_process = function ($selector) {
    var $ = jQuery.noConflict();

    var share_image_hover = function () {
        h = $(this).height();
        h = (h / 2) - 85;
        //src = $(this).attr('src');
        $(this).siblings('.sharebutcont').attr('style', 'top:' + h + 'px');
        //$(this).siblings('.sharebutcont').attr('style','top:'+h+'px; left:'+h+'px');
        //$(this).siblings('.sharebutcont').attr('style','{right:'+h+'px'});
    };
    var image_load = function () {
        h = ($(this).height() / 2) - 30;
        wl = 15; //15;
        wr = $(this).width() - wl - 45;
        $(this).siblings('.galleryArrowsR').attr('style', 'top:' + h + 'px; left:' + wr + 'px');
        $(this).siblings('.galleryArrowsL').attr('style', 'top:' + h + 'px; left:' + wl + 'px');
    };

    var prevpage = false, nextpage = false;
    if (pas_feature_active('gallery')) {// Gallery mode active in pas url  /
        if ($("a.prevpage").hasClass('hidden') == false) prevpage = true; // Show prev arrow
        if ($("a.nextpage").hasClass('hidden') == false) nextpage = true; // Show next arrow
    }
    prevpage = false, nextpage = false; // Disable arrows in this version

    var img_id = 0;
    var htmlBlock = '';
    $selector = $selector || $('.post-single-content').find('p img, .wp-caption > img');
    $selector
        .each(function () {
            if($(this).parents('.big_cont_recommend').length){
                return true;
            }
            img_id++;
            h = $(this).height();
            h = (h / 2) - 85;
            image_src = $(this).attr('src');

            htmlBlock = '';
            if (prevpage) htmlBlock += '<div class="galleryArrowsL"><span class="imgArrowLeft" onclick="galleryNevigate(\'prevpage\')"></span></div>';
            if (nextpage) htmlBlock += '<div class="galleryArrowsR"><span class="imgArrowRight" onclick="galleryNevigate(\'nextpage\')"></span></div>';

            if (!bt.isMobile) {	// Disable share buttons on gallery mode
                htmlBlock += '<div class="sharebutcont" style="top:' + h + 'px">' +
                    '<span class="imgsharef" data-show-like="1" data-like-id="' + $(this).attr('data-like-id') + '" onclick="return facebookSharePopup(this)"></span>';
                if (bt_twitter_account != '') htmlBlock += '<span class="imgsharet" onclick="return twitterPopupFunction()"></span>';
                htmlBlock += '<span class="imgsharep" onclick="return pinterestPopupFunction(\'' + image_src + '\');"></span>' +
                    '</div>';
            }


            $(this).parent().append(htmlBlock);
            $(this).parent().parent().removeClass('disable_href').attr('href', '#img_id_' + img_id); // Disable show image in separate page when clicking on it
        })
        .hover(share_image_hover)
        .on('load',image_load);
};

