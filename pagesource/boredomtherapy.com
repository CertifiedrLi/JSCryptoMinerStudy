//module bt-home-slider.js
(function () {
    var slider = function () {
        var slidesBox;
        var slideTemplate;

        var renderPost = function (post) {
            return $(replaceStrs(slideTemplate, post));
        };

        var move = function (moveBy, firstLast, arrIndex, preOrApp) {
            var arr = this.slides;
            var newArr = arr.splice(moveBy);
            this.slides = newArr.concat(arr);


            slidesBox.children()[firstLast]().remove();
            this.slides[arrIndex].elem[preOrApp](slidesBox);
        };

        this.next = function () {
            move.call(this, 1, 'first', this.appendToIndex, 'appendTo');
            $(bt).trigger('slider-next');
        };
        this.prev = function () {
            move.call(this, -1, 'last', 0, 'prependTo');
            $(bt).trigger('slider-prev');
        };

        this.init = function () {
            slidesBox = this.container;
            slideTemplate = this.template;
            this.appendToIndex = this.appendToIndex || 4;

            if (this.slides) {
                this.slides.forEach((function (v, k) {
                    v.x = k === 0 ? this.slides.length - 1 : k - 1;
                    v.x = v.x !== 0 ? v.x : this.slides.length;
                    v.y = this.slides.length;
                    this.slides[k].elem = renderPost(v);
                    if (k < this.visibleSlides) {
                        this.slides[k].elem.appendTo(slidesBox);
                    }
                }).bind(this));
            }
        };
    };

    bt.slider = slider;
    $(bt).trigger('slider-init');
})();


