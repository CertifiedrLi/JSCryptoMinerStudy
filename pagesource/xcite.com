<script type="text/javascript">
prepareWidget = function(SC, render) {
	   for (var i=0; i < SC.page.products.length; i++) {
					var p = SC.page.products[i];
					//cash price for eng
					var cashprice = p.c_cashprice;
					if(typeof(cashprice) !== 'undefined' && cashprice){
						cashpriceArray = cashprice.split(' ');
						p.cashprice = cashpriceArray[0];
						p.cashprice2 = cashpriceArray[1]+' '+'KD';
					}
					
					//cashback  for eng
					p.cashback = p.c_cashback;
					var cashback = p.cashback;
					if(typeof(cashback) !== 'undefined' && cashback){
						cashbackArray = cashback.split(' ');
						p.cashback = cashbackArray[0];
						p.cashback2 = cashbackArray[1]+' '+'KD';
					}
					
				  }
	  render(SC);
	  (testFreaks = window.testFreaks || []).push(["load", "items"]);
		var thisWidget = new MT.Widget('product-widget-emr-personal-products', {
							animation: {"enable":false,"animationName":null,"animationDelay":300,"itemSelector":null},
							parallax: {"enable":false,"type":null,"overlay":null,"video":{"src":null,"volume":false},"image":{"src":null,"fit":null,"repeat":null},"file":{"poster":null,"mp4":null,"webm":null,"volume":false}},
							carousel: {"enable":true,"pagination":false,"autoPlay":false,"items":4,"singleItem":false,"lazyLoad":true,"lazyEffect":false,"addClassActive":true,"navigation":true,"navigationText":["<i class=\"fa fa-angle-left scarab-prev\"><\/i>","<i class=\"fa fa-angle-right scarab-next\"><\/i>"]},
							carouselConfig: null,
							kenburns: {"enable":false,"images":[],"overlay":null}                                });
		thisWidget.initCarousel(function(el){ this.initCarouselAnimation(el, false); this.initCountdown(); }.bind(thisWidget));
		jQuery( ".widget-wrapper" ).trigger( "click" );
		jQuery('.owl-controls').show();
};
ScarabQueue.push(["recommend", {
	logic: "PERSONAL",
	containerId: "emarsys-personal-widget-template-container",
	templateId: "emarsys-personal-widget-template",
	success: prepareWidget
}]);
</script>
    	
	
	
