<script>
    function initializeFeatureSlider(slidesRoot) {

        var slides = slidesRoot.find('ul.slides').children('li');

        slides.each(function(index){
            var span = $(this).children('span.feature-slider-image');
            var video = $('<video muted width="100%" height="100%">').attr('id', 'slideVideo' + (index + 1));
            if (span.length == 0) {
                console.warn('Cannot find metadata for slide ' + index + '! Skipped.');
                return; // continue to next slide
            }

            if (span.data('video') !== undefined) { // slide has video
                if (span.data('image') !== undefined) {
                    video.attr('poster', span.data('image'));
                    $('<img width="100%" height="100%" title="Your browser does not support the <video> tag">')
                            .attr('src', span.data('image'))
                            .appendTo(video);
                }
                $('<source>').attr('src', span.data('video')).appendTo(video);
                if (span.data('videoAlt') !== undefined) {
                    $('<source>').attr('src', span.data('videoAlt')).appendTo(video);
                }
            } else if (span.data('image') !== undefined) { // slide only has image
                // NOTE: We are intentionally using <video> here because we get the desired layout for free.
                video.attr('poster', span.data('image'));
                $('<img width="100%" height="100%">').attr('src', span.data('image')).appendTo(video);
            }

            span.append(video);
        });

        var videos = slidesRoot.find('video');

        var slideTimer;
        var slideDuration = parseInt(0);
        var animationDuration = parseInt(0);

        if (isNaN(slideDuration) || slideDuration <= 0) { slideDuration = 6000; }
        if (isNaN(animationDuration) || animationDuration < 0) { animationDuration = 0; }

        var previousIndex = function (index) {
            return (index <= 0) ? slides.length - 1 : --index;
        }

        var nextIndex = function (index) {
            return (index >= slides.length - 1) ? 0 : ++index;
        }

        var playVideo = function (video) {
            if (video.get(0).readyState >= 2) { // HAVE_CURRENT_DATA
                video.get(0).play();
            } else {
                // Video hasn't loaded enough to play yet. Try again a little later.
                setTimeout(function () { playVideo(video); }, 500);
            }
        }

        var playSlide = function (index) {
            var slide = slides.eq(index);
            var media = slide.find('span.feature-slider-image').children().first();
            if (media.length == 0) {
                console.warn('Slide ' + index + ' contains no valid media. Skipped!');
                return;
            }

            // NOTE: The slide media type is set in a data attribute. We are using <video> to display static image
            // slides because it is already laid out the exact same way as videos.
            if (slide.data('mediaType') == 'image') {
                var previousSlide = slides.eq(previousIndex(index));
                // Crossfade the incoming slide in (on top of the outgoing/previous slide), unless we don't want it
                // animated, or the incoming slide is already active (first slide ever), or it's not an image (video).
                if (animationDuration > 0 && !slide.hasClass('active-slide') && previousSlide.data('mediaType') == 'image') {
                    slide.css({opacity: 0}).removeClass('hidden-slide').addClass('active-slide');
                    slide.animate({opacity: 1}, animationDuration, 'linear', function () {
                        previousSlide.removeClass('active-slide').addClass('hidden-slide');
                        slideTimer = setTimeout(function () { playSlide(nextIndex(index)); }, slideDuration);
                    });
                } else {
                    slides.removeClass('active-slide').addClass('hidden-slide');
                    slide.removeClass('hidden-slide').addClass('active-slide');
                    slideTimer = setTimeout(function () { playSlide(nextIndex(index)); }, slideDuration);
                }
            } else if (slide.data('mediaType') == 'video') {
                slides.removeClass('active-slide').addClass('hidden-slide');
                slide.removeClass('hidden-slide').addClass('active-slide');

                // Play the next slide when this video ends. See comments for videos.bind('pause', ...) for further
                // discussion on why we are binding to the "pause" event.
                media.one('pause', function () { playSlide(nextIndex(index)); });
                playVideo(media);
            } else {
                console.warn('Cannot determine media type of slide ' + index + '! Skipped.');
                setTimeout(function () { playSlide(nextIndex(index)); }, 200); // small delay prevents quick ∞ loops
            }
        }

        // Auto-rewind videos at the end of each playback
        // NOTE: We bind to the "pause" event instead of "ended" here because of a WebKit bug, which triggers the event
        // twice and does not trigger it again until the end of the third playback. The "pause" event is a suitable
        // alternative because it is triggered when the video ends anyway.
        // https://bugs.webkit.org/show_bug.cgi?id=150348
        videos.bind('pause', function () {
            if (this.readyState >= 1) { // HAVE_METADATA
                this.currentTime = 0;
            }
        });

        // And so it begins...
        if (slides.length > 0) { playSlide(0); }

    }

    jQuery(document).ready(function() {
        var slidesRoot = $('#1845761333');
        var mediaQuery = window.matchMedia('(min-width: 981px)');
        var retireMediaQuery = function () { mediaQuery.removeListener(handleMediaQueryChange); }
        var handleMediaQueryChange = function (mediaQuery) {
            if (mediaQuery.matches) {
                initializeFeatureSlider(slidesRoot);
                retireMediaQuery();
            }
        }

        if (mediaQuery.matches) {
            // Only initialize the Feature Slider if window is wide enough (prevent loading unncessary data)
            initializeFeatureSlider(slidesRoot);
        } else {
            // Otherwise, we still listen to see if the window ever becomes wide enough
            mediaQuery.addListener(handleMediaQueryChange);
        }
    });
