String.prototype.strip = function (ssize) {
    var txt = this.toString();
    if (txt.length > ssize) {
        txt = txt.substr(0, ssize).split(" ");
        delete txt[txt.length - 1];
        txt = txt.join(" ") + '&nbsp;...';
    }
    return txt;
}

var next_dnes_page = 0;
var total_pages = 5;
var list_news_data = false;

function list_news(page_increment) {

    var id = "dnes-last-news";
    if (next_dnes_page == total_pages-1 && page_increment > 0) {
        return false;
    }
    if (next_dnes_page < total_pages-1 && page_increment > 0) {
        next_dnes_page += page_increment;
        $("#next_dnes").removeClass('inactiven');
    }
    if (next_dnes_page > 0 && page_increment < 0) {
        next_dnes_page += page_increment;
        $("#next_dnes").removeClass('inactiven');
    }
    
    if (!list_news_data) {
	$.getJSON('/new/ajax_dnes_last_news.json?q='+Math.random(), function (response) {
	    list_news_data = response;
	    draw(response);
	});
    } else {
	    draw(list_news_data);
    }
    
    function draw(response) {
        var html = '';
        if (typeof(response[next_dnes_page])!='undefined') {
			response = response[next_dnes_page];
        }
        $.each(response, function (i, item) {
            html += '<a href="http://dnes.dir.bg/news/' + item.url + '-' + item.id + '?nt=4"' + (i == 1 ? ' class="last"' : '') + '>'+ item.title + '<span>' + item.updated_time + '</span></a>';
        });
        $("#" + id).html(html);
        if (next_dnes_page>0) {
		$("#prev_dnes").removeClass('inactivep');
	} else {
		$("#prev_dnes").addClass('inactivep');
	}
	if (next_dnes_page==total_pages-1) {
		$("#next_dnes").addClass('inactiven');
	}
    }

    return false;
}


