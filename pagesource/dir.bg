<script>
function run_blocks_movement() {
    var storage = {};

    var state_data = get_var('state');
    if (state_data != false) {
        var index = state_data.indexOf("-");
        state_data = state_data.substr(index + 1);
    }


    var settings = load_settings(state_data);

    if (!settings[id1] || !settings[id2]) {
        return false;
    }

    var block_order = {
        'leftColumn':settings[id1],
        'rightColumn':settings[id2]
    };

    var adblock_pos = {
        adblock_00_b1: 0,
        adblock_00_b3: 2,
        adblock_00_b2: 1,
        adblock_00_b4: 3,
        adblock_00_auto_c: 6,
        adblock_00_fun: 9,
        adblock_00_end_c: 11
    };

    for (container in block_order) {

        var ad_blocks = [];
        var init_block_number = 0;

        $("#" + container).find(".adblock").each(function (i, item) {
    	    //$(item).html($(item).attr('id'));
            ad_blocks[i] = item;
        });

        var idx = 0;
        var adblock = false;
        if (ad_blocks.length) {
			adblock = ad_blocks.shift();
			var cadsid =$(adblock).attr('id');
			if (typeof(adblock_pos[cadsid])!='undefined') {
				init_block_number = adblock_pos[cadsid];
			}
        }

        for (block in block_order[container]) {
    	    var block_id = block_order[container][block];
    	    
    	    /* prevent from double script call after dom operations */
    	    $("#"+block_id).find("script").each(function(i, item) {
				$(item).attr("src", null);
    	    });
    	    
    	    
            if (adblock) {
				$("#"+block_id).insertBefore($(adblock));
            } else {
				$("#"+container).append($("#"+block_id));
            }
            if (idx==init_block_number) {
//				idx = 0;
				if (ad_blocks.length) {
					adblock = ad_blocks.shift();
					var cadsid =$(adblock).attr('id');
					if (typeof(adblock_pos[cadsid])!='undefined') {
						init_block_number = adblock_pos[cadsid];
					}
				} else {
					adblock = false;
				}
            } else {
//        	idx++;
            }
            
            idx++;
        }
    }
}
run_blocks_movement();
</script>

