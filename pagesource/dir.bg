function getUserInfo() {
  $.ajax({
    type: "GET",
    url: '/JSAJAX/get_user_info.php?enc=utf-8&now='+Math.random(),
    cache: false,
    async: false,
    success: function(data) { 
      var v = data.split(':');
      var username = v[0];
      var show_beta = v[1];
      var nickname = v[2] ? v[2] : v[0];
      var avatar = v[3];
      var unread = v[4];
      if (username) {
        $("#idsettings").css({ display:'block'});
        $('#usernick').html(nickname.substring(0,16));
        $('#avatar').attr('src', 'https:'+avatar);
        if(unread > 0) {
          $("#notifi").css({ display:'block'}); 
        $('#notifi').html(unread);
      } else {
        $("#notifi").css({ display:'none'});
      } 
      $('#notifi2').html(unread);
      } else {
        $("#lenta-login").css({ display:'block'});
      }
    }
  });
}
function live_news(block, imageid) {
    if (!block) {
        block = 'dnes';
    }
    var url = "/new/ajax_live_news_"+block+".json?_p=" + Math.random();
    var id = "live-" + block + "-news";
    var obj_id = "#" + id;
    
    if (block=='tv') {
		id = "tvlinks";
		obj_id = "#tvlinks";
    }

    $.getJSON(url, function (response) {
	var news_list, dnes_hot_theme = false;
	if (block=='dnes') {
		news_list = response.news;
		dnes_hot_theme = typeof(response.hot_theme.title)!='undefined' ? response.hot_theme : false;
	} else {
		news_list = response;
	}
        $.each(news_list, function (i, item) {
            var copy = $(obj_id).find("a:first").clone();
            $(copy).attr('href', item.link);
            if (typeof(item.img75)!='undefined') {
				$(copy).attr('rel', item.img75);
				$(copy).mouseover(function () {
                    change_image({href:item.link, rel:item.img75}, imageid);
                });
            }
            $(copy).html(item.title);
            $(copy).mouseover();
            $(obj_id).prepend(copy);
            var last = $(obj_id).find("a:last");
            $(last).remove();
        });
        if (dnes_hot_theme) {
    	    $('#temata').html( ( dnes_hot_theme.is_live ? '<span style="color: #f60;">На живо: </span>' : '' )+'<a href="'+dnes_hot_theme.link+'">'+dnes_hot_theme.title+'</a>');
    	    $('#last').html('<a href="'+dnes_hot_theme.last_art_link+'" style="border-bottom:none;">'+(dnes_hot_theme.last_art_time ? '<b class="orange" style="color: #F60">'+dnes_hot_theme.last_art_time+'</b> ' : '')+dnes_hot_theme.last_art_title+'</a>');
        }
        
    });
}


