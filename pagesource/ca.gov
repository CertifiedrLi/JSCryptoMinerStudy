<script>
    function showMyPages() {
        favoriteTitle.innerHTML = "My Pages"
        var modal = document.getElementById("favoritesModal");
        modal.style.display = "block";
    }

    // When the user clicks on <span> (x), close the modal
    function closeMyPages() {
        var modal = document.getElementById("favoritesModal");
        modal.style.display = "none";
    }

    // When the user clicks anywhere outside of the modal, close it
    window.onclick = function (event) {
        var modal = document.getElementById('favoritesModal');
        if (event.target == modal) {
            modal.style.display = "none";
        }
    }



    var linkTitlePairs;
    var showFavoritesLink = document.getElementById("showFavoritesLink");
    var showFavoritesLinkHtml = "<a href=\"javascript:showMyPages();\">My Pages</a>&nbsp; &nbsp;";

    $(document).ready(function () {
        //setCookie("link", '', 365);
        var addFavoritesLinkIcon = document.getElementById("addFavoritesLinkIcon");
        addFavoritesLinkIcon.className += "ca-gov-icon-plus-line";
        var addFavoritesLinkText = document.getElementById("addFavoritesLinkText");
        addFavoritesLinkText.innerHTML = "Save Page";
		showFavoritesLink.innerHTML = showFavoritesLinkHtml;
        getFavorites();
    });

    function getFavorites() {
        var name = "link";
        var value = null;
        var cookies = document.cookie.split(/\s*;\s*/);

        for (var i = 0; i < cookies.length; i++) {
            if (cookies[i].substring(0, name.length + 1) == (name + "=")) {
                value = decodeURIComponent(cookies[i].substring(name.length + 1));
                linkTitlePairs = value;
                break;
            }
        }

        if (linkTitlePairs === undefined) {
            linkTitlePairs = "";
        }

        if (linkTitlePairs.length > 0) {

            var firstColLinks = "";
            var secondColLinks = "";

            var linkTitlePair = linkTitlePairs.split("|");

            var linkDivide = Math.round((linkTitlePair.length - 1) / 2);
            var linkCount = 0;

            for (var i = 0; i < linkTitlePair.length - 1; i++) {
                linkCount = linkCount + 1;
                var linkTitleParts = linkTitlePair[i].split("^");

                if (linkCount > linkDivide) {
                    secondColLinks = secondColLinks + "<a class=\"ca-gov-icon-close-fill\" style=\"color: #D34A37;text-decoration: none; font-size: 1.2em;\" href=\"javascript:deletelink('" + linkTitleParts[0] + "^" + linkTitleParts[1] + "|');\"></a>&nbsp; &nbsp; <a class=\"favLink\" href=\"" + linkTitleParts[1] + "\">" + linkTitleParts[0] + "</a><br /><br />"

                }
                else {
                    firstColLinks = firstColLinks + "<a class=\"ca-gov-icon-close-fill\" style=\"color: #D34A37;text-decoration: none; font-size: 1.2em;\"  href=\"javascript:deletelink('" + linkTitleParts[0] + "^" + linkTitleParts[1] + "|');\"></a>&nbsp; &nbsp; <a class=\"favLink\" href=\"" + linkTitleParts[1] + "\">" + linkTitleParts[0] + "</a><br /><br />"
                }

                firstColumn.innerHTML = firstColLinks;
                secondColumn.innerHTML = secondColLinks;
            }

			showFavoritesLink.innerHTML = showFavoritesLinkHtml;

        }
        else {

            setTimeout(function () { firstColumn.innerHTML = "<p style=\"color:#FFFFFF; font-size: 1.2em;\">No Links to Pages Exist.</p>"; }, 1000);
            secondColumn.innerHTML = "";
			showFavoritesLink.innerHTML = "";
        }
    }

    function addCurrentToMyPages() {
        var addTitle = document.getElementsByTagName("title")[0].innerHTML;
        addTitle = addTitle.replace("CA.Gov : ", "");
        addTitle = addTitle.replace("&amp;", "and");
        addTitle = addTitle.replace("&amp;", "and");
        addTitle = addTitle.replace("&amp;", "and");
        addTitle = addTitle.replace("&amp;", "and");
        addTitle = addTitle.replace("&amp;", "and");
        addTitle = addTitle.replace("&", "and");
        addTitle = addTitle.replace("&", "and");
        addTitle = addTitle.replace("'", "â€™");
        addTitle = addTitle.replace("'", "â€™");
        addTitle = addTitle.replace("'", "â€™");
        addTitle = addTitle.replace("'", "â€™");
        var addUrl = window.location.href;;
        var newItem = addTitle + "^" + addUrl + "|";

        var linkString = linkTitlePairs;
        var testString = newItem;

        if (linkString.indexOf(testString) == -1) {
            var newlist = linkTitlePairs + newItem;
            setCookie("link", newlist, 365);
            getFavorites();
            showMyPages();
            setTimeout(function () { favoriteTitle.innerHTML = "My Pages<span style=\"color:#72CE6F; font-size: 0.6em;\">&nbsp; &nbsp; Added Link</span>" }, 200);
            setTimeout(function () { favoriteTitle.innerHTML = "My Pages" }, 4000);
        }
        else {
            showMyPages();
            setTimeout(function () { favoriteTitle.innerHTML = "My Pages<span style=\"color:#F3ED9D; font-size: 0.6em;\">&nbsp; &nbsp; Item Already Exists</span>" }, 200);
            setTimeout(function () { favoriteTitle.innerHTML = "My Pages" }, 4000);

        }


    }

    function deletelink(item) {
        var removeItem = item;
        var removeArray = removeItem.split("^");
        var r = confirm("Are you sure you want to delete the following link? \n\n\xA0\xA0\xA0\xA0\xA0" + removeArray[0] + "\n\n");
        if (r == true) {
            var newlist = linkTitlePairs.replace(removeItem, "")
            setCookie("link",newlist,365);
            setTimeout(getFavorites(), 500)
            setTimeout(function () { favoriteTitle.innerHTML = "My Pages<span style=\"color:#D34A37; font-size: 0.6em;\">&nbsp; &nbsp; Link Removed</span>" }, 200);
            setTimeout(function () { favoriteTitle.innerHTML = "My Pages" }, 2000);
        }
        else {
            getFavorites();
        }
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(";");
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == " ") {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

</script>
