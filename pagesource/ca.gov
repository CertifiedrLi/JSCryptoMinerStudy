    <div class="header-single-banner" role="img" aria-label="Point Pinos Monterey  | 2017 State of California " style="background-image: url('http://www.ca.gov/-/media/Global-Files/Homepage-Backgrounds/Central-Coast/Winter-Night-Point-Pinos-Monterey-GettyImages-484508880.ashx');">
        <div class="explore-invite" style="cursor:pointer;">

            <div style="float: left; color:#f5f5f5; padding-top:20px; text-shadow:Â 2px 2pxÂ #000000; cursor:default;">
                    <span id="spanCaption" style="font-size: 1.1em;">Point Pinos Monterey  </span><br />
                                
                    <span id="abbrCopy"  style="font-size:0.8em;">&copy;  2017 State of California </span>
</div>

            <script>

                var oc = "";
                var oldCity = "";
                var oldCookieInfo = "";
                var nc = "";
                var newCity = "";
                var counter = 0;
                var starttime = 0;
                var captiontext = "";
                var copyrighttext = "";
                function refreshOnCityChange() {
                    counter = counter + 1;
                    oc = document.getElementsByClassName("located-city-name");
                    OldCity = oc[0].innerHTML;
                    oldCookieInfo = getCookie('cagov__geo');
                    starttime = new Date().getTime();
                    captiontext = document.getElementById("spanCaption").innerHTML;
                    copyrighttext = document.getElementById("abbrCopy").innerHTML;
                    document.getElementById("spanCaption").innerHTML = "";
                    document.getElementById("abbrCopy").innerHTML = "";
                    locationImageLooper();
                }

                function locationImageLooper() {
                    var currenttime = new Date().getTime();
                    var milisec_diff = currenttime - starttime;
                    if (milisec_diff < 20000) {
                        var cookieLocation = getCookie('cagov__geo')
                        var strTester = (oldCookieInfo == cookieLocation);
                        if (strTester) {
                            setTimeout("locationImageLooper()", 1000);
                        }
                        else {
                            oc = document.getElementsByClassName("located-city-name");
                            OldCity = oc[0].innerHTML;
                            console.log("Old Location: " + oldCookieInfo);
                            console.log("New Location: " + cookieLocation);
                            window.location.reload();
                        }
                    }
                    else {
                        document.getElementById("spanCaption").innerHTML = captiontext;
                        document.getElementById("abbrCopy").innerHTML = copyrighttext;
                    }
