        <script>
            ( function() {
                var time_start = Date.now(),
                    time_last = time_start;

                var M = function(a) {
                    return a.reduce(function(prev, val){return prev+val;},0)/a.length;
                };

                var D = function(a) {
                    var m = M(a);

                    return Math.sqrt(M(a.map(function(val){
                        return Math.pow(val - m,2);
                    })));
                };

                var MED = function(a) {
                    var length = a.length,
                        length_2 = Math.floor(length / 2);

                    a.sort();

                    return a[length_2];
                };

                window.__CHRONOLOGY = function(title) {
                    var now = Date.now();

                    console.log('%c@ %dms (waited %dms): %s%c', 'color: #8A2BE2;', now - time_start, now - time_last, title.toUpperCase(), 'color: #000;');

                    time_last = now;
                };

                window.__STAT = function(data) {
                    return M(data) + ' ± ' + D(data) + ', median: ' + MED(data);
                };
            } )();

            __CHRONOLOGY('big bang');
        </script>

         

