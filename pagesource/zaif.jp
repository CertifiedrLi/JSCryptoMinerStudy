        $(function(){
            $(window).resize(function() {
                clearTimeout(_resize_timer);
                _resize_timer = setTimeout(function() {
                    resize();
                }, 200);
            });

            $('.btn-change-currency').on('click', function(e){
                clearCoinTimer();
                changeCurrency($(this), true);
            });

            $('#btn-ask').on('click', function(e){ changeLocation($(this)); });
            $('#btn-bid').on('click', function(e){ changeLocation($(this)); });
            $('#btn-trade').on('click', function(e){ changeLocation($(this)); });
            bindLinkPanelEvent($('#panel-trade'), $('#btn-trade'));
            bindLinkPanelEvent($('#panel-instant-ask'), $('#btn-ask'));
            bindLinkPanelEvent($('#panel-instant-bid'), $('#btn-bid'));

            getCurrencyList(true);
            resize();
        });

        function submitForm() {
            delay_submit(document.querySelector('.g-recaptcha'));
        }

        function delay_submit(btn) {
            $(btn).text("処理中");
            btn.disabled = true;
            $(btn).delay(500).queue(function(){
                btn.form.submit();
            });
        }

        var resize = function(){
            var h = document.documentElement.clientHeight;
            var navi_h = $('.navbar').height();
            $('.cover-section').height(h - navi_h);


            if ($('#img_catchphrase_xs').is(':hidden')){
                if (h < 730) {
                    $('#img_catchphrase').hide();
                } else {
                    $('#img_catchphrase').show();
                }
            }
        }

        var changeLocation = function(btn){
            var url = btn.attr('data-url')

            if (url != undefined && url != ''){
                location.href = location.protocol + '//' + location.hostname + url;
            }
        }

        var clearCoinTimer = function(){
            clearInterval(_coin_timer);
        }

        var clearPickUp = function($panels){
            $panels.each(function(index, element){
                var $btn = $(element).children('li').eq(0).children('div');
                $btn.removeClass('pickup');
             });
        };

        function getCurrencyList(is_change_currency){
            clearCoinTimer();

            var params = {
                url: location.protocol + '//json.zaif.jp/top_page/price.json',
                method: "GET",
                addParam: '?rel=' + getUnixTimeStamp(),
            };

            var settingInfo = {
                                'is_change_currency': is_change_currency,
                              };

            xmlJsonRequest(params, settingInfo, callBack_trade);
        };

        var changeCurrency = function($panel, is_change_currency){
            var $panel_lines = $panel.children('li');
            var index_name = $panel.attr('data-coin-name');
            var settingInfo = _currencyInfo[index_name];

            settingInfo['currency_pair'] = index_name;
            settingInfo['panel_lines'] = $panel_lines;

            if(is_change_currency){
                clearPickUp($('.btn-change-currency'));
                setCurrencyName($panel_lines, settingInfo);

                initLinkAreaItems($('#price-ask'), $('#arrow-ask'), $('#btn-ask'), $('#panel-instant-ask'), settingInfo, 'instant_url');
                initLinkAreaItems($('#price-bid'), $('#arrow-bid'), $('#btn-bid'), $('#panel-instant-bid'), settingInfo, 'instant_url');
                initLinkAreaItems($('#price-trade'), $('#arrow-trade'), $('#btn-trade'), null, settingInfo, 'trade_url');

                $panel_lines.eq(0).children('div').addClass('pickup');
            }

            if (settingInfo.instant_url != undefined){
                $('#panel-instant-ask').removeClass('no-instant hidden-xs');
                $('#panel-instant-bid').removeClass('no-instant hidden-xs');

                bindLinkPanelEvent($('#panel-instant-ask'), $('#btn-ask'));
                bindLinkPanelEvent($('#panel-instant-bid'), $('#btn-bid'));

                if($('#price-ask').text() != _instant_exchange[index_name].ask){
                    $('#price-ask').fadeOut(0).text(_instant_exchange[index_name].ask).fadeIn(500);
                }

                if($('#price-bid').text() != _instant_exchange[index_name].bid){
                    $('#price-bid').fadeOut(0).text(_instant_exchange[index_name].bid).fadeIn(500);
                }
            }

            setTradeLinkArea($panel_lines, settingInfo.trade_url);
        }

        var bindLinkPanelEvent = function($panel, $btn){
            $panel
                .on('click', function(e){ changeLocation($btn); })
                .hover(function() {
                        $(this).css('cursor', 'pointer');
                    }, function() {
                        $(this).css('cursor', 'default');

                });
        }

        var setCurrencyName = function($panel_lines, settingInfo){
            var currencyInfo = { name : ($panel_lines.eq(1).text().split('/'))[0],
                                 pair_name: $panel_lines.eq(1).text()}

            if($('#currency_name_trade').text() != currencyInfo.pair_name){
                if(settingInfo.formal_name != undefined){ currencyInfo.name = settingInfo.formal_name}
                if(settingInfo.formal_pair_name != undefined){ currencyInfo.pair_name = settingInfo.formal_pair_name}
                // ToDo
                //currencyInfo = changeFormalName(currencyInfo);
                $('#currency_name_trade').fadeOut(0).text(currencyInfo.pair_name).fadeIn(500);

                $('#icon-trade').html('')
                                .append('<img src="' + settingInfo.icon_src + '"></img>').fadeIn(500);

                if(currencyInfo.name){

                    $('#currency_name_ask').fadeOut(0).text(("{0}を買う").replace("{0}", currencyInfo.name)).fadeIn(500);
                    $('#currency_name_bid').fadeOut(0).text(("{0}を売る").replace("{0}", currencyInfo.name)).fadeIn(500);

                    $('#icon-instant-ask').html('');
                    if(currencyInfo.name){ $('#icon-instant-ask').append('<img src="' + settingInfo.icon_src + '"></img>')};

                    $('#icon-instant-bid').html('');
                    if(currencyInfo.name){ $('#icon-instant-bid').append('<img src="' + settingInfo.icon_src + '"></img>')};

                    $('#panel-instant-ask').fadeIn(500);
                    $('#panel-instant-bid').fadeIn(500);
                } else {
                    $('#panel-instant-ask').fadeOut(0);
                    $('#panel-instant-bid').fadeOut(0);
                }
            }
        }

        var changeFormalName = function(currencyInfo){
            if ( currencyInfo.name == 'PEPE') {
                currencyInfo.name = 'PEPECASH'
                currencyInfo.pair_name = 'PEPECASH/JPY'
            }else if ( currencyInfo.name == 'CMS:E') {
                currencyInfo.name = 'CMS:ETH'
                currencyInfo.pair_name = 'CMS:ETH/JPY'
            }
            else if ( currencyInfo.name == 'CMS:X') {
                currencyInfo.name = 'CMS:XEM'
                currencyInfo.pair_name = 'CMS:XEM/JPY'
            }
            else if ( currencyInfo.name == 'AirFX') {
                currencyInfo.name = '';
            }
            else if ( currencyInfo.name == 'Futures') {
                currencyInfo.name = '';
                currencyInfo.pair_name = '先物'
            }
            return currencyInfo;
        }

        var initLinkAreaItems = function($price, $arrow, $btn, $box_panel, settingInfo, url_get_name){
            $price.text('   -   ');
            $arrow.fadeOut(0).removeClass('text-goes-up text-danger').fadeOut(0);

            $btn.addClass('disabled').attr('data-url', '').disabled = true;
            if(settingInfo[url_get_name]){
                $btn.removeClass('disabled').attr('data-url', settingInfo[url_get_name]).disabled = false;
            }

            if($box_panel != null){
                $box_panel.addClass('no-instant hidden-xs')
                          .off('mouseenter mouseleave');
            }
        };

        var setTradeLinkArea = function($panel_lines, trade_url){
            if (trade_url == undefined) return;

            var $price_line = $panel_lines.eq(2);
            var $arrow_line  = $panel_lines.eq(3);

            if( $('#price-trade').text() != $price_line.text()){
                $('#price-trade').fadeOut(0)
                                 .text($price_line.text())
                                 .fadeIn(500);

                if ($('.link-area').is(':hidden')) $('.link-area').fadeIn(500);
            }
            _coin_timer = setTimeout('getCurrencyList(false)',_coin_timer_interval);
        }

        var callBack_instant = function(data, settinginfo){
            var price_ask = data[_redis_colname_list[settinginfo.currency_pair]['ask-col-name']];
            var price_bid = data[_redis_colname_list[settinginfo.currency_pair]['bid-col-name']];
            var sell_close = data.sell_close;
            var buy_close = data.buy_close;

            if($('#price-ask').text() != add3Digit(price_ask)){
                $('#price-ask').fadeOut(0).text(add3Digit(price_ask)).fadeIn(500);
                exchangeUpdateArrow($('#arrow-ask'), price_ask, sell_close, false);
            }

            if($('#price-bid').text() != add3Digit(price_bid)){
                $('#price-bid').fadeOut(0).text(add3Digit(price_bid)).fadeIn(500);
                exchangeUpdateArrow($('#arrow-bid'), price_bid, buy_close, false);
            }

            setTradeLinkArea(settinginfo.panel_lines, settinginfo.trade_url);
        };

        var callBack_trade = function(data, settinginfo){
            var noData = Object.keys(data).length === 0;
            var length = $('ul.btn-change-currency').length;

            if(noData){
            } else {
                if (length > 0){
                    for (var i=0 ; i<length ; i++){
                        var $panel = $('ul.btn-change-currency').eq(i);
                        var $panel_lines = $panel.children('li');
                        var $panel_parent = $panel.parent('div');
                        var coin_name = $panel.data('coin-name');
                        var unit_point =  _currencyInfo[coin_name].unit_point ? _currencyInfo[coin_name].unit_point : 0;

                        if (data['instant_exchange']){
                            _instant_exchange['btc_jpy'].ask =  exchangeDisplayPrice(data['instant_exchange'].btc_ask, unit_point)
                            _instant_exchange['btc_jpy'].bid =  exchangeDisplayPrice(data['instant_exchange'].btc_bid, unit_point)
                            _instant_exchange['mona_jpy'].ask =  exchangeDisplayPrice(data['instant_exchange'].mona_ask, unit_point)
                            _instant_exchange['mona_jpy'].bid =  exchangeDisplayPrice(data['instant_exchange'].mona_bid, unit_point)
                        }

                        if(coin_name &&  data[coin_name]
                            && $panel_lines.eq(2).text() != exchangeDisplayPrice(data[coin_name].last_price, unit_point)){
                            exchangeUpdatePrice($panel_lines.eq(2),
                                                $panel_lines.eq(3),
                                                data[coin_name].last_price,
                                                data[coin_name].chart_price,
                                                unit_point);

                            exchangeUpdateArrow($panel_lines.eq(3),
                                                       data[coin_name].last_price,
                                                       data[coin_name].chart_price,
                                                       true);

                            var default_color = changeColorHex($panel_parent.css("background-color"));
                            var set_color = changeColorHex(data[coin_name].action == "bid" ? "rgba(66, 165, 245, 0.4)" : "rgba(171, 71, 188, 0.3)");
                            $panel_parent.animate({ backgroundColor: set_color }, { duration: 600, easing: 'swing', })
                                         .animate({ backgroundColor: default_color }, { duration: 200, easing: 'swing', });
                        }
                    }
                }
                var activePanel = $('.pickup').closest('ul');
                changeCurrency(activePanel, settinginfo.is_change_currency);

                if ($('.currency-list-area').is(':hidden')){
                    $('.currency-list-area').fadeIn();
                }
            }
        }

        var xmlJsonRequest = function(params, callback_setting_info, callBack){
            if( window.XMLHttpRequest ){
                var xhr = new XMLHttpRequest();
                xhr.open( params.method, params.url + params.addParam, true );
                xhr.responseType = 'json';
                xhr.onreadystatechange = function(){
                    if( xhr.readyState == 4 && xhr.status == 200 ){
                        callBack( xhr.response, callback_setting_info );
                    }
                }

                xhr.send( null );
            }
        }

        var getUnixTimeStamp = function(){
            var date = new Date();
            return date.getTime();
        };

        var exchangeUpdateArrow = function($arrow, price, close, withBreak) {
            try {
                var p1_ = new BigDecimal(String(price));
                var p2_ = new BigDecimal(String(close));
                var is_down = p2_.compareTo(p1_) === -1;

                var per = exchangeUpdateChanges(price, close, 2);
                var arrow_class = is_down ? 'glyphicon-arrow-up' : 'glyphicon-arrow-down';
                var arrow_html = '<span class="glyphicon ' + arrow_class + '" aria-hidden="true"></span>'
                if(withBreak)
                    arrow_html += '<br class="visible-xs-block">';

                $arrow.removeClass('text-goes-up text-danger')
                      .fadeOut(0)
                      .html(arrow_html + per)
                      .addClass(is_down ? 'text-goes-up' : 'text-danger')
                      .fadeIn(500);

            } catch (e) {
                $arrow.fadeOut(0);
            }
        };

        var exchangeUpdateChanges = function(price, close, scale) {
            var price_ = new BigDecimal(String(price));
            var close_ = new BigDecimal(String(close));
            var is_down = price_.compareTo(close_) === -1;

            var change_ = price_.subtract(close_);
            var value = change_.setScale(scale, BigDecimal.prototype.ROUND_HALF_UP);
            if (!is_down) value = '+' + value;
            var per = change_.multiply(new BigDecimal(String(100))).divide(price_, 2, BigDecimal.prototype.ROUND_HALF_UP);
            per = Math.abs(per)

            return per + '%';
        };

        var exchangeUpdatePrice = function($price, $arrow, price, close, unit_point) {
            try {
                var p1_ = new BigDecimal(String(price));
                var p2_ = new BigDecimal(String(close));
                var is_down = p2_.compareTo(p1_) === -1;

                price = exchangeDisplayPrice(price, unit_point);

                $price.removeClass('text-goes-up text-danger')
                      .fadeOut(0)
                      .html(price)
                      .addClass(is_down ? 'text-goes-up' : 'text-danger')
                      .fadeIn(500);


            } catch (e) {
                $arrow.fadeOut(0);
            }
        };

        var exchangeDisplayPrice = function(price, unit_point){
            var p1_ = new BigDecimal(String(price));
            if(unit_point === 0){
                price = Math.floor(price);
            } else {
                price = p1_.setScale(unit_point, BigDecimal.prototype.ROUND_DOWN);
            }
            price = add3Digit(price);

            return price;
        }

        var add3Digit = function(price){
            var num = new String(price).replace(/,/g, "");
            while(num != (num = num.replace(/^(-?\d+)(\d{3})/, "$1,$2")));
            return num;
        }

        var changeColorHex = function(rgba){
            var parts = rgba.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/);
            if ( parts != null && parts.length == 4 )
            {
                delete ( parts[ 0 ] );
                for ( var i = 1 ; i <= 3 ; ++i )
                {
                    parts[ i ] = parseInt(parts[ i ]).toString(16);
                    if ( parts[ i ].length == 1 )
                        parts[ i ] = '0' + parts[ i ];
                }
                rgba = "#" + parts.join( "" );
            }
            return rgba
        }
    </script>




