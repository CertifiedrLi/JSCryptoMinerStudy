    objbutton.click();
	  
	  //內頁左選單item highlight
	  var bdd=$(".breadcrumb").text().replace(/[\r\n]/g,"");
    var sarr=bdd.split('>');
    var i=2;
    $("menu.aside-menu li a").each(function(){
      var s=($(this).text()).replace(/(^[\s]*)|([\s]*$)/g, "");
      if (i<sarr.length && i==2) {
        var q=sarr[i].replace(/(^[\s]*)|([\s]*$)/g, "");
        if (s==q) { 
          $(this).addClass("selected");
          i++;
        }
      }
      else if (i<=sarr.length && i > 2) {
        var q=sarr[i].replace(/(^[\s]*)|([\s]*$)/g, "");
        if (s==q) { 
          //open level 
          var par=$(this).parents('menu .aside-menu-sub');
          if($(par).attr('class') == 'aside-menu-sub'){
            $(par).attr('style','display: block;');
          }
          
          var a= $(this).html();
        	var b= "<span class=\"selected\">"+a+"</span>";
          $(this).html(b);
          i++;
        }
      }
      if (i>=sarr.length) { return false; }
    });
})
</script>
