<script>

	function getParameterByName(name) {
		name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
		var regex = new RegExp("[\\?&]" + name + "=([^&#]*)");
		var results = regex.exec(location.search);
		return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	}

	String.prototype.getParamValue = function() {
		return getParameterByName(this);
	};

	var pageDate = (new Date()).getDate();
	if( localStorage.getItem("pageDate") != pageDate ){
		localStorage.setItem("pageCount", 1 );
		localStorage.setItem("skinzCount", 0 );
		localStorage.setItem("advCount", 0 );		
		localStorage.setItem("udmCount", 0 );		
		localStorage.setItem("pageDate", pageDate );
		localStorage.removeItem("utm_source");
		localStorage.removeItem("utm_medium");
		localStorage.removeItem("utm_term");
	}
	else{
		localStorage.setItem("pageCount", 1 + parseInt(localStorage.getItem("pageCount")) );
	}	

	if( localStorage.utm_source == "" || localStorage.utm_source == undefined){
		localStorage.utm_source = getParameterByName("utm_source");
	}
	if( localStorage.utm_medium == "" || localStorage.utm_medium == undefined){
		localStorage.utm_medium = getParameterByName("utm_medium");
	}
	if( localStorage.utm_term == "" || localStorage.utm_term == undefined){
		localStorage.utm_term = getParameterByName("utm_term");
	}

	
