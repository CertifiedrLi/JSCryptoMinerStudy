	new window.Gather();
		function checkOtherSessions()
	{
		if (parseInt(TvrainAnalytics.data.user_id) == 0 || isNaN(parseInt(TvrainAnalytics.data.user_id)))
		{
			setTimeout(checkOtherSessions, 1000)
			return
		}
		wsq.send({"type":"check_simult_other_ip_uid"}, function(data) {
			if (data.cnt > 0)
			{
				var tpl_dialog = _.template($('#tpl_check_simult_other_ip_uid').html());
				window.close_other_sessions = function() {
					var data = {
						type: 'close_other_sessions',
						data: {
							location: location.pathname,
							user_id: TvrainAnalytics.data.user_id,
							hash_session: TvrainAnalytics.data.hash_session
						}
					}
					wsq.send(data, function(data) {
						console.log(data)
					})
				}
				// $('#dialog_check_simult_other_ip_uid').html(tpl_dialog({cnt:data.cnt})).modal();

				close_other_sessions()

				var data = {
					type: "event",
					data: {
						category: 'similar_view',
						action: 'dialog',
						label: 'site/index',
						value: data.cnt,
						user_id: TvrainAnalytics.data.user_id,
						hash_view: TvrainAnalytics.data.hash_view,
						hash_long: TvrainAnalytics.data.hash_long,
						hash_session: TvrainAnalytics.data.hash_session
					}
				};
				wsq.send(data);
			}

			if (data.cnt == -1)
			{
				setTimeout(checkOtherSessions, 1000)
			}
		})
	}
</script>
