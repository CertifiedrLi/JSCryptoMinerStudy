<script type="text/javascript">
  // suggest
  var request;
  var previousQuery;
  var searchInput = $('.searchText');
  var container = $('.suggestion-list');
  var suggest = function(e) {
    if (e.keyCode === 40 || e.keyCode === 9) {
      if (!request) {
        setTimeout(function() {
          $('.suggestion-option:first').focus();
        }, 1);
      }

      e.preventDefault();
    } else if (previousQuery !== e.target.value) {
      previousQuery = e.target.value;
      setTimeout(function() {
        if (request) {
          request.abort();
        }

        request = $.ajax('/complete?query=' + e.target.value).always(function() {
          request = null;
        });
      }, 1);
    }
  };

  searchInput.on('focus', suggest);
  searchInput.on('keydown', suggest);
  searchInput.on('blur', function(e) {
    setTimeout(function() {
      if ($('.suggestion-option:focus').length === 0) {
        form.removeClass('open');
      }
    }, 1);
  });

  container.on('keydown', function(e) {
    if (e.keyCode === 38 || (e.keyCode === 9 && e.shiftKey)) { // ↓ or ShiftTab
      e.preventDefault();
      if ($('.suggestion-option:first').text() === $('.suggestion-option:focus').text()) {
        setTimeout(function() {
          searchInput.focus();
        }, 1);
      }
      $('.suggestion-option:focus').parent().prev().children().focus();
    } else if (e.keyCode === 9) {
      // Tab
      e.preventDefault();
      setTimeout(function() {
        $('.suggestion-option:focus').parent().next().children().focus();
      }, 1);
    }
  });
</script>

