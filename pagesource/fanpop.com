fanpop.hottest_image_thumbnails.reload_lowres_thumbs = fanpop.hottest_image_thumbnails.reload_lowres_thumbs || function() {
	$('.hottest_thumb_div .thumb_block_img.reload_thumb').each( function(){
			$(this).css('background-image', 'url('+$(this).attr('data-hires-thumb')+')');
			$(this).removeClass('reload_thumb');
	});
};

fanpop.hottest_image_thumbnails.infinite_scroll = true;


$(function(){
		$(window).bind("scroll.hottest_images", function() {
				var wintop = $(window).scrollTop();
				var winbottom = wintop + $(window).height();
				var more_elem = $('#mainCol .hottest_thumb_div .more_thumbs_link');
				if (more_elem.length > 0) {
					var moretop = more_elem.offset().top;
					var morebottom = moretop + more_elem.height();
					var triggerpoint = -100;

					if ((morebottom <= winbottom) && (moretop >= wintop)) {
						more_elem.addClass('show');
					} else {
						more_elem.removeClass('show');
					}
					if (typeof window.fanpop !== "undefined" && typeof window.fanpop.hottest_image_thumbnails !== "undefined" && typeof window.fanpop.hottest_image_thumbnails.infinite_scroll !== "undefined" && window.fanpop.hottest_image_thumbnails.infinite_scroll == true) {
						if (moretop+triggerpoint < winbottom) {
							var more_elem_a_tag = more_elem.children('a');
							var destination_url = more_elem_a_tag.attr('data-url')
							if (typeof destination_url !== "undefined") {
								//more_elem.removeClass('show');
								if (!more_elem_a_tag.hasClass('triggered')) {
									if (typeof window.fanpop.hottest_image_thumbnails.infinite_scroll_count === "undefined") {
										window.fanpop.hottest_image_thumbnails.infinite_scroll_count = 1;
									} else {
										window.fanpop.hottest_image_thumbnails.infinite_scroll_count++;
