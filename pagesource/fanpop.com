					padding: 8px 16px;
					border: 3px solid #FFFFFF;
					border-top-left-radius: 5px;
					border-top-right-radius: 5px;
					border-bottom: none;
					background-color: rgba(48,0,79,0.7);
					color: #FFFFFF;
					cursor:pointer;
					transition: 1s;
				}
				
				#converse_login_button.show {
					bottom: 0;
				}
			</style>
			<div id="chat_ajax_response" style="display:none"></div>
			
			<script type="text/javascript">
				window.fanpop = window.fanpop || {};
				window.fanpop.chat = window.fanpop.chat || {};
				window.fanpop.chat.jid = '';
				window.fanpop.chat.sid = '';
				window.fanpop.chat.rid = '';
				window.fanpop.chat.windowState = 'focus';
				window.fanpop.chat.converse_loaded_once = false;
				window.fanpop.chat.relogin = false;
				window.fanpop.chat.converse_params = {
				};
				window.fanpop.chat.avatars = window.fanpop.chat.avatars || {};
				
				window.fanpop.chat.auto_open_muc = true;
				
				
				$('#converse_login_button').remove();
				$('body').append('<div id="converse_login_button">Start Chat</div>');
				$('#converse_login_button').on('click.login', function() {
						$(this).removeClass('show');
						$.ajax({
								url: '/chat/ajax_set_disconnect',
								data: { status: 'loggedin' }
						}).done(function() {
								//window.fanpop.chat.launch_converse();
								if (window.fanpop.chat.converse_loaded_once==false) {
									window.fanpop.chat.initialize_converse();
								} else {
									window.fanpop.chat.relogin = true;
									converse.account.relogin();
								}
						});
				});

				window.fanpop.chat.start_converse = function() {
					$.ajaxSetup({cache: true});
					jQuery.getScript('/javascripts/conversejs/converse.release-0.10.1.min.js', function() {
							$.ajaxSetup({cache: true});
							jQuery.getScript('/javascripts/conversejs/converse-overrides.js', function() {
									
									window.fanpop.chat.initialize_converse();
									
							});
							$.ajaxSetup({cache: false});
					});
					$.ajaxSetup({cache: false});
				};
				window.fanpop.chat.initialize_converse = function(relogin) {
					
					require(['converse'], function (converse) {
							if (typeof relogin === "undefined" || relogin == false) {
								converse.listen.on('initialized', function(event) {
										if (window.fanpop.chat.relogin == true) {
											window.fanpop.chat.relogin = false;
											$('#conversejs #controlbox').css('display', 'block');
										}
										$('#conversejs #chatrooms input#show-rooms').click();
										if (typeof window.fanpop.chat.auto_open_muc !== "undefined" && window.fanpop.chat.auto_open_muc == true) {
											converse.rooms.open('global-chat@conference.fanpop.com');
											
												$(document).on('focus', '.chat-textarea', function(e){
														$(this).attr('readonly', 'readonly');
												});
											
										}
								});
							}
							
							window.fanpop.chat.converse_settings = {
								bosh_service_url: 'https://imlegend.fanpop.com:5281/http-bind',
								auto_login:true,
								authentication: 'prebind',
								
								play_sounds:true,
								allow_otr:false,
								auto_away:0,
								auto_xa:0,
								allow_contact_requests: false,
								allow_contact_removal: false,
								show_controlbox_by_default:true,
								message_archiving:'always',
								hide_offline_users:true,
								hide_muc_server: true,
								
								jid: window.fanpop.chat.jid,
								sid: window.fanpop.chat.sid,
								rid: window.fanpop.chat.rid,
							};
							
							converse.initialize(window.fanpop.chat.converse_settings);
							window.fanpop.chat.converse_loaded_once = true;
							
							if (typeof relogin === "undefined" || relogin == false) {
								converse.listen.on('message', function (event, xml) {
										if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) {
											console.log('message received:\n'+xml.outerHTML);
										}
										if (window.fanpop.chat.windowState == "focus") return;
										var from_array = xml.getAttribute('from').split(/[@\/]/);
										var from = from_array[0];
										var from_string = "from "+from;
										if (from_array[1] == "conference.fanpop.com") {
											if (typeof from_array[2] !== "undefined" && from_array[2] != null)
											from = from_array[2];
											from_string = "from " + from + " in "+from_array[0];
										}
										
										var body = '';
										$(xml).find('body').each(function() {
												if ($(this).text() != '') {
													body += $(this).text() + "\n";
												}
										});
										if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) {
											console.log('from: '+JSON.stringify(from_array));
											console.log('body: '+body);
										}
										if (body != '') {
											var icon = window.fanpop.chat.avatars[from];
											if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) { console.log("noty icon: "+icon); }
											if (typeof icon === "undefined") {
												var failsafe = window.setTimeout(function() {
														failsafe = null;
														if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) { console.log('noty: failsafe'); }
														if (typeof Notify === "function") {
															Notify(from_string, body, icon);
														}
												}, 500);
												window.fanpop.chat.load_avatars($('#conversejs'), from, function() {
													if (typeof failsafe !== "undefined" && failsafe != null) {
														if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) { console.log('noty: failsafe not run; use icon!'); }
														window.clearTimeout(failsafe);
														icon = window.fanpop.chat.avatars[from];
														if (typeof Notify === "function") {
															Notify(from_string, body, icon);
														}
													}
												});
											} else if (icon == "pending") {
												var failsafe = window.setTimeout(function() {
														failsafe = null;
														if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) { console.log('noty: failsafe'); }
														if (typeof Notify === "function") {
															Notify(from_string, body, icon);
														}
												}, 500);
												var polling = window.setInterval(function() {
														if (typeof failsafe !== "undefined" && failsafe != null) {
															icon = window.fanpop.chat.avatars[from];
															if (typeof icon !== "undefined" && icon != "pending") {
																window.clearTimeout(failsafe);
																window.clearInterval(polling);
																if (typeof Notify === "function") {
																	Notify(from_string, body, icon);
																}
															}
														} else {
															window.clearInterval(polling);
														}
												}, 50);
												window.fanpop.chat.load_avatars($('#conversejs'), from, function() {
													if (typeof failsafe !== "undefined" && failsafe != null) {
														if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) { console.log('noty: failsafe not run; use icon!'); }
														window.clearTimeout(failsafe);
														icon = window.fanpop.chat.avatars[from];
														if (typeof Notify === "function") {
															Notify(from_string, body, icon);
														}
													}
												});
											} else {
												if (typeof window.fanpop.chat.debug !== "undefined" && window.fanpop.chat.debug == true ) { console.log('noty: already have icon'); }
												if (typeof Notify === "function") {
													Notify(from_string, body, icon);
												}
											}
										}
								});
								
								converse.listen.on('statusChanged', function(event, message) {
										if (message == "logout") {
											$('#conversejs').css('display', 'none');
											$.ajax({
													url: '/chat/ajax_set_disconnect',
													data: { status: 'offline' }
											}).done(function() {
												// Do not show "Start Chat" button because there is no login function yet
												$('#converse_login_button').addClass('show');
											});
										}
								});
							}
							
					});
					
				};
				
				
				
				window.fanpop.chat.load_avatars = function(scope, id, callback) {
					window.fanpop.chat.avatars = window.fanpop.chat.avatars || {};
					if (typeof scope === "undefined") {
						scope = $('#conversejs');
					}
					if (typeof id === "undefined") {
						var avatars_need_loading = $('.fanpopavatar.preload', scope);
						avatars_need_loading.each(function() {
								var user_id = $(this).attr('data-id');
								if (typeof user_id === "undefined" || user_id == "") {
									// avatar info is missing, destroy the avatar placeholder div
									$(this).remove();
								} else {
									if (typeof window.fanpop.chat.avatars[user_id] === "undefined") {
										// no avatar; use ajax action to load
										window.fanpop.chat.avatars[user_id] = "pending";
										$.ajax('/chat/ajax_fetch_user_icon/'+user_id).done(function(data) {
												window.fanpop.chat.avatars[user_id]=data;
												window.fanpop.chat.load_avatars($('#conversejs'), user_id);
												if (typeof callback === "function") {
													callback();
												}
										});
									} else if (window.fanpop.chat.avatars[user_id] == "pending") {
										// avatar is being loaded via ajax; skip
									} else {
										// avatar info exists; load it
										window.fanpop.chat.load_avatars($('#conversejs'), user_id);
									}
								}
						});
					} else {
						var avatar = window.fanpop.chat.avatars[id];
						if (typeof avatar !== "undefined" && avatar != "pending") {
							var avatars_need_loading = $('.fanpopavatar[data-id="'+id+'"]', scope);
							var background_image = "url("+avatar+")";
							avatars_need_loading.each(function() {
									var bg_img = $(this).css('background_image');
									if (bg_img == background_image || bg_img == background_image+";") {
										// this particular avatar is already loaded, do nothing
									} else {
										$(this).css('background-image', background_image).removeClass('preload');
									}
							});
						}
					}
				};
				
				$(window).on("blur focus", function (ev) {
						window.fanpop.chat.windowState = ev.type;
						if (typeof converse !== "undefined" && typeof converse.settings !== "undefined") {
							if (window.fanpop.chat.windowState == "blur") {
								converse.settings.set('play_sounds', true);
							} else {
								converse.settings.set('play_sounds', false);
							}
						}
				});
				
				

				$('#chat_ajax_response').load("/chat/ajax_start_conversejs");
			</script>
		
			<div class="login_lb chat">
				<div class="lb">
					<div class="login">
						Login or Join Fanpop to Chat
						<div class="fb_login"><div class="fb_button">Login with Facebook</div></div>
						OR
						<div class="fp_join"><div class="fp_join_button">Join Fanpop - It's Free!</div></div>
						<div class="fp_login">Already have an account?<br />Sign In to Fanpop</div>
					</div>
				</div>
			</div>
			<script type="text/javascript">
			$(document).on('click.chat_login', '.sendXMPPMessage', function(e) {
					e.preventDefault();
					if (!$('.login_lb.chat').hasClass('placed')) {
						$('body').append($('.login_lb.chat').addClass('placed'));
					}
					$('.login_lb.chat').addClass('show');
					if (typeof ga_event !== "undefined") { ga_event('!public_chat', 'show_login', 'Login Lightbox Shown (Group Chat Click)'); }
			});
			$('.login_lb.chat').bind('click', function(e) {
					if ($('.lb', e.target).length > 0) {
						e.preventDefault();
						$('.login_lb.chat').removeClass('show');
						if (typeof ga_event !== "undefined") { ga_event('!public_chat', 'hide_login', 'Login Lightbox Closed (no login)'); }
					}
			});
			$('.login_lb.chat .lb .fb_login').bind('click.fb_login', function(e) {
					FB.login(function(response) {
						if (response.status === 'connected') {
							$('.login_lb.chat .lb').empty().append('<div id="LoginBox"></div>');
							ajax('/login/ajax_login_facebook_user_2011', $('#LoginBox'));
						// Logged into your app and Facebook.
						} else if (response.status === 'not_authorized') {
						// The person is logged into Facebook, but not your app.
						} else {
						// The person is not logged into Facebook, so we're not sure if
						// they are logged into this app or not.
						}
					}, {scope: 'public_profile,email,user_friends'});
					if (typeof ga_event !== "undefined") { ga_event('!public_chat', 'facebook_login', 'Facebook Login Opened'); }
			});
			$('.login_lb.chat .lb .fp_login').bind('click.fp_login', function(e) {
					$('.login_lb .lb .login').empty().append('<div>Please wait...</div>');
					if (typeof ga_event !== "undefined") { ga_event('!public_chat', 'fanpop_login', 'Fanpop Login Opened'); }
					window.location = '/login';
			});
			$('.login_lb.chat .lb .fp_join').bind('click.fp_join', function(e) {
					$('.login_lb.chat .lb .login').empty().append('<div>Please wait...</div>');
					if (typeof ga_event !== "undefined") { ga_event('!public_chat', 'fanpop_registration', 'Fanpop Registration Opened'); }
					window.location = '/register';
			});
			</script>
		
	


		


		



		

