<script type="text/javascript">
	var BANNER_COOKIE_NAME = "bannerlist_cookie";

	var utag_data = utag_data || {};

	$(function() {
		createNextBannerItemsCookieValue();
	});

	function getTealiumBannerInfo() {
		var ids = [], names = [], items$ = $(".tealium-banner .item"), info = {};

		if (items$.length === 0) {
			return;
		}

		items$.each(function(index) {
			var id = $(this).data("bannerId"), name = $(this).data("bannerName");

			ids.push("P" + (index + 1) + "-" + id);
			names.push("P" + (index + 1) + "-" + name);
		});

		info.Banner_id = ids.join("|");
		info.Banner_name = names.join("|");

		return info;
	}

	function pushBannersToTealium() {
		var info = getTealiumBannerInfo();

		if (info) {
			utag_data.Banner_id = info.Banner_id;
			utag_data.Banner_name = info.Banner_name;
		}
	}

	function tapOrClickForLocation(item, url, type) {
		if (navigator.userAgent.match(/iPad/i)) {
			item.hammer().on("tap", function() {
				if (type == "window") {
					window.open(url);
				} else {
					location.href = url;
				}
			});
		} else {
			if (type == "window") {
				item.attr("onclick", "window.open('" + url + "')");
			} else {
				item.attr("onclick", "location.href='" + url + "'");
			}
		}
	}

	function bannerSorter(x, y) {
		var valueX = $(x).attr('data-item-order') + '' + $(x).data('banner-id');
		var valueY = $(y).attr('data-item-order') + '' + $(y).data('banner-id');

		if (valueX < valueY) {
			return -1;
		} else if (valueX > valueY) {
			return 1;
		}
		return 0;
	}

	function filterBannerItemsWithCookie() {
		var bannerList = $("#carousel-hero .item");

		// Segmente ozel bannerklar onde gorunmesi saglandi
		var userAltSegment = $.cookie('customer_segment_ALTS');
		if (userAltSegment) {
			$.each(bannerList, function(index, element) {
				$element = $(element);
				if ($element.data('segments').search(new RegExp(userAltSegment, "i")) > -1) {
					$element.attr('data-item-order', '0' + $element.attr('data-item-order'));
				}
			});
		}

		bannerList.parent().html(bannerList.get().sort(bannerSorter));

		// Bannerların order degerleri bir listeye unique olarak atiliyor
		var orderArray = $.map(bannerList, function(element, index) {
			return $(element).attr('data-item-order');
		});
		orderArray = $.unique(orderArray);

		// Bannerda bulunan cookie listesi aliniyor
		var bannerCookie = [];
		try {
			bannerCookie = $.cookie(BANNER_COOKIE_NAME).split(',');
			// Cookie deki banner sayisi ile su anki banner sayisi tutmuyorsa cookiedeki banner listesi ignore ediliyor
			if (bannerCookie.length != orderArray.length) {
				bannerCookie = [];
			}
		} catch (e) {
		}

		// Bannerda cift bulunan bannerlar cookiedeki listeye gore degistiriliyor
		$.each(orderArray, function(orderIndex, orderElement) {
			var orderBannerList = $.map(bannerList, function(element, index) {
				if ($(element).attr('data-item-order') === orderElement) {
					return element;
				}
				return;
			});
			var bannerOrder = 0;
			try {
				// Cookie de order 1 den basliyor.
				bannerOrder = parseInt(bannerCookie[orderIndex]) - 1;
				if (!$.isNumeric(bannerOrder)) {
					bannerOrder = 0;
				}
			} catch (e) {
			}

			// Eger ayni order a sahip birden fazla item var ise cookiedeki degere gore biri siliniyor
			var bannerLength = orderBannerList.length;
			if (bannerLength > 1) {
				bannerOrder = (bannerOrder + 1) % bannerLength;
				$(orderBannerList).filter(":not(:eq(" + bannerOrder + "))").remove();
			}

			bannerCookie[orderIndex] = bannerOrder + 1;
		});

		// Preview mode ise hepsi gosteriliyor degil ise 5. den sonrasi siliniyor.
		if (!"") {
			$("#carousel-hero .item:gt(4)").remove();
		}

		// Banner listesi cookie de guncelleniyor
		$.cookie(BANNER_COOKIE_NAME, bannerCookie.join(","), {
			expires : 1,
			path : '/'
		});
	}
	
	
	function videoLoopControl(e) {
		var _video = $(e).find("video");
		$.each(_video, function(i, b) {
			if ($(b).data("src") != null) {
				$(b).attr("src", $(b).data("src"));
			}
			if ($(this).is("[data-loop-time]")) {
				$(this).on('timeupdate', function() {
					if ($(this).get(0).currentTime > $(this).attr('data-loop-time') - .5 && $(this).is(':not([wait])')) {
						$(this).attr('wait', 'true').css('opacity', 1).css('z-index', 0).css('position', 'relative');

						var video = document.createElement('video');
						video.style.opacity = 0;
						$(video).css({
							position : 'absolute',
							left : 0,
							top : 0,
							opacity : 0,
							zIndex : 1
						}).attr({
							'data-loop-time' : $(this).attr('data-loop-time'),
							'muted' : $(this).attr('muted'),
							'class' : $(this).attr('class'),
							'src' : $(this).attr('src'),
							'id' : $(this).attr('id') + '1',
							'autoplay' : 'true',

						});
						var $parent = $(this).parent();
						$parent.append(video);
						$(video).on('play', function() {

							$(video).animate({
								opacity : 1
							}, 500, function() {
								$('video[wait]').remove();
								videoLoopControl(e);
								$(video).css('position', '').css('left', '').css('top', '').css('z-index', '').attr('id', 'uxr-fp-video');
							});
						})

					}
				})
			}
		});
	}

	function createNextBannerItemsCookieValue() {

		filterBannerItemsWithCookie();

		var bannerCount = $("#carousel-hero .item").length;

		if (bannerCount > 1) {
			for ( var i = 0; i < bannerCount; i++) {
				var indicatorClass = i == 0 ? 'active pip' : 'pip';
				var indicatorHtml = "<li data-target='#carousel-hero' data-slide-to='" + i + "' class='" + indicatorClass + "'></li>";
				$("#carousel-hero .wrap ol.carousel-indicators").append(indicatorHtml);
			}
		}

		$("#carousel-hero .carousel-inner .item:first").addClass("active");

		$.each($("#carousel-hero .carousel-inner .item"), function(index) {
			var _img = $(this).find("img");
			$.each(_img, function() {
				if ($(this).data("src") != null) {
					$(this).attr("src", $(this).data("src"));
				}
			});
			
			videoLoopControl(this);
			index = index + 1;

			var _div = $(this).find(".cursorPointer");

			var _url = "";

			if (_div.data("window-open-url")) {
				_url = _div.data("window-open-url");
			} else if (_div.data("location-href")) {
				_url = _div.data("location-href");
			}

			/* RedirectLink boş ise, hiçbir yönlenme olmadan aynı sayfada banner video oynatılabilsin */
			if ($.trim(_url) == "") {
				return;
			}

			var separator = _url.indexOf("?") != -1 ? "&" : "?";

			if (_url.indexOf("#") != -1) {
				var divPart = _url.substring(_url.indexOf("#"), _url.length);
				_url = _url.substring(0, _url.indexOf("#")) + separator + "place=banner-" + index + divPart;
			} else {
				_url = _url + separator + "place=banner-" + index;
			}

			if (_div.data("window-open-url")) {
				tapOrClickForLocation(_div, _url, "window");
			} else {
				tapOrClickForLocation(_div, _url, "location");
			}

		});

		pushBannersToTealium();
	}
</script>

	
	
