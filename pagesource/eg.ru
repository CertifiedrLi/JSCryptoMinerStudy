<script>
    $(document).ready(function () {



        if (!$.cookie('egregion2') || $.cookie('egregion2')=='MOW' ) {

            region = 'MOW';

            
            $.ajax({
                url: "/geo/",
                dataType: 'json',
                type: "POST",
                success: function (data) {



                    if (data.status == 'success') {
                        region = data.region;
                        if (region=='MOW') {region='MOW2'};
                        $.cookie('egregion2', region, { expires: 365, path: '/' });

                    }
                },
                error: function (data) {
                    region = 'MOW';

                },
                timeout: 2000
            });
                    } else {
            region = $.cookie('egregion2');
        }


        if (region=='MOW2') {region='MOW'};
        if (region != 'MOW'   ) {
            if ( region != 'KAV') {
                $('#common_news_block').load('/tmp/cached-egnews'+region+'.html');
            }
            $('#regionblock').load('/tmp/cached-egregion'+region+'.html');
        }



        $('#common_news').show();
        $('#regionblock').show();

    });
</script>



