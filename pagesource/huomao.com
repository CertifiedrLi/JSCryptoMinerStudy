<script>
	var link_scroll;
	var link_li_list = 1;
	var scrollwidth = 100;
	var totalpage=10;
	var link_scroll_totalpage = 12;
	function linkscrollonce_right(totalpage,scrollwidth)
	{	
		var scroll = document.getElementById('link-scrollbox');
		if ((scroll.scrollLeft+10)>scrollwidth*link_li_list)
		{	
			scroll.scrollLeft = scrollwidth*link_li_list;
			clearInterval(link_scroll);
			link_li_list ++;
		}
		else
		{				
			scroll.scrollLeft = scroll.scrollLeft+10;
		}
	}

	function linkscrollonce_left(scrollwidth)
	{
		var scroll = document.getElementById("link-scrollbox");
		scroll.scrollLeft = scroll.scrollLeft-10;
		if ((scroll.scrollLeft-10)<scrollwidth*(link_li_list-2))
		{
			scroll.scrollLeft = scrollwidth*(link_li_list-2);
			clearInterval(link_scroll);			
			link_li_list --;
		}	
	}

	function linkscrollleft(totalpage,scrollwidth)
	{
		clearInterval(link_scroll);
		link_scroll=setInterval('linkscrollonce_left('+scrollwidth+')',10);
	}

	function linkscrollright(totalpage,scrollwidth)
	{	
		clearInterval(link_scroll);	
		if(link_li_list < totalpage)
		{
			clearInterval(link_scroll);
			link_scroll = setInterval('linkscrollonce_right('+totalpage+','+scrollwidth+')',10);					
		}
	}

	$("#link-scrollright").click(
		function(){
			linkscrollright(link_scroll_totalpage,100);
		}
	);

	$("#link-scrollleft").click(
		function(){
			linkscrollleft(link_scroll_totalpage,100);
		}
	);
</script>
