    // 搜索相关功能
    (function($,window){
        var search = {
            prevInput: '', //之前的输入
            isInput: false,
            data: [],  // 存放ajax数据
            init: function(){
                // ajax请求设置热搜
                this.append_hot()
            },
            hotAfter: function(){
                // 设置输入框文本
                this.set_input()
                // 相关事件eventHandle
                this.eventHandle()
            },
            Unicode: {
                stringify: function (str) {
                    var res = [],
                        len = str.length;
                    for (var i = 0; i < len; ++i) {
                        res[i] = ("00" + str.charCodeAt(i).toString(16)).slice(-4);
                    }
                    return str ? "\\u" + res.join("\\u") : "";
                },
                parse: function (str) {
                    str = str.replace(/\\/g, "%");
                    return unescape(str);
                }
            },
            // 设置输入框文本
            set_input: function(){
                var search = location.search 
                var arr = []
                var result = ''
                if (search) {
                    arr = location.search.substring(1).split('=')
                    if (arr[0] == 'kw') {
                        result = this.Unicode.parse(arr[1])
                    }
                }
                // 空的情况
                if (result == '' || result == '%u') {
                    result = this.data[0] ? this.data[0].name : ''
                }
                $('#search_kw').val(result.toString().trim())
            },
            // ajax请求设置热搜
            append_hot: function(){
                var self = this
                var str = ''
                $.ajax({
                    url:`/plugs/getHotWords`,
                    type:'GET'
                }).done(function(result){
                    if (result.code == 200) {
                        var data = [].concat(result.data)
                        self.data = data
                        self.prevInput = data[0].name

                        if (data.length>10) {
                            data.length = 10
                        }

                        if (data.length > 0) {
                            for (var i=0;i<data.length;i++) {
                                str += '<dl class="num'+ (i+1) +'">\
                                        <i>'+ (i+1) +'</i>\
                                        <span class="txt">'+ data[i].name +'</span>\
                                    </dl>'
                            }
                            $('.search-tipsBox-hot').append(str)
                            $('.search-tipsBox-hot').removeClass('hidden')
                        }

                    }
                    self.hotAfter()
                }).fail(function(err){
                    self.hotAfter()
                })
            },
            // 相关事件eventHandle
            eventHandle: function(){
                var self = this;

                // 阻止form表单的默认事件
                $('#searchForm').on('submit',function(){
                    return false
                })

                // 点击搜索
                $('.search-button').click(function(){
                    var search_value = ''
                    var str = $('#search_kw').val().trim()
                    var old = readCookie('search-history')
                    var curJson = null
                    var allJson = null
                    var delitem = null
                    var isFind = false
                    if ( str == '' ) {
                        if ( self.data.length>0 ) {
                            search_value = self.data[0].name
                            str = search_value
                            curJson = {name:search_value}
                        }
                    } else {
                        search_value = str
                        curJson = {name:search_value}
                    }
                    if (search_value == '') {
                        return 
                    } else {

                        if (old == '') {
                            // 不存在
                            writeCookie('search-history',JSON.stringify([curJson]))
                        } else {
                            // 存在
                            allJson = JSON.parse(old)
                            for (var i=0;i<allJson.length;i++) {
                                if (allJson[i].name == search_value) {
                                    delitem = allJson.splice(i,1);
                                    break;
                                } 
                            }
                            if (isFind) {
                                allJson.unshift(delitem)
                            } else {
                                allJson.unshift(curJson)
                            }
                            // 最多10条
                            if (allJson.length>5) {
                                allJson.length = 5
                            }
                            writeCookie('search-history',JSON.stringify(allJson))
                        }
                        str = self.Unicode.stringify(str)
                        window.location.href="/plugs/getSearch?kw="+ str;
                    }
                    return false
                });
                
                // 搜索下拉
                $('#search_kw').on('focus',function(e){
                    var cookieStr = readCookie('search-history')
                    
                    self.prevInput = $(this).val()

                    if ( !self.isInput ) {
                        $(this).val('')
                    }
                    

                    if (cookieStr!= '') {
                        var cookieJson = JSON.parse(cookieStr)
                        var $searchTipsBoxHistory = $('.search-tipsBox-history')
                        var str = ''

                        for (var i=0;i<cookieJson.length;i++) {
                            str += '<dl>\
                                <span class="txt">'+ cookieJson[i].name +'</span>\
                                <span class="close"></span>\
                            </dl>'
                        }
                        $searchTipsBoxHistory.find('dl').remove()
                        $searchTipsBoxHistory.find('dt').removeClass('hidden')
                        $searchTipsBoxHistory.append(str)
                    }

                    if (cookieStr == '' && self.data.length==0) {
                        // 没有历史，也没有热搜
                        $('.search-tipsBox').addClass('hidden')
                    } else {
                        $('.search-tipsBox').removeClass('hidden').addClass('show')
                    }
                    
                });

                $('#search_kw').on('blur',function(e){
                    // 如果是在搜索按钮点击，不需要赋值（因为后面要赋热搜的第一位）
                    if ( e.relatedTarget && e.relatedTarget.className && e.relatedTarget.className.toLocaleLowerCase() == 'search-button') {
                        return
                    }
                    $(this).val(self.prevInput)
                });

                $('#search_kw').on('change',function(){
                    var value = $(this).val()

                    if (value != '') {
                        self.prevInput = value
                    } else {
                        self.prevInput = self.data[0].name
                    }
                    self.isInput = true
                });
                
                // 删除单条历史记录
                $('.search-tipsBox-history').on('click','.close',function(){
                    var $dl = $(this).closest('dl')
                    var index = $dl.index() - 1
                    var allJson = JSON.parse(readCookie('search-history'))

                    allJson.splice(index,1)
                    writeCookie('search-history',JSON.stringify(allJson))
                    $dl.remove()

                    // 最后一条需要
                    if ( JSON.parse(readCookie('search-history')).length == 0 ) {
                        $('.search-tipsBox-history .clear').click()
                    }
                    return false;
                })
                // 清空所有历史记录
                $('.search-tipsBox-history').on('click','.clear',function(){
                    $(this).closest('.search-tipsBox-history').find('dl').remove()
                    $(this).closest('.search-tipsBox-history').find('dt').addClass('hidden')
                    delCookie('search-history')
                    $('#search_kw').val('')
                    if (self.data.length==0) {
                        $('.search-tipsBox').removeClass('show').addClass('hidden')
                    }
                })

                // 点击历史记录
                $('.search-tipsBox-history').on('click','dl',function(){
                    var txt = $(this).find('.txt').text()
                    $('#search_kw').val(txt)
                    $('.search-tipsBox').removeClass('show')
                    $('.search-button').click()
                })
                // 点击热搜
                $('.search-tipsBox-hot').on('click','dl',function(){
                    var txt = $(this).find('.txt').text()
                    $('#search_kw').val(txt)
                    $('.search-tipsBox').removeClass('show')
                    $('.search-button').click()
                })

                // 点击外部，隐藏搜索下拉（搜索框除外）
                $(window).on('click',function(e){
                    if (e.target.id == 'search_kw') {
                        return 
                    }
                    $('.search-tipsBox').removeClass('show')
                })
                // 搜索下拉阻止冒泡（防止隐藏下拉）
                $('.search-tipsBox').on('click',function(){
                    return false;
                })
            }
        }

        $(function(){
            search.init()
        })

    })($,window)
</script>

