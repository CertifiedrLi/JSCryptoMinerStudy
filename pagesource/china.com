<script>
//#769 siteNavSub
function siteNavSub() {
	if ($('.siteNavSub').length < 0) { return false };
	var elem = $(".siteNavSub");
	$(".siteNavSub").hover(function() {
		$(this).find('.siteNavSubCon').show();
	},
	function() {
		$(this).find('.siteNavSubCon').hide();
	});
	$(".siteNavSub").each(function(i,e) {
		var items = $(this),
				subBtn = items.find('a').first();
				items.find('.siteNavSubCon a').each(function() {
					var subCon = $(this);
					if (subCon.text().indexOf(subBtn.text()) > -1) {
						subCon.hide();
					}
				});
	});
}
siteNavSub();
</script>


