<script type="text/javascript">

$(document).ready(function()
{
    // Set our timer global and give a timeout for stop typing.
    var timer, timeout = 1000;
    
    // Just clearing anything the user has in the text field on load.
    $(".search-form input[type=text]").val("");
    
    // Watch for the user to type in the text field.
    $(".search-form input[type=text]").keyup(function()
    {
        // Clear timer if it's set.
        if (typeof timer != undefined)
            clearTimeout(timer);
        
        // Set status to show we're typing.
        //$("#status").html("Typing ...").css("color", "#009900");
        
        // Set status to show we're done typing on a delay.
        timer = setTimeout(function()
        {
            $.ajax({
                    url: 'http://newpost.gr/articles/ajaxsearch',
                    type: 'POST',
                    crossDomain: true,
                    data: {
                            data:$('.search-form input[type=text]').val(),
                            },
                    success: function(response){
                        if(response){
                                $(".search-section .row").empty();
                                $(".search-section .row").append(response).fadeIn(2000);
                            }
                            else{
                                if( $(".search-section .row h1").length){

                                }else{
                                    $(".search-section .row").empty();
                                    $(".search-section .row").append('<h1></h1>');
                                }
                            }
                            
                            
                        }
                });
        }, timeout);
    });
});

</script>

