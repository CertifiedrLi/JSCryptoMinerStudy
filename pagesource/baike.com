/**科普轮播js start **/
var kepuImgs = $(".kepuli");
var points = $('.classify-page ul li') ;
var play = null ;
var index = 0 ;


function change() {
	$('.classify-page ul li').each(function(i,o ) {
		o.className = "";
	});
	//alert("change->" + index) ;
        if (points === undefined || points.length == 0 || points.length <= index){
		return;
	}
	points[index].className = "on";
	$(".kepuli").hide() ;
	$(kepuImgs[index]).show() ;
}
function setKepuAuto() {
	play = setInterval(function() {
		if (index <  $('.classify-page ul li').length - 1) {
			index ++;
		} else {
			index = 0;
		}
		change();
	}, 4000)
}

$(document).ready(function(){ 
	$('.classify-page ul li').each(function(i,o) {
		$(o).bind("mouseover", function() {
			index = i;
			change();
			clearKepuAuto();
		});
		$(o).bind("mouseout", function() {
			setKepuAuto(index);
		});
	});
});
function clearKepuAuto() {
	clearInterval(play);
	play = null;
}
setKepuAuto();
/**科普轮播js end **/



