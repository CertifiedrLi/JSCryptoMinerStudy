        <script>
            $(document).ready(function(){
              //$('.ad_cl').click(function(){
              $('body').on('click','.ad_cl', function() {
                var ad_id = $(this).attr('data-id');
                $.ajax({
                  url: 'http://www.bd-pratidin.com/home/click_counter/'+ad_id,
                })
              })
              
              // impression
            $( ".ad_cl" ).each(function() {
                var ad_id = $(this).attr('data-id');
                console.log( "ad id : " + ad_id );
                if (ad_id!='') {
                    $.ajax({
                      url: 'http://www.bd-pratidin.com/home/impression/'+ad_id,
                    })
                }
            });
              
                // hide #back-top first
                $("#back-top").hide();
                // fade in #back-top
                $(function () {
                    $(window).scroll(function () {
                        if ($(this).scrollTop() > 100) {
                            $('#back-top').fadeIn();
                        } else {
                            $('#back-top').fadeOut();
                        }
                    });
                    $('#back-top a').bind("mouseover", function(){
                        var color  = $('#back-top').css("background-color");
                        $('#back-top').css("background", "#E3E3E3");

                        $(this).bind("mouseout", function(){
                            $('#back-top').css("background", color);
                        })    
                    })   
                    // scroll body to 0px on click
                    $('#back-top a').click(function () {
                        $('body,html').animate({
                            scrollTop: 0
                        }, 800);
