		function init_chapas2()
		{
			var cir=0, num_circle=0;

			var x_ini, y_ini, old_x, chapa_x, set_opacidad=0, vel_auto=0.6, aaa=0;
			var left, right, old_left=0;
			var a = [], b = [];
			var ca = 0, cb = 1;
			var old_ca=0, old_cb=0;
			var moviendo = 0, moviendo_hard=0, old_hard=0;
			var opacidad_gfx = 0;
			var col_circle_on  = "rgba(255,255,255, .8)";
			var col_circle_off = "rgba(138,138,138, .4)";
			var gfx_ancho, gfx_ancho2, gfx_alto;
			var pixels_undo = 90;
			var chapa_txt = [], chapa_txt_nega = [], chapa_gfx = [];
			var js_chapa_txt = [], js_chapa_txt_nega = [], js_chapa_gfx = [];
			var hardware = 0, canvas=0, thga=0;
			var movil = 0;
			var id = $('#zona_chapas');

			if (Modernizr.csstransforms && Modernizr.csstransitions)
				hardware = 1;

			if (Modernizr.canvas)
				canvas = 1;

			if (movil)
				col_circle_off = "rgba(138,138,138, .01)";

			id.on("selectstart dragstart", function(e) { e.preventDefault(); }); // ie78
			init_chapas();

			$('#chapa_gfx_0 img').load(function() {
				init_chapas();
			}).each(function() {
				if (this.complete)
					$(this).load();
			});


			for (i=0; i<4; i++) {
				init_circle(i);
				estado_cara(i, 0);

				chapa_txt[i]      = $("#chapa_txt_"+i);
				chapa_gfx[i] 	  = $("#chapa_gfx_"+i);
				chapa_txt_nega[i] = $("#chapa_txt_nega_"+i);

				js_chapa_txt[i]      = document.getElementById("chapa_txt_"+i);
				js_chapa_gfx[i] 	 = document.getElementById("chapa_gfx_"+i);
				js_chapa_txt_nega[i] = document.getElementById("chapa_txt_nega_"+i);
			}

			estado_cara(num_circle, 1);
			setInterval(function() {
				if (!moviendo) {
					cir+=0.007;

					if (cir>=1) {
						cir = 0;
					} else {
						if (canvas)
							$('#circle'+num_circle).circleProgress({value: cir});
					}

					if (cir == 0) {
						init_circle(num_circle);
						estado_cara(num_circle, 0);

						if (++num_circle>3)
							num_circle = 0;

						estado_cara(num_circle, 1);
						lanza_chapa();
					}
				}

			}, 80);

			// clics en los circulos
			$(".circle_click").on("click", function() {
				var cual = parseInt($(this).attr("data-cual"));

				if (cual==ca || moviendo_hard)
					return;

				cb = cual;
				lanza_chapa();
			});

			// ponemos color a los textos
			if (!movil) {
				$(".circle_click")
					.mouseover(function() {
						$("div:eq(1)", this).css({"color": "#ffffff"});
						$("p", this).css({"color": "#dddddd"});
					}).mouseout(function() {
						if ( $(this).attr("data-cual") != num_circle)
							$("div:eq(1), p", this).css({"color": "#888888"});
					});
			}

			function lanza_chapa()
			{
				hard(0);
				moviendo = 1;
				moviendo_hard = 1;
				old_left = -1;
				chapa_x  = gfx_ancho2;
				organiza_chapas();
				get_direccion(chapa_x);
				clip_txt(0);

				setTimeout(function() {
					mueve_chapa_auto(chapa_x, "ease-in-out");
				}, 20);
			}

			function init_circle(num_circle)
			{
				var size = movil ? 30 : 58;
				if (canvas)
					$('#circle'+num_circle).circleProgress({'value': 0, 'size': size, 'fill': {color: col_circle_on},  emptyFill: col_circle_off});
				cir = 0.001;
			}

			function estado_cara(cual, estado)
			{
				var col1, col2, opa;

				if (estado) {
					col1 = "#ffffff";
					col2 = "#dddddd";
					opa  = 1;
					var url = chapa_gfx[cual].attr("data-url");
					$("#url_0").attr("href", url);
				} else {
					col1 = "#888888";
					col2 = "#888888";
					opa = movil ? 0.6 : 1;
				}

				$("#circle_txt_"+cual).css({"color": col1});
				$("#circle_txt_"+cual+" p ").css({"color": col2});
				$("#circle"+cual+" img ").css({"opacity": opa, "transition":"all 0.5s"});
			}


			//-----------------------------------------------------------------------------
			//
			//-----------------------------------------------------------------------------
			$(window).bind('orientationchange resize', function(e){ init_chapas() });

			function init_chapas()
			{
				var y_degra = 26;
				gfx_alto = $("#chapa_gfx_0").height()+27;
				gfx_ancho  = $("#chapa_gfx_0").width();
				gfx_ancho2 = gfx_ancho+0;
				gfx_ancho2 = -gfx_ancho2;
				pixels_undo = parseInt(gfx_ancho/20);
				pixels_despla_si = 10;
				set_opacidad = 1;

				if (gfx_ancho<800) {
					pixels_despla_si = 5;
					set_opacidad = 0;
					gfx_alto += 44;
					y_degra += 44;
					vel_auto=0.3;
				}

				$('#chapa_degra_abajo').css({"bottom": y_degra+"px"});

				id.height(gfx_alto);
			}

			var touchstart = 0;
			var espera = 0;
			id.on('mousedown touchstart', function(e) {
				if (e.type=="mousedown")
					e.preventDefault();

				if (touchstart)
					return false;

				if (moviendo_hard) {
					espera = 1;
				} else {
					espera = 0;
					touchstart = 1;
				}

				x_ini = get_x_cursor(e, "x");
				y_ini = get_x_cursor(e, "y");
				old_x = x_ini;
			});

			id.on('click', function(e) {
				if (moviendo || moviendo_hard || espera) {
					e.preventDefault();
    				e.stopPropagation();
    				e.stopImmediatePropagation();
					return;
				}
			});

			//-----------------------------------------------------------------------------
			// MUEVE DEDO
			//-----------------------------------------------------------------------------
			$(document).on('mousemove touchmove', function(e) {
				var dif_y = get_x_cursor(e, "y") - y_ini;

				if (touchstart && Math.abs(dif_y) > 19 && moviendo==0) {
					touchstart = 0;
					e.stopPropagation();
					return;
				}

				if (moviendo)
				    e.preventDefault();

				if (!touchstart || moviendo_hard || espera)
					return;

				chapa_x = get_x_cursor(e, "x")-x_ini;
				aaa = chapa_x-old_x;
				old_x = chapa_x;


				if (chapa_x<0)
					cb = (ca+1)&3;
				else
					cb = (ca-1)&3;

				hard(0, '');
				organiza_chapas();

				if (moviendo==0 && Math.abs(chapa_x) > pixels_despla_si) {
					set_chapa_direccion(chapa_x);
					moviendo = 1;
					x_ini   += chapa_x;
					chapa_x  = get_x_cursor(e, "x")-x_ini;
				}

				if (moviendo) {
					get_direccion(chapa_x);
					chapa_set_posicion(chapa_x);
				}
			});

			//-----------------------------------------------------------------------------
			// STOP
			//-----------------------------------------------------------------------------
			$(document).on('mouseup touchend', function(e) {
				espera = 0;

				if (!touchstart || moviendo_hard)
					return;

				touchstart = 0;
				chapa_x = get_x_cursor(e, "x")-x_ini;

				if (Math.abs(chapa_x)<pixels_undo) {
					chapa_x = 0;
				} else {
					chapa_x = left ? gfx_ancho2 : gfx_ancho;

					if (aaa<0) {
						if (left==1)
							chapa_x = gfx_ancho2;
						else
							chapa_x = 0;
					} else {
						if (right)
							chapa_x = gfx_ancho;
						else
							chapa_x = 0;
					}
				}

				get_direccion(chapa_x);

				if (moviendo)
					mueve_chapa_auto(chapa_x, "ease-out");
			});


			function organiza_chapas()
			{
				if (ca==old_ca && cb==old_cb)
					return;

				hard(0, '');
				old_ca = ca;
				old_cb = cb;

				for (pos=0; pos<4; pos++) {
					z = 2;
					if (pos==ca)
						z = 4;

					if (pos==cb)
						z = 3;

					reset_chapa(pos, z);
				}

				js_chapa_gfx[cb].style.display = "block";
				js_chapa_txt[cb].style.display = "block";
			}

			function reset_chapa(pos, z)
			{
				var display, opacidad=1;
				switch (z) {
					case 4:
						display  = "block";
						opacidad = 1;
						break;

					case 3:
						display  = "none";
						opacidad = 0;
						break;

					case 2:
						display  = "none";
						opacidad = 0;
						break;
				}

				chapa_gfx[pos].css(     {"display": display, "z-index": z, "opacity": opacidad, "left": "0", "transform": "translateX(0px)"});
				chapa_txt[pos].css(     {"display": display, "z-index": z, "opacity": opacidad, "left": "0", "right": "auto", "transform": "translateX(0px)"});
				chapa_txt_nega[pos].css({"left": "0", "right": "auto", "transform": "translateX(0px)"});
			}

			function set_chapa_direccion(chapa_x)
			{
				if (hardware) {
					if (chapa_x>0) {
						chapa_gfx[cb].css({"left": "0px", "z-index": "4", "opacity": 1, "transform": "translateX(-"+gfx_ancho+"px)"});
						chapa_txt[cb].css({"display": "block", "z-index": 4, "opacity": 1, "left": "0", "right": "auto", "transform": "translateX(0px)"});
						chapa_txt_nega[cb].css({"left": "0", "right": "auto", "transform": "translateX(0px)"});
						js_chapa_gfx[ca].style.zIndex = "3";
						chapa_txt[ca].css({"display": "block", "z-index": 3, "opacity": 1, "left": "0", "right": "auto", "transform": "translateX(0px)"});
					} else { 			// izquierda
						chapa_gfx[cb].css({"left": "0px", "z-index": "3", "transform": "translateX(0px)"});
						chapa_txt[cb].css({"display": "block", "z-index": 3, "opacity": 0, "left": "0", "right": "auto", "transform": "translateX(0px)"});
						chapa_gfx[ca].css({"z-index": "4", "transform": "translateX(0px)"});
						chapa_txt[ca].css({"display": "block", "z-index": 4, "opacity": 1, "left": "0", "right": "auto", "transform": "translateX(0px)"});
					}
				} else {
					if (chapa_x>0) {	// derecha
						chapa_gfx[cb].css({"left": "-"+gfx_ancho+"px", "z-index": "4", "opacity": 1});
						chapa_txt[cb].css({"display": "block", "z-index": 4, "opacity": 1, "left": "-"+gfx_ancho+"", "right": "auto"});
						chapa_txt_nega[cb].css({"left": "0", "right": "auto"});

						js_chapa_gfx[ca].style.zIndex = "3";
						chapa_txt[ca].css({"display": "block", "z-index": 3, "opacity": 1, "left": "-"+gfx_ancho+"", "right": "auto"});
					} else { 			// izquierda
						chapa_gfx[cb].css({"left": "0px", "z-index": "3"});
						chapa_txt[cb].css({"display": "block", "z-index": 3, "opacity": 0, "left": "0", "right": "auto"});
						js_chapa_gfx[ca].style.zIndex = "4";
						chapa_txt[ca].css({"display": "block", "z-index": 4, "opacity": 1, "left": "0", "right": "auto"});
					}
				}
			}

			// redraw
			function chapa_set_posicion(chapa_x)
			{
				if (chapa_x>gfx_ancho)
					chapa_x = gfx_ancho;

				if (chapa_x<-gfx_ancho)
					chapa_x = -gfx_ancho;

				if (right)
					chapa_x -= gfx_ancho;

				if (set_opacidad) {
					opacidad_gfx = Math.abs((1/300) * chapa_x);
					opacidad_gfx = Math.min(Math.max(0, opacidad_gfx), 1.0);
				} else {
					opacidad_gfx = 1;
				}

				var css = {"left": chapa_x+"px", "opacity": 1}

				if (hardware)
					css = {"transform": 'translateX('+chapa_x+'px)', "opacity": 1};

				if (left) {
					c1 = ca;
					c2 = cb;
				} else {
					c1 = cb;
					c2 = ca;
				}

				chapa_gfx[c1].css(css);
				chapa_txt[c1].css(css);
				chapa_gfx[c2].css({"opacity": opacidad_gfx});

				clip_txt(chapa_x);
			}

			function clip_txt(chapa_x)
			{
				var cc;
				var x_clip = gfx_ancho+chapa_x;

				if (left) {
					cc   = cb;
					css1 = {"right": "auto", "opacity": opacidad_gfx};
					css2 = {"right": "auto"};

					if (hardware) {
						css1.transform = "translateX( "+x_clip+"px)";
						css2.transform = "translateX(-"+x_clip+"px)";
					} else {
						css1.left =  x_clip+"px";
						css2.left = -x_clip+"px";
					}
				} else {
					cc   = ca;
					css1 = {"left": "auto", "opacity": opacidad_gfx};
					css2 = {"left": "auto"};

					if (hardware) {
						css1.transform = "translateX("+x_clip+"px)";
						css2.transform = "translateX(-"+x_clip+"px)";
					} else {
						css1.right = -x_clip+"px";
						css2.right =  x_clip+"px";
					}
				}

				chapa_txt[cc].css(css1);
				chapa_txt_nega[cc].css(css2);
			}


			// izq: -800  / der: 800 / nada:0
			function mueve_chapa_auto(chapa_x, tipo)
			{
				moviendo      = 0;
				moviendo_hard = 1;
				opacidad_gfx  = 0.01;
				get_direccion(chapa_x);
				var timer = (vel_auto*1000);

				if (hardware) {
					hard(vel_auto, tipo);
					chapa_set_posicion(chapa_x);
				} else {
					hard(0);
					var cc     = left ? ca:cb;
					var x_inih = parseInt(chapa_gfx[cc].css('left'));

					if (right)
						x_inih += gfx_ancho;

					var inter = parseInt(timer/100);
					x_inc     = (chapa_x-x_inih)/90;

					thga = setInterval(function() {
						x_inih += x_inc;
						chapa_set_posicion(x_inih);
					}, inter);
				}

				if (chapa_x == 0) {
					setTimeout(function() { chapa_reset_variables(); }, timer);
				} else {
					init_circle(ca);
					estado_cara(ca, 0);
					num_circle = cb;
					estado_cara(num_circle, 1);

					setTimeout(function() {
						ca = cb;
						cb = (cb+1)&3;

						hard(0);
						chapa_reset_variables();
						organiza_chapas();

					}, timer);
				}
			}
			function chapa_reset_variables()
			{
				if (thga)
					clearInterval(thga);

				if (espera)
					touchstart = 1;

				moviendo_hard = 0;
				moviendo = 0;
				espera = 0;
			}


			function debug(t) { $("#debug").html(t) }

			function get_direccion(chapa_x)
			{
				if (chapa_x>0 || (chapa_x>=0 && right)) {
					left  = 0;
					right = 1;
				} else {
					left  = 1;
					right = 0;
				}

				if (old_left!=left) {
					old_left=left;
					set_chapa_direccion(chapa_x);
				}
			}

			function hard(t, tipo)
			{
				if (t==old_hard)
					return;

				old_hard = t;
				var css = t>0.0 ? "all "+t+"s "+tipo : "none";

				for (pos=0; pos<4; pos++) {
					js_chapa_gfx[pos].style.transition = css;
					js_chapa_txt[pos].style.transition = css;
					js_chapa_txt_nega[pos].style.transition = css;
				}
			}

			function get_x_cursor(e, cual)
			{
				var touch;
				if (e.originalEvent) {
					if (e.originalEvent.touches && e.originalEvent.touches.length)
						touch  = e.originalEvent.touches;
					else if (e.originalEvent.changedTouches && e.originalEvent.changedTouches.length)
							touch = e.originalEvent.changedTouches;
				}

				if (cual=='x')
					return e.pageX || e.originalEvent.clientX || ((touch) ? touch[0].clientX : false) || ((e.currentPoint) ? e.currentPoint.x : false);
				else
					return e.pageY || e.originalEvent.clientY || ((touch) ? touch[0].clientY : false) || ((e.currentPoint) ? e.currentPoint.y : false);
			};
		}

	init_chapas2();

</script><div class='bgcEB pad_portada'><table width='976' class='tf'><tr><td width='654' rowspan='2' valign='top'><div class='mar_t2'><div class='caja_main'><div class='barra31_2 ffnav upp ffnav upp'><div class='txt nw oh'>Destacados</div></div><div style='margin:5px 1px 8px 1px' class='pad_rl5 fftit s14 desta_degra br4 a_u_off img_rend_opt'><div class='fr  desta_no_main' style='width:204px'><span class='uAr06vA 45CBCBC0C22D1F1FCCCCCC19224B4DC3434C4FC219424F4E1F494FCB4642464AC21EC443C11F2A2C26232B2B1FC2CB4AC11ECC4AC1C21E414ACBCB484344C14F49CB1E211E494F1EC043C14E46CB46C14A1E424F4EC0C14AC11E424AC1CB4AC21E49461F'><div class='pr br3'><img src='https://i11b.3djuegos.com/juegos/13202/star_wars_battlefront_2__2017_/fotos/noticias/star_wars_battlefront_2__2017_-3987025.jpg' class='br3 b_avatar_alfa wi100' alt=''><div class='pa desta_degra_abajo wi100 he40 b0'></div><div class='pa cF ts_desta s13 ffnav b upp' style='bottom:3px; left:7px'>Noticia</div></div></span><a href='https://www.3djuegos.com/noticias-ver/179544/star-wars-battlefront-2-no-permitira-comprar-cartas-ni/' class='db lh18 c1 pad_t5 pad_b10 pad_l3'>Star Wars Battlefront 2 no permitirá comprar Cartas ni Cajas de Botín</a></div><div class='fl mar_r10 desta_no_main' style='width:204px'><span class='uAr06vA 45CBCBC0C22D1F1FCCCCCC19224B4DC3434C4FC219424F4E1F494FCB4642464AC21EC443C11F2A2C262324251F484A1EC31ECB4A4B1EC0C143C24349CB4A1E484FC21ECBC143C21EC0C14FC64342CB4FC21E4B431EC2C31EC04FC2CB4CC14A4B4F1E4B431F'><div class='pr br3'><img src='https://i11c.3djuegos.com/juegos/1042/_noticias_/fotos/noticias/_noticias_-3987834.jpg' class='br3 b_avatar_alfa wi100' alt=''><div class='pa desta_degra_abajo wi100 he40 b0'></div><div class='pa cF ts_desta s13 ffnav b upp' style='bottom:3px; left:7px'>Especial</div></div></span><a href='https://www.3djuegos.com/noticias-ver/179568/la-u-tad-presenta-los-tres-proyectos-de-su-postgrado-de/' class='db lh18 c1 pad_t5 pad_b10 pad_l3'>U-Tad presenta los tres proyectos de su postgrado de