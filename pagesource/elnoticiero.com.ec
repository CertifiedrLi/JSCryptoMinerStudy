            localizacion = 0;
        } else if (url.substring(url.lastIndexOf("/") + 1) == "?localizacion=2") {
            localizacion = 2;
        }

        if (document.URL.split("/").length >= 7){
            //flag = 3;
            national.removeAttribute('href');
            international.removeAttribute('href');
            world.removeAttribute('href');
            document.getElementsByClassName("location")[0].style.display = 'none';
        }

        if (localizacion == 1) {
            national.className = 'active';
            international.className = '';
            world.className = '';
        } else if (localizacion == 2) {
            international.className = 'active';
            national.className = '';
