    function backgroundImage() {
      'use strict';

      var backgroundImgDefer = document.getElementsByClassName('background-image');
      var length = backgroundImgDefer.length;

      for (var i = 0; i < length; i++) {
        if (backgroundImgDefer[i].getAttribute('data-src')) {
          var dataSrc = backgroundImgDefer[i].getAttribute('data-src');

          backgroundImgDefer[i].style.backgroundImage = 'url("' + dataSrc + '")';
        }
      }
    }
    function deferTag(tag, placeholderData, realData) {
      'use strict';
      var tagDefer = document.getElementsByTagName(tag);
      var length = tagDefer.length;

      for (var i = 0; i < length; i++) {
        if (tagDefer[i].getAttribute(placeholderData)) {
          var dataSrc = tagDefer[i].getAttribute(placeholderData);
          tagDefer[i].setAttribute(realData, dataSrc);
        }
      }
    }

    function changeclassactive() {
        var url = location.href;
        var localizacion = 0;
        var flag = 0;
        var national = document.getElementById("national");
