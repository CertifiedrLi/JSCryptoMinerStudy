    $tabs_widget_criticas = $(".js-widget-criticas-tabs")
    $contents_widget_criticas = $(".js-widget-criticas-contents")

    function clear_widget_criticas(){
        $tabs_widget_criticas.removeClass("active")

        $contents_widget_criticas.removeClass("active")
    }

    function activate_widget_criticas(tab){
        if(tab=="criticas"){
            var $tab_criticas_widget_criticas = $(".js-criticas-tab")
            var $content_criticas_widget_criticas = $(".js-criticas-content")

            $tab_criticas_widget_criticas.addClass("active")
            $content_criticas_widget_criticas.addClass("active")
        } else if(tab=="artigos"){
            var $tab_artigos_widget_criticas = $(".js-artigos-tab")
            var $content_artigos_widget_criticas = $(".js-artigos-content")

            $tab_artigos_widget_criticas.addClass("active")
            $content_artigos_widget_criticas.addClass("active")
        } else if(tab=="videos"){
            var $tab_videos_widget_criticas = $(".js-videos-tab")
            var $content_videos_widget_criticas = $(".js-videos-content")

            $tab_videos_widget_criticas.addClass("active")
            $content_videos_widget_criticas.addClass("active")
        } else if(tab=="trailers"){
            var $tab_videos_widget_criticas = $(".js-trailers-tab")
            var $content_videos_widget_criticas = $(".js-trailers-content")

            $tab_videos_widget_criticas.addClass("active")
            $content_videos_widget_criticas.addClass("active")
        } else if(tab=="calendario"){
            var $tab_videos_widget_criticas = $(".js-calendario-tab")
            var $content_videos_widget_criticas = $(".js-calendario-content")

            $tab_videos_widget_criticas.addClass("active")
            $content_videos_widget_criticas.addClass("active")
        }
    }

    $tabs_widget_criticas.click(function(){
        var tab = $(this)[0].dataset.tab

        clear_widget_criticas();
        activate_widget_criticas(tab);
    })
</script>

