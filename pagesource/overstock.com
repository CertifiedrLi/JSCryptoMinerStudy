<script>
var os = os || {};

os.onloadFileBuilder = function fileBuilder() {
    return {
        doesBrowserSupport: function(type) {
            var tokenList = document.createElement("link").relList;

            if (!tokenList || !tokenList.supports) {
                return false;
            }
            try {
                return tokenList.supports(type);
            } catch (e) {
                if (e instanceof TypeError) {
                    console.log("The DOMTokenList doesn't have a supported tokens list");
                } else {
                    console.error("That shouldn't have happened");
                }
            }
        },
        buildAndAppendFile: function(obj) {
            var element = document.createElement(obj.elementType);

            for (var attributeName in obj.attributes) {
                var attributeValue = obj.attributes[attributeName];
                element.setAttribute(attributeName, attributeValue);
            }

            document.body.appendChild(element);
        },
        requestFile: function(path) {
            var xhr = new XMLHttpRequest();

            xhr.open('GET', path);
            xhr.send(null);
        }
    };
};

window.addEventListener('load', function() {
    var arr = os.onloadFiles || [];
    var referrerOSTKdomain = /www.overstock.com/.test(document.referrer);

    arr.forEach(function(fileObject) {
        var file = new os.onloadFileBuilder();
        var rel = (fileObject.attributes || {}).rel;
        var doesBrowserSupport = file.doesBrowserSupport(rel);
        var flagCheck = fileObject.hasOwnProperty('flag') ? fileObject.flag : true;

        if (rel === 'prefetch' && !doesBrowserSupport && !referrerOSTKdomain) {
            // If Browser does not support prefetch than make an xhr request
            file.requestFile(fileObject.attributes.href || fileObject.attributes.src);
        } else if (rel === 'prefetch' && !referrerOSTKdomain) {
            // Only prefetch on first OSTK page to save MS by not reloading
            file.buildAndAppendFile(fileObject);
        } else if (rel !== 'prefetch' && flagCheck) {
            file.buildAndAppendFile(fileObject);
        }
    });
});
