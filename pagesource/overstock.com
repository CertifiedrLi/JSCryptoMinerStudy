<script>os.BlackFridayCountdown = function() {
    var $cdArea = jQuery('.bf-countdown-area');

    return {
        init: function() {
            this.daysArea = $cdArea.find('.bf-cd-days');
            this.hoursArea = $cdArea.find('.bf-cd-hours');
            this.minutesArea = $cdArea.find('.bf-cd-minutes');
            this.secondsArea = $cdArea.find('.bf-cd-seconds');
            this.today = new Date();
            this.bfDate = new Date('December 9, 2016 00:00:00 EST');
            this.todayTime = this.today.getTime();
            this.bfTime = this.bfDate.getTime();
            this.tickTime = 1000;
            this.calculateTimeLeft();
            this.startCounting();
        },
        calculateTimeLeft: function() {
            var timeDifference = this.bfTime - this.todayTime + ((this.today.getTimezoneOffset() - this.bfDate.getTimezoneOffset()) * 60 * 1000),
                    days = Math.floor(timeDifference / (1000 * 60 * 60 * 24)),
                    hours = Math.floor(timeDifference / (1000 * 60 * 60)),
                    minutes = Math.floor((timeDifference / 1000 / 60) % 60),
                    seconds = Math.floor((timeDifference / 1000) % 60);

            this.days = this.addLeadZero(days);
            this.hours = this.addLeadZero(hours);
            this.minutes = this.addLeadZero(minutes);
            this.seconds = this.addLeadZero(seconds);
            this.timeDiff = this.bfTime - this.todayTime + ((this.today.getTimezoneOffset() - this.bfDate.getTimezoneOffset()) * 60 * 1000);

            this.updateCountdown();
        },
        addLeadZero: function(num) {
            if(num < 10) {
                return '0' + num;
            } else {
                return num;
            }
        },
        updateCountdown: function() {
            this.daysArea.text(this.days);
            this.hoursArea.text(this.hours);
            this.minutesArea.text(this.minutes);
            this.secondsArea.text(this.seconds);
        },
        startCounting: function() {
            var _this = this;

            setInterval(function() {
                _this.todayTime += _this.tickTime;
                _this.calculateTimeLeft();
                
                if (_this.timeDiff > 0) {
                    
                    jQuery('.bf-countdown-area').css('display', 'inline-block');
                } else if (_this.timeDiff <= 0) {
                    
                    jQuery('.bf-countdown-area').css('display', 'none');
                }
            },this.tickTime);
        }
    };
}

