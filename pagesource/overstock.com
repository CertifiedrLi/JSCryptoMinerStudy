        /**
         *
         *
         * @private
         */
        this._submitLogic = function _submitLogic(){
            var self = this;

            self._getGroupId();
            self._blurAndFocus();

            var theyMatch = 'go';

            jQuery(self.emailInput).keydown(function(event){
                if(event.keyCode==13){
