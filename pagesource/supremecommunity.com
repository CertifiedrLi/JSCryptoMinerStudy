<script>
    var vChecked = false;
    $( document ).ready(function() {
        // SCAPP Version Check
        if (navigator.userAgent.indexOf('scapp') > -1) {
            var vLoop = 0, version="";
            var iosversion = "1.3";
            var androidversion = "";

            if (/iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream) {
                version = iosversion;
            } else if (/android/i.test(navigator.userAgent)) {
                version = androidversion;
            }

            window.location.href = 'scapp://checkversion?v='+version;
            var versionChecked = setInterval(function() {
                if(vChecked) {
                    clearInterval(versionChecked);
                } else {
                    if(vLoop>= 5){
                        clearInterval(versionChecked);
                        if (window.confirm('Your app seems to be outdated, please update to have the latest working features. Thanks!')) {
                            window.location.href =  "scapp://request/get?url=itms-apps://itunes.apple.com/app/id1341148556";
                        };
                    }
                }
                vLoop++;
            }, 100);
        };

        function addLoadEvent(func) {
            var oldonload = window.onload;
            if (typeof window.onload != 'function') {
                window.onload = func;
            } else {
                window.onload = function () {
                    if (oldonload) {
                        oldonload();
                    }
                    func();
                }
            }
        }

        addLoadEvent(function () {
            outdatedBrowser({
                bgColor: '#f25648',
                color: '#ffffff',
                lowerThan: 'transform',
                languagePath: ''
            })
        });
    });
</script>
