      $("body").on("click", ".js-switchvert", function() {
          var vname = $(this).data("vname");
          if (!vname) {
              return false;
          }
          if (vname == "local") {
              return true;
          }
          if (vname == SM_NEXT_VERT) {
              return false;
          }
    
