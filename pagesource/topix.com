          sm_reInitialize(true);
          $(".js-str-nav").removeClass('x-expanded');
          return false;
      });

      $("body").on("click", ".js-nav-more", function() {
          if ($(".js-str-nav").hasClass('x-expanded')) {
              $(".js-str-nav").removeClass('x-expanded');
          } else {
              $(".js-str-nav").addClass('x-expanded');
          }
          return false;
      });

      $("body").on("click", ".js-imglink", function(e) {
          if (!SM_IPAD)
              return true;

          var blockid = $(this).data("blockid");
          if (blockid) {
              var didHover = sm_doTabletHover(blockid);
              return !didHover;
          }
          return true;
      });

      $(function() {
          sm_init();
      });

