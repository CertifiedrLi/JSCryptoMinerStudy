	$('.lineberita').css('font-size', '14px');
	setCookie('text-size','14','360');
	return false;
    });
	
	$(".increaseFont").click(function(){
    var currentFontSize = $('#beritaNumum').css('font-size');
    var currentFontSizeNum = parseFloat(currentFontSize, 10);
    var newFontSize = currentFontSizeNum*1.2;
    $('#beritaNumum').css('font-size', newFontSize);
	$('.lineberita').css('font-size', newFontSize);
	setCookie('text-size',newFontSize,'360');
    return false;
  });
   
     $(".decreaseFont").click(function(){
    var currentFontSize = $('#beritaNumum').css('font-size');
    var currentFontSizeNum = parseFloat(currentFontSize, 10);
    var newFontSize = currentFontSizeNum*0.8;
    $('#beritaNumum').css('font-size', newFontSize);
	$('.lineberita').css('font-size', newFontSize);
	setCookie('text-size',newFontSize,'360');
    return false;
  });   
	
	function setCookie(c_name,value,expiredays) {
				var exdate=new Date();
				exdate.setDate(exdate.getDate()+expiredays);
				document.cookie=c_name+ "=" +escape(value)+ ((expiredays==null) ? "" : ";expires="+exdate.toGMTString());
			}
	
});

