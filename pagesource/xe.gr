                        onClose: function () {
                            $(".simplemodal-container").removeClass("premiumpromo");
                        }
                    });
                    $(".h_lightbox .left_navigation").css("display", "none");
                    //}
                })
            }
        }); //end modal show
    }


    var slideIndex = 1;
    h_showDivs(slideIndex);

    function h_plusDivs(n) {
        h_showDivs(slideIndex += n);
    }

    function h_currentDiv(n) {
        h_showDivs(slideIndex = n);
    }

    function h_showDivs(n) {
        var i;
        var x = document.getElementsByClassName("h_lightbox_slide");
        var dots = document.getElementsByClassName("h-nav-badge");
        if (n > x.length) {slideIndex = 1}
        if (n < 1) {slideIndex = x.length}
        for (i = 0; i < x.length; i++) {
            x[i].style.display = "none";
        }
        for (i = 0; i < dots.length; i++) {
            dots[i].className = dots[i].className.replace(" active-badge", "");
        }
        x[slideIndex-1].style.display = "block";
        dots[slideIndex-1].className += " active-badge";
        dots[slideIndex-1].className += " badge" +i;

        $(".navbadges_container").css("display","block");
        $(".right_navigation").css("display","block");
        $(".left_navigation").css("display","block");


        if ($(".h_lightbox #slide1").is(":visible") ) {
            $(".left_navigation").css("display","none");
        }

        if ( $(".h_lightbox #slide3").is(":visible") ) {
            $(".navbadges_container").css("display","none");
            $(".right_navigation").css("display","none");
        }
    }

    function getParameterByName(name, url) {
        if (!url) {
            url = window.location.href;
        }
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    function Jsonise() {
        var Object_json =
                '[{' +
                '"productId": {' +
                '"id": "PremiumUserTrialPack",' +
                '"bundle": "",' +
                '"variation": "",' +
                '"outOfBundle": "false"' +
                '},' +
                '"params": {' +
                '"quantity": "1"' +
                '}' +
                '}]';

        return Object_json;
    }

</script>

