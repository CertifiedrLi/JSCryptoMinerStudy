            opacity: 20,
            onShow: function (dialog) {

                // add class to style this modal only
                // remove class onClose
                $(".simplemodal-container").addClass("premiumpromo");

                $(".btn_container a").click(function (e) {

                    e.preventDefault();
                    $.modal.close();

                    $(".h2_lightbox").modal({
                        overlayClose: true,
