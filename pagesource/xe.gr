<script type="text/javascript">

    function getRemarketing(){

        var remarketingName= "logistes_forotexnikoi";

		if ((localStorage.getItem('remTime')!= null)){
			var todayDate = new Date();
			var oldDate = new Date(localStorage.getItem('remTime'));
			//alert(oldDate);
			var days = ((todayDate.getTime() - oldDate.getTime() ) / (1000*60*60*24.0));
			//alert(Math.round(days))
			if (Math.round(days) >= 10){
				localStorage.removeItem('remTime');
				localStorage.removeItem('remarketing');
				// Change Item__category to Item__master_type if it targets jobs section
				if (XE.adServing.keywords.Item__master_type == remarketingName) {
					localStorage.setItem('remTime', new Date());
					localStorage.setItem('remarketing',remarketingName);
					return localStorage.getItem('remarketing');
				} else {
					return localStorage.getItem('remarketing');
				}
			} else {
				return localStorage.getItem('remarketing');
			}
		} else {
			// Change Item__category to Item__master_type if it targets jobs section
			if (XE.adServing.keywords.Item__master_type.indexOf(remarketingName) != -1 ) {
				localStorage.setItem('remTime', new Date());
				localStorage.setItem('remarketing',remarketingName);
				return localStorage.getItem('remarketing');
			}
		}
    }

	XE.adServing.keywords = {
		"Item__category" : "",
		"Item__master_type" : "",
		"Item__master_subtype" : "",
		"Item__type" : "",
		"Item__make" : "",
		"Item__subject" : "",
		"Transaction__type" : "",
		"Transaction__channel" : "",
		"Transaction__price_from" : "",
		"Transaction__price_to" : "",
        
        "section" : "xe_stelexos",
        
        "students" : localStorage.getItem('students-visit'),
        "remarketing" : localStorage.getItem('remarketing'),
        "adult" : sessionStorage.getItem('isAdult')
	}

    XE.adServing.other = {
        "remarketing" : getRemarketing()
    }
	
	XE.clusters.serialized.Item__make = '';
	XE.clusters.serialized.Item__type = '';
	XE.clusters.serialized.Item__subject = '';
</script>
    
	
