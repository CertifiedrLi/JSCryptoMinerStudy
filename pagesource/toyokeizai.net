<script type="text/javascript">
$(function(){
 var _ids = [];
 $('span.mw-comment-count a').each(function(){var el = $(this);_ids.push(el.attr('href').split("/").pop());});
 $.unique(_ids);
 try{
 $.ajax({
  url:'//toyokeizai.net/hfds/comment/count.json',
  data:'ids='+_ids.join(','),
  dataType: 'jsonp',
  jsonpCallback: 'commentCount',
  cache: true,
  type:'get'
 }).done(function(data){
  $('span.mw-comment-count a').each(function(){
   var el = $(this);
   try{
    var id = el.attr('href').split("/").pop();
    var mwcount = data[id]||0;
    if (mwcount > 0) {
     el.text(mwcount).show('fade');
    } else {
     el.remove();
    }
   }catch(err){}
  });
 });
 }catch(e){}
});
</script>
