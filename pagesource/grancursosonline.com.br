	fbLogin = function (redirecturl) {
		FB.login(function(response) {
			if (response.status === 'connected') {
				FB.api('/me?fields=first_name,email', function(response) {
					$('.spinner-facebook').html('<img alt="ajax-loader" src="assets/img/ajax-loader.gif?1">');
					$.ajax({
						type: 'post',
						url: 'site/identificacao/login-facebook',
						async: true,
						cache: false,
						dataType: 'json',
						data: 'email=' + response.email
						+ '&nome=' + response.first_name
						+ '&redirecturl=' + redirecturl,
						success: function (json) {
							$('.spinner-facebook').html('');

							if (json['status']) {
								window.location.replace(json['url']);
							} else {
								alert(json['msg']);
							}
						}
					});
				});
			}
		}, {scope: 'public_profile,email'});
	}

	
