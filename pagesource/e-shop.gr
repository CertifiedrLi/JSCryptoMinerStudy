<script type="text/javascript">
$('.home-productsBuy-wrap').on('click','#go-left',function(){var product_wrapper=$(this).closest('.home-productsBuy-scroller');showMore(product_wrapper[0],'left');});
$('.home-productsBuy-wrap').on('click','#go-right',function(){var product_wrapper=$(this).closest('.home-productsBuy-scroller');showMore(product_wrapper[0],'right');});
function showMore(wrapper, side)
{
	var past_style="width:100%;height:140px;overflow:hidden;";
	var new_style;
	var margin=parseInt($(wrapper).css('margin-left').replace("px",""));
	var step=186;
	var products=$(wrapper).find('.home-productBuy-wrap');
	var last_ele_pos=products[products.length-1].offsetLeft;
	if(side==="left"){
		margin+=step;
      	if (margin>=230){margin=235;$(wrapper).find('.left-side').removeClass('show');}
      	new_style=past_style+' margin-left:'+margin+'px';
      	$(wrapper).attr('style',new_style);
	}else if(side==="right"){
		margin-=step;
		if(margin<230){$(wrapper).find('.left-side').addClass('show');}
    	if(last_ele_pos<400){margin=((products.length-1)*186)-235;margin=margin*(-1);$(wrapper).find('.right-side').removeClass('show');}
		new_style=past_style+' margin-left:'+margin+'px';
		$(wrapper).attr('style',new_style);
    }
	if(last_ele_pos>=420)$(wrapper).find('.right-side').addClass('show');
}

var slides_active=false;window.onscroll=function(e){if(!slides_active){slides_active=true;getcontents('https://www.e-shop.gr/slideproducts?id=1','products_neesafixis');getcontents('https://www.e-shop.gr/slideproducts.phtml?id=2','products_prosfores');}}
</script>


