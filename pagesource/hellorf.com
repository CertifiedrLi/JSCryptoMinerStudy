    function addFeedback(){
        var result=$('input[name="feedback1"]:checked').val();
        var content=$("#content").val();
        var searchkey=$("#feed_searchkey").val();
        
        if (typeof(result) == "undefined"){
          set_alert("请选择找到你要找的图片了吗？");
          return false;
        }
        if (content==""){
          set_alert("请输入您的建议");
          return false;
        }

        var data = {
                result:result, 
                content:content, 
                keyword:searchkey
            };

        $.post("/feedback", data, function (data){
            if (data.result){
                $("#modal-feedback").modal("hide");
                $("#modal-feedbacksucceed").modal("show");
            }else{
                set_alert(data.message);
            }
        });
    } 

</script>


