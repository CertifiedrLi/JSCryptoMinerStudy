<script type="text/javascript">
    $(document).ready(function() {
      var search_callback = function(data){
        window.location.href = "/image/search?file="+data;  
      }

      $('#searchBtn-url').click(function(){
          $(".loadingbox").show();
          var search_imgurl = $('#search-imgurl').val();
          var data = {
              'url' : search_imgurl,
          };
          $.post('http://www.hellorf.com/image/simi-search', data, function(result){
            if(result.result){
                search_callback(result.data);
            }else{
              set_alert(result.message);
              setTimeout(function(){window.location.reload();},3000);
            }
          });
          return false;
      });

      $('#stfile').change(function(e) {
          var file = e.target.files[0];  
          var suffix = file.name.toLowerCase().substr(-3);
          if ($.inArray(suffix, ['jpg','png']) < 0) {
              $(this).val('');
              set_alert('仅支持jpg，png格式');
              return false;  
          } else {
              $("#uploadFrom").ajaxSubmit({
                type: 'post', 
                url: 'http://www.hellorf.com/image/simi-search', 
                beforeSend : function(){
                    $(".loadingbox").show();
                },
                success: function(result){
                    if(result.result){
                      search_callback(result.data);
                    }else{
                      set_alert(result.message);
                      setTimeout(function(){window.location.reload();},3000);
                    }
                },
                error: function(data,status,e){
                    set_alert(e);
                    setTimeout(function(){window.location.reload();},3000);
                }
            });
            return false;
          }
      });
    });
</script>
