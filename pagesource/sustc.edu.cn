<div class="mailbox"></div>
<script>
$(document).ready(function() {
	$('.mailbox').click(function(){
		mailboxShow(false);
	});
	$('#btn_presidentmail').click(function(){
		mailboxShow(true);
	});
	$('.h').click(function(){
		mailboxShow(false);
	});
	var mailboxShow = function(isVisible){
		if(isVisible){
			$('.mailbox').show();
			$('.mailform').show();
		}else{
			$('.mailbox').hide();
			$('.mailform').hide();
		}
	}
	
	var isValid = function(){
		if (!$("#name").val()) {
            // alert('请输入姓名');
            document.getElementById("name_1").innerHTML = "请输入姓名！";
            return false;
        } else {
            document.getElementById("name_1").innerHTML = "";
        }

        var email = $("#email").val().match(/^[\w-]+(\.[\w-]+)*@[\w-]+(\.[\w-]+)+$/);
        if (!email) {
            //alert('请填写正确的邮件地址');
            document.getElementById("email_1").innerHTML = "请输入正确的邮箱地址！";
            return false;
        } else {
            document.getElementById("email_1").innerHTML = "";
        }

//         var phone = $("#phone").val().match(/^1[3|4|5|7|8][0-9]\d{4,8}$/);
//         if (!phone) {
//             alert('请正确填写联系方式！');
//             document.getElementById("phone_1").innerHTML = "请输入正确的联系电话！";
//             return false;
//         } else {
//             document.getElementById("phone_1").innerHTML = "";
//         }
            document.getElementById("phone_1").innerHTML = "";

        if (!$("#title").val()) {
            //alert('请填写标题！');
            document.getElementById("title_1").innerHTML = "标题不能为空！";
            return false;
        } else {
            document.getElementById("title_1").innerHTML = "";
        }

        if (!$("#content").val()) {
            // alert('请填写相关内容');
            document.getElementById("content_1").innerHTML = "内容不能为空！";
            return false;
        } else {
            document.getElementById("content_1").innerHTML = "";
        }
         return true;
	}
	$('.mailform .btn_submit').click(function(){
		var name = $('#name').val();
		var email = $('#email').val();
		var phone =$('#phone').val();
		var title =$('#title').val();
		var content = $('#content').val();
		if (isValid()){
			$.ajax({
				url : '/mailbox',
				data : {
					name : name,
					mail:email,
					mobile:phone,
					title:title,
					content:content
				},
				success : function(data, textStatus, jqXHR) {
					mailboxShow(false);
				}
			});	
		}
		
	});
	
});
</script>
