function createTrackingReferrerCookie()
{
    var referrerCookieExpiration = new Date();
    referrerCookieExpiration.setMinutes(referrerCookieExpiration.getMinutes() + mobileRedirectionBySizeContentConfigs.referrerCookieExpiration);

    var siteDomain = mobileRedirectionBySizeContentConfigs.trackingReferrerDomain;
    if ( siteDomain === '')
    {
        siteDomain = document.location.hostname.replace('www.','.');
    }

    document.cookie = 'xtref=' + document.referrer + ';expires='+ referrerCookieExpiration.toGMTString() + ';domain =' + siteDomain + ';path=/';
}

function getMobileRedirectionUrl(mobileRedirectionUrl)
{
    var trackingReferrerMode = mobileRedirectionBySizeContentConfigs.trackingReferrerMode;

    if( trackingReferrerMode === mobileRedirectionBySizeContentConfigs.trackingReferrerHashMode)
    {
        mobileRedirectionUrl = mobileRedirectionUrl + '#xtref=' + document.referrer;
    }
    else if ( trackingReferrerMode === mobileRedirectionBySizeContentConfigs.trackingReferrerCookieMode )
    {
        createTrackingReferrerCookie();
    }

    // Default return mobile url without tracking referrer
    return mobileRedirectionUrl;
}

function detectDipWidth()
{
    var ua = navigator.userAgent
    if( ua.match(/(iPad|iPhone|iPod)/g) )
    {
        // simply return screen.width because it's not subject to rotation on safari
        return screen.width;
    }
    else if( ua.match(/Android/g) )
    {
        // detect orientation
        // This represent the width in PORTRAIT orientation
        // On some device like Nexus, the device.height in landscape will not be equal to device.width in portrait
        // because of the Android software touch button.
        var deviceWidth = window.screen.width;
        if( window.orientation == 90 || window.orientation == -90 )
        {
            deviceWidth = window.screen.height;
        }

        if( ua.match(/Chrome/g) )
        {
            return deviceWidth;
        }

        return Math.floor(deviceWidth / window.devicePixelRatio);
    }

    return window.screen.width;
}

function mobileRedirectionReadCookie( name )
{
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++)
    {
        var c = ca[i];
        while (c.charAt(0) == ' ')
        {
            c = c.substring(1, c.length);
        }
        if (c.indexOf(nameEQ) == 0)
        {
            return c.substring(nameEQ.length, c.length);
        }
    }
    return false;
}

function redirectWithDeviceSize() {

    if (mobileRedirectionBySizeContentConfigs.deviceMaxPixelWidth) {
        var head = document.getElementsByTagName('head')[0];
        var linkTags = head.getElementsByTagName('link');

        var mobileRedirectionUrl = '';

        for( var i=0; i<linkTags.length; i++ )
        {
            var link = linkTags[i];

            if( link.getAttribute('rel') === 'alternate' && link.getAttribute('media') === 'handheld' && link.href !== '' )
            {
                mobileRedirectionUrl = getMobileRedirectionUrl(link.href);
                break;
            }
        }

        if( mobileRedirectionUrl !== '' && detectDipWidth() < mobileRedirectionBySizeContentConfigs.deviceMaxPixelWidth )
        {
            window.location.href = mobileRedirectionUrl;
        }
    }
}

function lauchmobileRedirection() {
    if(mobileRedirectionBySizeContentConfigs.cookieName == '' || mobileRedirectionReadCookie(mobileRedirectionBySizeContentConfigs.cookieName) == false) {
        redirectWithDeviceSize();
    }
}

(function(){
    if (typeof mobileRedirectionBySizeContentConfigs != "undefined" && mobileRedirectionBySizeContentConfigs.autoLaunch == 1) {
        lauchmobileRedirection();
    }
})();

