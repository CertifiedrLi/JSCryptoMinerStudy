<script>
$('html').click(function(e) {
  if($(e.target).parents('.speed_config').length == 0) {
    $('.speed_config').hide();
  }
});

$(document).ready(function(){
	$('.speed_pref').live( "click", function() {
		$(this).find(".speed_config").show();
	});

	$("#speed_div ul.speed_config_ul li").live( "click", function() {
		var rate = $(this).find("span.rt").text();
		$("#speed_div span.current_speed").html('');
		$("#speed_div span.current_speed").each(function() {
			if (rate.toString() == 'Auto') {
				$(this).html(rate.toString());
			} else {
				var avail = $(this).data('bitrate_labels').toString().split(',');
				var found = 0;
				for (bi=0; bi<avail.length; bi++) {
					if (avail[bi].toString() == rate.toString()) {
						found = 1;
					}
				}
				if (found == 1) {
					$(this).html(rate.toString());
				} else {
					if (avail.length > 1) {
						$(this).html(avail[1].toString());
					} else {
						$(this).html(avail[0].toString());
					}
				}
			}
		});
	});
});
</script>

