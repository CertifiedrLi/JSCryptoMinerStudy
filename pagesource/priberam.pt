<script type="text/javascript">
function sizing() {
    var formfilterswidth=$("div[id$='pnl_Dicionario']").width();
    //console.log("W: " + formfilterswidth);
    $("#txt_Palavra").width((formfilterswidth - 64) + "px");
}

function clearAO(wordWithCode) {
    return wordWithCode.replace("|DropdAO", "").replace("|DropaAO", "").replace("|", "");
}

$(document).ready(function () {
    
    $(".btnDict").click(function () {
        var palavra = clearAO($("#txt_Palavra").val());
        $("#txt_Palavra").val(palavra);
        if (palavra.length > 1)
            window.location = "http://www.priberam.pt/dlpo/default.aspx?pal=" + palavra;
    });

    $("#txt_Palavra").bind("enterKey", function (e) {
        var palavra = clearAO($("#txt_Palavra").val());
        $("#txt_Palavra").val(palavra);
        if (palavra.length > 1)
            window.location = "http://www.priberam.pt/dlpo/default.aspx?pal=" + palavra;
    });
    $("#txt_Palavra").keyup(function (e) {
        if (e.keyCode == 13) {
            $(this).trigger("enterKey");
        }
    });
    //Hardcode for iPad (bug)
    $("#txt_Palavra").width("120px");
    sizing();
    $(window).resize(sizing);

    function log(message) {
        $("<div>").text(message).prependTo("#log");
        $("#log").scrollTop(0);
    }

    $("#txt_Palavra").autocomplete({
        source: function (request, response) {
            $.ajax({
                url: "/desktopmodules/EVD_dicionarioshortcut/palavraautocomplete.aspx",
                dataType: "text",
                data: { q: request.term },
                success: function (data) {
                    response(data.split("\n"));
                    //console.log("data: " + data);
                }
            });
        },
        select: function(event, ui) {
            var op = clearAO(ui.item.label);
            console.log("select:" + op);
            $("#txt_Palavra").val(op);
            $("#hdf_selected").val(op);
            console.log("select (hidden):" + $("#hdf_selected").val());
            return false;
        },
        focus: function (event, ui) {
            var op = clearAO(ui.item.label);
            $("#txt_Palavra").val(op);
            console.log("focus:" + op);
            return false;
        },
        minLength: 2
    }).data("ui-autocomplete")._renderItem = function (ul, item) {
        var imgAAO = "<img src='http://www.priberam.pt/DLPO/images/ico_aAO.gif' align='absmiddle' />";
        var imgDAO = "<img src='http://www.priberam.pt/DLPO/images/ico_dAO.gif' align='absmiddle' />";
        return $("<li>")
          .data("ui-autocomplete-item", item)
          .append("<a>" + item.label.replace("|DropaAO", " " + imgAAO).replace("|DropdAO", " " + imgDAO).replace("|","") + "</a>")
          .appendTo(ul);
    };
});
</script>


