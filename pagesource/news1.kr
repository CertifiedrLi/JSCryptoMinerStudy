    function initVideo(data) {

        var playList = makePlayList(data.streams);
        var param = {video_el:'video1',//비디오태그 엘레멘트 아이디 필수임
            poster: data.img_url,//포스터
            src:playList,//array[{src:url,contentId:cdn 아이디,fetched:false,default:(bool)기본해상도여부,res:해상도(영상의높이값))}]
            defaultResolution:360,//기본재생 해상도 없을 시 360
//            playLog:{param:'',ref:''
            preload:'none',
            title:data.title
        };
        News1Video.getVOD(param);
    }

    function changeVideo(el) {
        var idx = $(el).data('idx');
        var poster = $(el).data('poster');
        var title = $(el).data('title');
       var playList =  makePlayList(_aside_tv_cdn[idx]);
        News1Video.changeVideo({poster:poster,src:playList,title:title})
    }

    function makePlayList(data){
        var playList = [];
        _.each(data, function(item, idx) {
            var url = !_.isUndefined(item.url)? item.url : "";

            if (!_.isUndefined(item.url)) {
                playList.push({
                    'src': item.url,
                    'contentId': item.content_id,
                    'fetched': false,
                    'res': item.height
                });
            } else {
                // TODO: url이 없어도 추가해야 하나?
                playList.push({
                    'contentId': item.content_id,
                    'fetched': false,
                    'res': item.height
                });
            }
        });
        return playList
    }


</script>

