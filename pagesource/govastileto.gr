<script type="text/javascript">
	function InitLatestArticlesSlider(/*int*/ slideDelayInSeconds) {
		var isAutoScrollPaused = false;
		var isAutoScrollStopped = false;

		var container = $('.box#latestarticles .content-container-top');

		// bind events
		$(document).ready(function () {
			SetLatestArticlesSliderHeight(container);

			window.setTimeout(function () { SetLatestArticlesSliderHeight(container); }, 2000);
		});

		$(window).on('resize orientationchange container-resize', function () {
			SetLatestArticlesSliderHeight(container);
		});

		container.on('cycle-initialized', function (e, opts) {
			SetLatestArticlesSliderHeight(container);
		});

		container.on('cycle-before', function (e, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {
			$(incomingSlideEl).find('.photo img').each(function () {
				if ($(this).attr('src') != $(this).attr('_src')) {
					// Trigger the next slide for loading and recalculate contained height
					$(this).attr('src', $(this).attr('_src')).load(function () {
						container.stop().animate({ height: $(incomingSlideEl).outerHeight(false) }, 250, null);
					});
				} else {
					container.stop().animate({ height: $(incomingSlideEl).outerHeight(false) }, 250, null);
				}
			});

			$('.box#latestarticles .thumbnail-container img').css({ opacity: 0.7 });
			$('.box#latestarticles .thumbnail-container div.idx_' + $(incomingSlideEl).data('index') + ' img').fadeTo('fast', 1);
		});

		$('.box#latestarticles .content-container').css('visibility', 'visible').cycle({
			fx: 'fade',
			speed: 500,
			timeout: 0,
			slides: '.slide',
			pager: '#latestarticles .paging-container'
		});

		$(".box#latestarticles .thumbnail-container").css('visibility', 'visible').find('div').each(function () {
			$(this).css('left', 7 + (93 * $(this).data('index')));
		});

		$(".box#latestarticles .thumbnail-container div, .box#latestarticles .paging-container span").click(function () {
			isAutoScrollStopped = true;
			$('.box#latestarticles .content-container').cycle('goto', $(this).data('index'));
		});

		$('.box#latestarticles .prev').click(function (event) {
			event.preventDefault();
			isAutoScrollStopped = true;
			$('.box#latestarticles .content-container').cycle('prev');
		});

		$('.box#latestarticles .next').click(function (event) {
			event.preventDefault();
			isAutoScrollStopped = true;
			$('.box#latestarticles .content-container').cycle('next');
		});

		$('.box#latestarticles .content-container-top .slide').hover(function () {
			isAutoScrollPaused = true;

			if (!IsBreakpoint('xs')) $('.box#latestarticles .next, .box#latestarticles .prev').stop().fadeIn(250);
		}, function () {
			isAutoScrollPaused = false;

			if (!IsBreakpoint('xs')) $('.box#latestarticles .next, .box#latestarticles .prev').stop().fadeOut(250);
		});

		// auto-slide only if frontstory control is fully loaded and visible
		$(window).load(function () {
			if (slideDelayInSeconds > 0) {
				window.setInterval(function () {
					if (!isAutoScrollPaused && !isAutoScrollStopped && ($(window).scrollTop() < ($('#latestarticles').offset().top + 100))) {
						$('.box#latestarticles .content-container').cycle('next');
					}
				}, slideDelayInSeconds * 1000);
			}
		});
	};

	function SetLatestArticlesSliderHeight(container) {
		var opts = container.data('cycle.opts');

		if (opts) {
			height = $(opts.slides[opts.currSlide]).outerHeight(false);
			opts.container.height(height);
		}
	};
</script>
