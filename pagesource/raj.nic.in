<script type="text/javascript" language="JavaScript">
    var update = new Date(document.lastModified)
    var theMonth = update.getMonth() + 1
    var theDate = update.getDate()
    var theYear = update.getFullYear()
    var theHours = update.getHours();
    var theMinutes = update.getMinutes();
    var ampm = theHours >= 12 ? 'PM' : 'AM';
    theHours = theHours % 12;
    theHours = theHours ? theHours : 12; // the hour '0' should be '12'
    theHours = theHours < 10 ? '0' + theHours : theHours;
    theMinutes = theMinutes < 10 ? '0' + theMinutes : theMinutes;
    theMonth = theMonth < 10 ? '0' + theMonth : theMonth;
    theDate = theDate < 10 ? '0' + theDate : theDate;
    document.getElementById('lsd').innerHTML = theDate + "-" + theMonth + "-" + theYear + "  " + theHours + ':' + theMinutes + ' ' + ampm;
</script>
