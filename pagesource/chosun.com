<script type="text/javascript">
function _getLiveNewsList () {
	var _url = "/site/data/rss/mobile_rss.xml"
		, _tot = 10
		, _script_ga = 'onclick="ga(\'send\', \'event\', \'HeadRight\', \'service\', \'livenews\');"'
		, _exp = new RegExp("http://[^>]*(site/data|sitedata)/(img_dir|thumb_dir|image|thumbnail)[^>]*_thumb\\.jpg", "gim");
	$.ajax({
		url: _url
		, dataType: 'xml'
		, rsync: true
		, success: function(xmlData) {
			try {
				var items = $(xmlData).find("item");
				if (items == null || items.length == 0) return;

				var htmlstr = '', _idx = 0;
				for (var no = 0; no < items.length; no++) {
					var title_element = $(items[no]).find("title")
						, link_element = $(items[no]).find("link")
						, desc_element = $(items[no]).find("description")
						, pubdate_element = $(items[no]).find("pubDate");
					var _title = $(title_element).text()
						, _link = $(link_element).text()
						, _desc = $(desc_element).text()
						, _pubdate = $(pubdate_element).text()
						, _thumb = '', _date = '';
						if ( _title.length >= 45) {
						  //var _title = _title.substring(0,45) + '...'
						}
					var arr_thumb = _desc.match(_exp);
					if (arr_thumb != null) _thumb = arr_thumb[0];
					_date = new Date(_pubdate);
					_date = (_date.getHours()<10 ? "0" : "") + _date.getHours() + ":" + (_date.getMinutes()<10 ? "0" : "") + _date.getMinutes();

					htmlstr += '<dl class="live_item">'
						+ '<dd class="time">' + _date + '</dd>';
					if (_thumb != '') htmlstr += '<dd class="thumb"><a href="' + _link + '" ' + _script_ga + '><img src="' + _thumb + '" onError="this.style.display=\'none\';"></a></dd>';
					htmlstr += '<dt><a href="' + _link + '" ' + _script_ga + '>' + _title + '</a></dt>'
						+ '</dl>';

					if (++_idx >= _tot) break;
				}
				$("#sec_live_con_id #live_more_id").css('display', 'block');
				$("#sec_live_con_id #live_more_id").before(htmlstr);
			}
			catch (e) {alert(e);}
		}
		, error: function(status, error) {
		}
    	});
}
    $(document).ready(function() {
      if (window.navigator.userAgent.indexOf('MSIE 8') == -1 || window.navigator.userAgent.indexOf('MSIE 7') == -1) {
          //no ie8
          _getLiveNewsList();
          $("#sec_live_con_id").scrollbar();
      } else {
          //yes ie8
          _getLiveNewsList();
      }
  });
</script>


