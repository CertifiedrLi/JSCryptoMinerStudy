    success: function(data) {
     $('#home-news-loading-text').remove();
     if(data=='no-data')
     {
      $('#home-news-view-more-btn').css('display','none');
     }
     else
     {
      $('#bc-home-news-main-wrap').append(data[0]);
      $('.cz-pagination').empty().append(data[1]);   
      if(home_latest_news_loaded >= 161) {
        $('#home-news-view-more-btn').css('display','none');
      }

      home_latest_news_loaded+=1;
     }
    }
     });  
    });
});
