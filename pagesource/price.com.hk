function calculateMarksRef() {
	var result = [];
	var last_ck1 = 0;
	$.each(ref_tracked_productids, function(index, elem) {
		elem = '#track_'+elem+'_'+index;
		var temp_pid = elem.substr(7, 6), h, ck1, ck2;
		if ( $(elem).length ) { 
			h 	= $(elem).height();
			ck1 = $(elem).offset().top;
			if(last_ck1 == 0)
				last_ck1 = ck1;
			if(ck1 > last_ck1)
				last_ck1 = ck1;
			if(ck1 == 0)
				ck1 = last_ck1;
			if(ck1 < last_ck1)
				ck1 = last_ck1;
			result.push(ck1);
		}
	});
	return result;
}
function checkMarksRef(marks, scrollDistance) {
	$.each(marks, function(key, val) {
		var count = 1;
		if (key%ref_tracked_estimation== 0 && scrollDistance >= val){
			for(i=0;ref_tracked_estimation>i;i++){
				if(typeof tracked_elm_ref.eq(key+i).attr("id") !== "undefined"){
					var product_id = Number(tracked_elm_ref.eq(key+i).attr("id").substr(6, 6));
					if (product_id && $.inArray(product_id, cache) === -1){
						pid.push(product_id);
						cache.push(product_id);
					}
				}
			}
			pushViewCountRef(key,i);
		}
	});
}
function pushViewCountRef(key,i){
	if (pid.length >= ref_tracked_estimation || (pid != '' && key%ref_tracked_estimation== 0)) {
		resultPid = pid.toString().replace(/,/g,'_');
		var target = tracked_elm_ref.eq(key).find('.p').find('img');
		if (target.attr('src') == undefined && tracked_elm_ref.eq(key).closest('div[class^="tab-pane"]').css('display') == 'block'){
			var url = target.data('src');
			target.attr('src', url);
			target.removeAttr('data-src');
		}
		resultPid.length = 0;
		pid = [];
	}
}

function switchTabPushViewCount(target_element){
	var targets = $('#'+target_element).find('li');
