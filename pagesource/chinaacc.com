	(function($,win){
		$.fn.doHoverOriginal = function (options) {
			//各种属性、参数
			var defaults = {
				sonSelector:''
			}
			var _opt = $.extend(defaults, options);
			this.each(function () {
				//插件实现代码
				var _this=$(this);

				_this.hover(function(){
					if(_opt.sonSelector==''){
						$(this).addClass('on');
					}else{
						$(this).find(_opt.sonSelector).addClass('on');
					}
				},function(){
					if(_opt.sonSelector==''){
						$(this).removeClass('on');
