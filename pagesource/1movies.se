</script>
<script>
    document.addEventListener('DOMContentLoaded',function()
    {
        /*
         * #search #new_suggestion
         * */
        var search = $("#new_search");
        var suggestion = $("#new_suggestion");
        var closeSuggest = function (e) {
            if($(e.target).attr("data-click") != 1)
            {

                suggestion.stop().animate({opacity: 0}, 300);
                setTimeout(function()
                {
                    suggestion.css(
                        {
                            display: "none"
                        });
                },300);
                $(document).off("click", closeSuggest);
            }
        };
        var sendSearchReq = function()
        {

            suggestion.css(
                {
                    display: "block"
                });
            suggestion.stop().animate({opacity: 1}, 200);
            //$(search).on("keyup", checkEnter);
            $(document).on("click", closeSuggest);
            var test = "";
            if(location.href.indexOf("dev_test") > 0)
            {
                test = "&dev_test";
            }
            var formdata = new FormData();
            formdata.append("_csrf-frontend",$('meta[name="csrf-token"]').attr("content"));
            if(window.searchCaptcha && window.searchCaptcha == 2 && grecaptcha.getResponse() != '')
            {
                formdata.append("g-recaptcha-response",grecaptcha.getResponse());
            }
            var $query = encodeURIComponent(search.val().replaceAll("/", "\\s"));
            $.ajax({
                type: "post",
                url: "/devtestsearch?query="+$query+test+"&cheklen",
                data: formdata,
                dataType: "JSON",
                cache: false,
                contentType: false,
                processData: false,
                success: function(data)
                {

                    suggestion.find(".s_blocks").show();
                    $('.loading-container').hide();

                    var
                        show_more = suggestion.find(".show_more"),
                        s_blocks = suggestion.find(".s_blocks");
                    if(data != "none" && data != "")
                    {
                        s_blocks.html(data.html);
                        Site.suggestHover();
                        show_more.css({display: "block"}).attr("href",  "/search_all/"+$query).text("Show all ("+data.count+")");
                        Site.needToPreload($(".s_blocks [data-preoad-inf]"));
                    }
                    else
                    {
                        var out = "<div>Nothing to show</div>";
                        s_blocks.html(out);
                        show_more.css({display: "none"}).text("Show all");
                    }
                },
                error: function()
                {
                    suggestion.find(".s_blocks").show();
                    $('.loading-container').hide();
                    var
                        show_more = suggestion.find(".show_more"),
                        s_blocks = suggestion.find(".s_blocks");
                    show_more.css({display: "block"}).text("Show all");
                    var out =  "";
                    out = "<div>Nothing to show</div>";
                    s_blocks.html(out);
                    show_more.css({display: "none"}).text("Show all");
                }
            });
        };
        $("#loupe_glass").click(function()
        {
            var search = $("#new_search");
            var show_more  = suggestion.find(".show_more");
            if(search.val().trim().length > 0)
            {
                if($("#new_suggestion .s_blocks .sh_mo.active").length && show_more.css("display") != "none" && $("#new_suggestion").css("display") != "none")
                {
                    location = $("#new_suggestion .s_blocks .sh_mo.active").attr("href");
                }
                else
                {
                    location = "/search_all/"+encodeURIComponent(search.val().replaceAll("/", "\\s"));
                }
            }

        });
        var checkEnter = function(event)
        {
            var show_more  = suggestion.find(".show_more");
            if(event.which == 13)
            {

                if($("#new_suggestion .s_blocks .sh_mo.active").length && show_more.css("display") != "none" && $("#new_suggestion").css("display") != "none")
                {
                    location = $("#new_suggestion .s_blocks .sh_mo.active").attr("href");
                }
                else
                {
                    location = "/search_all/"+encodeURIComponent(search.val().replaceAll("/", "\\s"));
                }

            }
        };
        window.keyUpTrigger = false;
        search.keyup(function(e)
        {
            if(window.grecaptcha && $("#search_captcha_trigger").length)
            {
                if(!window.searchCaptcha)
                {
                    grecaptcha.reset();
                    grecaptcha.execute();
                    window.searchCaptcha = 1;
                    return false;
                }

                if(window.searchCaptcha == 1 && grecaptcha.getResponse() != '')
                {
                    window.searchCaptcha = 2;
                }
                else if(window.searchCaptcha != 2)
                {
                    setTimeout(function()
                    {
                        if(window.searchCaptcha == 1 && grecaptcha.getResponse() == '')
                        {
                            grecaptcha.reset();
                            grecaptcha.execute();
                        }
                    }, 3500);
                    return false;
                }
            }
            if(e.which == 38 || e.which == 40)
            {
                Site.check_keyUp(e);
                return false;
            }
            $(search).on("keyup", checkEnter);
            if(search.val().trim().length > 2)
            {
                $('.loading-container').show();
                suggestion.show().css({opacity: 1});
                suggestion.find(".show_more").hide();
                suggestion.find(".s_blocks").hide();
                var search_value = search.val();
                window.keyUpTrigger = true;
                setTimeout(function()
                {
                    if(search_value == search.val() && window.keyUpTrigger)
                    {
                        window.keyUpTrigger = false;
                        sendSearchReq();
                    }
                },250);
            }
            else
            {
                if(suggestion.css("display")== "block")
                {
                    suggestion.stop().animate({opacity: 0}, 200);
                    setTimeout(function()
                    {
                        suggestion.css(
                            {
                                display: "none"
                            });
                    },200);
                    $(document).off("click", closeSuggest);
                }
            }
        });
        /*
         * end #search #new_suggestion
         * */
        /*
         * #menu mobile
         * */
    });
</script>            <div id="toTop" alt="Back to top"></div>
