function cherche_prix(a,b,c){
$.ajax({
	type:"POST",
	url:"/services/products.aspx/GetProductFR",
	async:true,
	cache:true,
	contentType:"application/json;charset=utf-8",
	dataType:"json",
	processData:false,
	data:"{\"productId\":\""+a+"\", documentId:\""+""+"\"}",
	success:function(data){
		if (data.d.Product!=null) {
			imgA=data.d.Product.DefaultImage.split('|'),imgA[1]='products/250by250',img250=imgA.join('');
			if(data.d.Product.IsAvailable == false){
				$('#'+a).html('<img data-src="'+img250+'" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" onload="lzld(this)" alt=""/><span class="marque">'+b+'</span><span class="description">'+c+'</span><span class="prix">'+data.d.Product.DisplaySalePriceAfter+'</span>');
				$('#'+a).attr('href','javascript:pdp('+a+');');
			}else{
				$('#'+a).html('<div style="width:177px;height:177px;background:#cccccc"></div><span class="marque">Produit indisponible</span><span class="description">Produit indisponible</span><span class="prix">00,00€</span>');
			};
		}else{
		$('#'+a).html('<div style="width:177px;height:177px;background:#cccccc"></div><span class="marque">Produit indisponible</span><span class="description">Produit indisponible</span><span class="prix">00,00€</span>')
		}
	}
})};


function mycarousel_initCallback(carousel,state){
		$("#carrousel").mouseenter(function(){carousel.stopAuto()}).mouseleave(function(){carousel.startAuto()});
		$('.indicators li').bind('click',function(){
			$('.indicators li').removeClass('active');
			$(this).addClass('active');
			$('.encart_lien a').css({"opacity":"0"});
			$('.texte p').css({ "opacity":"0", "left": "-100%"  });
			carousel.scroll($.jcarousel.intval($(this).attr('rel')));
			return false;
		});
	}
function mycarousel_itemLoadCallback(carousel,state){
			e=carousel.first;

			if(e<4){
			$('.indicators li').removeClass('active');
			$('.indicators li:eq('+eval(e-1)+')').addClass('active');

			$('.encart_lien a').animate({ opacity:"0"}, 200);
			$('.encart_lien #lien'+e+'').animate({ opacity:"1"}, 700);
			$('.texte p').animate({ opacity:"0", left: "-100%"  }, 300);
			$('.titrage'+e+'').animate({ opacity:"1", left: "250px"  }, 1000);
				
			}else{
			e=1;		
			$('.indicators li').removeClass('active');
			$('.indicators li:eq(0)').addClass('active');
			$('.encart_lien a').animate({ opacity:"0"}, 200);
			$('.encart_lien #lien1').animate({ opacity:"1"}, 700);
			$('.texte p').animate({ opacity:"0", left: "-100%"  }, 300);
			$('.titrage1').animate({ opacity:"1", left: "250px"  }, 1000);
		};
	}
</script>
