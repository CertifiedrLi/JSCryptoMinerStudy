<div class="membership_pico hidden">

    <div class="singup">
        <div class="title">Abonelik oluştur<span> (yeni abonelik)</span></div>
        <div class="desc">Karar’a hemen abone olarak abonelerimize özel avantajlardan yararlanın.</div>
        <form id="membership_form" onsubmit="membership();return false;" class="form" action="" method="post">
            <div class="form1">
                <span>Ad Soyad</span>
                <input type="text" name="name" id="membership_name" class="validate[required]" placeholder="">
            </div>
            <div class="form1">
                <span>Telefon</span>
                <input type="text" name="phone" id="membership_phone" class="validate[required,custom[phone]]"
                       placeholder="">
            </div>
            <div class="form1">
                <span>E-posta Adresi</span>
                <input type="text" name="email" id="membership_email" class="validate[required,custom[email]]"
                       placeholder="">
            </div>
            <div class="form1">
                <span>Adresiniz</span>
                <input type="text" name="address" id="membership_address" class="validate[required]"
                       placeholder="">
            </div>
            <div class="form1">
                <span>Mesaj</span>
                <input type="text" name="message" placeholder="" id="membership_message">
            </div>
            <div class="form1 error"></div>
            <div class="form1 captcha" style="margin-left: 103px;width: 310px;">

            </div>

            <div class="form2">
                <input type="submit" name="giris" value="Abone Ol">
            </div>
        </form>
    </div>
</div>

    <script type="text/javascript">

        activeForm = false;
        var open_membership = function (e) {

            var div = $('.membership_pico')[0];

            window.modal = picoModal({
                content: div.outerHTML,
                closeButton: true,
                closeHtml: "<span class='adCountDown'>X</span>",
                width: 500,
                height: 550,
                closeStyles: {
                    position: "absolute", top: "-10px", right: "-10px",
                    background: "#eee", padding: "5px 10px", cursor: "pointer",
                    borderRadius: "10px", border: "1px solid #ccc"
                },
                overlayStyles: {
                    background: 'rgb(0,0,0)',
                    opacity: 0.7
                }
            }).afterShow(function (modal) {
                $(modal.modalElem()).find('.membership_pico').removeClass('hidden');
                var captcha=$('<div id="g-recaptcha" class="g-recaptcha" data-sitekey="6LcSJB8TAAAAAFd5SZqu3Je532BbGZvgZpGt9fjr"></div>')
                $(modal.modalElem()).find('.captcha').append(captcha);
                activeForm = $(modal.modalElem()).find('#membership_form');
                $(activeForm).validationEngine('attach', {focusFirstField: false, autoPositionUpdate: true});
                grecaptcha.render('g-recaptcha', {
                    sitekey: '6LcSJB8TAAAAAFd5SZqu3Je532BbGZvgZpGt9fjr'
                });
            }).afterClose(function (modal) {

                modal.destroy();
            }).show();
            e.stopPropagation();
            e.stopImmediatePropagation();
            return false;
        }

        $('.membership').click(open_membership);

        function membership(e) {
            var check = $(activeForm).validationEngine('validate', {focusFirstField: false, autoPositionUpdate: true});
            var control=$('[name="g-recaptcha-response"]').val();
            if (check === true && control!=='') {
                $.post('/ajax/member/membership',
                        $(activeForm).serialize(),
                        function (data) {
                            if (!data.success) {
                                $("#membership_form").show('shake', 55);
                                $(".error").show();
                                $(".error").html('<p class="tc""><strong>Hata: </strong>' + data.message + '</p>');
                                if (data.redirect) {
                                    document.location.href = data.redirect;
                                }
                            }
                            if (data.success) {
                                if (data.redirect) {
                                    document.location.href = data.redirect;
                                } else {
                                    var $elem=$(window.modal.modalElem());
                                    $elem.find('form').remove();
                                    $elem.find('.desc').remove();
                                    $elem.find('h2').html('Abonelik işleminizi ilgili birimlere ilettik ve kısa zamanda sizinle iletişime geçmelerini sağlayacağız. Teşekkür ederiz.');
                                    window.setTimeout(function(){
                                        window.modal.close();
                                    },15000);
                                }
                            }
                        },
                        "json"
                );
            }
        }
    </script>
</div>



