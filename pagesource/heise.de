                    }

                    // initialise webtrekk
                    if (typeof wt_init_media === "function") {
                        wt_init_media("prophet.heise.de", "288689636920174");
                        for ( var i=0; i < webtrekkEvents.length; i++ ){
                            (function(i) {
                                kdp.kBind(webtrekkEvents[i], function(event) {
                                    sendToWebtrekk(webtrekkEvents[i], event);
                                });
                            })(i);
                        }
                    }

                    // Binding Event-Listeners to remove the Title-Overlay on ho Startpage in the Buehne :-D
                    for ( var i=0; i < removeTitleOverlayEvents.length; i++ ){
                        (function(i) {
                            kdp.kBind( removeTitleOverlayEvents[i], function(event) {
                                document.querySelector('.stage-videos-title.overlay[data-kaltura-id="' + playerId + '"]').style.display = 'none';
                            });
                        })(i);
                    }

                    // If debug or debugEnv is `true` this Log-Function will give some helpful information
                    function logOutput(event,eventData) {
                        // Infos about Player-Events
                        if(debug) {
                            console.log('Kaltura player event triggered: ' + event + ', event data: ' + JSON.stringify(eventData));
                        }

                        // Infos about the environment: mob/desktop and prod/dev
                        if(debugEnv) {
                            console.log('Are we on a productive Server? %s', ((isProd) ? 'yes' : 'no'));
                            console.log('Is this mobile? %s', ((isMobi) ? 'yes' : 'no'));
                        }

                        if(debug || debugEnv) {
                            console.log('------');
                        }
                    }

                    /*
                     * Here is the place were the tracking-lib gets called
                     * Key "xp" has to be present on Dev-Environment, otherwise
                     * it should be "cp".
                     * Value of key "st" tells whether we're on a Desktop ("heise")
                     * or mobile ("mobheise") site.
                     * Because the "playerPlayed"-Event gets triggered sometimes
                     * e.g. after "seeked"-Event but sometimes not (no pattern
                     * receivable) we need to take care of our measurements by
                     * saving the pause-State ("playerPaused") because only after
                     * that a "playerPlayed"-Event is valid for us.
                    */
                    function iomTracking(event,eventData) {
                        // If the player gets paused we save this state
                        if(event === "playerPaused") {
                            isPlayerPaused = true;
                        }
                        /*
                         * If the play-Events gets triggered, but there has not
                         * been a pause-Event before, we don't want to Track
                         * this Event.
                         */
                        if(event === "playerPlayed" && isPlayerPaused === false) {
                            return;
                        }
                        /*
                         * If the play-Event gets triggered and the Player has
                         * been paused sometime before, we change the saved
                         * pause-State-Variable to the new state.
                         */
                        if(event === "playerPlayed" && isPlayerPaused) {
                            isPlayerPaused = false;
                        }

                        var options = {
                            "cp" : "videoplayer",
                            "st" : (isMobi ? 'mobheise' : 'heise'),
                            "sv" : "ke",
                        }

                        if(!isProd) {
                            options["xp"] = options["cp"];
                            delete options["cp"];
                        }

                        iom.c(options, 1);

                        logOutput(event,eventData);
                    }

                    function sendToWebtrekk(event, eventData) {
                        var debug = true;
                        console.log(event);
                        console.log(eventData);

                        // Parameter (aus der youtube-Doku von Webtrekk):
                        //    "Titel",  // mi (Bez. der Multimedia-Datei; nur bei "play")
                        //    "play",   // mk (aktuelle Aktion: play, pause, stop, pos, seek eof)
                        //    "0",      // mt1.toString() position reached by the user
                        //    "600",    // mt2.toString() Math.ceil(player.getDuration)
                        //    "",       // Kategorie mg1?
                        //    "0",      // Bandbreite bw?
                        //    "1",      // vol Math.round(player.getVolume())
                        //    "0"       // player.isMuted()

                        // Unsere Webtrekk-Uebergabeparameter; ueber die Aktion hinaus merken.
                        wk_title       = wk_title    || kdp.evaluate('{mediaProxy.entry.name}');
                        wk_duration    = wk_duration || Math.ceil(kdp.evaluate('{duration}'));
                        wk_volume      = wk_volume   || Math.ceil(kdp.evaluate('{video.volume}') * 100);
                        wk_currentTime = Math.ceil(kdp.evaluate('{video.player.currentTime}'));
                        wk_seek        = "";

                        switch (event) {
                            case "playerPlayed":
                                hasBeenPlayed = true;
                                wk_action = "play";
                                break;
                            case "playerPaused":
                                wk_action = "pause";
                                break;
                            case "playerPlayEnd":
                                wk_action = "eof";
                                break;
                            case "seek":
                                wk_seek = "seek";
                                break;
                            case "seeked":
                                // Hier soll der letzte Status play/pause gesendet werden.
                                // Dies passiert schon automatisch, daher muessen wir nichts tun.
                                break;
                            case "volumeChanged":
                                wk_volume = Math.ceil(kdp.evaluate('{video.volume}') * 100);
                                break;
                            case "mute":
                                wk_isMuted = "1";
                                break;
                            case "unmute":
                                wk_isMuted = "0";
                                break;
                        } // switch

                        console.log("hasBeenPlayed", hasBeenPlayed);
                        if (hasBeenPlayed === true) {
                            console.log("wt_sendinfo_media", wk_title, (wk_seek || wk_action), wk_duration, wk_volume, wk_isMuted);
                            wt_sendinfo_media(
                                wk_title,
                                (wk_seek || wk_action),
                                wk_currentTime,
                                wk_duration,
                                "",
                                "0",
                                wk_volume,
                                wk_isMuted
                            );
                        }
                    } // sendToWebtrekk
                    /*
                     * Um dem Auto-Play Busting von Safari zu entgehen
                     * weisen wir den Player hier explizit noch mal an
                     * das Video zu starten, da der User ja bereits vorher
                     * auf das Vorschaubild mit dem Play-Button geklickt
                     * hat und damit signalisiert hat das Video gucken
                     * zu wollen.
                     */
                    kdp.sendNotification('doPlay');
                }, // readyCallback
                // Do not use "cache_st" in production (says the documentation)
                // "cache_st": "1_z7923h0i"
            };

            var kalturaPlayerConfiguration_1_z7923h0i_21 = kalturaPlayerConfiguration;

