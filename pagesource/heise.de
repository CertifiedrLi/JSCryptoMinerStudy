                    }
                },
                "readyCallback" : function(playerId) {
                    // Insert 1 Line of CSS to Kaltura-Player-iFrame
                    // This line hides the play button on top of the video
                    // and the black overlay background
                    var playerCss = document.createElement('style');
                    playerCss.type = 'text/css';
                    playerCss.innerHTML = '.mwPlayerContainer .largePlayBtn, .mwPlayerContainer .mwEmbedPlayer.mwEmbedPlayerBlackBkg {display: none !important;}';
                    frames[playerId+'_ifp'].document.body.appendChild(playerCss);

                    // Some config and ENV-Variables
                    var kdp             = document.getElementById(playerId),
                        events          = [
                            'seeked',
                            'openFullScreen',
                            'closeFullScreen',
                            'playerPlayed',
                            'playerPaused'
                        ],
                        context         = window.document.domain,
                        prodDesktop     = "www.heise.de",
                        prodMobi        = "m.heise.de",
                        prodSys         = [prodDesktop,prodMobi],
                        // Is our current domain in the list of prod-Systems?
                        isProd          = (prodSys.indexOf(context) !== -1),
                        /*
                         * Is the current domain a mobile domain in prod Environment
                         * or does the domain start like the mobile domain and we
                         * are not in prod Environment?
                         * In short: Are we on a prod or dev mobile site?
                         */
                        isMobi          = ((isProd && context === prodMobi) || (!isProd && context.substr(0,prodMobi.length) === prodMobi)),
                        // Infos about Player-Events
                        debug           = false,
                        // Infos about the environment: mob/desktop and prod/dev
                        debugEnv    = false,
                        /*
                         * For correct measurement we need to save the pause-State
                         * Further details are in comments at the iomTracking()-Function
                         * Initial value is `true` so we recognize the initial Play-Event
                         */
                        isPlayerPaused  = true,

                        // fuer webtrekk:
                        webtrekkEvents = [
                            'playerPlayed',
                            'playerPaused',
                            'playerPlayEnd',
                            'seek',
                            'seeked',
                            'mute',
                            'unmute',
                            'volumeChanged'
                        ],
                        hasBeenPlayed  = false,
                        wk_action      = "pause",
                        wk_currentTime = 0,
                        wk_duration    = 0,
                        wk_isMuted     = null,
                        wk_title       = "",
                        wk_volume      = 0,
                        removeTitleOverlayEvents = [
                            'AdSupport_StartAdPlayback',
                            'firstPlay'
                        ];

                    // Binding Event-Listener to the events (above listed)
                    for ( var i=0; i < events.length; i++ ){
                        (function(i) {
                            kdp.kBind( events[i], function(event) {
                                iomTracking(events[i],event);
                            });
