					if(searchengine==0){
						document.searchform.action = 'ysearchgw.php';
					}else if(searchengine==1){
						document.searchform.action = 'search.php';
					}else if(searchengine==2){ //advance search
						window.location = 'search.php';
					}else if(searchengine==3){ //price
						document.searchform.action = 'ysearchgw.php';
						document.searchform.target = '_blank';
					}
				}
				 	
				function searchtype(id,txt){
					document.getElementById('searchbox_menu').style.display = 'none';
					document.getElementById('currentsearchitem').innerHTML = txt;
					changeSearchAction(id);
