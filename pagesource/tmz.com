    <script>
      (function() {
        var adSizeList = {
          leaderboard: [[728, 90]],
          leaderboard_flex: [[728, 90], [970, 66], [1010, 150], [970, 250], [1010, 250]],
          medium_rectangle: [[300, 250]],
          medium_rectangle_flex: [[300, 250], [300, 600]]
        }
        var type;
        var size;
        var pos;
        var $ad;
        var amazonCleared = false;
        $('.wbads').each(function() {
          $ad = $(this);
          type = $ad.data('adsize') || null;
          size = adSizeList[$ad.data('adsize')] || null;
          pos = $ad.data('pos') || null;

          (function(index, pos, tile, size) {
            $ad.attr('id', 'wbgpt-promotion-' + index);

            googletag.cmd.push(function() {
              if (type === 'skin') {
                wbgpt.createSlot(WB_PAGE.wbgpt_ad_unit_path + '/skin', [[3, 3]], 'wbgpt-promotion-' + index);
              } else if (pos === 'interstitial') {
                wbgpt.createOutOfPageSlot(WB_PAGE.wbgpt_ad_unit_path + '/interstitial', 'wbgpt-promotion-' + index)
                  .setTargeting('pos', pos);
              } else if (pos === 'slimcut') {
                wbgpt.createSlot(WB_PAGE.wbgpt_ad_unit_path, [[2, 2]], 'wbgpt-promotion-' + index)
                  .setTargeting('pos', pos);
              } else {
                wbgpt.createSlot(WB_PAGE.wbgpt_ad_unit_path, size, 'wbgpt-promotion-' + index)
                  .setTargeting('pos', pos)
                  .setTargeting('tile', tile);
              }
            });

            if (size) {
              if (typeof window.OX_dfp_ads === 'undefined') {
                window.OX_dfp_ads = [];
              }
              window.OX_dfp_ads.push([WB_PAGE.wbgpt_ad_unit_path, size, 'wbgpt-promotion-' + index]);

              var apstagSlot = {
                slotID: 'wbgpt-promotion-' + index,
                sizes: size
              };
              if (!amazonCleared) {
                window.apstagSlots = [];
                amazonCleared = true;
              }
              if (typeof window.apstagSlots === 'undefined') {
                window.apstagSlots = [];
              }
              window.apstagSlots.push(apstagSlot);
            }

          })(WB_PAGE.wbgpt_promotion_ad_id, pos, WB_PAGE.wbgpt_tile, size);

          console.log('%c[WBADS/WBGPT] Promotion Ad Slot (#' + WB_PAGE.wbgpt_promotion_ad_id + ') Present', 'background: purple; color: white');
          console.log(this);

          WB_PAGE.wbgpt_promotion_ad_id += 1;
          WB_PAGE.wbgpt_tile += 1;
        });

        // if WB_PAGE.wbgpt_promotion_ad_id is not 1 (default state), then there must have been promotion ads.
        if (WB_PAGE.wbgpt_promotion_ad_id !== 1) {
          submitOpenXSlots();
          submitAmazonSlots();
          googletag.cmd.push(function() {
            wbgpt.displaySlotById('wbgpt-promotion-1');
          });
        }
      })();
