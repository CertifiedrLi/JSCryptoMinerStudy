<script>
var closesel=true,getcnt=false;
$( function() {
    var handle = $( "#custom-handle" );
    var handle2 = $( "#custom-handle2" );
    $( "#minratinpW" ).slider({
        min:10,
        max:99,
        value:$( "#minratinp" ).val(),
        create: function() {
            handle.text( $( this ).slider( "value" ) /10);
        },
        slide: function( event, ui ) {
            handle.text( ui.value / 10 );
            $( "#minratinp" ).val(ui.value);
            getcnt=true;
        }
    });

    $( "#minvotinpW" ).slider({
        min:100,
        max:100000,
        value:$( "#minvotinp" ).val(),
        create: function() {
            handle2.text( $( this ).slider( "value" ));
        },
        slide: function( event, ui ) {
            handle2.text( ui.value );
            $( "#minvotinp" ).val(ui.value);
            getcnt=true;
        }
    });

} );

var enblbtn=false;
function getrescount()
{   
    enblbtn=true;
    //return;
    if (! getcnt) return;
    enblbtn=true;
    $('.ckbx label input').removeAttr('disabled');
    $.post( "//kinonix.net/?do=asearch&counts=1", $('#megasearchform').serialize(),function(data){
        $('#ssearchbtnsub').val('Показать: '+data);
    });
    $('.ckbx label input').attr('disabled','disabled');
    getcnt=false;
}

$(document).ready(function(){
    
    $('#megasearchform').css('visibility','visible');

    
    $('#yearstart').change(function(){
        var y=parseInt($(this).val());
        //console.log(y);
        $('#yearend').find('option').each(function(){
            if (parseInt($(this).val()) < y) { $(this).hide(); $(this).attr('disabled','disabled'); }
            else { $(this).show(); $(this).removeAttr('disabled')};
            
        });
        
        if (! $('#yearend').val() || y > parseInt($('#yearend').val())) $('#yearend').val('');
    });
    $('#yearstart,#yearend').change(function(){
        getcnt=true;
    });
    
    
    $('.radinp').each(function(){ $(this).attr('name',$(this).children('input').attr('name'));});  
    $('.radinp').mouseup(function(){
        $('.radinp[name="'+$(this).attr('name')+'"]').removeClass('checked');
        $(this).addClass('checked');
        getcnt=true;
    });
    
    $('.ckbx').each(function(){ $(this).attr('name',$(this).find('input').attr('name'));});
    $('.ckbx').mouseup(function(){
            
            
            $(this).toggleClass('checked');         
            var ine=$(this).find('input');

            if ($(this).hasClass('checked'))
            {
                ine[0].checked=true;
            }else
            {
                ine[0].checked=false;
            }
            getcnt=true;//get res cnt
            
    });
    $('.ckbx label input').attr('disabled','disabled');
    
    

    $('#ssearchbtnsub').click(function(){
        if (!enblbtn) return false;
        $('#pagefield').val(0);$('#megasearchform').find('input').removeAttr('disabled');
    });
    
    $('#ratrad').click(function(){ $('#ratrangs').show(200);});
         
       
    
    $('.ordrad').click(function(){
        
        //if ($(this).val()=='rats') $('#ratrangs').show(200)
        //else $('#ratrangs').hide(250);
        $('#ratrangs').hide(250);
        getcnt=true;
    });
    
    $('#minratinp').on('input',function(){
        $('#minratcapt').html(parseInt($(this).val()) / 10);
        getcnt=true;
    });
    $('#minvotinp').on('input',function(){
        $('#minvotescapt').html($(this).val());
        getcnt=true;
    });
    
    //$('.tp:first input').attr('checked','checked');
    //$('#actorfield').click(function(){ $(this).val(''); });
    $('#actorfield').keyup(function(){
        getcnt=true;
        if ($(this).val().length<2) { $('#actor_sug').removeClass('open'); return;}
        $.get('//kinonix.net/engine/ajax/advanced_search.php',{act_sugg:$(this).val()},function(data){
            
            if (data=='0')
            {
                $('#actor_sug').removeClass('open')
                return;
            } 
            $('#actor_sug').addClass('open').html(data);
            
            $('.actsug').click(function(){
                $('#actorfield').val($(this).text());
                $('#actor_sug').html('').removeClass('open');
                getcnt=true;
            });
        });
    });
    
    //$('#directorfield').click(function(){ $(this).val(''); });
    $('#directorfield').keyup(function(){
        getcnt=true;
        if ($(this).val().length<2) { $('#director_sug').removeClass('open'); return;}
        $.get('//kinonix.net/engine/ajax/advanced_search.php',{dir_sugg:$(this).val()},function(data){            
            if (data=='0')
            {
                $('#director_sug').removeClass('open')
                return;
            }            
            $('#director_sug').addClass('open').html(data);
            
            $('.dirsug').click(function(){
                $('#directorfield').val($(this).text());
                $('#director_sug').html('').removeClass('open');
                getcnt=true;
            });
        });
    });
    
    $('#catlist,#stranlist,#quallist,#loclist,#pglist').click(function (){    
        closesel=false;    
    });
    $('.catbox').mouseup(function (){
        
        closesel=false;
        var catssel=$("#catlist li label input:checked").length;
        if (catssel>0)
        {
            $('#cexd').show();
            $('#catselectbtn').html('Выбрано '+catssel+' жанров. <span style="cursor:pointer" id="clearcat">[Убрать]</span>');
            $('#clearcat').click(function(){ $('#cexd').hide(); $('#catselectbtn').html('Выбор жанров');$("#catlist li").removeClass('checked'); $("#catlist li label input:checked").removeAttr('checked');$('.rodos').click();getcnt=true; return false;});
            
        }
        else { $('#catselectbtn').html('Выбор жанров');$('#cexd').hide();}
    });
    
    $('.stranbox').mouseup(function (){
        closesel=false;
        var stransel=$("#stranlist li label input:checked").length;
        if (stransel>0)
        {
             $('#stexd').show();
            $('#stranselectbtn').html('Выбрано '+stransel+' стран. <span style="cursor:pointer" id="clearstran">[Убрать]</span>');
            $('#clearstran').click(function(){ $('#stexd').hide(); $('#stranselectbtn').html('Выбор стран');$("#stranlist li").removeClass('checked'); $("#stranlist li label input:checked").removeAttr('checked');$('.rodos').click();getcnt=true; return false;});
            
        }
        else{ $('#stranselectbtn').html('Выбор стран'); $('#stexd').hide();}
    });
    
    $('.qualbox').mouseup(function (){
        closesel=false;
        var qualsel=$("#quallist li label input:checked").length;
        if (qualsel>0)
