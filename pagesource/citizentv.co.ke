		//Breaking and developing stories close buttons
		$('#close-breaking-news').on('click', function() {
			$('.breaking-news').hide();
			return false;
		});
		$('#close-developing-news').on('click', function() {
			$('.developing-news').hide();
			return false;
		});
		
		//Show more or less
		var wWidth = $(window).width();
		if(wWidth <= 360) {
			//Mobile size
			$('.mobile-more-less-stories').hide();
			$('.show-more').hide();
			$('.more-less-toggle').on('click', function() {
				$('.mobile-more-less-stories').toggle();
				$('.more-less-toggle').toggle();
				$('.less-more-toggle').toggle();
				return false;
			});
		} else if(768 >= wWidth && wWidth > 360) {
			//Tab
			$('.tab-more-less-stories').hide();
			$('.show-more').hide();
			$('.more-less-toggle').on('click', function() {
				$('.tab-more-less-stories').toggle();
				$('.more-less-toggle').toggle();
				$('.less-more-toggle').toggle();
				return false;
			});
		} else {
			$('.more-less-toggle').hide();
		}
		//Show more or less
