<script type="text/javascript">
						
			if($(window).width() >= 768){
				var slider = $('.home-sld').bxSlider({
				  mode: 'fade',
				  auto:true,
				  pause:10000,
				  autoHover: true
				});

				$(".home-slider .bx-pager div").hover(function(){
					var newSlideNo = $($(this).find("a")[0]).attr('data-slide-index');
					if(newSlideNo != slider.getCurrentSlide()){
						slider.goToSlide(newSlideNo);
					}
				});
			}
			if($(window).width() < 768){
				var slider = $('.home-sld').bxSlider({
				  mode: 'fade',
				  auto:true,
				  pause:10000,
				  autoHover: true,
				  pagerType:'short'
				});
			}
			
			function anSlider(){

					var anWidth = $('.analistet-holder').width();
					if($(window).width()>=992){
						var x = anWidth/3;
						$('.analist-slider').bxSlider({
						 	mode: 'horizontal',
						 	slideWidth: x,
						 	minSlides:3,
							maxSlides:3,
							moveSlides:1,
							pager:false
						});
					}
					if($(window).width() < 992 && $(window).width() >= 768){
						var x = anWidth/2;
						$('.analist-slider').bxSlider({
						 	mode: 'horizontal',
						 	slideWidth: x,
						 	minSlides:2,
							maxSlides:2,
							moveSlides:1,
							pager:false
						});
					}
					if($(window).width() < 768){
						var x = anWidth/1;
						$('.analist-slider').bxSlider({
						 	mode: 'horizontal',
						 	slideWidth: x,
						 	minSlides:1,
							maxSlides:1,
							moveSlides:1,
							pager:false,
							adaptiveHeight:true
						});
					}
			}
			anSlider();

			

		</script>
