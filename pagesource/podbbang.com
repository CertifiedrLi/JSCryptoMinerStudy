(function($){
    $.fn.shuffle = function() {
        var allElems = this.get(),
            getRandom = function(max) {
                return Math.floor(Math.random() * max);
            },
            shuffled = $.map(allElems, function(){
                var random = getRandom(allElems.length),
                    randEl = $(allElems[random]).clone(true)[0];
                allElems.splice(random, 1);
                return randEl;
           });
        this.each(function(i){
            $(this).replaceWith($(shuffled[i]));
        });
        return $(shuffled);
    };
})(jQuery);

$(function(){
	var _radio = $('#slides'),
		_now_rdo = 0,
		_max_rdo = $('div > ul', _radio).size() - 1,
		_move_rdo_w = $('div > ul', _radio).width();

	$('div', _radio).width(function() {
		var _w = 0;
		$('ul', this).each(function() {
			_w += $(this).width();
		});
		return _w;
	});
	
	var makebooksRoll;
	var makebooksRollRun = function() {
		makebooksRoll = setInterval(function() {
			radio_sort(_now_rdo, _max_rdo);
			if (_now_rdo < _max_rdo) {
				_now_rdo++;
				$('div', _radio).stop(true, true).animate({ marginLeft: '-'+(_move_rdo_w * _now_rdo) }, 250);
			} else {
				_now_rdo = 0;
				$('div', _radio).stop(true, true).animate({ marginLeft: 0 }, 250);
			}
		}, 3000);
	};
	makebooksRollRun();
	
	_radio.mouseenter(function() {
		clearTimeout(makebooksRoll);
	}).mouseleave(function() {
		makebooksRollRun();
	});

	$('.radio-w .toggle-lst a').click(function(e) {
