var loadok = 'no';
function displayLogin(ret){
	if(loadok == 'no'){
    	$.ajax({
        	url:'/mypage/login_layer',
        	type:'POST',
        	success:function(data){
				if($.trim(data) !== ''){
					parent.$('#login_wrap').html(data);
					parent.$('#login_wrap').show();
					parent.$('#login_wrap input[type=radio]').ezMark();
					parent.$('#login_wrap input[type=checkbox]').ezMark();
					if(ret){
						parent.p_loginForm.return_path.value = ret;
					}
					loadok = 'yes';
				}
			}
		});
	} else {
		parent.$('#login_wrap').show();
	}
}

function checkLayer(){
	var params = {};
	window.location.search.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(str, key, value) { params[key] = value; });

	var ret = '/';
	if(params.login || params.login==''){
		if(params.returl){
			ret = params.returl;
		}
		displayLogin(ret);
	}
}

