var scroll_h, vtop, vpos;
function get_size(){
    scroll_h = document.documentElement.scrollHeight;
    if(scroll_h > document.body.clientHeight*1.5){
        vtop = document.body.clientHeight + 200;
        vpos = document.body.clientHeight - 80;
        initMoving(document.getElementById('btn_top'),vpos,vtop,230); 
    } else {
        $('#btn_top').hide();       
    }
}



