    <script type="text/javascript">
    
    /* start main banner rolling */
    var bar_top = 150;
    var ini_top = 150;
    var bar_h = 32;
    var max_top = 150 + (32*4) + 3;//285;
    var idx = 1;
    var idx_max = 5;
    function slide_banner(){
        bar_top = bar_top + bar_h;
        if(bar_top > max_top){
            bar_top = ini_top;  
            $('#main_bnr').hide();
            $('#main_bnr').css('top',bar_top+'px');
            $('#main_bnr').show();
            $('#img_bnr'+idx_max).hide();
            idx = 1;
            $('#img_bnr'+idx).show();
        } else {
            $('#main_bnr').stop(true, true).animate({top:bar_top+'px'},300);
            $('#img_bnr'+idx).hide();
            idx = idx + 1;
            $('#img_bnr'+idx).show();
        }
    }
    
    var rbanner = setInterval(slide_banner,8000);
    function stop_banner(num){
        clearTimeout(rbanner);
        bar_top = ini_top + (bar_h*(num-1));
        $('#img_bnr'+idx).hide();
        idx = num;
        $('#main_bnr').hide();
        $('#main_bnr').css('top',bar_top+'px');
        $('#main_bnr').show();
        $('#img_bnr'+idx).show();
        
    }
    function start_banner(){
        rbanner = setInterval(slide_banner,8000);
    }
    
    /* end main banner rolling */

