		_index = $(this).index();
		$('div', _radio).stop(true, true).animate({ marginLeft: '-'+(_move_rdo_w * _index) }, 250);
		_now_rod = _index;
	});

	function getRandomInt(j)
	{
		return Math.floor(Math.random() * 100) % j;
	}
});

function radio_sort(now, max){
	var next	= (now < max) ? now+1 : 0;
	
	// radioWrap 의 li 개수
	var radio_wrap_size	= $('#radioWrap'+next+' li').size();
	
	// 개수가 꽉 찬 경우 return
	if(radio_wrap_size == 3)
	{
		return;
	}
	// 다음 라디오 영역 index
	var next_radio	= (next == max) ? 0 : next+1;

	for(var i=0; i<(3-radio_wrap_size); i++){
		// 다음 라디오 영역의 첫번째 li를 복제 및 해당 라디오 영역에서 제거
		$('#radioWrap'+next_radio+' li').eq(0).clone(true).appendTo($('#radioWrap'+next));
		$('#radioWrap'+next_radio+' li').eq(0).remove();
	}
	// class 명 제거 및 추가
	$('#radioWrap'+next+' li').removeClass('last');
	$('#radioWrap'+next+' li').eq(2).addClass('last');
}

//메인 레이어 배너 쿠키
function setCookie( name, value, expiredays ) {    
	var todayDate = new Date();
	// 자정인 경우 
	if(expiredays == 'year'){
		todayDate.setFullYear(todayDate.getFullYear() + 1);
	} else if(expiredays == 'today'){
		todayDate.setDate(todayDate.getDate()+1);
		todayDate.setHours(0);
		todayDate.setMinutes(0);
		todayDate.setSeconds(0);
	} else {
		todayDate.setDate( todayDate.getDate() + expiredays );
	}
	document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
} 

function getCookie( name )  {
	var nameOfCookie = name + "=";      
	var x = 0;      
	while ( x <= document.cookie.length )      {
		var y = (x+nameOfCookie.length);          
		if ( document.cookie.substring( x, y ) == nameOfCookie ) {              
			if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )                  
				endOfCookie = document.cookie.length;            
				return unescape( document.cookie.substring( y, endOfCookie ) );          
			}          
		x = document.cookie.indexOf( " ", x ) + 1;         
		if ( x == 0 ) break;      
	}      
	return "";  
} 
