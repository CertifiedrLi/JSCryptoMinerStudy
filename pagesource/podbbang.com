var iscateload	= false;
function catelist(no){
	if(!no) return false;
	if(iscateload) return false;

	$.ajax({
		type:'GET',
		async: false,
		url:'/home/catelist?cate='+no,
		dataType:'text',
		success:function(data){
			if($.trim(data) != ""){
				$('.newcatewrap').append(data);
				$('#rank_container .rank_cate .tab-cont ul li').mouseover(function(){
					$('#rank_container .rank_cate .tab-cont ul li a').removeClass('on');
					$(this).find('a').addClass('on');
					$('#rank_container .rank_cate .thumb-area').hide();
					$('#cr_view'+$(this).find('.num').text()).show();
				});
				iscateload	= true;
			} 
		}
	});
}

