$(function() {
    // 팥후원 했어요! 롤링
    var _target = $('div.section_sponsor .list');
    _target.height($('ul:first', _target).height());
    var podRun = function() {
        var _now = 0;
        pod = setInterval(function() {
            _clone = $('ul:first', _target).clone();
            $('ul', _target).stop(true, true).animate({ top: '-=410px' }, 500, function() {
                _target.append(_clone);
                $(this).css('top', 0);
            });
            $('ul:first', _target).remove();
            _target.height($('ul:first', _target).height());
        }, 4000);
    };
    podRun();
    
    $('div.section_sponsor .list').mouseenter(function() {
        clearTimeout(pod);
    }).mouseleave(function() {
        podRun();
    });
});


