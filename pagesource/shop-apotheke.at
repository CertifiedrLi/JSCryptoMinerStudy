    <script id="product-template" type="template/hogan">
        <div class="clearfix">
            {{#title}}<h2>{{title}}</h2>{{/title}}
            {{#link}}<a href="https://www.shop-apotheke.at/search.htm?q={{link}}" class="econda-crosssell-show-all" rel="nofollow">Alle anzeigen</a>{{/link}}
        </div>
        <div class="slider-row">
            {{#items}}
                <div class="col-xs-{{colcount}}">
                    <div class="product-info-box">
                        <div class="product-image">
                            {{#saving}}
                                <div class="discount">-{{saving}}%<sup>3</sup></div>
                            {{/saving}}
                            <a class="track-emcs product-info-box-link loading-img-container" href="{{deeplink}}" rel="nofollow" data-emcs2="na" data-emcs3="{{crossSellerCode}}">
                                <img width="{{imageWidth}}" height="{{imageHeight}}" alt="{{name}}" title="{{name}}" data-lazy="{{iconurl}}">
                            </a>
                        </div>
                        <a class="track-emcs product-info-box-link" href="{{deeplink}}" rel="nofollow" data-emcs2="na" data-emcs3="{{crossSellerCode}}">{{name}}</a>
                        {{#showRating}}
                        <div class="rating">
                            <a href="{{deeplink}}#ratings" class="track-emcs entry-rating-stars" data-score="{{bewertung_durchschnitt}}" rel="nofollow" data-emcs2="na" data-emcs3="{{crossSellerCode}}"></a>
                            <span class="total-ratings">({{bewertung_anzahl}})</span>
                        </div>
                        {{/showRating}}
                        <div class="dosage">
                            {{menge}} {{einheit}} {{#darreichungsform}}| {{darreichungsform}}{{/darreichungsform}}
                        </div>
                        <div class="product-info-box-price">
                            {{#oldprice}}<span class="product-old-price"><s>{{oldprice}}</s></span>{{/oldprice}}
                            <span class="product-info-box-base-price">{{price}}</span>
                        </div>
                        {{#new_baseprice}}<span class="product-info-box-consistent-price">({{new_baseprice}})</span>{{/new_baseprice}}

                            {{#validMultiplierForRP}}
                                <div class="rp-points-and-multiplier-wrapper">
                                    <span class="text-rp-multiplier">{{validMultiplierForRP}}x RedPoints</span>
                                </div>
                            {{/validMultiplierForRP}}
                    </div>
                </div>
             {{/items}}
         </div>
    </script>

