                    {{/crossSellerSaving}}
                    <a href="{{deeplink}}" class="track-emcs product-info-box-link loading-img-container" rel="nofollow" data-emcs2="na" data-emcs3="{{crossSellerCode}}">
                        <img width="{{imageWidth}}" height="{{imageHeight}}" alt="{{crossSellerName}}" title="{{crossSellerName}}" src="{{crossSellerProductImage}}">
                    </a>
                    <div class="product-name-and-rating">
                       <a class="track-emcs product-info-box-link" href="{{deeplink}}" rel="nofollow" data-emcs2="na" data-emcs3="{{crossSellerCode}}">{{crossSellerName}}</a>
                     </div>
                </div>

                <div class="sign col-xs-1">=</div>

                <div class="summary col-xs-4">
                    {{#totalListPrice}}VK<sup>4</sup>:
                    <span class="old-price">€ {{totalListPrice}}</span>{{/totalListPrice}}
                    {{#totalPrice}}
                        <div class="sum">
                            <span class="sum-amount">€ {{totalPrice}}</span>
                        </div>
                        inkl. MwSt. <span class="condition-sum">und</span> <span class="tooltipper" data-title="Tooltip Versandkosten" data-placement="right"><a href="/faq.htm#frage-1-2-h" target="_blank">Versand</a></span><br />
                    {{/totalPrice}}
                    {{#totalSaving}}<span class="savings-line">Sie sparen<sup>3</sup> {{totalSaving}}%</span>{{/totalSaving}}
                </div>
            </div>
            <div class="row">
                <span data-gtm="add-" class="hidden"></span>
                <span data-gtm="add-" class="hidden"></span>
                <a data-gtm="addAll" class="btn btn-default track-emcs" href="/add-to-cart.htm?product[]={{originalCode}}&product[]={{crossSellerCode}}&cartAction=add" rel="nofollow" data-emcs2="na" data-emcs3="{{crossSellerCode}}">
Beide Produkte in den Warenkorb                </a>
            </div>
        </div>
    </script>

