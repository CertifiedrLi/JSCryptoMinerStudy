		});
	});

	jQuery(".__re_send_report").on("click", function(){
		var __re_subtitle = jQuery("input[name=__re_subtitle]").is(":checked"),
			__re_audio = jQuery("input[name=__re_audio]").is(":checked"),
			__re_quality = jQuery("input[name=__re_quality]").is(":checked"),
			__re_not_working = jQuery("input[name=__re_not_working]").is(":checked"),
			__re_other = jQuery("input[name=__re_other]").is(":checked"),
			__re_captcha = jQuery(".g-recaptcha-response").val(),
			__re_error_desc = jQuery(".re_textarea").val();

		jQuery(".__re_send_report").hide();
		jQuery(".sending_txt").show();

		jQuery.ajax({
			type: 'POST',
			url: "/flags_center2?ajax=true&type=send_flag",
			data: {subtitle:__re_subtitle, audio:__re_audio, quality:__re_quality, not_working:__re_not_working, other:__re_other, captcha:__re_captcha, error_desc: __re_error_desc, post_id:"49573"},
			dataType: "json",
			cache:false,
			success: function(data) {
				console.log(data.status);
				if (data.status == "ok") {
					jQuery(".sending_txt").hide();
					jQuery(".sent_ok").show();
					jQuery(".re_error_msg").hide();					
				}
				else if (data.status == "fail") {
					jQuery(".sending_txt").hide();
					jQuery(".sent_ok").hide();
					jQuery(".re_error_msg span").text(data.error_msg);
					jQuery(".re_error_msg").show();					
					jQuery(".__re_send_report").show();
				} else {
					jQuery(".sending_txt").hide();
					jQuery(".re_error_msg span").text("خطأ غير معروف، حاول مجدداً في وقت لاحق.");
					jQuery(".re_error_msg").show();
					jQuery(".__re_send_report").show();					
				}
			},
			error: function(data) {
				jQuery(".sending_txt").hide();
				jQuery(".re_error_msg span").text("خطأ غير معروف، حاول مجدداً في وقت لاحق.");
				jQuery(".re_error_msg").show();
				jQuery(".__re_send_report").show();
			}
		})
	});
</script>

