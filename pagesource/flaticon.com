<script id="tpl-collection" type="text/x-handlebars-template">
	<section class="collection{{#if active}} expanded {{setHeight}}{{/if}}{{#unless status}} locked{{/unless}}" data-order="{{order}}" data-color="{{color}}" data-bookmark="{{bookmark_id}}">

		<header class="collection-header">
			<span class="collection-name" title="{{name}}">{{name}}</span>
			<span class="badge badge-gray">{{icons}}</span>
			{{#if status}}
			<div class="header-options">
				<button class="flaticon-pencil nostyle"></button>
				<button class="flaticon-bin nostyle" data-bookmark="{{bookmark_id}}"></button>
			</div>
			<div class="edit-name">
				<input type="text" class="collection-name-input" value="{{name}}" data-original="{{name}}" maxlength="100">
				<button class="flaticon-disk nostyle"></button>
			</div>
			{{else}}
			{{/if}}

			<div class="locked-options">
				<button class="nostyle flaticon-bin" data-bookmark="{{bookmark_id}}"></button>
				<i class="flaticon-lock"></i>
			</div>
		</header>
		<div class="collection-content">
			{{#if status}}
			<nav class="collection-options">
				<ul>
					<li class="simptip-position-bottom simptip-movable">
						<div class="popover popover-left paint-popover">
							<div class="popover-container">
								<button class="trigger_popover popover-button hidden"></button>
								<div class="popover-content">
									<ul class="color-selector">
										<li><button class="green" data-color="91DC5A"></button></li>
										<li><button class="blue" data-color="006DF0"></button></li>
										<li><button class="purple" data-color="933EC5"></button></li>
										<li><button class="yellow" data-color="FFDA44"></button></li>
										<li><button class="red" data-color="D80027"></button></li>
										<li><button class="white" data-color="FFFFFF"></button></li>
										<li><button class="black" data-color="000000"></button></li>
									</ul>
									<div class="col-cpicker" onload=""></div>
								</div>
							</div>
						</div>
					</li>
					<li class="simptip-position-left simptip-movable" data-tooltip="Undo"><button class="flaticon-undo nostyle"></button></li>
					<li class="simptip-position-right simptip-movable" data-tooltip="Redo"><button class="flaticon-redo nostyle"></button></li>
					<li>
						<div class="popover">
							<div class="popover-container menu-options">
								<button class="collection-options popover-button nostyle simptip-position-left simptip-movable" data-tooltip="Options" data-disabled="Options">
									<i class="flaticon-dots"></i>
								</button>
								<div class="popover-content">
									<ul class="option-buttons">
										<li><button class="nostyle toggle-edit flaticon-pencil hidePhone"></i> Edit icons</button></li>
										<li class="simptip-position-left simptip-movable" data-tooltip="Upgrade to get unlimited collections"><button data-bookmark="{{bookmark_id}}" class="nostyle flaticon-duplicate" onclick="Collection.duplicate()"> Duplicate collection</button></li>
										<li><button class="nostyle toggle-sort flaticon-collection_sort"> Sort icons</button></li>
										<!-- <li><button  data-bookmark="{{bookmark_id}}" class="nostyle restore-backup flaticon-disk"> Restore backup</button></li> -->
										<li class="simptip-position-left simptip-movable"><button class="nostyle toggle-share flaticon-share"> Share</button></li>
										<li class="simptip-position-left simptip-movable" data-tooltip="Add monocolor icons to paint!"><button class="nostyle toggle-paint flaticon-paint" id="paint_collection"></i> Paint collection</button></li>
									</ul>
								</div>
							</div>
						</div>
					</li>
				
				</ul>
			</nav>
			<header class="header-edit">
