<script type="text/javascript">
    var num_banners = 0;
    var show_banners = false;

    document.addEventListener("DOMContentLoaded", function(){
        if (!show_banners) return false;
        var banner_cookie = Cookie.get("banner_cookie");
        var today = new Date();
        var cybermonday = new Date("2017-11-27");

        if ((today.toDateString() === cybermonday.toDateString()) && (typeof gr.auth.me!='undefined') && !gr.auth.me.premium) {
            showCyberMondayBanner('banner_cybermonday');
            return;
        }

        var rand_show = (Math.random() >= 0.5) ? true : false;
        if (banner_cookie.length)
        {
            banner_cookie = banner_cookie.split("|");
            var first_display = new Date(banner_cookie[0]);
            var last_display  = new Date(banner_cookie[1]);
            var display_times = parseInt(banner_cookie[2], 10);
            var display_days  = parseInt(banner_cookie[3], 10);

            display_times = today.toDateString() != last_display.toDateString() ? 0 : display_times;

            if (display_days < (num_banners * 3) && display_times < 3 && rand_show)
            {
                document.body.classList.add("topbar-visible");

                toggle_banner();

                display_days = today.toDateString() != last_display.toDateString() ? display_days + 1  : display_days;
                display_times++;

                Cookie.set("banner_cookie", [first_display.toDateString(), today.toDateString(), display_times, display_days].join("|"), 60);
            }
        }
        else
        {
            document.body.classList.add("topbar-visible");
            toggle_banner();
            Cookie.set("banner_cookie",[today.toDateString(), today.toDateString(), 1,1].join("|"), 60);
        }

        setTimeout(function(){
            document.body.classList.remove('topbar-visible');
        }, 10000);
    });


    function banner_click(banner_id) {
        /*only one banner*/
        var banner_cookie = Cookie.get("banner_cookie");
        banner_cookie = banner_cookie.split("|");
        banner_cookie[2] = 3;
        banner_cookie[3] = num_banners *  3;
    }

    function toggle_banner() {
        var today = new Date();

        var form_completed = true;

        if (form_completed) {
            document.getElementById('topbar_tutpad').classList.toggle('hidden');
        } else {
            if ((today.getDay() % 2) == 0) {
                document.getElementById('topbar_tutpad').classList.toggle('hidden');
            } else {
                document.getElementById('topbar_piktab').classList.toggle('hidden');
            }
        }
    }

    function closeBannerTrigger(bannerGrCookieName){

        if (document.body.classList.contains('topbar-visible')) {
            document.body.classList.remove('topbar-visible');
            var banner_cookie = Cookie.get(bannerGrCookieName);
            var today = new Date();
            if (banner_cookie.length)
            {
                banner_cookie = banner_cookie.split("|");
                banner_cookie[2] = 3;
                banner_cookie[3] = parseInt(banner_cookie[3], 10) + 1 ;
                Cookie.set(bannerGrCookieName, banner_cookie.join("|"), 60);
            }
            else
            {
                Cookie.set(bannerGrCookieName,[today.toDateString(), today.toDateString(), 1,1].join("|"), 60);
            }
        }
    }

    function showCyberMondayBanner(bannerGrCookieName){
        var showPercentage = 99;
        var banner_cookie = Cookie.get(bannerGrCookieName);
        if (!banner_cookie.length){
            if (showPercentage > Math.round(Math.random()*100) + 1) {
                document.body.classList.add("topbar-visible");
                document.getElementById('topbar_cybermonday').classList.remove('hidden');
            }
        }
    }

</script>
