<script type='text/javascript'> 
        document.observe("dom:loaded", function(){
            var removeDefault = 1,
           	enableNew = 1,
            myOffSetNow = $('content').cumulativeOffset();       	
            if ( Prototype.Browser.IE7 || Prototype.Browser.IE8)
            {
				var enableNew = 0;
            }
            else
            {

            	if(removeDefault)
            	{
                	$('backtotop').remove();  
                }
                else{
                	$('backtotop').stopObserving('click');
                
               		if($('backtotop')){
              	    	$('backtotop').observe( "click", function(e){e.stop(); Effect.ScrollTo( $('top'), { duration:'0.5' }  ); } );
               		}
                }
            }
            if(enableNew){
	       		$(document).observe("scroll", function(e){
					documentOffset = document.viewport.getScrollOffsets();
		            if(documentOffset.top >= myOffSetNow.top)
		            {
			            if ( Prototype.Browser.IE7 )
			            {
			
			            }
			            else
			            {		            	
					        if($('scrolltotop')){
					          	$('scrolltotop').observe( "click", function(e){e.stop(); Effect.ScrollTo( $('top'), { duration:'0.5' }  ); } );
					        }			                	
						}
						if($('scrolltotop').getStyle('opacity') == 0){
							new Effect.Opacity('scrolltotop',{ from: 0, to: 0.9, duration: 0.5 });
						}
		            }
					else if( documentOffset.top <= myOffSetNow.top)
					{
						if($('scrolltotop').getStyle('opacity') >= 0.1)
						{
							new Effect.Opacity('scrolltotop',{ from: 0.9, to: 0, duration: 0.5 });
						}
					}
	        	})
			}            
        });
        </script>
<script type="text/javascript" src="//code.jquery.com/jquery-1.10.2.min.js"></script>
<script type="text/javascript">
jQuery.noConflict();
var currentSlide = 0;
var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];

jQuery(document).ready(function(){
jQuery(".timestamp").each(function(){
  var time = new Date(parseInt(jQuery(this).data('time'))*1000);
  var text = '- ';

  // get day
  var now = new Date();
  if(time.getMonth() == now.getMonth() && time.getDate() == now.getDate()) { text += 'Today, '; }
  if(time.getMonth() == now.getMonth() && time.getDate() == now.getDate()-1) { text += 'Yesterday, '; }
  if(text == '- ') { text += months[time.getMonth()] + ' ' + time.getDate() + ', '; }

  // get hour and minutes
  var hour = time.getHours();
  var am = true;
  if(hour > 12) { hour = hour - 12; am = false; }
  if(hour == 12) { am = false; }
  if(hour < 10) { hour = '0' + hour; }

  var min = time.getMinutes();
  if(min < 10) { min = '0' + min; }
  text += hour + ':' + min + ' ';
  if(am) { text += 'AM'; } else { text += 'PM'; }

  jQuery(this).html(text).show();
});

  var slidesCount = jQuery("#hero_images a").length;
  jQuery("#hero_images").width(slidesCount * 490).css('position', 'absolute').css('z-index', 1);
  jQuery("#hero_pages").css('z-index', '2');

  jQuery("#hero_pages").html('');
  jQuery("#hero_images a").each(function(){
    jQuery("#hero_pages").append('<a href="#" id="hero_page_link' + jQuery(this).index() + '"></a>');
  });
  jQuery("#hero_pages a:first").addClass("selected");
  jQuery("#hero_pages a").on('click', function(){
    jQuery("#hero_pages a").removeClass("selected");
    jQuery(this).addClass("selected");
    jQuery("#hero_images").animate({'left': jQuery(this).index() * -490});
    currentSlide = jQuery(this).index();
    return false;
  });

  setInterval(function(){
    jQuery("#hero_pages a").get(currentSlide).click();
    currentSlide++;

    if(currentSlide >= jQuery("#hero_pages a").length) {
      currentSlide = 0;
    }
  }, 3000);

