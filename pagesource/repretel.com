<script type="text/javascript">
$(function(){
	$("#facebook").html(output_estadisticas['facebook']['total']);
	$("#twitter").html(output_estadisticas['twitter']['total']);
	$("#instagram").html(output_estadisticas['instagram']['total']);
	$("#youtube").html(output_estadisticas['youtube']['total']);
	$("#gplus").html(output_estadisticas['gplus']['total']);
	
	$("img.lazy").lazyload({ 
		threshold : 200
	}).removeClass("lazy");

	$("#tendencia").append($("#widget_tendencias_datos").html());
		
	if (deviceDetect() == "mobile") {
		$("#widget_boton_mobile").append($("#widget_boton_datos").html());
	} else {
		$("#widget_boton").append($("#widget_boton_datos").html());
	}
	$("#widget_boton_lateral").append($("#widget_boton_lateral_datos").html());
	$("#widget_programacion_dektop").append($("#widget_programacion_desktop_datos").html());
	
	/* Recuperar Clima */
	$.simpleWeather({
		location: 'San Jose, CR',
		woeid: '',
		unit: 'c',
		success: function(weather) {
			temperatura = weather.temp + '&deg; ' + weather.units.temp;
			temperatura_maxima = 'Máx. ' + weather.high + '&deg; ' + weather.units.temp;
			temperatura_minima = 'Min. ' + weather.low + '&deg; ' + weather.units.temp;
			ubicacion = weather.city + ' - ' + weather.country;
			grafica = 'url(' + weather.image + ')';
			grafica_thumb = 'url(' + weather.thumbnail + ')';
			clima_responsive = weather.city + ' ' + temperatura;

			$('.clima-index .clima-image').css('background-image',grafica);
			$('.clima-index .temperatura').html(temperatura);
			$('.clima-index .ciudad').html(ubicacion);
	
			$('.clima span.temperatura').html(temperatura);
			$('#clima-desplegable .ciudad').html(ubicacion);

			$('#clima-desplegable .clima-list .image').css('background-image',grafica);
			$('#clima-desplegable .clima-list .temperatura_actual').html(temperatura);
			$('#clima-desplegable .clima-list .temperatura_maxima').html(temperatura_maxima);
			$('#clima-desplegable .clima-list .temperatura_minima').html(temperatura_minima);
			
			$('#clima-responsive .clima_responsive').html(clima_responsive);
	
		}, error: function(error) {
	  		$("#weather").html('<p>'+error+'</p>');
		}
	});
});
$(document).ajaxStop(function(){
	$("img.lazy").lazyload({ 
		threshold : 200
	}).removeClass("lazy");
});
