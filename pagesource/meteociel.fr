                                    // carte obs animation 12h

                                    images_url = new Array();
                                    images_url[0] = "/cartes_obs/archives/17-03-2018/temp2_1h-00.png";
images_url[1] = "/cartes_obs/archives/17-03-2018/temp2_1h-01.png";
