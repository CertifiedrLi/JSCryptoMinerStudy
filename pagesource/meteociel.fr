images_url[23] = "/cartes_obs/archives/17-03-2018/temp2_1h-23.png";
images_url[24] = "/cartes_obs/temp2_1h.png";

                                    var curImage = 24;
                                    var animated = 0;
                                    var numimages = 25;
                                    var precharge = 0;
                                    var speed = 500;
                                    var imageName = "obs_temps";
                                    var animLinkName = "animLink";

                                    images = new Array();

                                    function preloadImages()
                                    {
                                        precharge = 0;
                                        for(var i = 0; i < numimages; i++)
                                        {
                                            images[i] = new Image();
                                            images[i].onload = chargeFini;
                                            images[i].src = images_url[i];

                                        }
                                    }

                                    function chargeFini()
                                    {
                                        precharge++;
                                        document.getElementById(animLinkName).innerHTML = "Chargement (" + precharge  + "/" + numimages + ")";

                                        if(precharge == numimages)
                                        {
                                            curImage = 0;
                                            startAnim();
                                            activateMap();

                                        }
                                    }

                                    function activateMap()
                                    {
                                        if(curImage >= 24)
                                        {
                                            document.getElementById(imageName).useMap = "#observ";
                                        }
                                        else
                                        {
                                            document.getElementById(imageName).useMap = "";					  			
                                        }
                                    }


                                    function animate()
                                    {
                                        if(!animated) return;
                                        if( curImage < numimages)
                                        {
                                            document.getElementById(imageName).src = images_url[curImage];
                                        }
                                        curImage++;
                                        if(curImage == numimages+1) curImage = 0;

                                        if(curImage < numimages)
                                        {
                                            anim = setTimeout("animate()", speed);
                                        }
                                        else
                                        {	
                                            anim = setTimeout("animate()", 1500);
                                        }
                                        activateMap();
                                    }


                                    function faster()
                                    {
                                        if(animated == 1)
                                        {
                                            speed = speed - 100;
                                            if(speed < 100) speed = 100;
                                        }
                                        else
                                        {
                                            curImage = curImage + 1;
                                            if(curImage >= numimages) curImage = numimages-1;
                                            document.getElementById(imageName).src = images_url[curImage];
                                        }
                                        activateMap();
                                    }
                                    function slower()
                                    {
                                        if(animated == 1)
                                        {
                                            speed = speed + 100;
                                            if(speed > 2000) speed = 2000;
                                        }
                                        else
                                        {
                                            curImage = curImage - 1;
                                            if(curImage < 0) curImage = 0;
                                            document.getElementById(imageName).src = images_url[curImage];
                                        }
                                        activateMap();
                                    }

                                    function startAnim()
                                    {
                                        if(animated == 0)
                                        {
                                            animated = 1;
                                            animate();
                                            document.getElementById(animLinkName).innerHTML = "[ Stopper l'animation ]";
                                            document.getElementById(animLinkName).href = "javascript:stopAnim();";
                                            document.getElementById("next").innerHTML = " (+) ";
                                            document.getElementById("next").href = "javascript:faster();";
                                            document.getElementById("prev").innerHTML = " (-) ";
                                            document.getElementById("prev").href = "javascript:slower();";
                                        }
                                        activateMap();
                                    }
                                    function stopAnim()
                                    {
                                        if(animated == 1)
                                        {
                                            animated = 0;
                                            document.getElementById(animLinkName).innerHTML = "[ Animation sur 24h ]";
                                            document.getElementById(animLinkName).href = "javascript:startAnim();";
                                            document.getElementById("prev").href = "javascript:slower();";
                                            document.getElementById("prev").innerHTML = " &lt; ";
                                            document.getElementById("next").innerHTML = " &gt; ";
                                            document.getElementById("next").href = "javascript:faster();";
                                            clearTimeout(anim);
                                        }
                                    }

                                </script>


