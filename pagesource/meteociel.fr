<script type="text/javascript">

    function GetObjectFromNameCustom( nom )
    {
        var Pdiv = undefined;

        if ( document.getElementById && document.getElementById( nom ))
        {
            Pdiv = document.getElementById( nom );        
        }    
        else if ( document.all && document.all[ nom ] )
        {
            Pdiv = document.all[ nom ];
        }
        else if ( document.layers && document.layers[ nom ] )
        {
            Pdiv = document.layers[ nom ];
        }
        return Pdiv;
    }

    function DivStatus( nom )
    {
        var divID = nom;
        Pdiv = GetObjectFromNameCustom(nom);    
        if ( Pdiv != undefined)
        {
            Pdiv.style.display = ( Pdiv.style.display == 'none') ? 'block' : 'none';        
            EcrireCookie(nom, Pdiv.style.display);
        }
    }


    function EcrireCookie(nom, valeur)
    {        
        date=new Date;
        date.setMonth(date.getMonth()+1);
        document.cookie = nom + "=" + escape(valeur) + "; expires=" + date.toUTCString() + "; path=/; domain=.meteociel.fr";    }
    
    function EcrireCookieNoExpire(nom, valeur)
    {        
        document.cookie = nom + "=" + escape(valeur) + "; expires=Fri, 31 Dec 9999 23:59:59 GMT; path=/; domain=.meteociel.fr";    }

    function ChangeSigne ( nom )
    {
        var divID = nom;
        Pdiv = GetObjectFromNameCustom(divID);
        if (Pdiv != undefined )
        {
            var OldImg = Pdiv.innerHTML;    
            if (OldImg == '<img src="//static.meteociel.fr/Banner/plus.png">')
            {
                Pdiv.innerHTML = '<img src="//static.meteociel.fr/Banner/moins.png">';
            }
            else if (OldImg == '<img src="//static.meteociel.fr/Banner/moins.png">')
            {
                Pdiv.innerHTML = '<img src="//static.meteociel.fr/Banner/plus.png">';
            }
        }
    }

    // init the menu script
    menuIndex = 1;
    var ms = new Array();
    var im = new Array();
    ms[1] = '';
im[1] = '//static.meteociel.fr/Banner/moins.png';
ms[2] = '';
im[2] = '//static.meteociel.fr/Banner/moins.png';
ms[3] = '';
im[3] = '//static.meteociel.fr/Banner/moins.png';
ms[4] = '';
im[4] = '//static.meteociel.fr/Banner/moins.png';
ms[5] = '';
im[5] = '//static.meteociel.fr/Banner/moins.png';
ms[6] = '';
