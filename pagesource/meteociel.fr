ms[7] = '';
im[7] = '//static.meteociel.fr/Banner/moins.png';
ms[8] = '';
im[8] = '//static.meteociel.fr/Banner/moins.png';
ms[9] = '';
im[9] = '//static.meteociel.fr/Banner/moins.png';

    var menuItem;
    menuItem = GetObjectFromNameCustom('menutitre' + menuIndex);
    while( menuItem != undefined )
    {     

        menuItem.myId = menuIndex;        
        menuItem.onclick = function() {  ChangeSigne('pm' + this.myId); DivStatus( 'menu' + this.myId );  };

        // add plus/minus
        var pm = document.createElement("div");
        pm.id = 'pm' + menuIndex;
        pm.className="plusmoins";
        var img = document.createElement("img");
        img.src = im[menuIndex];
        pm.appendChild(img);
        menuItem.appendChild(pm);
        menuIndex++;
        menuItem = GetObjectFromNameCustom('menutitre' + menuIndex);
    }
