<script class="remove-script" type="text/javascript">
   function getSiteCookie (ckName) {
      var myCkName = ckName + "=";
      var allCookies = document.cookie.split(';');
      for(var i=0; i<allCookies.length; i++) {
         var c = allCookies[i];
         while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(myCkName) == 0) return c.substring(myCkName.length,c.length);
      }
      return "";
   }
   var ANONYMOUS = 'Anonymous';
   var MEMBER = 'Member';
   var AGENT = 'Agent';
   var CORPORATE = 'Corporate';
   var aemLoginStatus = getSiteCookie('aemLoginStatus');
   var CHECK_CLS = 'check-cls';
   var CHECK_DFNS_CLS = 'check-dfns-cls';
   var accessScreen;
   var accessDFNS;
   var memberType;
   var isDFNS;

   if (aemLoginStatus) {
      var aemLoginStatusArr = aemLoginStatus.split("~");
      if (aemLoginStatusArr[0])
         memberType = aemLoginStatusArr[0];
      if (aemLoginStatusArr[1])
         isDFNS = aemLoginStatusArr[1];
   }

   switch(memberType) {
      case MEMBER :
         accessScreen='member-cls';
         break;
      case AGENT :
         accessScreen='agent-cls';
         break;
      case CORPORATE :
         accessScreen='corporate-cls';
         break;
      default :
         accessScreen='anonymous-cls';
         break;
   }

   switch(isDFNS) {
      case "True" :
         accessDFNS='dfns-cls';
         break;
      case "False" :
         accessDFNS='hide-dfns-cls';
         break;
      default :
         accessDFNS='dfns-cls';
         break;
   }

   function checkScreen () {
      var elements = document.getElementsByClassName(CHECK_CLS);
      for( var i=0; i<elements.length; i++) {
         if( (' ' + elements[i].className + ' ').indexOf(' ' + accessScreen + ' ') == -1 ) {
            //elements[i].parentElement.removeChild(elements[i]);
            //elements[i].setAttribute("style","display:none !important;");
            elements[i].classList.add("hidden");
         } else {
            //elements[i].setAttribute("style","display:block !important;");
         }
      }
   }
   function checkDfnsScrn () {
      var elements = document.getElementsByClassName(CHECK_DFNS_CLS);
      var eleLength = elements.length;
      for( var i=0; i<eleLength; i++) {
         if( (' ' + elements[0].className + ' ').indexOf(' ' + accessDFNS + ' ') == -1 )
            elements[0].parentElement.removeChild(elements[0]);
      }
   }
   if(digitalData && digitalData.page){
        var user = digitalData.page.user || {};
        switch(aemLoginStatus){
            case MEMBER :
                 user.loginStatus = "Logged In";
                 user.userType = "Member";
                 break;
            case AGENT :
                 user.loginStatus = "Logged In";
                 user.userType = "Agent";
                 break;
            case CORPORATE :
                 user.loginStatus = "Logged In";
                 user.userType = "Corporate";
                 break;
            default :
                 user.loginStatus = "Not Logged In";
                 user.userType = "Anonymous";
                 user.customerId = "WebAnonymous";
                 break;
        }
        digitalData.page.user = user;
   }

   window.resCurrMemberType = memberType;
</script>
