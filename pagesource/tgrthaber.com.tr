<script type="text/javascript">
var _mn_timeSwap=3000;
var _mn_counter=1;
var anamansetTimeout = null;
_mn_item_count=15;

function anamanset_degistir(){    
    $item = "#mn_item_"+_mn_counter;
    var item2 = "mn_item_"+_mn_counter;

    $($item).fadeIn(100);
    $($item).find('.mn_link').removeClass('spot-gizle').fadeIn(300);
    $($item).find('.mn-abstract').removeClass('spot-gizle').fadeIn(1500);
    $('#anamanset .item').each(function(){
        if($(this).attr('id')!=item2) {
            $(this).find('.mn-abstract').fadeOut(200);
            $(this).find('.mn_link').fadeOut(300);
            $(this).fadeOut(500);
        }
    });
  
  
    $('#anamanset .mn_item').each(function(){
        $(this).removeClass('mn_selected');
    });
    $('#mnpg_'+_mn_counter).addClass('mn_selected');
    _mn_counter++;
    if(_mn_counter>_mn_item_count) _mn_counter=1;
}

$(document).ready(function(){
   
if(sayfa_id=="anasayfa") dv.loadCSSFile(TPL_URL+'css/anamanset.css');
else if(sayfa_id=="bilgilendirme_kategori" || sayfa_id=="yerel_sehir") dv.loadCSSFile(TPL_URL+'css/kategori_manset.css');
   anamansetTimeout = setInterval("anamanset_degistir()",_mn_timeSwap);
  _mn_item_count = $('#anamanset_pager .mn_item').length;
  
  $('#anamanset .spot-gizle').each(function(){
      $(this).removeClass('spot-gizle').hide();
      
  });
  
  $('#anamanset .item').mouseenter(function(){
      _mn_counter= $(this).attr("data-index");
      clearInterval(anamansetTimeout);
  }).mouseleave(function(){
      anamansetTimeout = setInterval("anamanset_degistir()",_mn_timeSwap);
  });
  $('#anamanset .mn_item').mouseenter(function(){
      _mn_counter= $(this).attr("data-index");
      $item = "#mn_item_"+$(this).attr("data-index");
      item2 = "mn_item_"+$(this).attr("data-index");
      
      
      
      $('#anamanset .item').each(function(){
          if($(this).attr('id')!=item2) {
              $(this).find('.mn-abstract').fadeOut(200);
              $(this).find('.mn_link').fadeOut(300);
              $(this).fadeOut(500);
          }
      });
      $($item).fadeIn(200);
      $($item).find('.mn_link').removeClass('spot-gizle').fadeIn(300);
      $($item).find('.mn-abstract').removeClass('spot-gizle').fadeIn(750);
      
      $('#anamanset .mn_item').each(function(){
          $(this).removeClass('mn_selected');
      });
      $('#mnpg_'+_mn_counter).addClass('mn_selected');        
      clearInterval(anamansetTimeout);
  }).mouseleave(function(){
      anamansetTimeout = setInterval("anamanset_degistir()",_mn_timeSwap);
  });
});
</script>
