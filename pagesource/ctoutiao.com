         *，我们自己的文章ID要通过全局变量获取
         * config为当前设置，返回值为更新后的设置。
         */
        function SetShareUrl(cmd, config) {
            if (ShareId) {
                config.bdUrl = "http://www.ctoutiao.com/" + ShareId + ".html";
            }
            if (ShareTitle) {
                config.bdText = ShareTitle;
            }
            if (ShareDesc) {
                config.bdDesc = ShareDesc;
            }
            return config;
        }

