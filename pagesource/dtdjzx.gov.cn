		<script language="javascript" type="text/javascript">
			//通知栏滚动功能。
			//注意：如果通知栏的内容太短，会导致有大片空白滚动的情况
			function scroll(obj) {
				var obj = $(obj);
				var containerWidth = obj.width();

				var scrollStep = 2; //移动步长为2像素
				var content = obj.children().first(); //目前要求滚动的html结构是固定的，只能有1个孩子；
				var contentWidth = content.width();
				if(content.length == 0 || !contentWidth) {
					return;
				}

				var copys = Math.ceil((contentWidth + containerWidth) / contentWidth) - 1; //计算需要复制的次数，保证能完成一次循环
				var contentContainer = $("<div />").css("position", "relative").css("width", "3000px");
				while(copys > 0) {
					content.clone().appendTo(contentContainer); //复制一份
					copys--;
				}
				content.appendTo(contentContainer);
				contentContainer.appendTo(obj);

				var offset = 0;
				var scrolling = function() {
					//当滚动条滚动了初始内容的宽度时滚动条回到最左端
					if(offset >= contentWidth) {
						offset = 0;
					} else {
						offset += scrollStep;
					}
					contentContainer.css("left", -offset);
				};

				var _timer = setInterval(scrolling, 40);

				obj.hover(function() {
					clearInterval(_timer);
				}, function() {
					_timer = setInterval(scrolling, 40);
				});
			}

			scroll('.notice_text');
		</script>
