            <img src="{{pics}}_230x230.jpg" alt="{{productTitle}}">
            <div class="extra-info">
              <span class="destination">{{city}}</span>
              <!--<span class="type">跟团游</span>-->
            </div>
          </div>
          <div class="item-info">
            <div class="item-title">
            {{#each titleTagList}}
              {{#if icon}}<image class="item-title-tag" src="{{icon}}"></image>{{/if}}
            {{/each}}{{productTitle}}
            </div>
            <div class="item-tag-container">
              {{#each tagList}}
                {{#if icon}}
                  <image style="position:relative;top:3px;height:16px;" src="{{icon}}"></image>
                {{/if}}
                {{#if ShowTextTag}}
                  <span style="padding:0 2px;height:16px;border: 1px solid {{borderColor}};color:{{textColor}};background:{{bgColor}}">{{text}}</span>
                {{/if}}
              {{/each}}
            </div>
            <div class="item-price">
              <div class="pi-price"><em>&yen;</em>{{priceStr}}<span class="qi">起</span></div>
              <div class="item-sales">30天售出{{seller}}件</div>
            </div>
          </div>
        </a>
      </li>
      {{/each}}
    </ul> 
  </template>

  <template class="J_FlightTpl">
     <ul class="clearfix">
      {{#each result}}
        <li>
          <a href="//sjipiao.fliggy.com/flight_search_result.htm?&depCity={{depCityCode}}&arrCity={{arrCityCode}}&tripType={{tripType}}&depDate={{depDate}}{{#if retDate}}&arrDate={{retDate}}{{/if}}" title="{{depCityName}}到{{arrCityName}}">
