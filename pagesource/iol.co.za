    };
    var onSubmit = function() {
        var values = getValues();
        var location = '/services-guide/' + values.servicesId + '?z=' + values.titleId + '+' + values.servicesId + '+' + values.searchInput;
        console_log('location', location, values);
        window.location = location;
    };
    var onReady = function() {
        $('.searchBar .red-btn').on('click', function() {
            onSubmit();
        });
        $('.servicesGuideSearch select').on('change', function() {
            onChange();
        });
        $('.searchBar .searchInput').on('keydown', function() {
            if (e.keyCode == 13) {
