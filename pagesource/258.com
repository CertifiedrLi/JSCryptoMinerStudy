            var uid=0;
            var arr = cookie.split(';');
            for (var i=0;i<arr.length;++i){
                var element=arr[i].split('=');
                if(element[0].replace(' ','') == 'stat_uid'){
                    uid=element[1];
                }
            }
            var s_ret = '0_' + uid;
        }else{
           var s_ret="0_0";
        }

        return s_ret;
    }
