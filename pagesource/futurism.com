				if(typeof ga !== typeof undefined){
					ga('set', 'dimension1', 'Infinite Scroll');
					ga('send', 'pageview', location.pathname + 'page/' + instance._numScrolls + 1);
				}
				amplitude.logEvent("Infinite Scroll Pageview");
				instance._numScrolls++;
			};
			return false;
		}
	});

