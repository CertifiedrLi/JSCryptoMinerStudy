<script type="text/javascript">
    var promo_code = 0;
    var gift_popup = 0;
    var package_country = '';
    var package_city = '';
    var see_package_person = '';
    var see_package_type = 'package';
    var see_package_url = 'https://www.sinemia.com/plans';
    function seeCities(){
        if (!modal.modalStatus) {
            modal.modalStatus = true;
        } else {
            return;
        }
        if (see_package_type == 'new_package') {
            var see_package_url_temp = 'https://www.sinemia.com/new-plans';
            if (see_package_person == 'double') {
                see_package_url_temp = 'https://www.sinemia.com/for-two/new-plans';
            }

            $('.popular-city-name a').each(function () {
                $(this).attr('href', $(this).attr('href').replace(see_package_url, see_package_url_temp) );
            });

            if (see_package_person == 'double') {
                see_package_url = 'https://www.sinemia.com/for-two/new-plans';
            } else {
                see_package_url = 'https://www.sinemia.com/new-plans';
            }
        } else {
            var see_package_url_temp = 'https://www.sinemia.com/plans';
            if (see_package_person == 'double') {
                see_package_url_temp = 'https://www.sinemia.com/for-two/plans';
            }

            $('.popular-city-name a').each(function () {
                $(this).attr('href', $(this).attr('href').replace(see_package_url, see_package_url_temp) );
            });

            if (see_package_person == 'double') {
                see_package_url = 'https://www.sinemia.com/for-two/plans';
            } else {
                see_package_url = 'https://www.sinemia.com/plans';
            }

        }

                    package_city = 'ashburn';
            redirectPackages();
            return false;
        
        $('#site_city .insider').css('margin-top',($(window).height()-250)/2+'px');
        $('#site_city').fadeIn();
        $('body').css('overflow','hidden');
    }
    function showCities() {
        if (see_package_type == 'new_package') {
            see_package_url = 'https://www.sinemia.com/new-plans';
        }
        if ($('#payment_country').val() == 'us') {
            $('#payment_zip_code').closest('div').removeClass('hide');
            $('#payment_country_city').closest('div').addClass('hide');
        } else {
            $('#payment_zip_code').closest('div').addClass('hide');
            $('#payment_country_city').closest('div').removeClass('hide');
        }
        $('#payment_country_city').empty().append('<option value="">Select</option>').show();
        $('.popular-city-name').empty();
        $('#site_city .extra-info').empty();
        $.post('https://www.sinemia.com/ajax/cities', {country: $('#payment_country').val(),_token: 'fO8qlN6yPWVjk7LWSHGnlVwbYZJ1nsb7TyRTN16o'}).done(function(data){
            $('.popular-city-name').empty();
            $('#site_city .extra-info').empty();
            if (data.status == 'success') {
                if (data.city.length) {
                    $('#site_city .extra-info').append('...available in every major theater in all :count cities.');
                    $('#site_city .extra-info').text($('#site_city .extra-info').text().replace(':count', (data.city.length)-15));
                    if ($('#payment_country').val() == 'us' || $('#payment_country').val() == 'au') {
                        $('#site_city .extra-info').empty();
                    }
                    $.each(data.city, function(key, value){
                        $('#payment_country_city').append('<option value="' + value.slug + '">' + value.name + '</option>');
                    });
                    $.each(data.short_list, function(key, value){
                        if (key < 15){
                            if (key == 0){
                                $('.popular-city-name').append('<div><a class="city-go" data-city-slug="' + value.slug + '" href="' + see_package_url + '/' + $('#payment_country').val() + '/' + value.slug + '">' + value.name + '</a></div>');
                            } else {
                                $('.popular-city-name').append('<div class="md-inline mv md-mv0"><div class="dot">.</div><a class="city-go" data-city-slug="' + value.slug + '" href="' + see_package_url + '/' + $('#payment_country').val() + '/' + value.slug + '">' + value.name + '</a></div>');
                            }
                        }
                    });
                } else {
                    //$('#site_city .extra-info').append('We are sorry we cannot serve in the your country.');
                }
            }
        }).fail(function(xhr){
            if (xhr.responseText){
                var errorMsg = '';
                $.each(JSON.parse(xhr.responseText), function(key, value){
                    if (value.length) {
                        $.each(value, function(key2, value2){
                            errorMsg += value2 + "<br>";
                        })
                    } else {
                        errorMsg += value + "<br>";
                    }
                });
                returnMsg(errorMsg, "Warning");
            }
        });
    }
    function redirectPackages() {
        var url = see_package_url;
        if (promo_code) {
            url = $('.banner-promo').attr('href');
        }
        if (!package_country) {
            package_country = $('#payment_country').val();
        }
        if (package_country) {
            url += '/' + package_country;
        }
        if (!package_city) {
            package_city = $('#payment_country_city').val();
        }
        if (package_city) {
            url += '/' + package_city;
        }
        if (gift_popup) {
            url += '/gift';
        }
        if ($('#payment_zip_code').val()) {
            url += '/?zip=' + $('#payment_zip_code').val();
        }
        window.location.href = url;
    }
    $('#payment_country').change(function(){
        showCities();
    });
    $("body").on("click tap", ".see-plans", function(e) {
        e.preventDefault();
        if ($(".fa-close").is(e.target)) {
            return false;
        } else if ($(this).data('href')) {
            window.location.href = $(this).data('href');
            return true;
        }
        promo_code = $(this).hasClass('promo-code');
        gift_popup = $(this).hasClass('gift-popup');
        see_package_person = $(this).data('package-person') || '';
        see_package_type = $(this).data('package-type') || 'package';
        if (gift_popup) {
            $('.buyYourself').removeClass('hide');
            $('.buyMyself').addClass('hide');
        } else {
            $('.buyMyself').removeClass('hide');
            $('.buyYourself').addClass('hide');
        }
        if (promo_code) {
            $('#see-packages-use-discount-button').removeClass('hide');
            $('#see-packages-button').addClass('hide');
        } else {
            $('#see-packages-button').removeClass('hide');
            $('#see-packages-use-discount-button').addClass('hide');
        }
        seeCities();
    });
    $("body").on("click", "#site_city .see-packages", function(e) {
        e.preventDefault();
        redirectPackages();
    });
    $("body").on("click", ".city-go", function(e) {
        e.preventDefault();
        var url = $(this).attr('href');
        if (promo_code) {
            url = $('.banner-promo').attr('href') + '/' + $('#payment_country').val() + '/' + $(this).data('city-slug');
        }
        if (gift_popup) {
            url += '/gift';
        }
        window.location.href = url;
    });
    $('#site_city .close-modal').on('click',function(e) {
        e.preventDefault();
        $('#site_city.select-city').fadeOut();
        $('body').css('overflow','auto');
        modal.modalStatus = false;
    });
    $('#payment_zip_code').keypress(function (e) {
        if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
            $('.see-packages', '#site_city').trigger('click');
            return false;
        } else {
            return true;
        }
    });
            $('#payment_country').val('us').trigger('change');
    </script>
