    function openPopupPromoCode(type) {
                    generateCampaignCode('tombraider', type);
                window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'popupView',
            'popupID': '262',
            'popupName': 'US_tombraider_Premiumda '
        });
    }

    function generateCampaignCode(txt_code, type)
    {
                    $code = txt_code + '_' + Math.random().toString(36).substring(2, 9);
                $code = $code.toUpperCase();

        $('.popup-promo h2').hide();
        $('.popup-promo h2.type'+type).show();

        $('#coupon-get-code').text($code);
                                            $(".popup-promo .use-coupon").attr('href', "https://www.sinemia.com/new-plans/promo/" + $code);
                            
        $('body').addClass('opened-popup-promo-code');
        setCookieNoDomain('popup_promo_code', 1, 2);
        popupPromoCodeCountDown();

        return $code;
    }


    function popupPromoCodeCountDown() {
        var p1 = $('.popup-promo-code-sticky-helper');
        var p2 = $('.popup-promo');
        var ppint = setInterval(function(){
            $sec = $('.lsec',p1).text();
            $min = $('.lmin',p1).text();
            if ($sec.substr(0,1) == '0') {
                $sec = $sec.substr(1,1);
            }
            if ($min.substr(0,1) == '0') {
                $min = $min.substr(1,1);
            }
            $sec_new = $sec-1;

            if ($sec_new < 10) {
                $addsec = "0";
            } else {
                $addsec = "";
            }
            if ($sec_new != '0') {
                $('.lsec').text($addsec+$sec_new);
            } else {
                $min_new = $min - 1;
                if ($min_new < 10) {
                    $addmin = "0";
                } else {
                    $addmin = "";
                }
                if ($min > 0) {
                    $('.lmin',p1).text($addmin+$min_new);
                    $('.lmin',p2).text($addmin+$min_new);
                    $('.lsec',p1).text('59');
                    $('.lsec',p2).text('59');
                } else {
                    $('.lmin',p1).text('00');
                    $('.lmin',p2).text('00');
                    $('.lsec',p1).text('00');
