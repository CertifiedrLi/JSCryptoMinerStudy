<script type="text/javascript">
    var MobileCodeLoaded = false;
    var isCollegeRep = false;
    $("#OpenUserLoginPopup").click(function () {
        $(".shadowLayer").addClass("opened");
        $("#popLoginScreen").addClass("fadeInDown");
        $("#popLoginScreen").show();
        LoadMobileCodes();
        isCollegeRep = false;

    });

    function LoadMobileCodes() {

        if (MobileCodeLoaded == false) {


            var codehtml = "";
            for (var i = 300; i <= 317; i++) {
                codehtml += "<option value='" + i + "'>0" + i + " </option>";
            }

            for (var i = 320 ; i <= 325; i++) {
                codehtml += "<option value='" + i + "'>0" + i + " </option>";
            }
            for (var i = 331 ; i <= 337; i++) {
                codehtml += "<option value='" + i + "'>0" + i + " </option>";
            }
            for (var i = 340 ; i <= 348; i++) {
                codehtml += "<option value='" + i + "'>0" + i + " </option>";
            }

            $("#rcode").html(codehtml);
            MobileCodeLoaded = true;
        }
    }

    $(document).ready(function () {
        $("#popForgotPassword").click(function () {
            $("#popLoginScreen").hide();
            $("#popForgotScreen").removeClass("slideOutUp");
            $("#popForgotScreen").addClass("fadeInDown");
            $("#popForgotScreen").show();
        });

        $("#popLogin").click(function () {
            BackToLogin();
            ShowLoginAgain();
        });
        $(document).keyup(function (e) {
            var shadbow = $("#shadowbox").hasClass("opened");
            if (shadbow == true) {
                if (e.keyCode === 27) {
                    CloseLogin();
                }
            }
        });

        $(document).mouseup(function (e) {
           
            return false;
            var shadbow = $("#shadowbox").hasClass("opened");
            if (shadbow == true) {

                var ilogin = 3;
                var popup1 = $("#mlogin");
                var popup2 = $("#mlogin2");
                var popup3 = $("#mregister");

                if (popup1.has(e.target).length == 0) {
                    ilogin = ilogin - 1;
                }
                if (popup2.has(e.target).length == 0) {
                    ilogin = ilogin - 1;
                }
                if (popup3.has(e.target).length == 0) {
                    ilogin = ilogin - 1;
                }

                if (ilogin == 0) {
                    CloseLogin();
                }
            }

        });

        $(".btnCloseLoginScreens").click(function () {
            CloseLogin();
        });

        function CloseLogin() {

            $(".ikdPopupsLoginScreens").removeClass("fadeInDown");
            $("#popRegisterScreen").removeClass("slideOutUp");
            $(".ikdPopupsLoginScreens").addClass("slideOutUp");
            $(".ikdPopupsLoginScreens").hide();


            $("#popLoginScreen").removeClass("slideOutUp");
            $(".shadowLayer").removeClass("opened");

            //re-set Email and Password Popup
            $("#getUserPassword").hide();
            $("#getUserEmailAddress").show();

            $(".modalTop").show();
            $(".modalBottom").hide();
            // Remove Error Class
            $("#getUserEmailAddress").removeClass("Email-error");
            $("#getUserPassword").removeClass("Email-error");

            $("#btnEmailCheck").show();
            $("#btnPasswordNext").hide();
            BackToLogin();
            $("#txtUserName").val("");
            $("#txtUserPassword").val("");
            ClearRegisterationFields();
        }

        function ClearRegisterationFields() {

            $("div.fancy-input-group").removeClass("error-field");


        }

        $(".registerationTypes li div.box").click(function () {

            $(".contentByType").show();

            if ($(this).hasClass("acStudent")) {
                $("#popRegisterScreen  h3.accountTitle").html("Register as Student");
                $(".registerationTypes2 li div.box").removeClass("active");
                $(".registerationTypes2 li div.box.acStudent").addClass("active");

                $("#boxStudentAccountType").addClass("active");
            }
            if ($(this).hasClass("acTutor")) {
                $("#popRegisterScreen  h3.accountTitle").html("Register as Tutor");
                $(".registerationTypes2 li div.box").removeClass("active");
                $(".registerationTypes2 li div.box.acTutor").addClass("active");
            }
            if ($(this).hasClass("acConsultant")) {
                $("#popRegisterScreen  h3.accountTitle").html("Register as Consultant");
                $(".registerationTypes2 li div.box").removeClass("active");
                $(".registerationTypes2 li div.box.acConsultant").addClass("active");
            }
            if ($(this).hasClass("acEmployee")) {
                $("#popRegisterScreen  h3.accountTitle").html("Register as Employer");
                $(".registerationTypes2 li div.box").removeClass("active");
                $(".registerationTypes2 li div.box.acEmployee").addClass("active");
            }


            $("#popRegisterScreen .modalDiv").addClass("active");
            $("#popRegisterScreen .modalMain").addClass("active");
            $(".registerationTypes").addClass("active");

        });

    });

    function ShowMessage(id, msg, CSSClass, time) {
        $("#" + id).addClass(CSSClass);
        $("#" + id).html(msg);
        setTimeout(function () {
            $("#" + id).html("");
            $("#" + id).addClass("");
        }, time);
    }

    function AddError(id) {
        $("#" + id).addClass("error-field");
    }

    function RemoveError(id) {
        $("#" + id).removeClass("error-field");
    }
    function RecoverPassword() {

        var a = $("#forgetEmailAddress").val();
        var k = validateEmail(a);
        if (k == false) {

            $("#forgetEmailAddress").parent(parentdiv).addClass(errorclass);
            return false;
        }
        $("#forgetEmailAddress").parent(parentdiv).removeClass(errorclass);
        $.ajax({
            type: "POST",
            data: "{'email' : '" + a + "'}",
            contentType: "application/json; charset=utf-8",
            url: '/UserLoginFunctions.asmx/SendForgetPassword',
            dataType: 'json',
            success: function (data) {

                if (data.d == true) {
                    $('#forgetEmailAddress').val("");
                    ShowMessage("lblforget", "Email has been sent on the provided Email", "ok", 5000)
                }
                else {
                    $("#forgetEmailAddress").parent(parentdiv).addClass(errorclass);
                    ShowMessage("lblforget", "Provided Email is not registered", "errorclass", 5000);
                }

            }

        });

    }

    function getPasswordForLogin() { UserLogin(); }
    function openLoginScreenPopup() {
        $(".ikdPopupsLoginScreens").addClass("slideOutUp");
        $(".ikdPopupsLoginScreens").hide();
        $("#popLoginScreen").removeClass("slideOutUp");
        $("#popLoginScreen").addClass("fadeInDown");
        $("#SpinnerDiv").hide();
        $("#popLoginScreen").show();
    }

    function openRegisterScreenPopup() {
        $("#haccountType").html("Register Type");
        $(".ikdPopupsLoginScreens").addClass("slideOutUp");
        $(".ikdPopupsLoginScreens").hide();
        $("#popRegisterScreen").removeClass("slideOutUp");
        $("#popRegisterScreen").addClass("fadeInDown");
        $("#popRegisterScreen").show();

        $("#popRegisterScreen .modalDiv").removeClass("active");
        $("#popRegisterScreen .modalMain").removeClass("active");
        $(".registerationTypes").removeClass("active");
        $(".contentByType").hide("");

    }

    function BackToLogin() {
        $("#popForgotScreen").addClass("slideOutUp");
        $("#popForgotScreen").removeClass("fadeInDown");
        $("#popForgotScreen").hide();
        $("#forgetEmailAddress").val("");
        RemoveError("divforgetemail");


    }

    function ShowLoginAgain() {
        $("#popLoginScreen").addClass("fadeInDown");
        $("#popLoginScreen").show();
    }

    var UserMemberType = 1;
    function RegisterUser() {



        var rv = ValidateRegisteration();
        if (rv == 1) {
            return false;
        }
        var lino = 1;
        var kType = 1;
        $("#ulregtypes").children("li").each(function () {

            var a = $(this).children("div").hasClass("active");

            if (a == true)
            {
                UserMemberType=parseInt($(this).children("div").children("input").val());
            }
            
        });
        $("#SpinnerDivForReg").show();
        $("#RegisterAnchorTag").hide();
        var ilmMember = {};
        ilmMember.isFromCollegeRep = isCollegeRep;
        ilmMember.member_first_name = $("#rname").val();
        ilmMember.email = $("#remail").val();
        ilmMember.password = $("#rpassword").val();
        ilmMember.member_Type_ID = UserMemberType.toString();
        ilmMember.mobile_number = $("#rcode option:selected").val().toString() + $("#rno").val().toString();
        ilmMember.MemberType = UserMemberType;
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: "{ilmMember:" + JSON.stringify(ilmMember) + "}",
            url: '/UserLoginFunctions.asmx/RegisterUser',
            dataType: 'json',
            success: function (j) {

                if (j.d[0] == true) {
                    window.location.href = j.d[1];
                }
                else {

                    $("#SpinnerDivForReg").hide();
                    $("#RegisterAnchorTag").show();
                    ShowMessage("lblRegisterLogin", "Email Already in use", "errorclass", 200000);
                }

            },
            error: function (err) {
                console.log(err);
            }
        });
    }

    var errorclass = "error-field";
    var parentdiv = "div.fancy-input-group";
    function ValidateRegisteration() {
        var Errors = 0;
        var Name = $("#rname").val();
        if (Name.trim() == "") {
            $("#rname").parent(parentdiv).addClass(errorclass);
            Errors = 1;
        }
        else {
            $("#rname").parent(parentdiv).removeClass(errorclass);
        }

        var Code = $("#rno").val();

        if (Code.trim() == "") {
            $("#rno").parent(parentdiv).addClass(errorclass);
            Errors = 1;
        }
        else {
            $("#rno").parent(parentdiv).removeClass(errorclass);
        }


        var Email = $("#remail").val();
        if (validateEmail(Email) == false) {
            $("#remail").parent(parentdiv).addClass(errorclass);
            Errors = 1;
        }
        else {
            $("#remail").parent(parentdiv).removeClass(errorclass);
        }


        var Password = $("#rpassword").val();
        if (Password.trim() == "") {
            $("#rpassword").parent(parentdiv).addClass(errorclass);
            Errors = 1;
        }
        else {
            $("#rpassword").parent(parentdiv).removeClass(errorclass);
        }

        var Rpassword = $("#rrepassword").val();
        if (Rpassword.trim() == "") {
            $("#rrepassword").parent(parentdiv).addClass(errorclass);
            Errors = 1;
        }
        else {
            if (Password.trim() == Rpassword.trim()) {
                $("#rrepassword").parent(parentdiv).removeClass(errorclass);
            }
            else {

                $("#rrepassword").parent(parentdiv).addClass(errorclass);
                Errors = 1;
            }
        }

        return Errors;

    }


    function UserLogin() {


        var userName = $('#txtUserName').val();

        if (validateEmail(userName) == false) {
            $("#txtUserName").parent(parentdiv).addClass(errorclass);
            //$("#btnEmailCheck").show();
            return false;
        }
        $("#txtUserName").parent(parentdiv).removeClass(errorclass);


        $("#SpinnerDiv").show();
        $("#btnEmailCheck").hide();

        $.ajax({
            type: "POST",
            data: "{'UserEmail' : '" + userName + "'}",
            contentType: "application/json; charset=utf-8",
            url: '/UserLoginFunctions.asmx/CheckUserName',
            dataType: 'json',
            success: function (data) {

                //[0] status
                //[1] name
                //[2] email
                //[3] image
                if (data.d[0] == 1) {

                    $("#UserAddressSuccess").html(data.d[2]);
                    $("#UserNameTitle").html("Hi " + data.d[1]);

                    $("#UserImage").attr('src', data.d[3]);
                    $(".modalTop").hide();
                    $(".modalBottom").show();
                    $("#getUserEmailAddress").hide();
                    $("#getUserPassword").show();
                    $("#btnEmailCheck").hide();
                    $("#btnPasswordNext").show();
                    $("#txtUserPassword").focus();

                    $("#SpinnerDiv").hide();
                }
                else {

                    $("#SpinnerDiv").hide();
                    $("#btnEmailCheck").show();
                    $("#txtUserName").parent(parentdiv).addClass(errorclass);
                    return false;
                }
            },
            error: function (err) {
                console.log(err);
            }
        });



    }
    function CheckUserNamePassword() {
        var userName = $('#txtUserName').val();
        var password = $('#txtUserPassword').val();

        if ($.trim(password).length === 0) {
            $("#txtUserPassword").parent(parentdiv).addClass(errorclass);
            $('#txtUserPassword').focus();
            return false;
        }
        $("#SpinnerDiv").show();
        $("#btnPasswordNext").hide();
        var ilmMember = {};
        ilmMember.MemberUsername = userName;
        ilmMember.MemberPassword = password;
        ilmMember.isFromCollegeRep = isCollegeRep;
        
        

        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: "{ilmMember:" + JSON.stringify(ilmMember) + "}",
            url: '/UserLoginFunctions.asmx/LoginUserNamePassword',
            dataType: 'json',
            success: function (j) {

                if (j.d[0] === 1) {
                    window.location.href = j.d[1];

                }
                else {

                    $("#SpinnerDiv").hide();
                    $("#btnPasswordNext").show();

                    $("#txtUserPassword").parent(parentdiv).addClass(errorclass);

                    ShowMessage("lblemaillogin", "Invalid Password Provided", "errorclass", 9000);

                }


            },
            error: function (err) {
                console.log(err);
            }
        });
        return false;
    }



    function validateEmail(sEmail) {
        var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        if (filter.test(sEmail)) {
            return true;
        }
        else {
            return false;
        }
    }

    //Set Error Class
    function SetEmailClasses() {
        $("#getUserEmailAddress").addClass("Email-error");
        $('#txtUserName').val("").focus();
    }

    function SetPasswordClasses() {
        $("#getUserPassword").addClass("Email-error");
        $('#txtUserPassword').val("").focus();
    }
</script>

