<script type="text/javascript">
	$(document).ready(function() {
		//header特效
		(function() {
			var headerMenu = $(".header-menu"),
				mobileMenu = $(".mobile-menu").children("ul"),
				width = $(window).width();
			if (width > 768) {
				headerMenu.children("li").hover(function(e) {
					headerMenu.children("li.cur").removeClass("cur");
					$(this).addClass("cur");
				});
			} else {
				mobileMenu.children("li").hover(function(e) {
					mobileMenu.children("li.cur").removeClass("cur");
					$(this).addClass("cur");
				});
			}
		}());
		$(".menu-icon").click(function() {
			var mobileMenu = $(".mobile-menu");
			if (mobileMenu.is(":hidden"))
				mobileMenu.css({
					width: 0
				}).show().animate({
					width: 150
				}, 200);
			else 
				mobileMenu.hide();
		});
	});
</script>

	<script type="text/javascript">
	$(document).ready(function() {

	    $('.removexl').next().hide();

		//大图轮转

	    $('.bg').bgStretcher({
	        images: [
						'http://www.bfsu.edu.cn/wp-content/uploads/2018/03/18东院大门俯视.jpg',			'http://www.bfsu.edu.cn/wp-content/uploads/2017/03/on_DSC1767.jpg',			'http://www.bfsu.edu.cn/wp-content/uploads/2017/11/homeslide.jpg',			'http://www.bfsu.edu.cn/wp-content/uploads/2017/10/1.jpg',			'http://www.bfsu.edu.cn/wp-content/uploads/2016/01/校门.jpg',	        ],
	        imageWidth: 1400,
	        imageHeight: 1050,
	        transitionEffect: 'fade',
	        sequenceMode: 'normal',
	        nextSlideDelay: '8000',
	        preloadImg: true
		});

		$(window).resize(function(){
			checkWindowSize();
		});

		function checkWindowSize(){
			var width = $(window).width();
			if (width < 768){
				$('.mask-box').hide();
				$('.mask-box-hr').hide();
			}else{
				$('.mask-box').show();
				$('.mask-box-hr').show();
			}
		}

		function maskInit(){
			var width = $(window).width();
			if (width > 768){
				var animateDuration = 1000;
			    $('.mask-header').hide();
			    $('.mask-bottom').hide();

				$('.mask-box').css({
					'height': 0,
					'bottom': 128,
				}).animate({
					'height': 136,
					'bottom': 30,
				}, animateDuration);

				$('.mask-box-hr').css({
					'right': 250,
					'width': 0
				}).animate({
					'right': 35,
					'width': 430,
				}, animateDuration, function(){
					$('.mask-header').css({
						'height': 0,
						'top': 70
					}).show().animate({
						'height': 45,
						'top': 20
					}, 500);

					$('.mask-bottom').css({
						'height': 0,
						'bottom': 60
					}).show().animate({
						'height': 45,
						'bottom': 0
					}, 500);
				});
			}
		}
	   	maskInit();

	   	$('.boxhelper li').hover(function(){
	   		$('.boxhelper li').removeClass('cur');
	   		$(this).addClass('cur');
	   	});

	   	$('.xwc').hover(function(){
	   		$('.sbox').hide();
	   		$('.xw').show();
	   	});

	   	$('.ggc').hover(function(){
	   		$('.sbox').hide();
	   		$('.gg').show();
	   	});

	   	$('.whc').hover(function(){
	   		$('.sbox').hide();
	   		$('.wh').show();
	   	});

	   	$('.jlc').hover(function(){
	   		$('.sbox').hide();
	   		$('.jl').show();
	   	});

	});
</script>

