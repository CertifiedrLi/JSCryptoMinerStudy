<script type="text/javascript">
function getMenuRaw(subMenu)
{
    var elm = document.getElementById('kw');
    var inputValue = '';
    if (typeof elm !== undefined)
    {
        inputValue = elm.value;
    }
        var menuData = null;
    if (window.sessionStorage)
    {
        var json_str = window.sessionStorage.getItem('navi_menu');
        if ((typeof json_str !== undefined) && json_str)
        {
            // cache hit
            menuData = JSON.parse(json_str);
            menuData = menuData['menu'];
        }  // end if()
    }  // end if()

    //
    // decide do we need ajax or not
    //
    if (!menuData || (subMenu && inputValue && (inputValue.length > 0)))
    {
        fetchMenu = (!menuData) ? true : false;
        searchKW = (inputValue && (inputValue.length > 0)) ? 
            inputValue : '';

		$.ajax({
			type:'POST',
			url: '/ajax/',
			data: {
				'mod'    : 'jsmenu',
				'act'    : 'getSearchItemValue',
				'searchItem' : searchKW,
                'naviMenu'   : fetchMenu
			},
			error: function(xhr, status, erorr) {
				console.log(xhr.responseText + 'Woops! ajax error ');

                //
                // build navi menu anyway
                //
                if (typeof menuData !== undefined)
                {
                    genMenu(menuData);
                }
			},
			success: function(json_str) {
				data = JSON.parse(json_str);
                if ((typeof data['menu'] !== "undefined") && data['menu'])
                {
                    menuData = data['menu'];
                    window.sessionStorage.setItem('navi_menu', json_str);
                }

                //
                // build navi menu
                //
                if ((typeof menuData !== undefined) && menuData)
                {
                    genMenu(menuData);
                }

                if (typeof genSubMenu === 'function')
                {
                    genSubMenu(data);
                }
			}
		});
    } else {
        genMenu(menuData);
	}
}

//處理字串有斜線的問題 ex NB/PC
function replaceslash(str) {
	return str.replace(/%2F/, "/");
}

function genMenu(data) {
	if (data.length != 0) {
		var appendStr = '';
		//
		//  Cate Level 1 here
		//
		for (var idx1 in data) {
			//  exclude items w/o title
			if (data[idx1]['title'] == undefined) { break; }

			var level1Title 	 = data[idx1]['title'];
			var level1Value      = data[idx1]['cate_value'];
			var level2Arr 	     = data[idx1]['tag'];
			var recommendBrand   = new Array();
			var promotion 		 	 = new Array();
			var promotionUrl     = '';
			var appendElementId  = "menu_catevalue" + level1Value;
            var append_elm = document.getElementById(appendElementId);
            if (append_elm === null)
                continue;
			
			appendStr +=
			'	<a class="navbtn" href="/x/iguang/' + replaceslash(level1Title) + '" title="' + level1Title + '">' + level1Title + '</a>' +
			'	<div class="navbar_dropmenu">' + 
			'		<div class="navbar_dm navbar_dm_cata group">';

			var typeUrl = '/x/iguang/';
			if (level1Title == '圖書') {
				typeUrl = '/x/book/';
			}
			//console.log(level1Title);
			//
			//  Cate Level 2 here
			//
			for (var idx2 in level2Arr) {

				var level3Arr = level2Arr[idx2]['tag'];
				if (level2Arr[idx2]['title'] 				== '推薦品牌') {
					recommendBrand = level2Arr[idx2];
				} else if (level2Arr[idx2]['title'] == '促銷活動') {
					promotion = level2Arr[idx2];
				} else if (level2Arr[idx2]['title'] == '書籍推薦') {
					promotion = level2Arr[idx2];
					promotionUrl = "/x/book/";
				} else {
					var level2Search = level2Arr[idx2]['searchTag'];
					var level2Title  = level2Arr[idx2]['title'];
					var level2Value  = level2Arr[idx2]['cate_value'];

					appendStr += 
			'			<div>' +
			'				<dl>'  +
			'					<dt><a href="'+ typeUrl + replaceslash(level2Search) + '" >' + level2Title + 
			'					</a></dt>';
					//
					//  Cate Level 3 here
					//
					for (var idx3 in level3Arr) {

						var level3Search = level3Arr[idx3]['searchTag'];
						var level3Title  = level3Arr[idx3]['title'];
						var level3Value  = level3Arr[idx3]['cate_value'];

						appendStr +=
			'					<dd><a href="'+ typeUrl + replaceslash(level3Search) + '">' + level3Title + '</a></dd>';
					}//  end for (third leve3)

					appendStr += 
			'				</dl>' +
			'			</div>';
				}//  end if
			}//  end for (second leve2)

			//
			//  check if cate has recommendation
			//
			if (recommendBrand['tag'] != undefined) {

				appendStr +=
				'		</div>' +
				'		<div class="navbar_dm navbar_dm_recd group">' +
				'			<dl class="navbar_dm_recd-dl group">';

				appendStr +=
			'				<dt><a>' + recommendBrand['title'] + '</a></dt>';

				var rcArr = recommendBrand['tag'];

				for (var rc in rcArr) {

					var rcSearch = rcArr[rc]['searchTag'];
					var rcTitle  = rcArr[rc]['title'];
					appendStr +=
			'				<dd><a href="/x/iguang/' + replaceslash(rcSearch) + '">' + rcTitle + '</a></dd>';
				}//  end for (recommendation)

					appendStr += 
					'			</dl>' + 
					'			<div class="navbar_dm navbar_dm_tag group">';
					// check if cate has promotion
					var color = ['#eb1478', '#f07e2c', '#f2af00', '#7ab800', '#079dd3', '#334ec8', '#8905b2'];
					var i = 0;
					
					if (promotion['tag'] != undefined) {
		
						var proArr = promotion['tag'];
		
						for (var pro in proArr) {
		
							if (proArr[pro]['searchTag'].search("iguang.tw") > -1) {
								var proURL = proArr[pro]['searchTag'];
							} else {
								var proURL = "/t/goto?url=" + proArr[pro]['searchTag'];
							}//  end if 
							var proTitle = proArr[pro]['title'];
							appendStr +=
							'		<span><a href="' + proURL + '" class="btn dm_tag dm_tag-ad1" style="background:' +
										color[i] + '" ref="nofollow">' + proTitle + '</a></span>';
							i++;
						}//  end for (promotion)
					}//  end if (promotion)

			}//  end if (recommedation)


			appendStr +=
			'			</div>' +
			'		</div>' +
			'	</div>';

            append_elm.innerHTML = appendStr;
			appendStr = '';
		}//  end for (first level)
	}//  end if(check if data is empty)

}

function doSubMenuInit(){
	var mainLmenu = $('.subMenu');
	mainLmenu.bind({
		mouseenter:function(){
			$(this).find('.noline_sub').delay(100).show(10);
		},//mouseenter end
		mouseleave:function(){
			$(this).find('.noline_sub').stop(true,true).hide();
		}//mouseleave end
	});//bind end
	var mainRmenu = $('.subBigMenu');
	mainRmenu.bind({
		mouseenter:function(){
			$(this).find('.navbar_dropmenu').delay(100).show(10);
		},
		mouseleave:function(){
			$(this).find('.navbar_dropmenu').stop(true,true).hide();
		}
	});

	//設定小刊頭的下拉選單的定位和寬度	
	var main_outer = $('.subMenu');
	main_outer.css('position', 'relative');

	//設定子選項的絕對定位、一樣寬度
	main_outer.each(function(index){
		var child = $(this).find('.noline_sub').first();
		var rowNum = child.find('dl').length;//有幾排子清單

		child.css({'position':'absolute', 'top':'100%', 'left':'0px', 'width': (110*rowNum) + 'px'});
		child.find('dl').css({'width':(100/rowNum) + '%', 'box-sizing':'border-box', '-webkit-box-sizing':'border-box', '-moz-box-sizing':'border-box'});
		
		//多排時，前幾排要有右側邊框
		for(var i=0; i < (child.find('dl').length-1); i++){
			var dl = child.find('dl').eq(i);
			dl.css('border-right', '1px solid #ffb8c9');
		}

		//子選項的高度跟子選項第一排一樣
		child.css('max-height', '800px');
		//child.css('height', child.find('dl').eq(0).height() + 'px');
		// console.log('index' + index);
		// console.log('childw =' + (100/rowNum) + '%');
		// console.log('ulnum =' + rowNum);
	});
	//
	// 下拉選單"我的慾望牆"的寬度修正
	//
	main_outer.first().find('.noline_sub').css('width', '100%');
}

$(document).ready(function()
{
    getMenuRaw(true);
	doSubMenuInit();

});
