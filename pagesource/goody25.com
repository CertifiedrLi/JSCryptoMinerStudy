<script>
function loadLatestMind(){
   $.ajax({
    url: 'index.php?route=common/footer/loadLatestMind',
    type: 'get',
    dataType: 'json',
    success:function(json){
      $('#new_article').html(json['html']);
      $('#new_article_a').removeAttr('onmouseover');
    }
  });
}

function loadTopBarMind(cat){
   $.ajax({
    url: 'index.php?route=common/footer/loadTopBarMind',
    type: 'post',
    dataType: 'json',
    success:function(json){
      for(var k in json){
       $('#top_article_'+k+'').html('');
        for(var x in json[k]){
          $('#top_article_'+k+'').append(json[k][x]['html']);
          $('#top_article_a_'+k+'').removeAttr('onmouseover');
        }
      }
    }
  });
}
</script>
<!-- change language -->
<script>
$(document).on('change','#choose_language','#choose_language',function(){
  $.ajax({
    url: 'index.php?route=common/footer/setSession&language='+this.value,
    type: 'get',
    dataType: 'json',
  });
  var expires = new Date();
  expires.setTime(expires.getTime() + 31536000000); //1 year  
  document.cookie ='language=' + this.value + ';expires=' + expires.toUTCString();
  location.reload(true);
});
</script>
