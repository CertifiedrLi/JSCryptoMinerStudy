        $(document).on('mouseout', '.download-right-box', function(){
            $(this).find('.submenu').hide();
        });

        /* 帖子收藏 */
        $(document).on('click', '.sclick', function(){
            var json = {tid:$(this).attr('tid'), subject: $(this).attr('subject'), sc:1};
            $.post('plugin.php?id=home_index', json, function( res ){
                if(res.ret == 1){
                    showDialog(res.msg, 'right');
                }else{
                    showDialog(res.msg, 'notice');
                }
            }, 'json');
        });

        var page1 = 2;
        var compiler1 = Handlebars.compile($('#recommend_thread').html());
        function loadRecommendThread(){
            $.ajax({
                type: "GET",
                url: "plugin.php?id=home_index",
                dataType: "json",
                data: {json:1, page: page1, isajax:1},
                success: function(res){
                    if(res.ret == 1){
                        page1 = res['data']['next_page'];
                        var html = compiler1(res['data']['list']);
                        $('#recommend').append(html)
                    }else if(res.ret == 0){
                        $('#loadcon').html('没有更多');
                    }
                },
                error: function(){

                }
            });
        }

        var page2 = 2;
        var compiler2 = Handlebars.compile($('#chosen_thread').html());
        function loadChosenThread(){
            $.ajax({
                type: "GET",
                url: "plugin.php?id=home_index",
                dataType: "json",
                data: {json:1, pages: page2, is_chosen:1},
                success: function(res){
