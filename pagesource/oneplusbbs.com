                    }else if(res.ret == 0){
                        $('#loadchosen').html('没有更多');
                    }
                },
                error: function(){

                }
            });
        }

        $('#loadcon').click(function(){
            loadRecommendThread();
        });
        $('#loadchosen').click(function(){
            loadChosenThread();
        });

        $('#recommend-fold').click(function(){
            if($(this).hasClass('fold')){
                $(this).removeClass('fold')
            }else{
                $(this).addClass('fold')
            }
            $('#recommend').toggle();
            $('#loadcon').toggle();
        });

        $('#chosen-fold').click(function(){
            if($(this).hasClass('fold')){
                $(this).removeClass('fold')
            }else{
                $(this).addClass('fold')
            }
            $('#chosen').toggle();
            $('#loadchosen').toggle();
        });

