<script type="text/javascript">
	var adsAlreadyDisplayed = [];
	var getOASAd = function(pubFormatDesktop, pubFormatMobile, minWidthDesktop, lazy) {
		width = window.innerWidth;
		if (width < 500 && typeof pubFormatMobile != 'undefined' && pubFormatMobile != null) {
			getOASAdShow(pubFormatMobile, lazy);
		} else if ((typeof minWidthDesktop == 'undefined' || width > minWidthDesktop) && pubFormatDesktop != null) {
			getOASAdShow(pubFormatDesktop, lazy);
		}
	};

	function getOASAdShow(selector, lazy) {
		var nbAdsWithoutLazyload = 3;
		var nbAds = -1;
		if (lazy == true) {
			// get number of the ad
			nbAds = selector.match(/\d+$/);
		}
		// for ads with lazy load system, check if we start lazy load (nbAdsWithoutLazyload)
		if (lazy == true && nbAds > nbAdsWithoutLazyload) {
			$('div#' + selector).prevAll().not("[id^=_ad_multi_content]").first().onScreen({
				visible: function () {
					// check if the ad already load
					if (adsAlreadyDisplayed.indexOf(selector) == -1) {
						adsAlreadyDisplayed.push(selector);
						$('#' + selector).show();
						apntag.anq.push(function () {
							apntag.showTag(selector);
						});
					}
				}
			});
		} else {
			$('#' + selector).show();
			apntag.anq.push(function () {
				apntag.showTag(selector);
			});
		}
	}
</script>
