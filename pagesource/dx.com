<script type="text/javascript">
    (function(win, doc) {
        var home_domain = doc.domain;
        if (home_domain == "www.dx.com" || home_domain == "dxqas.com" || home_domain == "www.dxqas.com") {
            if (!$.cookie("newMobile")) {
                var domain = home_domain == "dxqas.com" ? "dxqas.com" : home_domain.split(".")[1] + ".com";
                var browser = {
                    versions: function() {
                        var u = navigator.userAgent;
                        return {
                            mobile: !!u.match(/AppleWebKit.*Mobile.*/) && !!u.match(/AppleWebKit/), //是否为移动终端
                            iPad: /iPad/.test(u), //是否iPad
                            androidPad: /Tablet|Nexus 7|MI PAD/.test(u),
                        };
                    }(),
                    language: (navigator.browserLanguage || navigator.language).toLowerCase()
                }

                if (browser.versions.mobile && browser.versions.androidPad === false && browser.versions.iPad === false) {

                    $("#Itips,#bgApha").show();
                    
                    $("#Itips .link").click(function(e) {
                        var url = $(this).data("url");
                        // for dxqas.com
                        if (domain == "dxqas.com") {
                            url = "http://m.dxqas.com/mall";
                        }
                        $.cookie("newMobile", "1", {
                            expires: 30,
                            path: '/',
                            domain: domain
                        });
                        window.location.href = url;
                    });

                    $("#Itips .close").click(function() {
                        $("#Itips,#bgApha").hide();

                        $.cookie("newMobile", "0", {
                            expires: 30,
                            path: '/',
                            domain: domain
                        });
                    });
                }
            }
        }
    })(window, document);
</script>
