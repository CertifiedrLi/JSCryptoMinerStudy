		} );
		$( "#footer" ).removeAttr( "style" );
		var pathname = window.location.pathname;
		var str_array = pathname.split( '/' );
		if ( str_array[ 1 ].replace( /^\s*/, "" ).replace( /\s*$/, "" ) == "international" ) {
			//alert('Number1');
			$( ".international" ).show();
			$( ".d-two" ).hide();
			$( ".d-one" ).hide();
			$( ".call" ).hide();
			$( ".ltown" ).hide();
		} else {
			$( ".international" ).hide();

			//Whatsapp Number change
			var d = new Date();
			//alert(d.getDate());
			if ( ( d.getDate() ) % 2 == 0 ) {
				//alert('Number1');
				$( ".d-two" ).hide();
			} else {
				//alert('Number2');
				$( ".d-one" ).hide();
			}
		}

