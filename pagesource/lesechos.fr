<script>
var alignerBlocPartenaire = function(id)
{
var bloc_partenaire = $('#' + id).closest('.block-pub');
var dernier_article_une = $('.vitrine-alaune article:last');
if (!bloc_partenaire.length || !dernier_article_une.length)
{
return false;
}
if (dernier_article_une.offset().top - bloc_partenaire.offset().top < 0)
{
// Il faut remonter le bloc partenaire
// On diminue le bloc derniere minute
if ($('.vitrine-alaune .block-derniereminute').length)
{
// On conserve le margin-bottom du bloc
var margin_bottom_dmin = parseInt($('.vitrine-alaune .block-derniereminute').css('margin-bottom'));
$('.vitrine-alaune .block-derniereminute li').each(function(i, li) {
if ($(li).offset().top + $(li).height() + margin_bottom_dmin > dernier_article_une.offset().top)
{
// Cet item derniere minute est plus que le dernier article de la vitrine, on le supprime
$(li).remove();
}
});
}
}
// On deplace le bloc partenaire
bloc_partenaire.offset({ top: dernier_article_une.offset().top });
}
$(function() {
// Alignement du bloc partenaire
if ($('body').hasClass('une') && $('.vitrine-alaune .block-derniereminute').length)
{
var id_bloc_pub = 'div_pub_dfp_PAVE-1';
var id_bloc_partenaire = 'div_pub_dfp_BRAND-CONTENT-PREMIUM';
if (window.googletag && googletag.apiReady) {
if (window['slotRenderEnded_' + id_bloc_pub] !== true) {
// La pub n'est pas encore chargee, on attend que le rendu soit fait
window.googletag.pubads().addEventListener('slotRenderEnded', function(event) {
if (event.slot.getSlotElementId() == id_bloc_pub) {
//alignerBlocPartenaire(id_bloc_partenaire);
$("#" + id_bloc_pub).find("iframe").on("load", function(){
alignerBlocPartenaire(id_bloc_partenaire);
});
}
});
}
else
{
alignerBlocPartenaire(id_bloc_partenaire);
}
}
}
});
</script>
