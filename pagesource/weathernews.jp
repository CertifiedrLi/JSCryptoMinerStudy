	}
}

function isflash() {
	ga('send', 'pageview');
	var hasFlash = false;
	try {
		var fo = new ActiveXObject('ShockwaveFlash.ShockwaveFlash');
		if(fo) hasFlash = true;
	} catch(e) {
		if(navigator.mimeTypes ["application/x-shockwave-flash"] != undefined) hasFlash = true;
	}
	
	if(!hasFlash) {
		//document.body.classList.add('noflash');
		document.body.className += (' noflash');
		var key = wniutil.count.make_pagekey();
		if(key == '') key = 'mwstop';
		wniutil.count.log(key + "_noflash");

		append_count();
  		ga('send', 'event', 'noflash', 'impression', 'top');
	}
}

function append_count() {
	var atags = document.querySelectorAll('#swfError a');
	Array.prototype.forEach.call(atags, function(e, i) {
		e.addEventListener('click', count_goto);
	});

}

function count_goto(e) {
	e.preventDefault();
	var uri = e.target.getAttribute('href');
	var key = e.target.getAttribute('name');
	wniutil.count.goto(uri, 'button_' + key);
}

issmart();
</script>
