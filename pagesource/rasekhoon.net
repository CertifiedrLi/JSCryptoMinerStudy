        //show and hide small loading image
        function showloadimg(obj) {
            $(obj).children('img:first').css('display', 'inline');
            var padding_right = parseInt($(obj).children('a:first').css('padding-right').replace("px", ""));
            var padding_left = parseInt($(obj).children('a:first').css('padding-left').replace("px", ""));
            $(obj).children('a:first').css('padding-right', (padding_right - 10));
            $(obj).children('a:first').css('padding-left', (padding_left + 10));
        }
        function hideloadimg(obj) {
            $(obj).children('img:first').css('display', 'none');
            var padding_right = parseInt($(obj).children('a:first').css('padding-right').replace("px", ""));
            var padding_left = parseInt($(obj).children('a:first').css('padding-left').replace("px", ""));
            $(obj).children('a:first').css('padding-right', (padding_right + 10));
            $(obj).children('a:first').css('padding-left', (padding_left - 10));
