<script>
    $(function(){
        $("#local_news,#tz_zhou,#tz_yue,#tz_zong").html("<img src='/skin/2016/css/loading2.gif' style='display:block;width:60px;margin:0 auto;'>");
        //显示社区联播
        $.ajax({
            type: "GET",
            url: "http://cms.cncn.org.cn/api/local_recent_news.php?limit=6",
            cache: true,
            dataType: "jsonp",
            success: function(data){
                var local_news_data = '';
                if(data.status == 1){
                    for(var i in data.news_list){
                        local_news_data += '<li><span>・</span><a href="'+ data.news_list[i].titleurl +'" target="_blank">['+ data.news_list[i].local +']' + data.news_list[i].title +'</a></li>';
                    }
                    $("#local_news").html(local_news_data);
                }
            }
        });
        //社区总数和台账总数
        $.ajax({
            type: "GET",
            url: "http://cms.cncn.org.cn/api/rank_country_total.php",
            cache: true,
            dataType: "jsonp",
            success: function(data){
                if(data.status == 1){
                    var str = '社区官网'+data.sqnums+'个，台账'+data.logs+'篇';
                    $("#count_string").html(str);
                }
            }
        });
        //台账排行周月总
        $.ajax({
            type: "GET",
            url: "http://cms.cncn.org.cn/api/rank_community_week.php?limit=8",
            cache: true,
            dataType: "jsonp",
            success: function(data){
                if(data.status == 1){
                    var str = '';

                    for(var i in data.rank_list){

                        str += '<li><a href="'+ data.rank_list[i].weburl +'" target="_blank">'+ data.rank_list[i].communityname +'</a><small>'+ data.rank_list[i].logs +'篇</small></li>';
                    }
                    $("#tz_zhou").html(str);
                }
            }
        });
        $.ajax({
            type: "GET",
            url: "http://cms.cncn.org.cn/api/rank_community_month.php?limit=8",
            cache: true,
            dataType: "jsonp",
            success: function(data){
                if(data.status == 1){
                    var str = '';
                    for(var i in data.rank_list){
                        str += '<li><a href="'+ data.rank_list[i].weburl +'" target="_blank">'+ data.rank_list[i].communityname +'</a><small>'+ data.rank_list[i].logs +'篇</small></li>';
                    }
                    $("#tz_yue").html(str);
                }
            }
        });
        $.ajax({
            type: "GET",
            url: "http://cms.cncn.org.cn/api/rank_community_total.php?limit=8",
            cache: true,
            dataType: "jsonp",
            success: function(data){
                if(data.status == 1){
                    var str = '';
                    for(var i in data.rank_list){
                        str += '<li><a href="'+ data.rank_list[i].weburl +'" target="_blank">'+ data.rank_list[i].communityname +'</a><small>'+ data.rank_list[i].logs +'篇</small></li>';
                    }
                    $("#tz_zong").html(str);
                }
            }
        });

        $.getScript("/contact/autozone.php",function(){
            //底部联络员轮播
            var $dt = '';
            if ($("#contact>li").length > 3) {
                man_change();
            }
            $("#contact").mouseenter(function(event) {
                clearTimeout($dt);
            });
            $("#contact").mouseleave(function(event) {
                man_change();
            });
            function man_change() {
                $dt = setTimeout(function() {
                    $("#contact").animate({
                        'top': -$("#contact>li:eq(0)").height()
                    }, 600, 'swing', function() {
                        $("#contact").append($("#contact>li:eq(0)"));
                        $("#contact").append($("#contact>li:eq(0)"));
                        $("#contact").append($("#contact>li:eq(0)"));
                        $("#contact").css('top', '0');
                        man_change();
                    });
                }, 6000)
