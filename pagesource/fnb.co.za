<script>
	function insertCMBanners(id, data) {
		if (data.numOfOffers > 0) {
			$.when.apply($, data.offers.map(function(offer) {
				console.info('offer:');
				console.dir(offer);
	    		var bannerUrl = "/04banners/offers/"+offer.offerType+"/banner.html?type="+offer.offerIndicator+"&id="+offer.offerId;
				return $.ajax(bannerUrl, {  
	   				success: function(data) { 
	   					var marketingBanners = $('#'+id+ ' .marketing');
	   					if (marketingBanners.length > 1) {
	   						$(marketingBanners[0]).before(data);
	   					}
	   					else {
	   						$(marketingBanners).before(data);
	   					}

	   				},
					fail: function() {return true;}
				})
			})).then(function() {
			  	initCarousel('pcBanners'); 
			});
			
		}
		else {
		  	initCarousel('pcBanners'); 
		}
		return true;
	}
	function initCarousel(id) {
		var carouselId = '#'+id;
		var ci = $(carouselId + ' .carouselItem');
		$(carouselId).slick({	
			slidesToShow: 1,
			speed: 2500,
			autoplaySpeed: 3000,			
			variableWidth: false,			
			autoplay: true,
			infinite: true,	
			adaptiveHeight: false,
			arrows: true,
			dots: true,			
			swipe: true,
			pauseOnHover: false
		}).each(function() {
				var $this = $(this);
				var $track = $this.find('.slick-track');
				var $slides = $track.children();
				var minSlideHeight = $track.height();
				$slides.css('height', minSlideHeight);
		}); 
		$(carouselId).on('afterChange', function(event, slick, currentSlide, nextSlide){
			var ci = $('#pcBanners .slick-slide');
			var banner = ci[currentSlide+1].getAttribute('data-trackclick');
			submitTracking('Banners','viewed',banner,0) 
		});

		var pauseButton = $("#pauseButton");
		(pauseButton).detach().appendTo('.slick-dots')
		
		//bindOfferTracking();
		return true;
	}
	
	$( document ).ready(function() {
		var url = 'https://www.online.fnb.co.za/banking/cm/v1/offers/retrieveOfferByCookie?cmRequest={"locale":{"platform":"WebBrowser","country":"15","skin":"0"},"numOfOffers":"2"}';
		$('#pcBanners .carouselItem').addClass('marketing');
		$.ajax({
		  url: url,
		  dataType: 'jsonp',
		  jsonpCallback: 'jsonp',
		  timeout: 500,
		  success: function (data) {
			  insertCMBanners('pcBanners', data);
		  },
		  fail: function(){
			  initCarousel('pcBanners');
		  },
		  error: function(jqXHR, textStatus, errorThrown) {
			  initCarousel('pcBanners');
		  }
		});

	});
</script>  
