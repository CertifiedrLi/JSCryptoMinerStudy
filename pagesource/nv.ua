    function set_home_view_mode(page, mode) {
        var page = page || 'index';
        var mode = mode || 1;
        $.cookie(page + '_view_mode', mode, {expires: 364, path: '/'});
        get_home_view_mode(page);
    }

    function get_home_view_mode(page) {
        if ($('#for_same_item').length == 0) {
            return false;
        }
        var page = page || 'index';
        var mode = parseInt($.cookie(page + '_view_mode')) || 1;
        var h = 0, h2 = 0, d = 0;
        switch (mode) {
            case 1:
                    h = $('#for_same_item').find('.page_item:eq(0)').height() + 15;
                    $('#for_same_item').css('height', h + 'px');
                    $('#for_lenta').css('height', (h + $('#main_item_new').height() + 20) + 'px');
                    $('.lenpub').find('.more2').hide();
                    $('.lenpub').find('.more2.show_more').show();
                    $('.lenpub').addClass('not_full');
                break;
            case 2:
                    h = $('#for_same_item').find('.page_item:eq(0)').height() + $('#for_same_item').find('.page_item:eq(1)').height() + 15;
                    $('#for_same_item').css('height', h + 'px');
                    $('#for_lenta').css('height', (h + $('#main_item_new').height() + 20) + 'px');
                    $('.lenpub').find('.more2').hide();
                    $('.lenpub').find('.more2.show_more, .more2.show_up').show();
                break;
            case 3:
                    h = $('#for_same_item').find('.page_item:eq(0)').height() + $('#for_same_item').find('.page_item:eq(1)').height() + $('#for_same_item').find('.page_item:eq(2)').height() + 15;
                    $('#all_lenta,#best_lenta,#best_comments').show().each(function(){
                        var $this = $(this);
                        if ($this.height() > h2) {
                            h2 = $this.height() - $('#main_item_new').height() + 20;
                        }
                    }).removeAttr('style');
                    d = h2 - h > 0 ? h2 - h : 0;

                    $('#for_same_item').css('height', (h + d) + 'px');
                    $('#for_lenta').css('height', ((h + d) + $('#main_item_new').height() + 20) + 'px');
                    $('.lenpub').find('.more2').hide();
                    $('.lenpub').find('.more2.link_all, .more2.show_up').show();
                break;
        }
    }
</script>

