})(document.getElementsByTagName('script')[0]);

var dailyMotionPlayers = {};
var youTubePlayers = {};
var gemiusPlayer = {};

function dmAsyncInit()
{

    //gemiusStream.setEncoding("iso-8859-2");
    var IDENTIFIER = "px01Xv8k1W298ScCDv4Z16ei.BNs.MyLgnudlzjvt7D.t7";
    var HITCOLLECTOR = "https://gatr.hit.gemius.pl";

    // DM.init({ apiKey: 'your app id', status: true, cookie: true });
    function initDailymotionVideo(video){
        video.config.width = video.config.height = '100%';
        video.config.autoplay = false;
        video.config.mute = true;
        video.config.controls = false;
        video.config.related = false;
        var player = dailyMotionPlayers[video.id] = DM.player(document.getElementById(video.id), video.config);
        player.setAttribute('data-video-id', video.config.video);
        var playerId = 'dm-' + video.id;
        var materialIdentifier = video.id;
        var title = "";
        var channel = "";
        var totalTime = 100;
        var treeId = [100];
        var additionalPackage = [];
        var customPackage = [];
        customPackage.push({name : 'PLAYER_SKIN', value : 'light'});

        var videoData = $('#' + video.id + '-data');

        if(videoData.length){
            if(videoData.attr('duration')){
                totalTime = videoData.attr('duration');
            }

            if(videoData.attr('title')){
                title = videoData.attr('title')
            }

            if(videoData.attr('channel')){
                channel = videoData.attr('channel');
            }
        }


        if((typeof GemiusPlayer !== "undefined")){
            var additionalParameters = {"currentDomain" : "sozcu.com.tr"}
            gemiusPlayer[playerId] = new GemiusPlayer(playerId, IDENTIFIER, additionalParameters);

            gemiusPlayer[playerId].newProgram(playerId, {
                        'programType':'Video',
                        'programDuration':totalTime,
                        'programName':channel
                    });
        }


        var dmHandler = function(e)
        {
            var type = e.type;

            var videoId = "player-" + e.target.video.videoId;

            var playerId = 'dm-' + videoId;
            var materialIdentifier = videoId;

            switch (type){
                case 'pause':
                    type = 'pause';
                    break;
                case 'play':
                    type = 'play';
                    break;
                default:
                    type = false;

            }

            if(type){
                var currentTime = Math.round(e.target.currentTime);

                if(typeof GemiusPlayer !== "undefined"){
                    gemiusPlayer[playerId].programEvent(playerId, currentTime, type, {});
                }
            }
        };

        dailyMotionPlayers[video.id].addEventListener("play", dmHandler);
        dailyMotionPlayers[video.id].addEventListener("pause", dmHandler);
    }

    for (var i = 0; i < dailyMotionVideos.length; i++) {
        initDailymotionVideo(dailyMotionVideos[i]);   
    }
    
    dailyMotionVideos.push = initDailymotionVideo;

    $(window).scroll();
};



function onYouTubeIframeAPIReady() {
    $('.youtube-embed-video').each(function(){
        
        var video = $(this);
        var id = video.prop('id');

        if (!id) {
            return;
        }

        youTubePlayers[id] = new YT.Player(id);
    });
}


