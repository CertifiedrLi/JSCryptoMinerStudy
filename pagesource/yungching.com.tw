<!-- 搜尋區塊-->
<div class="m-search-block" ng-controller="searchCtrl" ng-cloak>
    <!-- 搜尋區塊 (跟買屋有些不同, 請勿從買屋 copy  ) -->

      <!-- 搜尋區塊 -->
<div class="ng-search-bar">
<ul class="m-search-for-lists">

  <li ga-buy-search ga_action="index_search_{{ i.name }}_click" ga_label="index_search_{{ i.name }}" ga_cat="buy_search"
    ng-repeat="i in ::searchFor" ng-class="{'is-active': i.key == searchCondition.searchFor}"
    ng-init="searchCondition.originalDomain = '//buy.yungching.com.tw'"
    ng-click="changeSearchCondition(i.key, false); searchCondition.originalDomain = '//buy.yungching.com.tw';" >{{ i.name }}</li>
</ul>

<div class="m-search-field">
  <!-- 捷運 -->
  <div ng-show="mainSelectOption.key == 'mrt'" class="m-search-select-wrap">
    <a href="#" class="search-select search-select-mrt">
      <span ng-show="searchCondition.addr[0][0] != '高雄市'">台北</span>
      <span ng-show="searchCondition.addr[0][0] == '高雄市'">高雄</span>
    </a>
    <div class="search-select-option-mrt select-option">
      <div class="label" ng-class="{'active': searchCondition.addr[0][0] !== '高雄市'}">
        <a href ng-click="changeMrtMap('台北市')">台北捷運 </a></div>
      <div class="label kh" ng-class="{'active': searchCondition.addr[0][0] === '高雄市'}">
        <a href ng-click="changeMrtMap('高雄市');">高雄捷運 </a></div>
    </div>
  </div>
  <!-- 捷運 -->

  <!-- 縣市/鄉鎮市區 -->
  <div ng-hide="mainSelectOption.key == 'mrt'" class="m-search-select-wrap">
    <a href class="search-select search-select-city"
       ga-buy-search ga_action="index_search_%%_location_click" ga_label="index_search_%%_location" ga_cat="buy_search">
      {{ limitStr(searchCondition.addr) }}
    </a>

    <div class="search-select-option-cities select-option">
      <a href class="close">&times;</a>
      <ul>
        <!-- 縣市 -->
        <li ng-show="isCitySelection" ng-repeat="c in cities">
          <a ng-click="toggleAreaSelect($index);" href
             ga-buy-search
             ga_action="index_search_%%_location_click"
             ga_label="index_search_%%_location_{{ c }}"
             ga_cat="buy_search">{{ c }}</a>
        </li>

        <!-- 鄉鎮區域 -->
        <li class="reset" ng-hide="isCitySelection">
          <a href ng-click="isCitySelection = true;"
            ga-buy-search
            ga_action="index_search_%%_location_click"
            ga_label="index_search_%%_location_reset"
            ga_cat="buy_search"><i class="fa fa-reply" aria-hidden="true"></i> 重選縣市</a>
        </li>

        <li ng-hide="isCitySelection" ng-repeat="a in areaList">
          <label>
            <input area="{{ a }}" index="{{$index}}" type="checkbox" class="chkbox area-chkbox"
               ga-buy-search
               ga_action="index_search_%%_location_click"
               ga_label="index_search_%%_location_{{ searchCondition.addr[0] }}{{ a }}"
               ga_cat="buy_search"
               ng-checked="selectedAreas.indexOf(a) > -1">
            <span>{{ a }}</span>
          </label>
        </li>
      </ul>
    </div>
  </div>

  <div class="m-search-select-wrap">
    <a href class="search-select search-select-price"
        ga-buy-search
        ga_action="index_search_%%_price_click"
        ga_label="index_search_%%_price"
        ga_cat="buy_search">{{ showRange('price') }}</a>

    <div class="search-select-option-price select-option">

      <label><input type="checkbox" index="-1" listtype="priceList" class="chkbox chkbox-any" chkbox-area
        ga-buy-search
        ga_action="index_search_%%_price_click"
        ga_label="index_search_%%_price_none"
        ga_cat="buy_search"><span>總價不限</span></label>

      <label ng-repeat="i in currPriceList">
        <input type="checkbox" index="{{$index}}" listtype="priceList" class="chkbox"
          chkbox-area
          ga-buy-search
          ga_action="index_search_%%_price_click"
          ga_label="index_search_%%_price_{{ currPriceList[$index - 1] || '' }}-{{ currPriceList[$index] || ''}}"
          ga_cat="buy_search">

        <span ng-if=" $index == 0 ">{{ currPriceList[$index] }} 萬以下</span>
        <span ng-if=" $index > 0 && $index < (currPriceList.length-1) ">{{ currPriceList[$index - 1] }} 萬 - {{ currPriceList[$index] }} 萬</span>
        <span ng-if=" $index == (currPriceList.length - 1) ">{{ currPriceList[$index - 1] }} 萬以上</span>
      </label>

      <div class="custom-block">
        <div class="input-wrap">
          <input index="0" listtype="priceList" class="custom-num num" type="text" size="5"> -
          <input index="1" listtype="priceList" class="custom-num num" type="text" size="5"> 萬
        </div>
      </div>
    </div>
  </div>

  <div class="m-search-select-wrap">
    <a href class="search-select search-select-area"
      ga-buy-search
      ga_action="index_search_%%_sqft_click"
      ga_label="index_search_%%_sqft"
      ga_cat="buy_search">{{ showRange('pyeong') }}</a>

    <div class="search-select-option-area select-option">
      <div style="height: 40px; line-height: 40px; padding-left: 0.7em; margin-bottom: 0.2em; border-bottom: 1px solid #eee;">
        <label class="inline">
          <input class="sp-chkbox" type="radio" ng-model="searchCondition.pyeongType" value="0" name="pyeongType-type">
          <span>建物</span>
        </label>
        <label class="inline">
          <input class="sp-chkbox" type="radio" ng-model="searchCondition.pyeongType" value="1" name="pyeongType-type">
          <span>土地</span>
        </label>
        <label class="inline">
          <input class="sp-chkbox" type="radio" ng-model="searchCondition.pyeongType" value="2" name="pyeongType-type">
          <span>主+陽</span>
        </label>
      </div>

      <label>
