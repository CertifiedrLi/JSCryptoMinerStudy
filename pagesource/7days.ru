<script type="text/javascript">
    $(function () {
        var owl = $(".j-shop-carousel");

        // init
        owl.owlCarousel({
            margin: 12,
            nav: true,
            responsive: {
                0: {
                    items: 3
                },
                600: {
                    items: 6
                },
                1000: {
                    items: 8
                }
            }
        });

        owl.on('translated.owl.carousel', function (e) {
            if (e.page && ((e.page.count - 1) === e.page.index)) {
                var nextPageNumber = Math.ceil(e.item.count / 10) + 1;

                $.ajax({
                    type: 'post',
                    url: '/ajax/route/shops',
                    data: {
                        'page': nextPageNumber,
                        'ids': [859543, 859541, 859545]
                    },
                    success: function (data) {
                        if (data) {
                            $(data)
                                .filter(function () {
                                    return this.nodeName == 'DIV';
                                })
                                .each(function () {
                                    var $div = $('<div></div>');
                                    $div.append($(this));
                                    owl
                                        .trigger(
                                            'add.owl.carousel',
                                            [$('<div class="owl-item">' + $div.html() + '</div>')]
                                        )
                                        .trigger('refresh.owl.carousel');
                                });
                        }
                    }
                });
            }
        });
    });
</script>

