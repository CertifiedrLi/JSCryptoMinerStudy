<script type="text/javascript">
    function getDeviceData(clientjs) {
        var data = clientjs.getBrowserData();
        data.isMobile = clientjs.isMobile();
        data.screen = clientjs.getScreenPrint();
        data.plugins = clientjs.getPlugins();
        data.isMobile = clientjs.isMobile();
        data.mimeTypes = clientjs.getMimeTypes();
        data.language = clientjs.getLanguage();
        data.timezone = clientjs.getTimeZone();
        data.storage = {
            localStorage: clientjs.isLocalStorage(),
            sessionStorage: clientjs.isSessionStorage(),
            cookie: clientjs.isCookie()
        };

        return data;
    }

    // save data about user
    // 1. get id from cookie
    var cookieName = 'sd_fingerprint',
        cookieValue = Cookies.get(cookieName);

    // 2. if ok than we already stored the data
    // 3. if fail than we need to generate fingerprint ID
    var clientjs = new ClientJS(),
        fingerprint = clientjs.getFingerprint();

    if (!cookieValue && fingerprint) {
        // 4. make request with fingerprint ID
        // 5. if this fingerprint with user ID not exists create new record
        $.ajax({
            type: 'POST',
            url: '/ajax/route/browser',
            data: {
                fingerprint: fingerprint,
                userId: null,
                data: getDeviceData(clientjs)
            },
            dataType: 'json'
        }).done(
            function (data) { // success
                if (data && data.id) {
                    Cookies.set(cookieName, fingerprint, { expires: 30 });
                }
            }
        );
    }
</script>
