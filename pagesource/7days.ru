						<script type="text/javascript">
            window.SD = window.SD || {};

            SD.dateDiffInHours = function (dateA, dateB) {
                var MS_PER_HOUR = 1000 * 60 * 60,
                    dateAUtc = Date.UTC(dateA.getFullYear(), dateA.getMonth(), dateA.getDate()),
                    dateBUtc = Date.UTC(dateB.getFullYear(), dateB.getMonth(), dateB.getDate());

                return Math.floor((dateBUtc - dateAUtc) / MS_PER_HOUR);
            };

            SD.isValidDate = function (date) {
                if (Object.prototype.toString.call(date) === "[object Date]") {
                    return isNaN(date.getTime()) ? false : true;
                }

                return false;
            };

            SD.needToMakeRequestForUserPathsViews = function () {
                var uid = $.cookie('7days_uid');
                if (!uid) {
                    return true;
                }

                var userPathsString = window.localStorage.getItem('userPaths'),
                    userPathsTimestamp = parseInt(window.localStorage.getItem('userPathsTimestamp'), 10);
                if (!userPathsTimestamp || !userPathsString) {
                    return true;
                }

                var createdDate = new Date(userPathsTimestamp),
                    now = new Date();
                if (this.isValidDate(createdDate) && this.dateDiffInHours(createdDate, now) <= 1) {
                    return false;
                }

                return true;
            };
