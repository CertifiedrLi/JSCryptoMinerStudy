<script>
//$.smartbanner({title:"AddText - to your photos",author:"FlamingText.com Pty Ltd",icon:"http://images.addtext.com/images/apple-touch-icon-precomposed.png"});
if (!window.pageYOffset){
	window.scrollTo(0,1);
	window.scrollTo(0,0);
}
$(document).ready(function(){
	setTimeout(function() {
		if (!window.pageYOffset){
			window.scrollTo(0,1);
			window.scrollTo(0,0);
		}
	},20);
	
	//make sure content column is at least as tall as side columns
	var leftColH = $(".ft-left-column").outerHeight();
	var rightColH = $(".ft-right-column").outerHeight();
	var contentColH = $(".ft-content-column").outerHeight();

	if (leftColH > contentColH || rightColH > contentColH){
		var tallestColH = (leftColH >= rightColH) ? leftColH : rightColH
		$(".ft-content-column").css("min-height", tallestColH);
	}
	
	
	$(".ft-fluid-header-social").append('<div class="ft-fluid-header-social-item">'
	+'<iframe allowtransparency="true" frameborder="0" scrolling="no" src="//platform.twitter.com/widgets/follow_button.html?screen_name=FlamingText&show_screen_name=false&show_count=false" style="height:20px; width: 100px;"></iframe>'
	+'</div>');
	

	//determine current used lang - for what Lang to be selected in languageSelect
	var langCode = "en_US";
	var langHost = langHost = $(".language-select").find("[data-langCode='"+langCode+"']").val();

	var host = window.location.host;

	//hide no js fallback content
	$(".no-js").addClass("hide");

	//don't show language select on:
	//sigs pages as it doesn't work to refresh page with POST
	//dynamic (logo) pages, as they have unique urls for some langauges
	//NOTE: dynamic is detected by presence of .ft-proxyform! (weak)
	if(host.indexOf("sigs.flamingtext.com") > -1
	||$(".ft-proxyform").length !== 0) {
		$(".ft-footer-languages").addClass("hide");
	} else {
		$(".language-select")
			.val(langHost)
			.removeClass("hide")
			.on("change", function(){
				//set lang cookie - expires in 5 years
				var date = new Date();
				date.setTime(date.getTime() + (5 * 365 * 24 * 60 * 60 * 1000));
				var langCode = $(this).find(":selected").attr("data-langCode");
				$.cookie("lang",langCode,{expires: date, path:"/", domain: '.flamingtext.com'});

				//reload page in new lang
				var newLangHost = $(this).val();
				//except for some pages, which are not localized to other languages:
				// share, sigs... more?
				var ftHost = "";
				
			
				//share pages are not on language domains.
				if(host.indexOf("share.flamingtext.") > -1)
					newLangHost = "http://"+ftHost+"share.flamingtext.com";
				var path = window.location.pathname;
				var params = window.location.search;	
				window.location = newLangHost + path + params;
			}
		);
	}
	function historyAPI() {
		//detection from modernizer.
		var ua = navigator.userAgent;
		// remove false positive browser(s)
		if (ua.indexOf('Android 2') !== -1 &&
		    ua.indexOf('Mobile Safari') !== -1 &&
		        ua.indexOf('Chrome') === -1) {
		    return false;
		}
		// Regular check for everyone else
		if (typeof window.history !== "undefined" && 'replaceState' in history) {
		    return true;
		}
	}
	if (historyAPI()) {
		//remove specific params from url, to clean up
		var rmParams = "(_loc|_action)"; //explicitly list what params to remove
		var currUrl = location.protocol + "//" + location.host + location.pathname;
		//if (!/^(f|ht)tps?:\/\//i.test(currUrl))
			//currUrl = "//" + currUrl;
		
		var params = location.search;
		var re = new RegExp("([&]*"+rmParams+")=[^&]+", "g");
		params = params.replace(re, "").replace(new RegExp("(\\?)$"), ""); //remove terminating paramstarters
		window.history.replaceState(null, null, currUrl+params+location.hash); //make sure to keep hash fragments!		
	}
	
	//speed up mobile menu & avoid polluting browser history
	if($(window).width() < 700){
		var evtType = ($("html").hasClass("has-touch")) ? "touchstart" : "click";

		$(".ft-fluid-jump-to-nav").on(evtType, function(e){
			e.preventDefault();
			e.stopPropagation(); // neeeded with Fastclick, to stop the click from happening
			$("#nav").addClass("active");
		});
		$(".js-close-mobile-menu-btn").on(evtType, function(e){
			//if #nav, we need to allow user click, as browser doesn't re-render :target rule when
			//updating url using history replaceState.
			//would prever to have this check outside the event, but user can click menu before doc.ready..
			if(location.hash !== "#nav") {
				e.preventDefault();
				e.stopPropagation(); // neeeded with Fastclick, to stop the click from happening
				$("#nav").removeClass("active");	
			} else if (historyAPI()) {
				setTimeout(function(){
					window.history.replaceState(null, null, currUrl+params);
				},250); //for some reason not working on iPod touch... not important.
			}
		});
	}
	
	//collapse some footer content for mobile
/*
	if(isMobile){
		var collapseSections = ["logos", "fonts", "tools", "other","company"];
	
		for (var i=0; i < collapseSections.length; i++){
			$(".js-ft-footer-"+collapseSections[i]).addClass("ft-collapse-content"); 
			collapseContent(
				$(".js-ft-footer-"+collapseSections[i]+" h3") //button
				,$(".js-ft-footer-"+collapseSections[i]+" ul") //content
			);	
		}
	}
*/
	
	// invoke ftFlyoutMenu widget
	$("header").flyoutMenu();
});
</script>

