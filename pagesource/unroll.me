      });
      sp.summary({
        success : function(data) {
            var incidentBody = data.incidents[data.incidents.length - 1].name;
            var truncate = '80';
            var incidentUrl = data.incidents[data.incidents.length - 1].shortlink;
            // adds the text description to the alert with truncation
            $('.alert__description').text(incidentBody.substring(0,truncate));
            // Creates direct link to incident on statuspage.io
            $('.alert__link').attr('href', incidentUrl);
            console.log(incidentUrl);
        }
      });
  </script>
  
