  <script type="text/javascript">
      $(document).ready(function() {
          function trackClickEvent (event) {
              if (!analytics) {
                  return;
              }
  
              if (event.target && typeof event.target.getAttribute === "function" && event.target.getAttribute('data-track')) {
                  var attributes = event.target.attributes,
                          eventPayload = {};
  
                  var numberOfAttributes;
  
                  try {
                      numberOfAttributes = attributes.length;
                  } catch (e) {
                      numberOfAttributes = 1;
                  }
  
                  for (var i = numberOfAttributes - 1; i >= 0; i--) {
                      var result = /data-track-(.+)/.exec(attributes[i].name);
  
                      if (result) {
                          var relevantAttribute = result.slice(1);
  
                          eventPayload[relevantAttribute] = attributes[i].value;
                      }
                  }
  
                  var pageNamesForUrls = {
                      "/": "Home Page",
                      "/features/": "Features Page",
                      "/app/": "iOS App Page",
                      "/team/": "Team Page",
                      "/faq/": "FAQ Page"
                  }
  
                  eventPayload["Page Name"] = pageNamesForUrls[window.location.pathname]
  
                  return analytics.track(event.target.getAttribute('data-track'), eventPayload);
              }
          }
  
          document.addEventListener('click', trackClickEvent);
      });
  
  	var sp = new StatusPage.page({ page : 'dqmkzcn7xycv' });
      sp.status({
        success : function(data) {
          // appends the status class to the alert for colour styling
          $('.alert--status').addClass(data.status.indicator);
