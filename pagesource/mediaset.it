	} else {
		return ("1 MIN");
	}
});

Handlebars.registerHelper("translateMonth", function(date) {
	var months = [ "GEN", "FEB", "MAR", "APR", "MAG", "GIU", "LUG", "AGO", "SET", "OTT", "NOV", "DIC" ];
		return date.substring(0,2)+' '+ months[parseInt(date.substring(3,5))-1]+ ' '+ date.substring(6,10);
});

