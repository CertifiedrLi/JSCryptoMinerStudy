function drawPagination(resultInfo, service, tab) {
	 var _nav = '';
	if(resultInfo.paging.previus != '' || resultInfo.paging.next != ''){
		_nav += '<div class="pagination">';
		if(resultInfo.paging.previus == ''){
			_nav += '<span style="opacity:0.4"><a>precedente</a></span>';
		}
		if(resultInfo.paging.previus != ''){
			_nav += '<span><a title="successiva" href="#" onclick="gotoServicePage(\''+service+'\',\''+resultInfo.paging.previus+'\');return false;">precedente</a></span>';
		}
		_nav += '<span class="pagg_wia">Pagina <b>'+ resultInfo.pageNumber +'</b> di <b>'+resultInfo.totalPage+'</b></span>';
		if(resultInfo.paging.next != ''){
			_nav += '<span><a title="successiva" href="#" onclick="gotoServicePage(\''+service+'\',\''+resultInfo.paging.next+'\');return false;">successiva</a></span>';
		}
		_nav += '</div>';
		$(tab).append(_nav);
	}
}

function gotoServicePage(service, url) {
	 gigya.accounts.getAccountInfo({ callback: getTokenPagination, include: 'loginIDs,profile',context : {"service": service, "url": url } });
}

function getTokenPagination(response) {

    var UIDSignature = '';
    var signatureTimestamp = '';
    var parameter = '';
	var _url = response.context.url;


    if (response.errorCode == 0) {
        UID = response['UID'];
        UIDSignature = response['UIDSignature'];
        signatureTimestamp = response['signatureTimestamp'];
        mail = response['loginIDs']['emails']


        parameter = 'UID=' + UID + '&UIDSignature=' + UIDSignature + '&signatureTimestamp=' + signatureTimestamp;

        $.getJSON(serviziBaseUrl + '/Customer/getTokenBigData.jsp?of=jsonp&callback=?&' + parameter, function(data) {
            if (data.response) {
                try {
                    $.each(data.response, function(i, responsevalue) {
                        username = responsevalue.username;
                        token = responsevalue.token;

                        //utente non loggato
                        if (token == undefined || token == '') {
                            //utente non loggato.
                            console.log('READLATER: getTokenPagination utente non loggato token null');

                        } else {
                            //utente loggato
							if(response.context.service == 'ReadLeater'){getReadLeater(token, _url+token+'&callback=?&fieldsTemplate=vodWeb');}
                            if(response.context.service == 'ToComplete'){getToComplete(token, _url+token+'&callback=?&fieldsTemplate=vodWeb');}
							if(response.context.service == 'BrandFollow'){getBrandFollow(token, _url+token+'&callback=?&fieldsTemplate=vodWeb');}
                            console.log('READLATER: getTokenPagination [username: ' + username + ' token: ' + token + ' url: ' + _url + '] user logged');
                        }
                    });

                } catch (er) {
                    console.log('READLATER: getTokenPagination [' + er + '] KO');
                }
            }
        });
    } else {
        //utente non loggato
        console.log('READLATER: getTokenPagination utente non loggato');
    }
}

