<h2 class="title">Rivedi la TV</h2>
<div class="boxLastWeek">
	<div class="viewAllBox">

		<div class="week">
			<ul class="days"></ul>
		</div>
		<ul id="channelsStatic" class="channels static"></ul>
	</div>
	<div id="lastweek" class="slideOp review slider"></div>
</div>

<script>
	
     var servertime = '2018-03-17-11-09';
	 var slick_option_home = {slidesToShow: 4.4,slidesToScroll: 4,lazyLoad: 'ondemand', dots: true,infinite: false,responsive: [{breakpoint: 1025,settings: {slidesToShow: 3.2,slidesToScroll: 3}},{breakpoint: 769,settings: {slidesToShow: 2.2,slidesToScroll: 2}},{breakpoint: 481,settings: {slidesToShow: 1.2,slidesToScroll: 1,arrows: false,dots: false}}]};
	 var slick_option = {slidesToShow: 3.2,slidesToScroll: 3,lazyLoad: 'ondemand', dots: true,infinite: false,responsive: [{breakpoint: 769,settings: {slidesToShow: 2.2,slidesToScroll: 2}},{breakpoint: 481,settings: {slidesToShow: 1.2,slidesToScroll: 1,arrows: false,dots: false}}]};
	$(function() {
		var idday = 0; var idch = '';
		lastWeek();
		$('.select ul').hide();
		$('.select').click(function() {$('.select ul').fadeToggle(500);});

		function lastWeek() {
			_dbg('lastweek', [idday], 'init');
			try {
				$.getJSON('/bacino/lastweek_'+ idday +'.sjson?callback=?', function(data) {
					//channels
					$('ul.channels').empty();
					$.each(data.channels, function(i, ch) {
						var chli = $('<li class="ic-'+ch.code.toLowerCase()+'" data-ch="'+ch.code+'"><a title="'+ch.channel+'" id="'+ch.channel+'">'+ch.channel+'</a></li>');

						$('#channelsStatic').append(chli);
						chli.click(function() {
							$('ul.channels li').removeClass('active');
							var matches = '';
							var _day = $('ul.days .active').attr('title');
							$('#lastweek').slick('unslick').empty().fadeOut(0);
							$('.homeMs #lastweek').slick(slick_option_home).fadeIn('slow');
							$('.brandpage #lastweek, .videopage #lastweek').slick(slick_option).fadeIn('slow');
							if(idch == $(this).attr('data-ch')){
								var matches = data.video;
								idch = '';
								videoLastWeek(matches);
							}else{
								idch = $(this).attr('data-ch');
								matches = $.grep(data.video, function(e) { return e.channels.code == ch.code });
								$(this).addClass('active');
								if(_day.indexOf('Oggi') == 0){
								// ora in onda
									_dbg('lastweek', [_day, ch.code], 'ora in onda');
									onair(ch.code, ch.url,matches);
								} else {
									videoLastWeek(matches);
								}
							}
							mappaCalore(this);
						});
					});

					//days
					$('ul.days').empty();
					var _filedata = new Date( data.date.replace( /(\d{2})\/(\d{2})\/(\d{4})/, "$2/$1/$3") );
					_dbg('lastweek', [_filedata.toLocaleDateString()], 'filedata');
					var today = new Date();
					$.each(data.days, function(i, d) {
					  if(i < 7){
						var _classli= '';
						var _classa= '';
						var _day = d.label;
						var _data = new Date( d.date.replace( /(\d{2})\/(\d{2})\/(\d{4})/, "$2/$1/$3") );
						if(i ==0) {
							_classli = 'yesterday';
							if(_data.toLocaleDateString() == today.toLocaleDateString()) {
								_day = 'Oggi '+_day.split(' ')[1];
							} else {
								var yesterday = new Date(today); //oggi
								yesterday.setDate(yesterday.getDate() -1); //ieri
								if(_data.toLocaleDateString() == yesterday.toLocaleDateString()) {
									_day = 'Ieri '+_day.split(' ')[1];
								} else if(window.outerWidth < 768){
									try{
									_day = _day.split(' ')[0].substring(0,3) +' '+_day.split(' ')[1];
									} catch(er) {}
								}
							}
						} else if(window.outerWidth < 768){
							try{
							_day = _day.split(' ')[0].substring(0,3) +' '+_day.split(' ')[1];
							} catch(er) {}
						}
						if(i ==idday) {
							_classa= 'active';
						}
						var spanday='';
						var arrayday=_day.split(' ');
						try{
							$.each(arrayday,function(i){
								spanday += '<span>'+arrayday[i] +'</span>';
							});
						} catch(er) {spanday = '<span>'+_day +'</span>';}

						$('ul.days').append('<li class="'+_classli+'"><a id="lwd_'+ i +'" title="'+_day+'" class="'+_classa+'">'+spanday+'</a></li>');
						$('#lwd_'+ i).click(function() {
							if(idday != d.id) {
								idday = d.id;
								$('#lastweek').slick('unslick').empty();
								$('ul.days li a').removeClass('active');
								$(this).addClass('active');
								lastWeek();
								mappaCalore(this);
							}
						});
					  }
					});
					$('.homeMs #lastweek').slick(slick_option_home);
					$('.brandpage #lastweek, .videopage #lastweek ').slick(slick_option);
				})
				.done(function(data) {
					videoLastWeek(data.video);
  				})
			} catch(er) {}
		}

		function videoLastWeek(video) {
			_dbg('videoLastWeek', ['item: ' + video.length], 'init');

			$.each(video, function(i, v) {
			   var _traffic = ''; var _tlclass = '';
				if(typeof (v.traffic_light) != 'undefined' && (v.traffic_light != '') && typeof (v.traffic_desc) != 'undefined' &&  (v.traffic_desc!= '')) {
					_traffic = '<p class="hidden">'+ v.traffic_desc +'</p>';
					_tlclass = 'ic-' + v.traffic_light;
				}
				var _duration = v.duration.indexOf('.') > 0 ? v.duration.substr(0,v.duration.indexOf('.')) : v.duration;
				var _min =  parseInt(_duration.substr(0, 2))*60 + parseInt(_duration.substr(3, 2));
				_duration = _min <= 1 ? '1 MIN' : _min + ' MIN'
				$('#lastweek').slick('slickAdd','<div id="lw_'+i+'" class="clip '+_tlclass+'"><a rel="nofollow" href="'+ v.urlweb +'" title="'+ v.title +'" data-brand="'+ v.brand.name +'" data-type="video" onclick="mappaCalore(this)"><img title="'+ v.brand.value + ' ' + v.title +'" alt="'+ v.brand.value + ' ' + v.title +'" src="'+ v.thumbnail +'"><div class="clip__info"><div class="centered"><h3 class="brand">'+ v.brand.value +'</h3><h2 class="title">'+ v.title +'</h2><p>'+ v.desc +'</p></div><h4 class="foot"><span class="channel ic-'+ v.channels.code.toLowerCase() +'"><span>'+ v.channels.channel +'</span></span><span class="timing"> | '+ _duration+'</span></h4></div><time class="label time">'+ v.time +'</time>'+_traffic+'</a></div>' );
			});

			try {
				targetWittytv();
			} catch(er) {}


		}
	function onair(channel, url, matches) {
		var param = servertime.split("-");
		year = param[0];
		month = param[1];
		day = param[2];
		hour = param[3];
		minute = param[4];
		var todayData = new Date(year, Number(month)-1, day);
		if(Number(hour)<6){
			 todayData = new Date(todayData - 1)
			 day = (todayData.getDate()<10) ? '0'+todayData.getDate() : todayData.getDate();
			 month = (todayData.getMonth()+1<10) ? '0'+(todayData.getMonth()+1) : todayData.getMonth()+1;
			 year = todayData.getFullYear();
		}
		if(channel != ''){
			var urlJson = 'http://www.mediaset.it/guidatv/inc/canali/' + year + month + '/' + year + month + day + '_' + channel + '.sjson?callback=?';
			_dbg('onairHome', ['urlJson: ' + urlJson], '');
			$.getJSON(urlJson, function(data) {
				drawBoxOraInOnda(data, channel, url, matches);
			});
		}
	}

	function drawBoxOraInOnda(jsonData, channel, url, matches) {
		var count=1;
		var curDate = hour + ':' + minute;
			//_dbg('drawBoxOraInOnda', ['curDate '+curDate, 'ch '+channel], '');
			$.each(jsonData.events, function(i, event) {
				var titolo = (event.displayTitle != '') ? event.displayTitle : event.title;
				var startProg = (event.startTime).split(":")
				var endProg = (event.endTime).split(":")
				var startTime = (Number(startProg[0])<6) ? (Number(startProg[0])+24)+':'+startProg[1] : event.startTime;
				var endTime = ((Number(hour)<6 && Number(endProg[0])<6) || (Number(endProg[0])<6) || (Number(startProg[0])<6) || (event.endTime == '06:00')) ? (Number(endProg[0])+24)+':'+endProg[1] : event.endTime;
				var currTime = (Number(hour)<6) ? (Number(hour)+24)+':'+minute : curDate;
				var inOnda = ((currTime >= startTime && currTime < endTime) || (currTime <= startTime ) );
				var elapsedTime = (toMinutes(curDate) - toMinutes(event.startTime));
				if((Number(startProg[0])<6) && (Number(hour)>=6)){
					elapsedTime = (toMinutes(curDate) - toMinutes(startTime));
				} else if ((Number(startProg[0])>=6) && (Number(hour)<6)){
					elapsedTime = (toMinutes(currTime) - toMinutes(event.startTime));
				} else if ((Number(startProg[0])<6) && (Number(hour)<6)){
					elapsedTime = (toMinutes(curDate) - toMinutes(event.startTime));
				}
				//_dbg('drawBoxOraInOnda', ['ch '+channel,'inOnda '+inOnda, 'currTime '+currTime, 'startTime '+startTime, 'endTime '+endTime, 'event.startTime '+event.startTime, 'event.endTime '+event.endTime, 'titolo '+titolo, 'test1 '+(currTime >= startTime),'test2 '+(currTime < endTime),'elapsedTime '+ elapsedTime], '');
				if(inOnda && count<=1 && elapsedTime >= 0){
					$('#lastweek').slick("slickAdd",'<div class="clip onair ic-'+channel.toLowerCase()+'" data-channel="'+channel+'"><div class="time">'+event.startTime+'-'+event.endTime+'</div><h2 class="title">'+titolo+'</h2><div class="label rose">ORA IN ONDA</div><a href="'+url+'" title="Guarda la diretta" class="button">Guarda la diretta</a></div>');
					count ++;
				}
			});

			if(count==1){
				// devo recuperare json giorno precedente per disegnare box
				var _todayData = new Date(year, Number(month)-1, day);
				_todayData = new Date(_todayData.getTime() -1);
				var _day = (_todayData.getDate()<10) ? '0'+_todayData.getDate() : _todayData.getDate();
				var _month = (_todayData.getMonth()+1<10) ? '0'+(_todayData.getMonth()+1) : _todayData.getMonth()+1;
				var _year = _todayData.getFullYear();
				var urlJson = 'http://www.mediaset.it/guidatv/inc/canali/' + _year + _month + '/' + _year + _month + _day + '_' + channel + '.sjson?callback=?';
				_dbg('LiveStreaming', ['urlJson: ' + urlJson], '');
				$.getJSON(urlJson, function(data) {
					var event = data.events[data.events.length-1];
					var titolo = (event.displayTitle != '') ? event.displayTitle : event.title;
					var elapsedTime = (toMinutes(curDate) - toMinutes(event.startTime));
					//_dbg('drawOnAir', ['ch '+channel, 'event.startTime '+event.startTime, 'event.endTime '+event.endTime, 'titolo '+titolo,'elapsedTime '+ elapsedTime], '');
					$('#lastweek').slick("slickAdd",'<div class="clip onair ic-'+channel.toLowerCase()+'" data-channel="'+channel+'"><div class="time">'+event.startTime+'-'+event.endTime+'</div><h2 class="title">'+titolo+'</h2><div class="label rose">ORA IN ONDA</div><a href="'+url+'" title="Guarda la diretta" class="button">Guarda la diretta</a></div>');
				});
			}
			videoLastWeek(matches);
		}

		function toMinutes(time_str) {
			var parts = time_str.split(":");
			return Number(parts[0] * 60) + Number(parts[1]);
		}
	});
</script>
