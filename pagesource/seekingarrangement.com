<script>


  var gender =  '' ;
  var type =  '' ;
  var interest =  '' ;

  $(document).ready(function () {

    $('.close_banner').click(function () {
      $('.asia-brand-banner').hide();
    });

    $('#gender_selection_div').click(function () {
//      $('.mide-content-head').hide();
      $('.asia-brand-banner').hide();
    });

    $('.navTrigger').click(function () {
      $(this).toggleClass('active');
      $(".menuItem").toggle();
      $(".menuItemLists").toggleClass('menuItemListActive');
      $(".home-layoutContainer").toggleClass('homeTopActive');
    });


    $('.gender_selection_man').insertAfter('.gender_selection_woman');
    // Refresh Captcha
    $('#refreshCaptcha').on('click', function () {
      $.ajax({
        method: 'GET',
        url: '/refresh-captcha',
        beforeSend: function () {
          $('#refreshCaptcha').hide();
          $('#refreshCaptcha-waiting').show();
        },
      }).done(function (response) {
        setTimeout(function () {
          $('#refreshCaptcha').show();
          $('#refreshCaptcha-waiting').hide();
        }, 1000);
        $('.captchaImg').prop('src', response.url);
      });
    });

    showhidediv();

      
    // process button clicked
    $(".gender_selection").click(function (e) {
      $('.mide-content-head').hide();
      gender = $(this).html();
      var value = "female";
      if (gender == "Man") {
        $('.type_selection,.interest_selection').removeClass('float_right');
        $('.type_selection_attractive').insertAfter('.type_selection_generous').addClass('float_right');
        //$('#gender_preference-male').insertAfter('#gender_preference-female').addClass('float_right');
        value = "male";
      }
      else {
        $('.type_selection,.interest_selection').removeClass('float_right');
        $('.type_selection_generous').insertAfter('.type_selection_attractive').addClass('float_right');
        $('.type_selection_attractive').removeClass('floarRigghtMargin');
        //$('#gender_preference-female').insertAfter('#gender_preference-male').addClass('float_right');
        value = "female";
      }

      // set value
      $("#sex").val($(this).data('value'));

      // show hide div
      showhidediv();
    });

    $(".type_selection").click(function (e) {
      type = $(this).html();
      $('.edu_email_address').hide();
      
      var value = "attractive";
      if (type == "Sugar Daddy" || type == "Sugar Mommy") {
//        $('.formListItemLabelOr_arrange_men').hide();
        $('.formListItemLabelOr_arrange_women').hide();
        $('.interest_selection_title').addClass('title_float');
        $('.formListItemLabelOr_arrange_men').addClass('formListItemLabelOr_arrange_float');
        $('.formListItemLabelOr_arrange_women').addClass('formListItemLabelOr_arrange_float');
        $('.interest_selection_arrange').insertAfter('.interest_selection_attractive').addClass('float_left2');
        $('.interest_selection_generous').removeClass('float_left2');
        //$('.formListItemLabelOr_arrange').removeClass('formListItemLabelOr_arrange_float');
        $('.edu_email_address').hide();

        value = "generous";
      }
      else {
        $('.formListItemLabelOr_arrange_women').hide();
        $('.formListItemLabelOr_arrange_men').show();
        $('.interest_selection_title').addClass('title_float');
        $('.formListItemLabelOr_arrange_men').addClass('formListItemLabelOr_arrange_float');
        $('.interest_selection_arrange').insertAfter('.interest_selection_generous').addClass('float_left2');
        $('.interest_selection_attractive').removeClass('float_left2');
        //$('.formListItemLabelOr_arrange').removeClass('formListItemLabelOr_arrange_float');

        value = "attractive";
        $('.edu_email_address').show();
      }

      // set value
      $("#account_type").val($(this).data('value'));

      // show hide div
      showhidediv();
    });

    $(".interest_selection").click(function (e) {
      interest = $(this).html();

      $('input[name="gender_preference[]"]').remove();

      // set value
      if (interest.indexOf("Both") > -1) {
        $(this).parent().append('<input type="hidden" id="men" name="gender_preference[]" value="' + $('#gender_preference-male').data('value') + '" />');
        $(this).parent().append('<input type="hidden" id="women" name="gender_preference[]" value="' + $('#gender_preference-female').data('value') + '" />');
      } else if (interest == "male") {
        $(this).parent().append('<input type="hidden" id="men" name="gender_preference[]" value="' + $('#gender_preference-male').data('value') + '" />');
      } else {
        $(this).parent().append('<input type="hidden" id="women" name="gender_preference[]" value="' + $('#gender_preference-female').data('value') + '" />');
      }

      // show hide div
      showhidediv();
    });

    // display link clicked
    $("#gender_text").click(function (e) {
      // reset value
      gender = "";
      $("#sex").val("");

      showhidediv();
    });

    $("#type_text").click(function (e) {
      // reset value
      type = "";
      $("#account_type").val("");

      showhidediv();
    });

    $("#interest_text").click(function (e) {
      // reset value
      interest = "";
      $("#men").val("");
      $("#women").val("");

      showhidediv();
    });

      
      
    // mailcheck
    $('#email').on('blur', function (event) {
      //TODO: Broken method
      //suggestion("email", "suggestion", "en");
    });
  });

  function expalinThis() {
    var inst_ls = $('[data-remodal-id=explainModal]').remodal();
    inst_ls.open();
  }

  function showhidediv() {
    // selection div
    $("#gender_selection_div").hide();
    $("#type_selection_div").hide();
    $("#interest_selection_div").hide();
    $("#email_selection_div").hide();
    $(".fbClass").hide();

    if (!gender.length) {
      $("#gender_selection_div").show();
    } else if (!type.length) {
      var value = "Sugar Mommy";
      if (gender == "Man") {
        value = "Sugar Daddy";
      }
      $("#generous_type").html(value);
      $("#type_selection_div").show();
    } else if (!interest.length) {
      $("#interest_selection_div").show();
    } else {
      $("#email_selection_div").show();
    }

    // display div
    $("#gender_display_li").show();
    $("#type_display_li").show();
    $("#interest_display_li").show();

    if (!gender.length) {
      $("#gender_display_li").hide();
    } else {
      $("#gender_text").html(gender);
    }

    if (!type.length) {
      $("#type_display_li").hide();
    } else {
      if (gender == 'Man' && type == "Sugar Mommy") {
        type = 'Sugar Daddy';
      } else if (gender == 'Woman' && type == "Sugar Daddy") {
        type = 'Sugar Mommy';
      }
      $("#type_text").html(type);
    }
    
    if ($.trim(type) == 'Sugar Baby') {
      $('.fbClass').show();
    } else {
      $('.fbClass').hide();
    }

    if (!interest.length) {
      $("#interest_display_li").hide();
    } else {
      $("#interest_text").html(interest);
    }
  }


  $(document).on('change', '#email', function () {
    $('#email').parsley().validate();
  });

  function getFacebookEmail() {
    if (FB) {
      FB.api('/me', 'GET', { fields: 'email' }, function (response) {
          $('.text-input').val(response.email);
          sessionStorage.setItem('fb_ref_email', response.email);
      });
    }
  };

  $('.fbClass').on('click', function () {
    if (FB) {
        FB.login(
          function (loginResponse) {
            if (loginResponse.status === 'connected') {
              getFacebookEmail();
            }
          },
          { scope: 'email' }
        )
    }
  });

  var $email = $('#email');
  var $hint = $(".emailHint");

  $email.on('change', function () {
    $hint.css('display', 'none').empty();
    $(this).mailcheck({
      suggested: function (element, suggestion) {
        if (!$hint.html()) {
          // First error - fill in/show entire hint element
          var suggestion = "Did you mean <span class='suggestion'>" +
            "<span class='address'>" + suggestion.address + "</span>"
            + "@<a href='#' class='emailDomain'>" + suggestion.domain +
            "</a></span>";

          /*window.ParsleyUI.addError($('#email').parsley(),"emailError", "Your email address isn't valid");*/

          $hint.html(suggestion).fadeIn(150);

        } else {
          // Subsequent errors
          $(".address").html(suggestion.address);
          $(".emailDomain").html(suggestion.domain);
        }
      }
    });
  });

  $hint.on('click', '.emailDomain', function () {
    $email.val($(".suggestion").text());
    $hint.fadeOut(200, function () {
      $(this).empty();
    });
    return false;
  });


  window.Parsley.on('field:error', function () {
    this.$element.closest('.FormGroup').addClass('error')
  });
  window.Parsley.on('field:success', function () {
    this.$element.closest('.FormGroup').removeClass('error')
  });

</script>