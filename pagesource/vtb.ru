            _utm_source = getParameterByName('utm_source') != null ? getParameterByName('utm_source') : 'direct';
            _utm_medium = getParameterByName('utm_medium') != null ? getParameterByName('utm_medium') : 'organic';
            _utm_campaign = getParameterByName('utm_campaign') != null ? getParameterByName('utm_campaign') : '';

            client_source = [{
                "utmSource": _utm_source,
                "utmMedium": _utm_medium,
                "utmCampaign": _utm_campaign,
                "utmGa": _ga,
                "actionId": ""
            }];

            document.cookie = "client_source=" + (JSON.stringify(client_source[0])) + ";path=/; expires=" + date.toUTCString() + ";domain=.vtb.ru;path=/";

        } else {
            //Иначе, считываем метки (параметры) присвоенные с основного сайта
            var _client_source = decodeURIComponent(document.cookie.replace(/(?:(?:^|.*;\s*)client_source\s*\=\s*([^;]*).*$)|^.*$/, "$1"));

            if (_client_source) {
                //преобразуем строку в "читабельную"
                var obj_client_source = JSON.parse(decodeURIComponent(_client_source));
 } else {
