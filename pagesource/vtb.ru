        }]
 
        document.cookie = 'client_source=' + (JSON.stringify(obj_client_source[0])) + ';path=/; expires=' + date.toUTCString() + ";domain=.vtb.ru;path=/";
    }

                _utm_source = obj_client_source.utmSource;
                _utm_medium = obj_client_source.utmMedium;
                _utm_campaign = obj_client_source.utmCampaign;
            }
        




        //Переменная наполняется в factories.js
        var _uninueId = '';

        var now = new Date();
        var time = now.getTime();
        time += 3600 * 1000;
        //Записываем в cookie значение из параметров
        //Проверяем, если значение пустое, то задаем стандартные значения
        document.cookie = "utm_source=" + (_utm_source ? _utm_source : 'direct') + "; expires=" + now.toUTCString() + ";path=/; expires=" + date.toUTCString() + ";domain=.vtb.ru;path=/";
        document.cookie = "utm_medium=" + (_utm_medium ? _utm_medium : 'organic') + "; expires=" + now.toUTCString() + ";path=/; expires=" + date.toUTCString() + ";domain=.vtb.ru;path=/";
        document.cookie = "utm_campaign=" + (_utm_campaign ? _utm_campaign : '') + "; expires=" + now.toUTCString() + ";path=/; expires=" + date.toUTCString() + ";domain=.vtb.ru;path=/";



