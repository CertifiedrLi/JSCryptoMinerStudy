                    var self = this;

                    if (value) {
                        if (!this.vertical) {
                            document.body.className += ' modal-open';
                        }
                    } else {
                        window.setTimeout(function() {
                            self.isMaskHide = false;
                            document.body.className = document.body.className.replace(/\s?modal-open/, '');
                        }, 500 || 0);
                    }
                }
            },
