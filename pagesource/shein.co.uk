      getGoodsUrl: SHEIN_W.getGoodsUrl,
      getLatestLightningDeals: function () {
        var self = this
        $.get('/getSheinFlashDeals', function (data) {
          self.lightningDealsData = data.lightning_deals

          var start_time = data.lightning_deals.start_time
          var end_time = data.lightning_deals.end_time

          if (!(start_time && end_time && $('.mc-flash-sale-container')[0])) {
            $('.lightning-deals-swiper').hide()
            $('.mc-flash-sale-container').hide()
            return
          }
          $.get('/currentTime', function (data) {
            var current = data.zzz
            if (current < start_time || current > end_time) {
              $('.lightning-deals-swiper').hide()
              $('.mc-flash-sale-container').hide()
              return
            }
            $('.lightning-deals-swiper').show()
            $('.mc-flash-sale-container').show()
            var clock = $(".j-falshsl-digits").FlipClock({
              autoStart: false,
              callbacks: {
                stop: function () {
                  setTimeout(function () {
                    self.getLatestLightningDeals()
                  }, 5 * 60 * 1000)
                  $('.lightning-deals-swiper').hide()
                  $('.mc-flash-sale-container').hide()
                }
              }
            })
            clock.setTime(end_time - current)
            clock.setCountdown(true)
            clock.start()
            $('.flip-clock-label').hide()
            var styleText = document.querySelector('style').textContent
            document.querySelector('style').textContent = styleText + '.flip-clock-wrapper ul li a div div.inn { color: ' + lightningDealsConfig.timeColor + ' !important; background-color: ' + lightningDealsConfig.timeBgColor + ' !important; }'
