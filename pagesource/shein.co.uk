<script type="text/javascript">
  var GB_s_windowH = $(window).height();
  var GB_s_beforeScrollTop = $(window).scrollTop();
  var bannerH
  var headerTopH
  var newHeader
  var scrollHeadernavH
  var fixHeaderH
  var headerH
function listNavListFun() {
    bannerH =  48
    headerTopH = 144
    newHeader=0,scrollHeadernavH=0;
    for(var i=0;i<$('.banner-wrapper').length;i++) {
        var topbannerHeight = $('.banner-wrapper').eq(i).height()
        newHeader = newHeader + topbannerHeight
        scrollHeadernavH = scrollHeadernavH + parseInt($('.banner-wrapper').eq(i).height())
    }
    $(".banner-wrapper").on("click",".j-icon-del",function (e) {
        newHeader = 0
        setTimeout(function(){
            for(var i=0;i<$('.banner-wrapper').length;i++) {
                newHeader += parseInt($('.banner-wrapper').eq(i).height())
                scrollHeadernavH += parseInt($('.banner-wrapper').eq(i).height())
            }
        },1000)
        newHeader += 96
        $('.j-cate-leftnav').css({'top':parseInt($('.j-cate-leftnav').css('top'))-48})
    });
    newHeader += 96
    headerH = 96
    fixHeaderH = 96
    GB_s_windowH = $(window).height();
    GB_s_beforeScrollTop = $(window).scrollTop();
    GB_s_lav_scroll();

}
function GB_s_lav_scroll() {
  var $pullCtn = $('.j-list-cate-inner'), $sideArray = [$('.j-cate-leftnav')];
  var scrollTop = $(window).scrollTop();

  var afterScrollTop = $(window).scrollTop(),
    scrollDelta = afterScrollTop - GB_s_beforeScrollTop;

  // console.log(scrollDelta);
  // scrollDelta>0 ---down
  // scrollDelta<0 ---up

  var pullCtnH = $pullCtn.height(),
    pullCtnPos = (pullCtnH + headerH) - scrollTop;

  var sidesInfo = [];
  var referenceIndex = 0;
  $sideArray.forEach(function(item, index){
    var i = {};

    i.h = item.height(),
      i.top = parseInt(item.css('top')),
      i.pos = i.h + i.top,
      i.isExceed = fixHeaderH + i.h > GB_s_windowH

    sidesInfo.push(i);

    if (index > 0 && i.h > sidesInfo[referenceIndex].h) {
      referenceIndex = index;
    }
  })

  var targetTop = GB_s_windowH-sidesInfo[referenceIndex].h,
    referenceH = sidesInfo[referenceIndex].h,
    referenceTop = sidesInfo[referenceIndex].top,
    referencePos = sidesInfo[referenceIndex].pos;


  if (scrollTop <= scrollHeadernavH) {
    $sideArray.forEach(function(item, index){
      // item.css('top', bannerH+headerTopH);
      item.css('top', newHeader);
    })
    //  $bcWrap.show();
  } else {
    //   $bcWrap.hide();

    if (pullCtnPos >= GB_s_windowH) {

      if (fixHeaderH + referenceH > GB_s_windowH) {


        if ( (scrollDelta>0 && referencePos>GB_s_windowH) || (scrollDelta<0 && referenceTop<fixHeaderH) ) {
          $sideArray.forEach(function(item, index) {
            if (sidesInfo[index].isExceed) {
              item.css('top', referenceTop-scrollDelta);
            } else {
              item.css('top', fixHeaderH);
            }
          })
          // down
        } else if (scrollDelta>0) {
          $sideArray.forEach(function(item, index) {
            if (sidesInfo[index].isExceed) {
              item.css('top', targetTop);
            } else {
              item.css('top', fixHeaderH);
            }
          })

          // up
        } else if (scrollDelta<0) {
          $sideArray.forEach(function(item, index) {
            item.css('top', $('.banner-wrapper').length*48+fixHeaderH);
          })
        }

      }


      else if (fixHeaderH + referenceH <= GB_s_windowH) {
        $sideArray.forEach(function(item, index){
          item.css('top', fixHeaderH);
        })
      }
    }

    else if (pullCtnPos < GB_s_windowH) {
      $sideArray.forEach(function(item, index){
        if (sidesInfo[index].isExceed) {
          item.css('top', targetTop-(GB_s_windowH-pullCtnPos));
        } else {
          item.css('top', pullCtnPos-GB_s_windowH+fixHeaderH);
        }
      })
    }
  }

  GB_s_beforeScrollTop = afterScrollTop;

}
$(window).resize(function() {
  GB_s_windowH = $(window).height();
  GB_s_lav_scroll();
});
GB_s_lnav_timeout = null;
$(window).scroll(
  function () {
    if(GB_s_lnav_timeout)clearTimeout(GB_s_lnav_timeout);
    GB_s_lnav_timeout = setTimeout(function(){
      GB_s_lav_scroll();
    }, 0)
  }
);
</script>

