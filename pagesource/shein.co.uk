<script type="text/javascript" data-id="sizeguide-vue">

    Vue.component('sizeguide-vue', {
        template: '#sizeguide-template',
        props: {
            sizeinfo: Array,
            cat_info: Object,
            is_dec: {type: Boolean, default: true},
            show_inch: {type: Boolean, default: false},
        },
        data: function() {
            return {
                special_symbol_tips: ''
            }
        },
        computed: {
            noUnitTitles: function() {
                var noUnitTitles = {};
                var sizeinfo = this.sizeinfo;
                var index = 0;
                var isBreak = false;
                for (var i = 0; i < sizeinfo.length; i++) {
                    for (var key in sizeinfo[i]) {
                        if (key.toLocaleLowerCase() != 'size') {
                            index = i;
                            isBreak = true;
                            break;
                        }
                    }
                    if (isBreak) {break;}
                }
                for (var key in sizeinfo[i]) {
                    noUnitTitles[key] = key.replace(/\(c[ ]*m\)/ig,"");
                }
                return noUnitTitles;
            },
            noUnitItems: function() {
                var noUnitItems = [];
                this.sizeinfo.forEach(function(value, index, array) {
                    noUnitItems.push({});
                    for (var key in value) {
                        noUnitItems[index][key] = value[key].replace(/c[ ]*m/ig,"").replace(/\,[a-z]+$/ig,"");
                    }
                });
                return noUnitItems;
            },
            inchItems: function() {
                var inchItems = [];
                var pattern_num=/\d+[.]*\d*/ig;

                this.noUnitItems.forEach(function(value, index, array) {
                    inchItems.push({});
                    for (var key in value) {
                        inchItems[index][key] = value[key].replace(pattern_num,function(value){
                            return (value/2.539999918).toFixed(1);
                        });
                    }
                });
                return inchItems;
            }
        },
        created: function() {
            var sizeinfo = this.sizeinfo, hasDash = false, hasSlash = false
            if(sizeinfo) {
                sizeinfo.forEach(function(item) {
                    for (var key in item) {
                        var value = item[key]
                        if(value && value.indexOf('-') > 0) {
                            hasDash = true
                        }
                        if(value && value.indexOf('/') > 0) {
                            hasSlash = true
                        }
                    }
                })
                if(hasDash && hasSlash) {
                    this.special_symbol_tips = '<em>&#8243;/&#8243;:</em> refers to the the front & back length of the clothes. <em>&#8243;&nbsp;-&nbsp;&#8243;:&nbsp;</em> refers to the elastic range of the clothes.'
                } else if(hasDash) {
                    this.special_symbol_tips = '<em>&#8243;&nbsp;-&nbsp;&#8243;:&nbsp;</em> refers to the elastic range of the clothes.'
                } else if(hasSlash) {
                    this.special_symbol_tips = '<em>&#8243;/&#8243;:</em> refers to the the front & back length of the clothes. '
                }
            }
        },
        mounted: function() {
            this.$nextTick(function () {
                this.bindEvent();
            });
        },
        updated: function() {
            // console.log('size guide updated');
            this.$nextTick(function () {
                this.bindEvent();
            });
        },
        methods: {
            bindEvent: function() {
                $('.j-sg-ctn td').hover(function() {
                    var e = $(this).prevAll().length+1,
                        t = $(this).closest("tr").prevAll().length+1;

                    if (e>1) {
                        if ($(this).hasClass('j-sgcolplus')) {
                            $(this).closest("table").find("tr:lt(" + t + ") > td:nth-child(" + (e+1) + ")").addClass("col-highlight");
                            $(this).closest("table").find("tr:lt(" + t + ") > td:nth-child(" + e + ").j-sgcol-noplus").addClass("col-highlight");
                        } else {
                            $(this).closest("table").find("tr:lt(" + t + ") > td:nth-child(" + e + ")").addClass("col-highlight");
                            $(this).closest("table").find("tr:lt(" + t + ") > td:nth-child(" + (e-1) + ").j-sgcol-noplus").addClass("col-highlight");
                        }

                        if ($(this).hasClass('j-sgcol2')) {
                            $(this).closest("table").find("tr:lt(" + t + ") > td:nth-child(" + (e+1) + "):not(.j-sgcol-noplus)").addClass("col-highlight");
                        }
                        if ($(this).hasClass('j-sgcol-2')) {
                            $(this).closest("table").find("tr:lt(" + t + ") > td:nth-child(" + (e-1) + ")").addClass("col-highlight");
                            $(this).closest("table").find("tr:lt(" + t + ") > td:nth-child(" + e + ").j-sgcol-noplus").removeClass("col-highlight");
                        }

                    }
                }, function() {
                    $(this).closest("table").find("tr > .col-highlight").removeClass("col-highlight");
                });
            },
            showCm: function() {
                $('.j-inch-table').hide();
                $('.j-cm-table').show();
            },
            showInch: function() {
                $('.j-cm-table').hide();
                $('.j-inch-table').show();
            },
        }
    });
</script>


