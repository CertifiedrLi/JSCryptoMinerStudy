<script>
    var GB_analysis_obj, gaEventCenter = new Vue()
    $(function () {
        var siteUID = 'uk', eventCategory = 'Banner', unViewed = [];
        function _getUrlNameString(url, name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
            var r = url.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }
        function addPromo(payload) {
            ga('shein.ec:addPromo', payload)
        }
        function setAction(action) {
            ga('shein.ec:setAction', action)
        }

        function handleBanner(params) {
            var action = params.action, name = params.name
            addPromo({
                name: name
            })
            if (params.action == 'ClickBanner') {
                setAction('promo_click')
            }
            if(params.loadPageTrigger){
                ga('shein.send', 'event', eventCategory, action, name,{nonInteraction:true})
            }else{
                ga('shein.send', 'event', eventCategory, action, name)
            }
        }

        $('img.ga_promo,div[data-type="HOT"] a').each(function (index, item) {
            unViewed.push(item)
        })

        $(document).on('click', 'img.ga_promo,div[data-type="HOT"] a', function () {
            var name = _getUrlNameString($(this).closest("a").attr("href")||"","ici") || $(this).data('name') || ''
            if(name=="")return;
            handleBanner({
                name: name,
                action: 'ClickBanner'
            })
        })

        $(window).scroll(function () {
            var $container = $(this);
            for(var i=0;i<unViewed.length;i++){
                var item = unViewed[i];
                var name = _getUrlNameString($(item).closest("a").attr("href")||"","ici") || $(item).data('name') || ''
                if(name==""){
                    unViewed.splice(i, 1);
                    i--;
                    continue;//end this loop;
                }

                if ($(window).height() >= item.getBoundingClientRect().bottom * 0.8) {
                    handleBanner({
                        name: name,
                        action: 'ViewBanner'
                    })
                    unViewed.splice(i, 1);
                    i--;
                }
            }
        });
        ga(function() {
            $(window).scroll();
        })

        // --------------- stylegallery

         var stylegalleryPage = '首页stylegallery'

        function handleStyleGarryBanner(params) {
            var action = params.action, name = params.name, position = params.position
            addPromo({
                name: name,
                position: position
            })
            if (params.action == 'ClickBanner') {
                setAction('promo_click')
            }
            ga('shein.send', 'event', stylegalleryPage, 'ViewItems', 'top_picks')
        }

        $(window).scroll(function() {

            $('.j-outfit-swiper:not([viewed=true])').each(function(){
                var $this = $(this)
                if($this.css('visibility') == 'hidden' || $this.height() == 0) return;
                if ($this.offset().top+$this.height()-$(window).scrollTop()-$(window).height()<0) {
                    $this.attr({'viewed':true})
                    if(typeof(indexVue) != 'undefined') {
                        var reVue = indexVue.$refs['stylegellery'];

                        if(reVue) {

                            var _arrList = []

                            // ga数据埋点
                            for (var i=0; i<5 && i<reVue.goods.length; i++) {
                                handleStyleGarryBanner({
                                    name: stylegalleryPage,
                                    position: i
                                })
                                reVue.gaCache[i] = true;
                            }
                        }
                    }
                }
            })
        });

        if(typeof indexVue != 'undefined' && indexVue.$refs['stylegellery']) {
            var reVue = indexVue.$refs['stylegellery'];
            gaEventCenter.$on('slideChangeEnd', function (json) {
                for (var i=json.swiper.activeIndex; i<json.swiper.activeIndex + 5; i++) {
                    if(!reVue.gaCache[i]) {
                        handleStyleGarryBanner({
                            name: stylegalleryPage,
                            position: i
                        })
                        reVue.gaCache[i] = true;
                    }
                }
            });
            gaEventCenter.$on('SlidePrevEnd', function () {
                GBGaSend({
                  category: stylegalleryPage,
                  action: 'ClickPrevArrow',
                  label: 'top_picks'
                })
            });
            gaEventCenter.$on('SlideNextEnd', function () {
                GBGaSend({
                  category: stylegalleryPage,
                  action: 'ClickNextArrow',
                  label: 'top_picks'
                })
            });
        }

        gaEventCenter.$on('quickViewAddBag', function (json) {

            if(json.value == 1 ){
                ga('shein.ec:addProduct', {
                    id: json.goods.detail.productRelationID || json.goods.detail.goods_sn,
                    name: json.goods.detail.goods_sn,
                    quantity: json.form.quantity,
                    category: json.goods.detail.cat_id,
                    price: GB_ga_transformPrice(json.goods.detail.salePrice.amount),
                    brand: 'shein'
                });
                ga("shein.ec:setAction", "add")
            }
            GBGaSend({
                category: stylegalleryPage,
                action: 'add to bag',
                label: 'Home-quickview',
                value: json.value
            })
        })

        gaEventCenter.$on('quickVieAddWish', function (json) {
            GBGaSend({
                category: stylegalleryPage,
                action: 'wishlist',
                label: json.sku,
                value: json.value
            })
        })

        gaEventCenter.$on('quickViewOpen', function (json) {
            if(json.value == 1 ){
                ga('shein.ec:addProduct', {
                    id: json.goods.detail.productRelationID || json.goods.detail.goods_sn,
                    name: json.goods.detail.goods_sn,
                    category: json.goods.detail.cat_id,
                    price: GB_ga_transformPrice(json.goods.detail.salePrice.amount),
                    position: json.index,
                    brand: 'shein'
                });
                ga("shein.ec:setAction", "promo_click")
                GBGaSend({
                    category: stylegalleryPage,
                    action: 'home-quickview',
                })
            }
        })

        $(document).on('click', '.j-tags-a', function () {
            GBGaSend({
                category: stylegalleryPage,
                action: 'home-label',
                label: $(this).find('.j-tags-name').text()
            })
        })
    })
</script>


