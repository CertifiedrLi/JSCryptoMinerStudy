                    this.$emit('cancel', opt);
                },
                clickMask: function(e) {
                    if (!this.force && !this.isInTree(e.target)) {
                        this.cancel('mask');
                    }
                },
                isInTree: function(target) {
                    return this.$refs.dialog.contains(target)
                }
            }
