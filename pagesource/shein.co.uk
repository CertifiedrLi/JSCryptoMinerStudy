<script type="text/x-template" id="vue-qty-temp">
    <div class="c-opt-qty" :class="['j-'+pagetype]">
        <span class="opt-qty-span iconfont icon-reduce" :class="{disabled:minusDisabled}" @click="minus"></span>
        <input class="opt-qty-input j-qty-input" :disabled="inputDisabled" ref="input" type="text" v-model.number="dataQuantity" @blur="dataQuantity = Number(dataQuantity)">
        <span class="opt-qty-span iconfont icon-add" :class="{disabled:plusDisabled}" @click="plus"></span>
    </div>
</script>

<script type="text/javascript">
    !function () {
        var eventCategory = '', eventAction = '-quickview'
        var analysis = {
            handleQty: function (type) {
                
                if (eventCategory) {
                    GBGaSend({
                        category: eventCategory,
                        action: type + eventAction,
                        label: this.item && this.item.detail && this.item.detail.goods_sn
                    })
                }
            },
            bindEvent: function () {
                var self = this, qty = self.dataQuantity

                if (this.pagetype == 'ItemList') {
                    eventCategory = PageGroupOverview
                } else if (this.pagetype == 'ItemDetail') {
                    eventCategory = '商品详情页'
                    eventAction = ''
                }
                if(this.pagetype == 'stylegalleryhome') {
                    eventCategory = '首页stylegallery'
                } else if(this.pagetype == 'stylegallerylist') {
                    eventCategory = 'stylegallery列表页'
                } else if(this.pagetype == 'cart') {
                    eventCategory = '推荐列表'
                }

                $(document).on('blur', '.c-opt-qty .j-qty-input', function (e) {
                    if (e.target.value != qty) {
                        if (eventCategory) {
                            GBGaSend({
                                category: eventCategory,
                                action: 'ClickQty' + eventAction,
                                label: self.item && self.item.detail && self.item.detail.goods_sn
                            })
                        }
                        qty = e.target.value
                    }
                })
            }
        }

        Vue.component('qty-vue', {
            template: '#vue-qty-temp',
            props: {
                item: Object,
                value: { type: Number, default: 1 },
                min: { type: Number, default: 1 },
                max: { type: Number, default: 999 },
                inputDisabled:  { type: Boolean, default: false },
                pagetype: { type: String, default: 'ItemList' },
            },
            data: function () {
                var self = this;
                return {
                    dataQuantity: this.value,
                    maxnum: this.max * 1 > 999 ? 999 : this.max
                };
            },
            created: function () {
                analysis.bindEvent.call(this)
            },
            mounted: function () {
            },
            methods: {
                minus: function () {
                    if (this.value <= this.min) return;
                    this.dataQuantity--;

                    analysis.handleQty.call(this, 'QtyLess')
                },
                plus: function () {
                    if (this.value >= this.maxnum) return;
                    this.dataQuantity++;

                    analysis.handleQty.call(this, 'QtyAdd')
                }
            },
            computed: {
                minusDisabled: function () {
                    return this.dataQuantity <= this.min
                },
                plusDisabled: function () {
                    return this.dataQuantity >= this.maxnum
                }
            },
            watch: {
                max: function (val) {
                    this.maxnum = this.max * 1 > 999 ? 999 : this.max
                },
                value: function (val) {
                    this.dataQuantity = val;
                },
                dataQuantity: function () {

                    if (!this.dataQuantity && (this.dataQuantity + '').trim() != '') {
                        this.dataQuantity = this.min;
                        this.$refs.input.focus()
                    }

                    if (this.dataQuantity >= this.maxnum) {
                        this.dataQuantity = this.maxnum
                    }

                    if (this.maxnum <= 0) this.dataQuantity = 1;

                    if ((this.dataQuantity + '').trim() != '') {
                        this.dataQuantity = Math.abs(Math.floor(this.dataQuantity))
                        this.$emit('input', Number(this.dataQuantity))
                    }
                }
            }
        })
    }()
</script>
