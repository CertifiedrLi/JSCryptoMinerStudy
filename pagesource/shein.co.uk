                    if(this.goods.detail) {
                        var currency = SHEIN_W.getCookie('currency') || SHEIN_W.getCookie('default_currency')
                        var currencySymbol = {"GBP":{"currency_id":"1","code":"GBP","symbol_left":"GBP£","symbol_right":"","decimal_place":"2","value":"0.73200000","date_modified":"2014-08-11 10:11:47","status":"0","default":"1","country_flag":"images/currency/GBP.jpg","title":"Pound Sterling"},"USD":{"currency_id":"2","code":"USD","symbol_left":"US$","symbol_right":"","decimal_place":"2","value":"1.00000000","date_modified":"2014-08-11 10:11:47","status":"0","default":"0","country_flag":"images/currency/USD.jpg","title":"US Dollar"},"CHF":{"currency_id":"13","code":"CHF","symbol_left":"CHF$","symbol_right":"","decimal_place":"2","value":"0.97145900","date_modified":"2014-08-11 10:11:47","status":"0","default":"0","country_flag":"images/currency/CHF.jpg","title":"Switzerland Francs"},"CAD":{"currency_id":"10","code":"CAD","symbol_left":"CA$","symbol_right":"","decimal_place":"2","value":"1.33388600","date_modified":"2014-08-11 10:11:47","status":"0","default":"0","country_flag":"images/currency/CAD.jpg","title":"Canadian Dollar"},"NOK":{"currency_id":"15","code":"NOK","symbol_left":"N.Kr.","symbol_right":"","decimal_place":"2","value":"7.86905900","date_modified":"2014-08-11 10:11:47","status":"0","default":"0","country_flag":"images/currency/NOK.jpg","title":"Norwegian Krone"},"EUR":{"currency_id":"3","code":"EUR","symbol_left":"€","symbol_right":"€","decimal_place":"2","value":"0.82975200","date_modified":"2014-08-11 10:11:47","status":"0","default":"0","country_flag":"images/currency/EUR.jpg","title":"Euro"}}[currency].symbol_left
                        if(currency == 'EUR' || currency == 'RUB') {
                            return (this.addBagForm.quantity * Number(this.goods.detail.salePrice.amount)).toFixed(2) + currencySymbol
                        } else {
                            return currencySymbol + (this.addBagForm.quantity * Number(this.goods.detail.salePrice.amount)).toFixed(2)
                        }
                    }
                },
                showSoldOutBtn: function() {
                    var self = this;
                    return this.goods.detail.is_on_sale==0 || (this.goods.attrSize && this.goods.attrSize.length && this.goods.attrSize.every(function(item){ return item.stock <= 0}))
                }
            },
            watch: {
                'openEvent.status.open': {
                    handler: function() {
                        var data = this.openEvent;
                        if(data.status.open) {
                            this.quickViewOpen(data);
                        }
                    },
                    deep:true
                },
                'addBagForm.quantity': {
                    handler: function() {
                        this.status.clearance = false
                    },
                }
            },
            methods: {
                initEvent: function() {
                    var self = this
                    var showInch = SHEIN_W.getCookie('showInch');
                    
                        if(showInch && showInch != 'undefined'){
                            self.status.isInch = Number(showInch)
                        }else{
                            $.get('/showInch', function(json) {
                                if(json.code == 0 && json.info) {
                                    var show_inch = Number(json.info.showInch)
                                    SHEIN_W.setCookie('showInch',show_inch,3600*24);
                                    self.status.isInch = show_inch;
                                }
                            })
                        }
                    
                },
                clear: function() {
                    this.status.wish = 0;
                    this.status.quickBtn = 0;
                    this.status.thumb = -1;

                    this.addBagForm.quantity = 1;
                    this.addBagForm.attrs[0].attr_id = 0;
                    this.addBagForm.attrs[0].attr_value_id = 0;

                    this.status.currenSize = -1;
                    this.status.selectsize = 0;

                    $('.j-opt-size input').removeAttr('checked');
                },
                // opt
                quickGetData: function(opt) {

                    if(this.status.loading.color[opt.goodsId]) return;
                    this.status.loading.color[opt.goodsId] = 1;

                    var self = this;
                    var goods = {};

                    $.ajax({
                        type:"GET",
                        url:'/product-item-'+opt.goodsId+'.html',
                        timeout:10000,
                        success:function(json) {
                            if(json.code ==0 && json.info) {

                                goods = json.info.goods;

                                goods.addtowish = self.wishlistCheck(opt.goodsId)    // 加入收藏检测
                                self.styleGelleryCheck(goods, opt.styleGalleryData) // 红人图检测
                                self.colorMoreCheck(goods, opt.originId);    // 多颜色商品检测
                                self.checkSizePrice(goods) // 尺码价格检测

                                self.loadImgByDpr(goods);

                                self.quickDataCache[opt.goodsId] = goods;

                                if(!opt.is_cache) {
                                    self.goods = goods;
                                }
                            }

                            if(typeof(opt.callback) == "function") {
                                opt.callback(goods, true);
                            }

                            self.status.loading.color[opt.goodsId] = 0;
                        },
                        error:function(json){
                            console.log('Network error!' + opt.originId + '::' + opt.goodsId);
                            if(typeof(opt.callback) == "function" && !opt.originId) {
                                console.log('close' + self.status.quickOpen)
                                opt.callback(json, false);
                            }
                            self.status.quickBtn = 0;
                            self.status.loading.color[opt.goodsId] = 0;
                        }
                    })

                },
                // 根据dpr取图
                loadImgByDpr: function(goods) {
                    var dprInsert = function(str, s) {
                        var index = str.indexOf('.jpg') != -1 ? str.indexOf('.jpg') : str.indexOf('.png');
                        if (index != -1) {
                            return (str.slice(0,index) + s + str.slice(index));
                        } else {
                            return str;
                        }
                    }
                    if (typeof(window.devicePixelRatio) != 'undefined' && window.devicePixelRatio > 1) {
                        goods.goods_imgs.main_image.origin_image = dprInsert( goods.goods_imgs.main_image.origin_image, '_thumbnail_900x' )
                        $.each(goods.goods_imgs.detail_image, function(i, value) {
                            value.origin_image = dprInsert( value.origin_image, '_thumbnail_900x' )
                        });
                    } else {
                        goods.goods_imgs.main_image.origin_image = dprInsert( goods.goods_imgs.main_image.origin_image, '_thumbnail_600x' )
                        $.each(goods.goods_imgs.detail_image, function(i, value) {
                            value.origin_image = dprInsert( value.origin_image, '_thumbnail_600x' )
                        });
                    }
                },
                /*
                    {
                        status: {
                            open: true,
                            pageType: 'home'
                        },
                        data: { //必须传值
                            goodsId: goodsId,
                            originId: originId,
                            index: xxx
                        },
                        callback: callback
                    }
                */
                quickViewOpen: function(opt) {
                    var self = this;
                    var id = opt.data.goodsId;
                    if(self.status.quickBtn) return;
                    self.status.quickBtn = 1;

                    //缓存及quick view弹窗数据处理
                    if(self.quickDataCache[id]) {

                        self.styleGelleryCheck(self.quickDataCache[id], opt.data.styleGalleryData) // 红人图检测
                        self.goods = self.quickDataCache[id];
                        self.goods.addtowish = self.wishlistCheck(id)    // 加入收藏检测
                        self.nextTick();

                        if(typeof(opt.callback) == "function") {
                            opt.callback();
                        }

                        if(typeof gaEventCenter != 'undefined' && !opt.data.originId) {
                            gaEventCenter.$emit('quickViewOpen', {
                                goods: self.goods,
                                value: 1,
                                index: opt.data.index
                            })
                        }

                    } else {
                        self.quickGetData({
                            goodsId: opt.data.goodsId,
                            originId: opt.data.originId,
                            styleGalleryData: opt.data.styleGalleryData,
                            callback: function(data, code) {
                                if(code) {
                                    self.imgPreLoadCheck(); // 图片预加载
                                    self.nextTick();
                                }

                                if(typeof(opt.callback) == "function") {
                                    opt.callback(data, code);
                                }

                                if(typeof gaEventCenter != 'undefined' && !opt.data.originId) {
                                    gaEventCenter.$emit('quickViewOpen', {
                                        goods: self.goods,
                                        value: code ? 1 : 0,
                                        index: opt.data.index
                                    })
                                }

                            }
                        });
                    }
                },
                quickViewClose: function() {
                    this.openEvent.status.open = false;
                    this.status.quickOpen = false;
                },
                //DOM渲染完成后数据处理函数
                nextTick: function() {
                    var self = this;
                    this.clear();

                    if(this.goods.detail) {
                        this.detailUrl = SHEIN_W.getGoodsUrl(
                            this.goods.detail.goods_url_name,
                            this.goods.detail.goods_id,
                            this.goods.detail.cat_id
                        );

                        this.addBagForm.goods_id = this.goods.detail.goods_id;

                        if(self.pageType == 'stylegalleryhome') {
                            this.detailUrl = this.detailUrl + '?fromQv=stylegalleryhome'
                        } else if(self.pageType == 'stylegallerylist') {
                            this.detailUrl = this.detailUrl + '?fromQv=stylegallerylist'
                        }
                    }

                    this.$nextTick(function () {
                        self.swiperInit();
                        self.sizeHoverInit();
                        self.oneSizeCheck();
                        self.status.quickOpen = true;
                    });
                },
                clickThumbnail: function(index) {
                    this.status.thumb = index;
                    this.vue_Swiper.slideTo(index+2, 300, true);
                },
                clickSize: function(item) {
                    if(this.status.currenSize == item.attr_value_id) return;
                    this.addBagForm.attrs[0].attr_id = item.attr_id;
                    this.addBagForm.attrs[0].attr_value_id = item.attr_value_id;
                    this.status.currenSize = item.attr_value_id;
                    this.status.selectsize = 0;
                    if(item.stock < this.addBagForm.quantity) {
                        this.addBagForm.quantity = item.stock*1;
                    }
                    this.status.stock = item.stock;
                    // 尺码价格
                    if(item.price) {
                        this.goods.detail.salePrice = item.price.salePrice
                        this.goods.detail.retailPrice = item.price.retailPrice
                        this.goods.detail.unit_discount = item.price.unit_discount
                    }
                },
                clickDetail: function() {
                },
                clickColor: function(index) {
                    var _data = {
                        data: {
                            goodsId: this.goods.relationProducts[index].goods_id,
                            originId: this.goods.detail.goods_id
                        },
                        callback: function() {}
                    }
                    this.quickViewOpen(_data);
                },
                changeShopTheLookGoods: function(id) {
                    var self = this;
                    self.status.loading.fullLoader = 1;
                    var _data = {
                        data: {
                            goodsId: id,
                        },
                        callback: function() {
                            self.status.loading.fullLoader = 0;
                        }
                    }
                    self.quickViewOpen(_data);
                },
                // 单尺寸检测选中
                oneSizeCheck: function() {
                    var self = this;
                    this.$nextTick(function () {

                        //唯一size自动勾选
                        if(self.goods.attrSize instanceof Array && self.goods.attrSize.length) {
                            var onesize = 0;
                            self.goods.attrSize.forEach(function(item) {
                                (item.stock > 0) && onesize++;
                            });

                            if(onesize == 0) {
                                self.status.stock = 0;
                            } else if(onesize == 1) {
                                self.status.stock = self.goods.detail.stock;
                                setTimeout(function() {
                                    if(self.$refs.sizeRadio && self.$refs.sizeRadio.length) {
                                        self.$refs.sizeRadio[0].click();
                                    }
                                }, 500)
                            } else {
                                self.status.stock = self.goods.detail.stock;
                            }

                        // 无尺码
                        } else {
                            self.status.stock = self.goods.detail.stock;
                        }
                    });
                },
                // 红人图检测
                styleGelleryCheck: function(goods, opt, is_cache) {
                    if(opt && opt._oimg && opt._simg) {
                        if(is_cache) {
                            if(goods.goods_imgs.main_image.origin_image != opt._oimg) {
                                goods.goods_imgs.main_image.origin_image = opt._oimg;
                                goods.goods_imgs.main_image.thumbnail = opt._simg;
                            }
                        } else {

                            goods.goods_imgs.detail_image.push({
                                origin_image: goods.goods_imgs.main_image.origin_image,
                                thumbnail: goods.goods_imgs.main_image.thumbnail,
                                medium_image: '',
                            });
                            goods.goods_imgs.main_image.origin_image = opt._oimg;
                            goods.goods_imgs.main_image.thumbnail = opt._simg;
                            if(opt.pageType) {
                                // goods.url = goods.url + '?fromQv=' + opt.pageType;
                            }
                        }
                    }
                },
                wishlistCheck: function(id) {
                    if(this.$root.$refs['itemRef'+id]) {
                        var _itemData = this.$root.$refs['itemRef'+id];
                        if(_itemData.itemdata.frontEndData && _itemData.itemdata.frontEndData.addedWish>0)
                        return 1;
                    } else {
                        return 0;
                    }
                },
                checkSizePrice: function(goods) {
                    goods.attrSize = goods.attrSize.filter(function(size) {
                        if(size.price && size.price.salePrice && size.price.retailPrice) {
                            return true
                        }
                        return false
                    })
                },
                // 多颜色选项初始化
                colorMoreCheck: function(goods, originId) {
                    if(originId) {
                        goods.relationProducts = this.quickDataCache[originId].relationProducts;
                        // this.clear();
                    } else {
                        if(goods.detail.color_image) {
                            goods.relationProducts.unshift({
                                color_image: goods.detail.color_image,
                                goods_id: goods.detail.goods_id
                            });
                        }
                    }
                },
                //图片预加载函数
                imgPreLoadCheck: function() {
                    if(this.goods.relationProducts.length) {
                        var self = this;
                        $.each(this.goods.relationProducts, function(index, val) {
                            self.quickGetData({
                                is_cache: true,
                                originId: self.goods.detail.goods_id,
                                goodsId: val.goods_id,
                                callback: function(item) {
                                    self.quickImgCache.push(item.goods_imgs.main_image.origin_image);
                                    self.quickImgCache.push(item.goods_imgs.main_image.thumbnail);
                                    $.each(item.goods_imgs.detail_image, function(i, val) {
                                        self.quickImgCache.push(val.origin_image);
                                        self.quickImgCache.push(val.thumbnail);
                                    });
                                }
                            });
                        });
                    }
                },
                addToWish: function(e) {
                    if(this.status.loading.addWish || this.goods.addtowish) return;
                    this.handleBtnWithXhr('addWish', 1);
                    var self = this;
                    
                    if(typeof Certona != 'undefined') {
                        Certona.save(this.goods.detail.goods_id)
                    }

                    $.get('/user/wishlist/add', {goods_id: this.goods.detail.goods_id}, function(json) {
                        if(json.data) {
                            if(json.data.code == 0 || json.data.code == 400401) {
                                self.goods.addtowish = 1;
                                self.$emit('add-wish-suc', self.goods.detail.goods_id);

                                if(typeof gaEventCenter !== 'undefined') {
                                    gaEventCenter.$emit('quickVieAddWish', {
                                        sku: self.goods.detail.goods_sn,
                                        value: 1
                                    })
                                }

                                if(typeof(fastwish) != 'undefined') {
                                    fastwish.reAddwish();
                                }

                                // yeahmobi
                                if(typeof(mkq) == 'function') {
                                    mkq('track', 'AddToWishlist', {
                                        content_name: self.goods.detail.goods_name,
                                        content_ids: [self.goods.detail.goods_id],
                                        content_type: 'product',
                                        value: self.goods.detail.salePrice.amount,
                                        currency: 'GBP'
                                    });
                                }

                            } else {
                                if(typeof gaEventCenter !== 'undefined') {
                                    gaEventCenter.$emit('quickVieAddWish', {
                                        sku: self.goods.detail.goods_sn,
                                        value: 0
                                    })
                                }
                            }
                        } else {
                            if(typeof gaEventCenter !== 'undefined') {
                                gaEventCenter.$emit('quickVieAddWish', {
                                    sku: self.goods.detail.goods_sn,
                                    value: 0
                                })
                            }
                            if(json.code == -1) {
                                self.$emit('open-login-modal', 'qv-add-wish', self);
                                GBGaSend({
                                    category: '登录注册页',
                                    action: 'PopUps',
                                    label: 'Sign/Register-AddToWishlist'
                                })
                            }
                        }
                        self.handleBtnWithXhr('addWish', 0);
                    })
                },
                addToBag: function(e) {
                    var self = this;
                    if(typeof Certona != 'undefined') {
                        Certona.addToBag(self.addBagForm.goods_id)
                    }

                    if(this.status.loading.addBag) return

                    self.handleBtnWithXhr('addBag', 1);

                    self.status.loading.showadd = 0;

                    for (var i in self.status.err) {
                        self.status.err[i] = 0;
                    }

                    // 没选尺寸直接加入购物车
                    if (self.goods.attrSize.length && (!self.addBagForm.attrs[0]['attr_id'] || !self.addBagForm.attrs[0]['attr_value_id'])) {
                        self.handleBtnWithXhr('addBag', 0);
                        self.status.loading.showadd = 1;
                        self.status.selectsize = 1;

                        if(typeof gaEventCenter !== 'undefined') {
                            gaEventCenter.$emit('quickViewAddBag', {
                                form: self.addBagForm,
                                goods: self.goods,
                                value: 0
                            })
                        }
                    } else {
                        // 清仓限购
                        if(self.goods.detail.is_clearance == 1 && self.addBagForm.quantity > self.goods.detail.limit_count) {
                            self.handleBtnWithXhr('addBag', 0);
                            self.status.clearance = true
                            self.status.loading.showadd = 1;

                            if(typeof gaEventCenter !== 'undefined') {
                                gaEventCenter.$emit('quickViewAddBag', {
                                    form: self.addBagForm,
                                    goods: self.goods,
                                    value: 1
                                })
                            }

                            return false
                        }

                        var url = '/cart/add?goods_id='+self.addBagForm.goods_id+'&quantity='+self.addBagForm.quantity
                        var params = {
                            goods_id: self.addBagForm.goods_id,
                            quantity: self.addBagForm.quantity
                        }
                        if(self.goods.attrSize.length && self.addBagForm.attrs[0]['attr_value_id']) {
                            url += '&size=' + self.addBagForm.attrs[0]['attr_value_id']
                            params.attrs = [{
                                attr_id: self.addBagForm.attrs[0].attr_id,
                                attr_value_id: self.addBagForm.attrs[0].attr_value_id,
                            }]
                        }

                        $.ajax({
                            url: url,
                            type: 'POST',
                            data: params,
                            success: function(res) {
                                self.handleBtnWithXhr('addBag', 0);

                                //如果是购物车页面
                                if( self.openEvent.status.pageType == 'cartPage' ){
                                    self.$emit('add-cart-suc', res);
                                } else {
                                //快捷购物车
                                    cart_module.recart(res);
                                }

                                if(res.code === 0) {
                                    self.status.loading.showadded = 1;
                                    setTimeout(function() {
                                        self.status.loading.showadded = 0;
                                        self.status.loading.showadd = 1;
                                        $(e.target).blur();
                                    }, 3000);

                                    self.quickViewClose();

                                    // cart_module.addCart(res);


                                    if(typeof gaEventCenter !== 'undefined') {
                                        gaEventCenter.$emit('quickViewAddBag', {
                                            form: self.addBagForm,
                                            goods: self.goods,
                                            value: 1
                                        })
                                    }

                                    //fbq
                                    if(typeof(doFbq) != 'undefined') {
                                        doFbq('AddToCart', {
                                            content_name: 'Shopping Cart',
                                            content_ids: [self.addBagForm.goods_id],
                                        })
                                    }

                                    // yeahmobi
                                    if(typeof(mkq) == 'function') {
                                        mkq('track', 'AddToCart', {
                                            content_name: self.goods.detail.goods_name,
                                            content_ids: [self.goods.detail.goods_id],
                                            content_type: 'product',
                                            value: self.goods.detail.salePrice.amount,
                                            currency: 'GBP'
                                        });
                                    }

                                } else {
                                    self.status.loading.showfail = 1;
                                    setTimeout(function() {
                                        self.status.loading.showfail = 0;
                                        self.status.loading.showadd = 1;
                                    },3000);

                                    if (res.code == 200401) {
                                        self.status.err.limit = 1;
                                    } else if (res.code == 300402) {
                                        self.status.err.soldout = 1;
                                    } else if(res.code == 300407) {
                                        self.status.cartLimitTips = true
                                        setTimeout(function() {
                                            self.status.cartLimitTips = false
                                        }, 2000)
                                    }

                                    if(typeof gaEventCenter !== 'undefined') {
                                        gaEventCenter.$emit('quickViewAddBag', 0)
                                    }
                                }

                            },
                            error: function() {
                                self.handleBtnWithXhr('addBag', 0);
                                self.status.loading.showfail = 1;
                                setTimeout(function() {
                                    self.status.loading.showfail = 0;
                                    self.status.loading.showadd = 1;
                                },2000);

                                if(typeof gaEventCenter !== 'undefined') {
                                    gaEventCenter.$emit('quickViewAddBag', 0)
                                }
                            }
                        });
                    }
                },
                handleBtnWithXhr: function(stat, val) {
                    // 防止多次提交
                    this.status.loading[stat] = val;
                },
                swiperInit: function() {
                    var self = this;
                    if(this.status.swiperInit) {
                        this.vue_Swiper = new Swiper ('.j-quickv', {
                            direction: 'horizontal',
                            prevButton: '.j-quickv-prev',
                            nextButton: '.j-quickv-next',
                            initialSlide: 0,
                            spaceBetween: 0,
                            observer:true,//修改swiper自己或子元素时，自动初始化swiper
                            observeParents:true,//修改swiper的父元素时，自动初始化swiper
                            loop: true,
                            onSlideChangeEnd: function(swiper){
                                if (self.openEvent.status.pageType != 'shopTheLookList') {
                                    if(swiper.isEnd) {
                                        self.status.thumb = -1;
                                    } else if (swiper.isBeginning) {
                                        self.status.thumb = $('.j-slideto').length - 2;
                                    } else {
                                        self.status.thumb = swiper.activeIndex - 2;
                                    }
                                }
                            }
                        });

                        this.status.swiperInit = 0;
                    }

                    this.vue_Swiper.init();

                    if (this.vue_Swiper.slides.length == 3) {
                        $('.j-quickv-prev').hide();
                        $('.j-quickv-next').hide();
                        this.vue_Swiper.lockSwipeToNext();
                        this.vue_Swiper.lockSwipeToPrev();
                        this.vue_Swiper.params.grabCursor = false;
                    } else {
                        $('.j-quickv-prev').show();
                        $('.j-quickv-next').show();
                        this.vue_Swiper.unlockSwipeToNext();
                        this.vue_Swiper.unlockSwipeToPrev();
                        this.vue_Swiper.params.grabCursor = true;
                    }
                },
                sizeHoverInit: function() {
                    setTimeout(function() {
                        $('.j-opt-size').off();
                        $('.j-opt-size').hover(function() {
                            var size = $(this).attr('size');
                            $('.j-size-tag-wrap').addClass('she-visibility1');
                            $(".j-size-tag").removeClass('she-inline-block');
                            $('.j-size-tag[size="'+ size.replace('"', '') +'"]').addClass('she-inline-block');
                        }, function() {
                            $('.j-size-tag-wrap').removeClass('she-visibility1');
                            $(".j-size-tag").removeClass('she-inline-block');
                        });
                    }, 0);
                },
                transformImg: transformImg,
                template: SHEIN_W.template,

            }
        });
    }()
