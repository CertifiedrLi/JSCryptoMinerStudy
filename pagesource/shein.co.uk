      var label = $(this).attr('title')
      var href = $(this).attr('href')
      var vcRegx = /.+-vc-(\d+).+/
      var cRegx = /.+-c-(\d+).+/
      var cat_id = vcRegx.exec(href) || cRegx.exec(href)
