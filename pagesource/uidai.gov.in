jQuery('#container-expand #expand').click( function() {
	// if link was expand then show and toggle text
	var currHTML = jQuery('#container-expand #expand').html();
	if (currHTML.indexOf("Expand All") == 0) {
		jQuery('#container-expand .acc_content').slideDown();
		jQuery("div").remove(".acc_icon_expand");
		jQuery('.acc_head').addClass("faq-expand-icon-minus");
		jQuery('#container-expand #expand').html("Collapse All");
	}
	// if link was collapse then hide and toggle text
	else {
		jQuery('#container-expand .acc_content').slideUp();
		jQuery('this').addClass('acc_icon_expand');
		jQuery('.acc_head').removeClass('faq-expand-icon-minus');
		jQuery(".acc_head").prepend('<div class="acc_icon_expand"></div>'); 
		jQuery(".accordion_in").removeClass('acc_active');
		jQuery('#container-expand #expand').html("Expand All");
	}
});
