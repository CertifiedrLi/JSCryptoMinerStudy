function getVersion() {
  var width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
  if(width >= 980) {
    return 'Desktop';
  } else if(width >= 740) {
    return 'Tablet';
  } else {
    return 'Mobile';
  }
}

function isWallpaperSafe() {
  var screenWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
  if(screenWidth >= 1600) {
    return 'Yes';
  }
  else {
    return 'No';
  }
}

function renderEnded(e){
    if(!e.isEmpty) {
      switch(e.slot.getSlotElementId()) {
        case "ads_rectangle":
          document.getElementsByClassName("ads_rectangle_container")[0].style.display = 'block';
          document.getElementsByClassName("rectangle-banner-new")[0].style.display = 'block';
          document.getElementsByClassName("banner300")[0].style.display = 'block';
          document.getElementsByClassName("rectangleDesno")[0].className += ' centered';
          if(e.size[1] >= 500) {
            document.getElementsByClassName("popularnoHolder")[0].style.display = 'none';
          }
          break;
        case "ads_wallpaper":
          document.getElementsByClassName("wallpaper_container")[0].style.display = 'block'; 
          //document.getElementsByTagName("body")[0].className = "has_skin"; 
          break;
        //case "ads_takeover":
          //document.getElementsByClassName("takeover_container")[0].style.display = 'block'; 
          //break;
        case "native-mobile":
          document.getElementsByClassName("vezane-container")[0].style.display = 'block'; 
          break;
      }
    }

    if (e.slot.getSlotElementId() == "native-kartica") {
      if(e.isEmpty) {
        // nema oglas
        document.getElementsByClassName("izdv")[0].style.display = 'flex';
      } else {
        imaBanner = true;
        $(document).ready(function(){
          $("#native-kartica iframe").css('height','300px');
        });
        // ima oglas
        document.getElementsByClassName("izdv")[0].style.display = 'none';
        document.getElementsByClassName("nativek")[0].style.display = 'flex';
      }
    }

}


function readCookie(name) {
  var nameEQ = name + "=";
  var ca = document.cookie.split(';');
  for(var i=0;i < ca.length;i++) {
    var c = ca[i];
    while (c.charAt(0)==' ') c = c.substring(1,c.length);
    if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
  }
  return null;
} 
  var regija = readCookie('MojaRegija');

