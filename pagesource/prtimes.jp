                                    start_date = new Date(val.start_date);
                                    list_date = $('<span>',{class:'list_date'}).text((start_date.getMonth() + 1) + '/' + start_date.getDate());
                                    list_week = $('<span>',{class:'list_week'}).text(["日", "月", "火", "水", "木", "金", "土"][start_date.getDay()]);
                                    start_time = ('0' + start_date.getHours()).slice(-2) + ':' + ('0' + start_date.getMinutes()).slice(-2);
                                    end_date = new Date(val.end_date);
                                    end_time = ('0' + end_date.getHours()).slice(-2) + ':' + ('0' + end_date.getMinutes()).slice(-2);
                                    list_time = $('<span>',{class:'list_time'}).text(start_time + ' ~ ' + end_time);

                                    list_date_wrap.append(list_date).append(list_week).append(list_time);

                                    live_list_container = $('<div>', {class:'live_list_container'});
                                    link_facebook = $('<a class="link_facebook" href="' + val.facebook_url + '">',{class: 'link_facebook'});
                                    list_mov_title = $('<div>', {class:'list_mov_title'}).text(val.title);
                                    list_company = $('<div>', {class:'list_company'}).text(val.company_name);
                                    live_list_container.append(link_facebook).append(list_mov_title).append(list_company);
                                    _li.append(list_date_wrap).append(live_list_container);

                                    $('ul.live_plans_list').append(_li);

                                });

                            }else {
                                $('div.live_plans').css('display','none');
                                $('ul.live_plans_list').css('display','none');
                            }
                        }).fail(function () {
                        });
                    });

                }
            });
