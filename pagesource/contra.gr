					<script>
						/*** UpperCaser function for clientside transformations ***/
						var UpperCaser = {
						MAP: {
						'ά': 'α',
						'έ': 'ε',
						'ό': 'ο',
						'ώ': 'ω',
						'ύ': 'υ',
						'ϋ': 'ϋ',
						'ΰ': 'ϋ',
						'ί': 'ι',
						'ϊ': 'ϊ',
						'ΐ': 'ϊ',
						'ή': 'η',
						'Ά': 'α',
						'Έ': 'ε',
						'Ό': 'ο',
						'Ώ': 'ω',
						'Ύ': 'υ',
						'Ί': 'ι',
						'Η': 'η'
						},
						toUpper: function(text) {
						if (!text) return '';
						var i, c;
						for (i = 0; i < text.length; i++) {
						c = text[i];
						if (this.MAP[c]) {
						text = this.replaceAt(text, i, this.MAP[c]);
						}
						}
						return text.toUpperCase();
						},
						replaceAt: function(text, index, char) {
						return text.substr(0, index) + char + text.substr(index + char.length);
						}
						};
						var capitalLink = $('.stories-breaking-news h2 a').text();
						var capitalText = UpperCaser.toUpper(capitalLink);
						$('.stories-breaking-news h2 a').text(capitalText);
						$('.article-content .byline_date').ready(function() {
						var capitalBylineLink = $('.article-content .byline_date').text();
						var capitalText = UpperCaser.toUpper(capitalBylineLink);
						var res = capitalText.split("|");
						$('.article-content .byline_date').html(res[0] + (res[1]?("<span class='time'>" + res[1] + "</span>"):""));
						});
						/*** UpperCaser function for clientside transformations ***/
					</script>
