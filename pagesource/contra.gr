		<style>
			@font-face {
			font-family: 'Arial Black';
			src: url('http://www.contra.gr/incoming/article4507156.ece/BINARY/original/ariblkarialblk.eot');
			src: url('http://www.contra.gr/incoming/article4507156.ece/BINARY/original/ariblkarialblk.eot?#iefix') format('embedded-opentype'), url('http://www.contra.gr/incoming/article4507159.ece/BINARY/original/ariblkarialblk.woff2') format('woff2'), url('http://www.contra.gr/incoming/article4507158.ece/BINARY/original/ariblkarialblk.woff') format('woff'), url('http://www.contra.gr/incoming/article4507157.ece/BINARY/original/ariblkarialblk.ttf') format('truetype');
			font-weight: normal;
			font-style: normal;
			}
			.ece_frontpage #main_content_news_bg_wrapper {
			background: #043793;
			padding-top: 50px;
			}
			.ece_frontpage #main_content_wrap {
			margin-bottom: 0;
			}
			.header-block {
			font-family: 'Arial Black', sans-serif;
			background: none;
			color: #357dd4;
			text-align: center;
			padding: 0;
			margin: 36px 0 0;
			font-size: 26px;
			font-style: italic;
			letter-spacing: -.75px;
			}
			.header-block span {
			margin: 0 auto;
			display: inline-block;
			background-color: #122456;
			font-style: italic;
			letter-spacing: -.75px;
			padding: 0 10px;
			}
			.latest_news_panes {
			padding: 0;
			}
			.latest_news_pane {
			border-bottom: none;
			max-height: 240px;
			overflow: hidden;
			overflow-y: scroll;
			}
			.latest_news_wrap {
			border: 1px solid #2f70bf;
			padding: 23px 5px 15px 9px;
			margin-top: -5px;
			}
			.latest_news_wrap .paging_dot {
			display: none;
			}
			.latest_news_info {
			width: 225px;
			}
			.jspTrack {
			background: #0e1c46;
			}
			.jspDrag {
			background: #2f70bf;
			}
			.jspVerticalBar {
			width: 5px;
			background: none;
			}
			.latest_news_pane > div {
			padding: 0;
			}
			.jspPane > div {
			height: auto;
			overflow: hidden;
			font-weight: bold;
			font-size: 12px;
			padding: 0 0 9px 0px;
			}
			.jspPane > div:first-child {
			padding-top: 7px;
			}
			#hp-readmore-cross {
			margin: 50px 0;
			top: 0;
			}
			#page .section-header-common {
			margin: 0;
			background: none;
			left: initial;
			top: initial;
			color: #ff9900;
			text-indent: initial;
			font-size: 28px;
			font-family: 'Arial Black';
			font-style: italic;
			letter-spacing: -.25px;
			position: relative;
			line-height: 22px;
			max-height: 40px;
			overflow: hidden;
			}
			#page .section-header-common.trends {
			left: -20px;
			}
			#page .section-header-common span {
			background: #043793;
			padding-right: 21px;
			display: inline-block;
			padding-bottom: 18px;
			padding-left: 20px;
			}
			#page .section-header-common span:after {
			width: 0;
			height: 0;
			border-style: solid;
			border-width: 40px 23px 0 0;
			color: #003596;
			border-color: #003596 transparent transparent transparent;
			content: "";
			display: inline-block;
			position: absolute;
			margin-left: 21px;
			-webkit-transform: rotate(360deg);
			}
			#hp-readmore-cross,
			#hp-readmore-cross .stories:nth-child(8) {
			background: #102257!important;
			}
			#hp-readmore-cross .stories:nth-child(8) {
			margin: 15px 0 17px 20px;
			}
			#hp-readmore-cross.hp-readmore-cross-new .stories {
			padding-bottom: 0;
			margin: 15px 0 6px 20px;
			}
			/* editors widget */
			.editorcont .catref {
			top: 9px;
			right: 20px;
			color: #7bb0ea;
			}
			.editorcont .ediname {
			color: #ff9900;
			position: initial;
			display: inline-block;
			left: initial;
			font-family: 'Arial';
			font-weight: bold;
			font-style: normal;
			margin: 7px 0 0 13px;
			letter-spacing: .6px;
			}
			.editorcont .ediname:hover,
			.editorcont .catref:hover {
			text-decoration: underline!important;
			}
			.editorcont .ediatricle {
			background: none;
			width: 298px;
			height: 91px;
			margin: 0;
			padding: 0 10px 16px;
