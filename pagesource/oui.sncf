<script>
(function() {
    var vscTabs = document.querySelector('.vsc__tabs');
    var tabInput = Array.prototype.slice.call(vscTabs.querySelectorAll('.vsc__tabs-state'));
    var tabPanel = Array.prototype.slice.call(vscTabs.querySelectorAll('.vsc__tabs-panel'));
    var tabPanels = vscTabs.querySelector('.vsc__tabs-panels');
    var tabActive = null;

    tabInput.forEach(function(element) {
        element.addEventListener('click', togglePanel);
    });        

    function togglePanel(event) {
        if (tabActive == event.target.id) {
            tabInput.forEach(function(element) {
                element.checked = false;
                CSS.removeClass(tabPanels, 'vsc__tabs-panels--expand');
            });
            tabActive = null;
            return;
        }

        tabPanel.forEach(function(element) {
            element.style.display = 'none';
        });

        tabActive = event.target.id;
        CSS.addClass(tabPanels,'vsc__tabs-panels--expand');
        vscTabs.querySelector("[data-panel=" + event.target.id +"]").style.display = 'block';
    }    
}());    
</script>        </div>
          
              
              
