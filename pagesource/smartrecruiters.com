$( document ).ready(function() {
	/* Attract - Select - Hire Diagram */
	$( '.slice-contents' ).mouseover(function() {
		var slice = $(this).parent().attr('data-slice');
		var group = $(this).parent().attr('data-group');
		$( '.dgm--visible' ).removeClass('dgm--visible') ;
		$( '.caption--visible' ).removeClass('caption--visible') ;
		$( '.slice-label' ).removeClass('slice-label--active') ;
		$( '.groupcopy .caption' ).removeClass('caption--visible') ;
			
		$( '.slicecopy .caption' ).css('display', 'none') ;
		$( '.groupcopy .caption').css('display', 'none') ;
		
		$( '.slice--grp'+group ).addClass('dgm--visible') ;
		$( '.slicecopy--grp'+group ).addClass('dgm--visible') ;
		
		$( '#slicecopy-'+slice+' .slice-label' ).addClass('slice-label--active') ;
		$( '#slicecopy-'+slice+' .caption' ).css('display', 'block') ;
		
		$( '.groupcopy .caption').not('#groupcopy-'+group+' > .caption').css('display', 'block') ;
		
		
		setTimeout( function() {
            $( '#slicecopy-'+slice+' .caption' ).addClass('caption--visible') ;
			$( '.groupcopy .caption').not('#groupcopy-'+group+' > .caption').addClass('caption--visible') ;
       }, 100);
	});
	
	
	var diagramTrigger = 'N';
	
	function showDiagram(func) {
		$( '.chartOuter' ).each( function () {
			if ( func( this ) === true && diagramTrigger == 'N' ) {	
				diagramTrigger = 'Y';
				setTimeout( function() {
					$( '#slice7 .slice-contents' ).trigger('mouseover');
			   }, 1000);
				
			}	
		} );
	}
	
	function isScrolledIntoView( elem ) {
			var docViewTop = $( window ).scrollTop();
			var docViewBottom = docViewTop + $( window ).height() - 100;
			var elemTop = $( elem ).offset().top;
			var elemBottom = elemTop + $( elem ).height();
			return ( ( elemTop <= docViewBottom ) && ( elemTop >= docViewTop ) );
		}

	$( window ).scroll(function () {
		showDiagram(isScrolledIntoView);
	} );
	
});	
</script>

