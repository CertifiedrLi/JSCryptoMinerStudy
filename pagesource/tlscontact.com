<script type='text/javascript'>
$$('.head.canvas .top.block .slide_desc.line').first().update($('slide_desc').innerHTML);

var home_slide_int;
var interval = 6000;
var home_slides = [];
//home_slides.push('https://tlscontact.com/static/default/en/images/home_slide_1.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_1.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_2.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_3.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_4.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_5.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_6.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_7.jpg');
home_slides.push('https://tlscontact.com//static/upload/home_slide_8.jpg');
var cur_slide = 1;
function switch_slide(slide_num) {
  slide_num = slide_num || cur_slide;
  var img_idx = slide_num - 1;
  $$('.head.canvas .top.block').first().style.backgroundImage = 'url('+home_slides[img_idx]+')';
  $$('.head.canvas .top.block .slide_desc.line div.ctrl li').each(function (li) {
    if (li.getAttribute('name') == slide_num) {
      li.addClassName('active');
    } else {
      li.removeClassName('active');
    }
  });
  $$('.head.canvas .top.block .slide_desc.line div.desc .content').each(function (dv) {
    if (dv.getAttribute('name') == slide_num) {
      dv.show();
    } else {
      dv.hide();
    }
  });
}

function force_switch_slide(slide_num) {
  cur_slide = slide_num;
  switch_slide(slide_num);
  cur_slide++;
  clearInterval(home_slide_int);
  home_slide_int = setInterval(auto_slide, interval);
}

function auto_slide() {
  if (cur_slide > home_slides.size()) {
    cur_slide = 1;
  }
  switch_slide(cur_slide);
  cur_slide++;
}
//auto_slide();
//home_slide_int = setInterval(auto_slide, interval);
$$('.head.canvas .top.block .slide_desc.line').first().show();
document.observe('dom:loaded', function() {
  auto_slide();
  home_slide_int = setInterval(auto_slide, interval);
});
</script>

