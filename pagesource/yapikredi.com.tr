<script type="text/javascript">
    (function (window, document, $) {
        if (typeof (window._bp) == "undefined") {
            window._bp = {};
        }
        
        function BehaviorPadYapiKrediExtend() {
            var self = this;

            this.toInternetFriendlyName = function (str) {
                str = str.toLowerCase();
                str = str.replace(/ /g, '');
                str = str.replace(/ğ/g, 'g');
                str = str.replace(/ü/g, 'u');
                str = str.replace(/ş/g, 's');
                str = str.replace(/ı/g, 'i');
                str = str.replace(/ö/g, 'o');
                str = str.replace(/ç/g, 'c');
                str = str.replace(/[^a-zA-Z 0-9]+/g, '');
                return str;
            }

            this.crateCookie = function (cookieName, cookieValue) {
                try {
                    var bpCookieDate = null;
                    var bpCookieExpires = null;
                    var bpCookieExpires = null;

                    bpCookieDate = new Date()
                    bpCookieDate.setTime(bpCookieDate.getTime() + (30 * 24 * 60 * 60 * 1000));
                    bpCookieExpires = "; expires=" + bpCookieDate.toGMTString();

                    if (!cookieValue) {
                        cookieValue = bpCookieDate.getTime();
                    }

                    document.cookie = cookieName + "=" + cookieValue + bpCookieExpires + "; path=/";
                } catch (e) {
                    if (window.console)
                        window.console.log(e);
                }
            }


            this.writeUSSegmentation = function () {
                try {
                    var container = $('#viewWriteToUsForm');
                    var cookieName = 'bp_';
                    var cookieValue = '';

                    if (container.find('#ddlSubject option:selected').parents('optgroup').attr('label') != undefined) {
                        cookieName += self.toInternetFriendlyName(container.find('#ddlSubject option:selected').parents('optgroup').attr('label')) + '_';
                    }

                    cookieName += self.toInternetFriendlyName(container.find('#ddlSubject option:selected').text());

                    this.crateCookie(cookieName, cookieValue);
                } catch (e) {
                    if (window.console)
                        window.console.log(e);
                }
            }

            this.sendGoogleAnalyticsPush = function (bannerContainerIdentifier, action) {
                try {
                    var selectedBanner = $('.slides li[data-identifier=' + bannerContainerIdentifier + ']');

                    if (selectedBanner != null) {
                        var gaCategory = selectedBanner.find(".motto").text();
                        var gaAction = action;
                        var gaOptLabel = selectedBanner.attr('data-bpbanner') == 'true' ? selectedBanner.attr('data-bprule') : 'Default';

                        ga('send', 'event', gaCategory, gaAction, gaOptLabel);
                    }
                } catch (e) {
                    if (window.console)
                        window.console.log(e);
                }
            };

            this.searchSegment = function (keyword) {
                this.crateCookie('bpsearch', keyword);
            };

            $(document).ready(function () {
                try {
                    // "Bankacılık Hizmetleri" navigation click segmentation
                    $('.topRightNav .blueList .bireysel a').bind('click', function () { self.crateCookie('bp_mp_bankhiz_bireysel'); });
                    $('.topRightNav .blueList .kobi a').bind('click', function () { self.crateCookie('bp_mp_bankhiz_kobi'); });
                    $('.topRightNav .blueList .ticari a').bind('click', function () { self.crateCookie('bp_mp_bankhiz_ticari'); });
                    $('.topRightNav .blueList .kurumsal a').bind('click', function () { self.crateCookie('bp_mp_bankhiz_kurumsal'); });
                    $('.topRightNav .blueList .ozel-bankacilik a').bind('click', function () { self.crateCookie('bp_mp_bankhiz_ozel'); });

                    // "İnternet Şubesi" navigation click segmentation
                    $('.topRightNav .redList .bireysel a').bind('click', function () { self.crateCookie('bp_mp_is_bireysel'); });
                    $('.topRightNav .redList .kurumsal a').bind('click', function () { self.crateCookie('bp_mp_is_kurumsal'); });
                    $('.topRightNav .redList .worldonline a').bind('click', function () { self.crateCookie('bp_mp_is_kredikarti'); });
                    $('.topRightNav .redList .uyelik-sifre a').bind('click', function () { self.crateCookie('bp_mp_is_uyelik'); });

                    // "Sizin İçin Buradayız" navigation click segmentation
                    $('.sib-detay .online-yardim a').bind('click', function () { self.crateCookie('bp_mp_siziicin_onlineyardim'); });
                    $('.sib-detay .sikayet-hatti a').bind('click', function () { self.crateCookie('bp_mp_siziicin_sikayet'); });
                    $('.sib-detay .en-yakin-yk a').bind('click', function () { self.crateCookie('bp_mp_siziicin_enyakin'); });

                    // Footer nav
                    $('#footer .kredi-hesaplama').bind('mouseover', function () { self.crateCookie('bp_yatirimcikosesi'); });
                    $('#footer .yapi-kredi-hakkinda').bind('mouseover', function () { self.crateCookie('bp_yapikredihakkinda'); });
                    $('#footer .piyasa-ozeti').bind('mouseover', function () { self.crateCookie('bp_kredihesaplama'); });
                } catch (e) {
                    if (window.console)
                        window.console.log(e);
                }
            });

        }

        if (window._bp) {
            window._bp.YapiKrediExtend = new BehaviorPadYapiKrediExtend();
            if (typeof (window._bp.syncx) != "undefined") {
                window._bp.syncx('http://www.worldcard.com.tr').done(function (syncedCookies) {
                    // console.log('Total', syncedCookies.length, 'cookie synced.');
                }).fail(function (reason) {
                    // console.log('Failed to sync xsite cookies. Reason:', reason);
                });
            }
        }

    })(window, document, window.bQuery || window.jQuery);
</script>

        


        
        

