                                nonInteraction: true
                            });

                            //onceki
                            ga('send', 'event', 'BN-MAINPAGE', '{Default} - {' + bannerIdentifier + '}', 'Show');
                        };
                    });
                    // Behavior Banner
                    $("#slides li[data-bpbanner=true]").not(".showed").each(function () {
                        if ($(this).hasClass("flex-active-slide") == true) {
