                        var bannerIdentifier = $(this).attr("data-identifier");
                        var bannerBprule = $(this).attr("data-bprule");

                        // Click send
                        $(this).find(".link-overlay").on("click", function () {
                            //ga('send', 'event', "BP_" + bannerTitle, 'BP_Click', 'BehaviorPad');
