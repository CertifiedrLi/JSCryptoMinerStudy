                            //    {
                            //        divElements[removeIndex].parentNode.parentNode.parentNode.remove();
                            //    }
                            //}

                            var slideContainer = document.getElementById('slides');
                            var banners = slideContainer.getElementsByTagName('li');

                            if (banners.length > maxBannerCount) {
                                var dbBannerCount = 0;
                                var bpBannerCount = 0;
                                for (var i = 0; i < banners.length; i++)
                                    if (banners[i].hasAttribute('data-bpbanner') && banners[i].getAttribute('data-bpbanner') == 'false')
                                        dbBannerCount++;
                                    else
                                        bpBannerCount++;

                                if (bpBannerCount == 0 || dbBannerCount == 0)
                                    while (banners.length > maxBannerCount)
                                        slideContainer.removeChild(banners[banners.length - 1]);

                                else if (dbBannerCount > maxDBBannerCount && bpBannerCount > maxBPBannerCount) {
                                    dbBannerCount = 0;
                                    bpBannerCount = 0;
                                    var i = 0;
                                    while (banners.length > maxBannerCount) {
                                        if (banners[i].hasAttribute('data-bpbanner') && banners[i].getAttribute('data-bpbanner') == 'false') {
                                            dbBannerCount++;
                                            if (dbBannerCount > maxDBBannerCount) {
                                                slideContainer.removeChild(banners[i]);
                                                i--;
                                            }
                                        }
                                        else {
                                            bpBannerCount++;
                                            if (bpBannerCount > maxBPBannerCount) {
                                                slideContainer.removeChild(banners[i]);
                                                i--;
                                            }
                                        }
                                        i++;
                                    }
                                }
                                else if (dbBannerCount > maxDBBannerCount) {
                                    maxDBBannerCount = maxBannerCount - bpBannerCount;
                                    dbBannerCount = 0;
                                    var i = 0;
                                    while (banners.length > maxBannerCount) {
                                        if (banners[i].hasAttribute('data-bpbanner') && banners[i].getAttribute('data-bpbanner') == 'false') {
                                            dbBannerCount++;
                                            if (dbBannerCount > maxDBBannerCount) {
                                                slideContainer.removeChild(banners[i]);
                                                i--;
                                            }
                                        }
                                        i++;
                                    }
                                }
                                else {
                                    maxBPBannerCount = maxBannerCount - dbBannerCount;
                                    bpBannerCount = 0;
                                    var i = 0;
                                    while (banners.length > maxBannerCount) {
                                        if (!(banners[i].hasAttribute('data-bpbanner') && banners[i].getAttribute('data-bpbanner') == 'false')) {
                                            bpBannerCount++;
                                            if (bpBannerCount > maxBPBannerCount) {
                                                slideContainer.removeChild(banners[i]);
                                                i--;
                                            }
                                        }
                                        i++;
                                    }
                                }
                            }
                        }

                        try {
                            ReArrangeBanners();
                        } catch (e) { }
