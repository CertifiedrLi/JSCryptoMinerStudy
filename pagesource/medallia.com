	    var target = $(this).data('target');
	    $('#'+target).velocity('scroll',{duration:500, easing: 'easeInOutQuad',offset: '-100px'});
    })
    .on('click','.diag-pag',function(){
	    $('.active').removeClass('active');
		$('.act-page-dot').removeClass('act-page-dot');
		var targetId = $(this).attr('id').split('-')[2];
		$(this).addClass('act-page-dot');
		$('#diag-slide-'+targetId).addClass('active');
		clearInterval(diagInterval);
    });
});
</script>
