<script>
    $(document).ready(function() {
        // public domain
        var $grid = $('.public-domain .grid-view').isotope({
            
            itemSelector: '.public-domain .grid-view > li',
            percentPosition: true
        });
        $grid.imagesLoaded().progress( function() {
            $grid.isotope('layout');
        });
        
        $('.public-domain-button').click(function() {
            $('.public-domain-results .show-more img').each(function(i, img){
                $(img).attr('src', $(img).attr('data-src'));
            });
            $grid.imagesLoaded().progress( function() {
                $('.public-domain-results .show-more').removeClass('show-more');
                $('.public-domain-button').hide();
                $grid.isotope('layout');
            });
        });
        // programs section
        function programsAdditionalActive() {
            $('.programs-items-additional-wrapper').removeClass('programs-items-additional-active').attr('aria-hidden', 'true');
            $('.more-programs').attr('aria-expanded', 'false');
            $('.more-programs i').switchClass('icon-angle-up', 'icon-angle-down', 50);
        };
        function programsAdditionalInactive() {
            $('.programs-items-additional-wrapper').addClass('programs-items-additional-active').attr('aria-hidden', 'false');
            $('.more-programs').attr('aria-expanded', 'true');
            $('.more-programs i').switchClass('icon-angle-down', 'icon-angle-up', 50);
            $('#more-programs-header').focus();
        };
        $('.more-programs').click(function() {
            if ($('.programs-items-additional-wrapper').hasClass('programs-items-additional-active')) {
                programsAdditionalActive();
                if (window['loc_ux_tracking'] != undefined){
                    loc_ux_tracking.trackUserInteractionEvent($(this), 'Programs and Services', 'Collapse', '');
                }
            } else {
                programsAdditionalInactive();
                if (window['loc_ux_tracking'] != undefined){
                    loc_ux_tracking.trackUserInteractionEvent($(this), 'Programs and Services', 'Expand', '');
                }
            }
        });
        // UX Interactions - Programs, Top Searches, Trending, Your Library, Exhibitions, Events, News, Public Domain
        if (window['loc_ux_tracking'] != undefined){
            $('.programs-items li a').click(function() {
                var label = $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Programs and Services', 'Click', label);
            });
            $('.programs-items-additional li a').click(function() {
                var label = $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'More Programs', 'Click', label);
            });
            $('.trend-top-searches ul li a').click(function() {
                var label = $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Top Searches', 'Click', label);
            });
            $('.trend-item a').click(function() {
                var listItem = $(this).closest('li');
                var index = $(listItem).parent().children().index(listItem) + 1;
                var label = '[' + index + ']:' + $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Trending', 'Click', label);
            });
            $('.your-library-links li a').click(function() {
                var label = $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Your Library', 'Click', label);
            });
            $('.your-library-exhibition.exhibitions ul li a').click(function() {
                var label = $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Exhibitions', 'Click', label);
            });
            $('.your-library-exhibition.events ul li a').click(function() {
                var label = $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Events', 'Click', label);
            });
            $('.your-library-exhibition.news ul li a').click(function() {
                var label = $(this).text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'News', 'Click', label);
            });
            $('.public-domain-results.search-results li h2 a').click(function(event) {
                var listItem = $(this).closest('li');
                var index = $(listItem).parent().children().index(listItem) + 1;
                var label = '[' + index + ']:' + $(this).closest('.public-domain-results li').contents().text().trim();
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Public Domain', 'Click', label);
            });

        }
        $('.alert-button').click(function() {
            $('.alert-wrapper').hide();
            if (window['loc_ux_tracking'] != undefined){
                loc_ux_tracking.trackUserInteractionEvent($(this), 'Alert Notice', 'Close', '');
            }
        });
        
        
        // old treding section
        $(window).on("load resize", function() {
            var trendItemImageWidth = $('.trend-item-image img').outerWidth();
            var trendItemImageHeight = $('.trend-item-image img').outerHeight();
            var trendItemTextHeight = $('.trend-item-text').outerHeight();
            var trendItemHeight = trendItemImageHeight + trendItemTextHeight;
            $('.trend-item').css({"height": trendItemHeight});
            $('.trend-item-text-featured').css({"width": trendItemImageWidth});
            //$('.trend-item-text').css({"height": trendItemTextHeight});
        });
        
        
        // featured
        $('.featured-slider').on('init', function(ev, el){
            $('video').each(function () {
                this.play();
            });
        });
        $('.featured-slider').slick({
            dots: true,
            dotsClass: 'featured-slider-dots',
            infinite: true,
            responsive: [
                {
                    breakpoint: 768,
                    settings: {
                        adaptiveHeight: true,
                        dots: false
                    }
                }
            ]
        });
        
        // UX Interaction Tracking - Carousel
        if (window['loc_ux_tracking'] != undefined){

            var locslider = $('.featured-slider').data('royalSlider');

            locslider.ev.on('rsBeforeMove', function(event, type, userAction) {
                // track only if slide change triggered by the user
                if (userAction) {
                    //alert(event.target.currSlideId);
                    if (type == 'prev' || type == 'next') {
                        loc_ux_tracking.trackUserInteractionEvent($(this), 'Homepage Carousel', 'Rotate', type);
                    } else {
                        var thumbnailNbr = type + 1;
                        var thumbnailTitle;

                        var index = 0;
                        $('.title-thumbnail').each(function() {
                            if (index == type) {
                                thumbnailTitle = $(this).text().trim();
                                return false;
                            }
                            index++;
                        });

                        loc_ux_tracking.trackUserInteractionEvent($(this), 'Homepage Carousel', 'Select', 'Thumbnail[' + thumbnailNbr + ']:' + thumbnailTitle);
                    }
                }

            });
        }
    });
    function trackSlideBtnClick(event, slideTitle) {
        var locslider = $('.featured-slider').data('royalSlider');
        var slideNbr = locslider.currSlideId + 1;
        if (window['loc_ux_tracking'] != undefined){
            loc_ux_tracking.trackUserInteractionEvent($(this), 'Homepage Carousel', 'Clickthrough', '[' + slideNbr + ']:' +slideTitle);
        }
        // prevent subsequent royal slider events from happening, e.g. rsBeforeMove and rsSlideClick
        // so that metrics for moving the slider aren't mistakenly recorded when a slide button is clicked
        // and simply allow the page to redirect per the rest of the button onclick event
        event.stopImmediatePropagation();
    };
</script>


