<script type="text/javascript">
$(function() {
    var footerFloat = function(){
        $(".item_footer_sublink a").hover(function(){
            var thisa = $(this).next(),
                win_w = $(window).width(),
                cl_r = this.getBoundingClientRect().right,
                cl_t = $(this).position().top,
                pop_w = thisa.outerWidth(),
                pop_h = thisa.outerHeight(),
                this_h = $(this).outerHeight();
                this_w = $(this).outerWidth();
                //alert(cl_r);
                //alert(win_w);
                //alert(wi);
                
            if ( pop_w <= win_w - cl_r ){
                thisa.show().css({"top":cl_t-pop_h/2+15,"left":cl_r+20});
                $(".ico_arw_footer_pop_1,.ico_arw_footer_pop_2").removeAttr("style");
                $(".ico_arw_footer_pop_1").css({"border-color":"transparent currentcolor transparent transparent","left":"-16px","border-style":"dashed solid dashed dashed"});
                $(".ico_arw_footer_pop_2").css({"border-color":"transparent #fff transparent transparent","left":"-15px","border-style":"dashed solid dashed dashed"});
            } else {	
                thisa.show().css({"top":cl_t-pop_h/2+15,"left":cl_r-pop_w-this_w-20});
                $(".ico_arw_footer_pop_1,.ico_arw_footer_pop_2").removeAttr("style");
                $(".ico_arw_footer_pop_1").css({"border-color":"transparent transparent transparent currentcolor","left":pop_w-2,"border-style":"dashed dashed dashed solid"});
                $(".ico_arw_footer_pop_2").css({"border-color":"transparent transparent transparent #fff","left":pop_w-3,"border-style":"dashed dashed dashed solid"});
            };
            
        },function(){
            $(".footer_pop").hide()
        });
    };
    window.onresize = function(){
       footerFloat();
    };
    footerFloat();
    
        
});
</script>
