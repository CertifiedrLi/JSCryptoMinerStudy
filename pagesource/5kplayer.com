			$("#r_banner").show();	
	}else{
		$("#r_banner").hide();
	}
	$(window).scroll(function(){
		var Top=$(window).scrollTop();
		if(Top>600){
			$("#r_banner").show();	
		}else{
			$("#r_banner").hide();
		}
	});
	$("#r_banner .close").bind("click",function(){
		$("#r_banner").hide();
	});
