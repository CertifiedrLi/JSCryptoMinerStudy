		return false;
	});

	$("#slider .close").click(function() {
		$("#popupmask").remove();
		$("#slider").fadeOut();
	});

	$(document).on("click", "#popupmask", function() {
		$("#popupmask").remove();
		$("#slider").fadeOut();
	});
});
</script>

