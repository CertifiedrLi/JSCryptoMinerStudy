    function se_splinks_check_visiblity_se_splinks_link_box(){ 
      var device_type = 'non_mobile';
      if(jQuery('body').hasClass('xs')){
        device_type = 'mobile';
      }
      var location = 'left'; 
      if(jQuery('#se_splinks_link_box').parents('#middle_horizontal_mobile').length > 0){
        location = 'middle';
      }
      if(location == 'left' && device_type == 'non_mobile'){
        return true;
      }
      if(location == 'middle' && device_type == 'mobile'){
        return true;
      }
      return false;
    }
    jQuery(function(){
      if(!se_splinks_check_visiblity_se_splinks_link_box()){
        return false;
      }
      jQuery.ajax({
        url: Drupal.settings.basePath + 'se_splinks_get_links_ajax',
        method: 'GET',
        dataType: 'JSON',
        success: function (response){
          var campaigns = response.content;
          var shuffledCampaigns = se_splinks_shuffle_array(campaigns);
          var campaigns_html = '';
          jQuery.each(shuffledCampaigns, function(index, campaign_html){
            campaigns_html = campaigns_html + campaign_html;
          });
          jQuery('#se_splinks_link_box').html(campaigns_html);
          se_splinks_display_news('se_splinks_link_box', response.counts);        
        }
      });
    });
