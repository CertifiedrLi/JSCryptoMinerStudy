<script type="text/javascript">
(function($){
	if(!$) return;

    var timestamp = $('html').data('timestamp');
    var param = {c:2, d:2, l:2};
    if (timestamp) {
        param.t = timestamp;
    }
    $.ajax({
        url: '/api/information/',
        type: 'GET',
        data: param,
        dataType: 'json',
        success: function(data) {
            $.each(data, function(i, topic) {
                var html = $([
                    '<li>',
                    '<a href="' + topic['URL'] + '">',
                    '<p class="date">',
                    '<span>',
                    topic['PUBLISH_AT'].replace(/(\d{4})-(\d{2})-(\d{2}) .*/, '$2月$3日'),
                    '</span>',
                    '</p>',
                    '<p class="title">',
                    '</p>',
                    '</a>',
                    '</li>'
                ].join(""));
				if (topic['TARGET_BLANK'] == '1') {
					$('a', html).attr('target', '_blank');
				}
                $('.title', html).text(topic['TITLE']);
                $('a', html).click(function(){
                    $.ajax({
                        type: 'POST',
                        url: '/information/click/',
                        dataType:'json',
                        data: {id: topic['ID'], ck:topic['CK']},
                        async: false
                    });
                    gaTrackEvent(this, 'ecnavi_top', 'important_info', topic['TITLE']);
                    return true;
                });

                $('ul', '#important_info').append(html);
            });
        }
    });
})(window['jQuery']);
</script>

