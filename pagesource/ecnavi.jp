<script type="text/javascript">
(function($){
	if(!$) return;

	function funcAnimeBlink() {
		$(this).delay(3000).fadeOut(500).fadeIn(500, funcAnimeBlink);
	};

	var timestamp = $('html').data('timestamp');
	var param = {c:1, d:2, l:4};
	if (timestamp) {
		param.t = timestamp;
	}
	$.ajax({
		url: '/api/information/',
		type: 'GET',
		data: param,
		dataType: 'json',
		success: function(data) {
			$.each(data, function(i, topic) {
				var part =  $('#topic_tmpl').children().clone();
				$(part).attr('href', topic['URL']);

				if (topic['TARGET_BLANK'] == '1') {
					$(part).attr('target', '_blank');
				}
				$('h3', part).text(topic['TITLE']);
				$('.update', part).text(topic['PUBLISH_AT'].replace(/(\d{4})-(\d{2})-(\d{2}) .*/, '$1年$2月$3日 掲載'));
				if (topic['IS_UPDATED_TODAY'] == "1") {
					$('.left', part).append('<p class="today">本日更新</p>');
				}
				if (topic['BODY']) {
					$('.lead', part).text(topic['BODY']);
				}
				if (topic['POINT']) {
					$('.point', part).html('<span class="gn_icon_point gn_size_20"></span>' + topic["POINT"]);
				}
				if (topic['IMAGE_PATH']) {
					$('.image', part).append('<img src="' + topic['IMAGE_PATH'] + '" onload="imageResize({image: this, width: 120, visible: true, expandRatio: 2});" onerror="photoNotFound({image: this});" />');
				}
				part.click(function(){
					$.ajax({
						type: 'POST',
						url: '/information/click/',
						dataType:'json',
						data: {id: topic['ID'], ck:topic['CK']},
						async: false
					});
                    gaTrackEvent(this, 'ecnavi_top', 'navic_info', topic['TITLE']);
					return true;
				});
				$(part).find('p.today').delay(3000).fadeOut(500).fadeIn(500, funcAnimeBlink);
				$('<li>').append(part).appendTo('.news_area ul.news');
			});
		}
	});

})(window['jQuery']);
</script>
