<script type="text/javascript">
var cookieKey = 'servicePointStatus';
$(function () {
    $.getJSON('/pointboard/count/', function (json) {
        if (!json.affCount) return;

        var servicePointList = $("ul#service_point");
        var existCategory = new Array();
        if ($.cookie(cookieKey)) {
            splitCategory = $.cookie(cookieKey).split(',');

            for (var i = 0; i < splitCategory.length; i++) {
                existCategory[splitCategory[i]] = true;
            }
        }

        for (var key in json.affCount) {
            var count = json.affCount[key];
            if (count == 0) continue;
            if (existCategory[key]) continue;

            dom = servicePointList.find('#service_point_' + key);
            dom.find('a').append('<span class="count">' + count + '</span>');
            dom.addClass('new');
        }

    });
});

function setCookie(category) {
    var cookieValue = $.cookie(cookieKey);

    if (!cookieValue) {
        cookieValue = category;
    } else {
        var value = cookieValue.split(',');
        for (var i = 0; i < value.length; i++) {
            if (value[i] == category) return;
        }

        cookieValue += ',' + category;
    }

    var t = new Date();
    t.setHours(t.getHours() + 12);
    document.cookie = cookieKey + '=' + cookieValue + ';' + 'expires=' + t.toUTCString();
}
</script>

