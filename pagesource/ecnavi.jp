<script type="text/javascript">
(function($){
	if (!$) {
		return;
	}
	$.ajax({
		url: '/global/header/api/notification/',
		success: function(notifications) {
			for (var i = 0; i < notifications.length; i++) {
				var target = '#save_navi ul.main.save li:eq(' + i + ') a';
				if (!notifications[i]) {
					continue;
				}
				if (notifications[i].new_release) {
					var funcAnimeBlink = function(){
						$(this).delay(1000).fadeOut(500).fadeIn(500, funcAnimeBlink);
					};
					var html = '<span class="new">NEW</span>';
					$(html).appendTo(target).hide().delay(1000).fadeIn(1000, funcAnimeBlink);
				}
				if (notifications[i].count){
					var html = '<span class="count">' + notifications[i].count + '</span>';
					$(html).appendTo(target).hide().delay(1000).fadeIn(1000);
				}
			}
		}
	});
})(window['jQuery']);

(function($){
	if (!$) {
		return;
	}
	$('#save_navi ul.main.save li a').each(
		function(idx, dom) {
			$(dom).click(function(){
					var clickedTime = [];
					var cdata = $.grep(
						document.cookie.split(';'),
						function(e,i){return /^header_menu_clicked=/.test($.trim(e));}
					)[0];
					if (cdata) {
						clickedTime = cdata.split('=')[1].split(',');
					}
					clickedTime[idx] = Math.floor(new Date()/1000);
					var domain = location.hostname.substr(location.hostname.indexOf('ecnavi.jp'));
					document.cookie = 'header_menu_clicked=' + clickedTime +
						'; expires=' + new Date(2030, 0) + '; domain=' + domain + '; path=/;';
			});
		}
	);
})(window['jQuery']);

</script>
