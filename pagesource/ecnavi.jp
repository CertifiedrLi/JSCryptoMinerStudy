<script type="text/javascript">
$(function () {
    var event_ids_string = [];
    var cookieKey = 'top_side_shopping_status_event';
    $.getJSON('/shopping/api/okaidoku/newer/', function (json) {
        if (!json) return;

        var okaidoku = $("#okaidoku");
        event_ids_string = json.event.join(',');
        var count = json.unread_count;
        if (count == 0) return;

        dom = okaidoku.find('li[data-id=event]').first();
        dom.find('a').append('<span class="count">' + count + '</span>');
        dom.addClass('new');
    });

    $('#okaidoku li[data-id=event]').each(function(){
        var $li = $(this);
        $li.find('a').on('click', function(){
            $.cookie(cookieKey, event_ids_string, { expires: 1, path: '/shopping/api/okaidoku/' });
        });

        return true;
    });
});
</script>

