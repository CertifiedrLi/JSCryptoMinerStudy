<script>
$(function() {

    // スタイルシートを追加
    stylesheet = document.styleSheets.item(0);
    stylesheet.insertRule('.type-focus {transform: scale(1.1);}' , stylesheet.cssRules.length);

    // フォーカスエフェクト
    function focusTarget(target) {
        $(target).css({transition: 'transform 500ms'}).delay(500).queue(function() {
            if (target === '#main_search') {
                $('#main_search li.dictionary a').click();
                $('#main_search input').focus();
            }
            $(this).addClass('type-focus').dequeue();
        }).delay(500).queue(function() {
            $(this).removeClass('type-focus').dequeue();
        });
    }

    // クリック
    $('#daily_point a[href*=#]').click(function() {
        if(location.href.replace(location.hash, '') !== this.href.replace(this.hash, '')) return;
        focusTarget(this.hash);
        return true;
    });

    // 訪問時
    if (location.hash && location.hash !== '#') {
        focusTarget(location.hash === '#websearch' ? '#main_search' : location.hash);
    }

});
</script>

