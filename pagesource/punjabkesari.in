    <script type="text/javascript" >
        
        /*  "CallNextPreDv" Function is use to show Next and Previous div after click on Next and Previous button.
           Param : nextPreDv (This parameter is indicate to show next and previous file (plus/minus) .
           Param : min (This is the minimum number of div). Example: this will be "1"
           Param : Max (This is the total number of div). Example: this will be total number of div.
           Param : divID ( This is the Div ID )
           Param : Hidden variable to store the current value of div.
        */

        function CallNextPreDv(nextPreDv, min, max,divID,txthideVarID) {
            var num = document.getElementById(txthideVarID).value;
            var i = 1;
            if (nextPreDv == 'plus') {
                num = parseInt(num) + 1;
                if (num > max)
                    num = min;
                document.getElementById(txthideVarID).value = num;
            }
            else {
                num = parseInt(num) - 1;
                if (num < 1)
                    num = max;
                document.getElementById(txthideVarID).value = num;
            }
            for (i = 1; i < 4; i++) { document.getElementById(divID + i).style.display = "none";}
            document.getElementById(divID + num).style.display = "block";
        }
 
    </script>
    
 
