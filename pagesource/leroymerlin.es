        function estableceCookieTienda() {
          var lmTiendaTextoMostrar = jQuery("#selectorTiendasCentro option:selected").text()+" ("+jQuery("#selectorTiendasProvincia option:selected").text()+")";
          var rutatienda = jQuery("#selectorTiendasCentro option:selected").val();
          var codtienda = rutatienda.substring(rutatienda.indexOf("=")+1);
          var provincia = jQuery("#selectorTiendasProvincia option:selected").val();
          jQuery.cookie("lmtiendatext", null, { path: '/', expires: 10000 });
          jQuery.cookie("lmtiendatext", lmTiendaTextoMostrar, { path: '/', expires: 10000 });
          jQuery.cookie("lmtiendacod", null, { path: '/', expires: 10000 });
          jQuery.cookie("lmtiendacod", codtienda, { path: '/', expires: 10000 });
          jQuery.cookie("lmprovincia", null, { path: '/', expires: 10000 });
          jQuery.cookie("lmprovincia", provincia, { path: '/', expires: 10000 });
          return codtienda;
        }

