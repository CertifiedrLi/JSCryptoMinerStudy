function autocompleteSource(request,response) {
  var term = request.term;
  var idx = term.lastIndexOf(' ') +1;
  var queryParam = term.substring(0, idx);
  var fragmentParam = term.substring(idx);
  var url = '/LeroySearcher/search/product/autocomplete?jsoncallback=?'; //?language=es&query='+query+'&fragment='+fragment;
  jQuery.getJSON(url,{language:'es',query:queryParam,fragment:fragmentParam},function(result){
    //console.log(result);
    if(result!=null) {
      var array = result.autocomplete;
      for(r in array) {
        if(typeof array[r] == "string"){
          array[r] = {label : highlight(array[r], term), value : array[r]};
        }
      }
      response(array);
    }
  });
}

// substring text
function substringText() {
  jQuery('.item .properties').each(function() {
    longText = (jQuery(this).html().length > 200);

    if (longText) {
      jQuery(this).html(jQuery(this).html().substring(0, 200));
      jQuery(this).append('...');
    }
  });
}


function highlight(s, t) {
  var matcher = new RegExp("("+jQuery.ui.autocomplete.escapeRegex(t)+")", "i" );
  return s.replace(matcher, "<strong>$1</strong>");
}


