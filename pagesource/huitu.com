    <script type="text/javascript">
        var _BDAD_CON = {
            t: ["新春汇", "无忧保全"],
            s: ["http://skin.huitu.com/images/event/188/index-banner.jpg", "http://skin.huitu.com/images/showto/freesafe/safeAd.png?v=20171229"],
            u: ["http://hi.huitu.com/event/188/", "http://www.huitu.com/wwwad/freesafe.html"]
        };
    </script>
    <script type="text/javascript">
        (function (window, para) {
            var titleArr = para.t,
                srcArr = para.s,
                urlArr = para.u;
            var $bdImgWrap = $("#bdImgWrap"),
                $img = null,
                interval = false,
                currentNum = 1;
            len = titleArr.length;

            function initH() {
                var imgH = '<div class="imgList" style="width: 1180px; height: 110px; position: absolute; left: 0; top: 0;"><a href="' + urlArr[0] + '" target="_blank"><img title="' + titleArr[0] + '" src="' + srcArr[0] + '" style="width:1180px;height:110px;" /></a></div>'
                var arr = [];
                if (len == 1) {
                    $bdImgWrap.html(imgH)
                    return;
                } else if (len > 1) {
                    for (var i = len - 1; i >= 0; i--) {
                        arr.push('<div class="bdImgList" style="z-index: 2; width: 13px; padding: 0px 9px; height: 110px; background: #' + (i == 0 ? "1098ca" : "A2A6A9") + '; position: absolute; border-right:1px solid #' + (i == (len - 1) ? "a2a6a9" : "607D8B") + '; left: ' + (i == 0 ? 0 : (1180 - 32 * (len - i))) + 'px; top: 0; "><div style="_position:relative;height:110px;display:table"><div style="vertical-align: middle; display: table-cell;_position:absolute;_top:50%;"><div style="_position:relative;_top:-50%;">' + titleArr[i] + '</div></div></div></div>')
                    }
                    $bdImgWrap.html(arr.reverse().join("") + imgH);
                    $img = $bdImgWrap.find("img").eq(0);
                }
            }
            initH();
            if (srcArr.length <= 1) return;
            function autoGo() {
                if (!interval) {
                    interval = setInterval(function () {
                        _bdggGo(currentNum);
                    }, 5000);
                }
            }
            autoGo();
            function _bdggGo(currNum) {
                var $bdImgList = $bdImgWrap.find(".bdImgList");
                $bdImgList.css({ "background": "#A2A6A9", "border-right": "1px solid #607D8B" }).eq(len - 1).css("border-right", "1px solid #A2A6A9");// 设置灰色
                $bdImgList.eq(currNum).css({ "background": "#1098CA", "border-right": "1px solid #A2A6A9" });
                for (var i = 0; i < len; i++) {
                    if (i <= currNum) {
                        $bdImgList.eq(i).css("left", (i * 32) + "px")
                    } else {
                        $bdImgList.eq(i).css("left", (1180 - ((len - i) * 32)) + "px")
                    }
                }
                $img.stop().attr("src", srcArr[currNum]).css({ "opacity": "0", "filter": "alpha(opacity=0)" }).animate({ opacity: 1 }, 400).parent().attr("href", urlArr[currNum]);
                if (currNum < len - 1) {
                    currentNum = currNum + 1;
                } else {
                    currentNum = 0;
                }
            }
            $bdImgWrap.on("mouseover", ".bdImgList", function () {
                var $this = $(this),
                    _index = $this.index(),
                    _bgColor = $(this).css("background-color");
                if (interval) clearInterval(interval);
                interval = false;
                if (_bgColor != "rgb(16, 152, 202)" && _bgColor != "#1098ca") {
                    setTimeout(function () {
                        _bdggGo(_index)
                    }, 300)
                }
            }).on("mouseout", function () {
                autoGo()
            })
        })(window, _BDAD_CON);
    </script>
