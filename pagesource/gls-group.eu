<script type="text/javascript">
  var showCookiePolicy = function (id) {
    document.getElementById(id).style.display = 'block';
    var elementHeight = document.getElementById(id).offsetHeight;
    document.getElementsByTagName('body')[0].style.marginBottom = elementHeight + "px";
    document.getElementById('cookiePopupMinimized').style.display = 'none';
  }
  var hideCookiePolicy = function (id) {
    document.getElementById(id).style.display = 'none';
    document.getElementsByTagName('body')[0].removeAttribute('style');
    document.getElementById('cookiePopupMinimized').style.display = 'block';
  }
  var acceptedCookiePolicy = function () {
    createCookie("gls-cookie-policy", "accepted", 390);
    hideCookiePolicy("cookiePopup", true);
  }
  var rejectedCookiePolicy = function () {
    createCookie("gls-cookie-policy", "rejected", 390);
    document.cookie = '_ga=;expires=Thu, 01-Jan-70 00:00:01 GMT;domain=.gls-group.eu;path=/'
    document.cookie = '_gat=;expires=Thu, 01-Jan-70 00:00:01 GMT;domain=.gls-group.eu;path=/'
    createCookie("glsCountryURL", "", -1);
    hideCookiePolicy("cookiePopup");
  }
  // initial cookie check
  if (getCookie("gls-cookie-policy")) {
	hideCookiePolicy("cookiePopup");
  } else {
    showCookiePolicy("cookiePopup");
  }
</script>
