(function( $ ) {   
    
    
    $('.dboom-video-wrapper .hook-redirector').click(function () {
	    
	    var url = $(this).parent().data( 'href' );
	    
	    window.location = url;
    });
    
    $('.dboom-video-wrapper .video').parent().click(function () {
	    
	    return false;

	    if($(this).children(".video").get(0).paused){
    	    $(this).children(".video").get(0).play();
			$(this).children(".playpause").fadeOut();
		}else{
			$(this).children(".video").get(0).pause();
			$(this).children(".playpause").fadeIn();
		}
	});
	
	$('.dboom-video-wrapper .video').on('ended',function() {
		$(this).get(0).play();
    });
	
	$(document).ready(function() {

        // Get media - with autoplay disabled (audio or video)
        var media = $('.dboom-video-wrapper .video');
        var tolerancePixel = 40;

        function checkMedia(){
            // Get current browser top and bottom
            var scrollTop = $(window).scrollTop() + tolerancePixel;
            var scrollBottom = $(window).scrollTop() + $(window).height() - tolerancePixel;

            media.each(function(index, el) {
                var yTopMedia = $(this).offset().top;
                var yBottomMedia = $(this).height() + yTopMedia;

                if(scrollTop < yBottomMedia && scrollBottom > yTopMedia){ 
                    $(this).get(0).play();
                    $(this).parent().children(".playpause").fadeOut();
                } else {
                    $(this).get(0).pause();
                    $(this).parent().children(".playpause").fadeIn();
                }
            });

            //}
        }
		
		$(document).on('scroll', checkMedia);
    });
}( jQuery ));    
</script>
