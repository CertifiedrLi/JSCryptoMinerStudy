    $(document).ready(function () {
        $("#clText").click(function () {
            $('#sourceText').val('').focus();
            clearLastTranslation();
            clearSelectionInfoResult();
            $(".selResult").hide();
            $('#editResult_test').val('');
            $("#trResult").hide();
            $("#adviseIconDiv").hide();
