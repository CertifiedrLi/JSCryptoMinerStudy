</div><script type="text/javascript">
        (function(){
            var container = $('#main-menu'),
                top = container.find('ul.l1').position().top,
                l2 = {top:container.find('ul.l2-top'), bottom:container.find('ul.l2-bottom')},
                width = container.width(),
                timer,
                showTimeout = 300,
                active
                    = container.find('ul.l1 > li.active'),
                activeLine = 'top',
                current = active,
                hovered = active,
                currentLine = 'top',
                hidden = container.find('ul.l2-hidden');

            show();


            function dShow() {
                if (timer) {
                    clearTimeout(timer);
                }
                timer = setTimeout(show, showTimeout);
            }

            function dHide() {
                if (timer) {
                    clearTimeout(timer);
                }
                timer = setTimeout(hide, showTimeout);
            }

            function showActive() {
                var tmp = current;
                current = active;
                l2.top.addClass('hidden');
                l2.bottom.addClass('hidden');
                show();
                current = tmp;
            }

            function show() {
                    var ul = current.find('ul.l2').html(),
                    pos = current.position(),
                    hoveredLine = currentLine;

                hovered.removeClass('hover');
                currentLine = pos.top > 30 ? 'bottom' : 'top';

                if (!hovered.hasClass('active') && currentLine != hoveredLine) {
                    l2[hoveredLine].addClass('hidden');
                    if (hoveredLine == activeLine) {
                        hovered = current;
                        showActive();
                    }
                }

                currentLine = pos.top > 30 ? 'bottom' : 'top';


                l2[currentLine].addClass('hidden');
                current.addClass('hover');
                hovered = current;

                if (hovered.hasClass('active')) {
                    activeLine = currentLine;
                }

                if (ul) {
                    hidden.html(ul);

                    l2[currentLine]
                        .html(ul)
                        .removeClass('hidden');

                    var
                        liCenter = pos.left + current.width() / 2,
                        subWidth = hidden.width(),
                        marginLeft = Math.round(liCenter - subWidth / 2);
                    if (marginLeft < 0) {
                        marginLeft = 0;
                    }
                    if (marginLeft + subWidth > width) {
                        marginLeft = width - subWidth;
                    }
                    if (subWidth > width) {
                        marginLeft = 0;
                    }

                    //console.log(width, pos, currentLine, liCenter, subWidth, marginLeft);

                    if (ul) {
                        l2[currentLine].css('margin-left', marginLeft + 'px');
                    }
                }


            }

            function hide() {
                current.removeClass('hover');
                showActive();
            }

            container.find('ul.l2-top, ul.l2-bottom').hover(
                function(){
                    if (timer) {
                        clearTimeout(timer);
                    }
                },
                dHide
            );

            container.find('li.l1').hover(
                function() {
                    current = $(this);
                    dShow();
                },

                dHide
            );



        })();
</script>
    
