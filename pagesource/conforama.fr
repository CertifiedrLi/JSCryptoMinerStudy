addPromoToGapArray = function(elementPromo, promoPosition){
	var promoName = jQuery(elementPromo).attr('gap-promo-name');
	var promoCreative = jQuery(elementPromo).attr('gap-promo-creative');
			
	if (typeof promoName !=='undefined' && promoName !== '' && typeof promoCreative !=='undefined' && promoCreative !== ''){
		var promo = {'productId': 'undefined', 'name': promoName, 'creative': promoCreative, 'position': promoPosition};
		for (var property in promo){
			if (promo[property] == ''){
				promo[property] = undefined;
			}
		}
		tc_vars_gap.promotionImpression.push(promo);
	}
}

createAutopromoGapArray = function(autopromoGap,prefixPosition){
	if (autopromoGap.length > 0) {
		var positionAutopromo = 1;
		
		autopromoGap.each(function() {
			var promoPosition = prefixPosition + "_" + positionAutopromo + "_" + autopromoGap.length;
			var elementPromo = this;
			addPromoToGapArray(elementPromo, promoPosition);
			positionAutopromo++;
		});
	}
}

initAutopromoGapEvent = function(autopromoGap,prefixPosition){
	if (autopromoGap.length > 0) {
		var	positionAutopromo = 1;
		
		autopromoGap.each(function() {
			var promoPosition = prefixPosition + "_" + positionAutopromo + "_" + autopromoGap.length;
			var promoName = jQuery(this).attr('gap-promo-name');
			var promoCreative = jQuery(this).attr('gap-promo-creative');
		
			$(this).click(function(){
				if (typeof promoName !=='undefined' && promoName !== '' && typeof promoCreative !=='undefined' && promoCreative !== ''){
					tagCommanderClickPrintEvent(this,  'gaPromotionClick', { 'eventCategory': 'Enhanced Ecommerce', 'eventAction': 'Promotion Click', 'productId' : 'undefined', 'name' : promoName, 'creative' : promoCreative, 'position' : promoPosition });
				}
				return true;
			});
			positionAutopromo++;
		}); 
	}
}

initPrefixList = function(results,list){
		results.each(function(){
			var prefixName = jQuery(this).attr('gap-promo-prefix-position');
			if (prefixName !== null){
				list.push(prefixName);
			}
		}); 
}
	
