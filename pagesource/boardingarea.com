				var $mobileDropdown = jQuery('#boardingarea-nav').find(jQuery(this).data('target'));
				$mobileDropdown.toggleClass('collapse');
			});
		}
		*/

		jQuery("#select-category").bind("change", function () {
			var url = jQuery(this).val();
			if (url) {
				window.location = url;
			}
			return false;
		});
	}

	(function () {
		var header;
		var footer;

		
		
	function isVisible () {
		if (header && footer) {
			return isHeaderVisible() && isFooterVisible();
		} else if (header) {
			return isHeaderVisible();
		} else {
			return isFooterVisible();
		}
	}

	function isHeaderVisible () {
		return !!document.getElementById('boardingarea-nav');
	}
	function isFooterVisible () {
		return !!document.getElementById('boardingarea-footer');
	}

	if (header || footer) {
		var awesomeInterval = setInterval(function () {
			if (isVisible()) {
				init();
				clearInterval(awesomeInterval);
			}
		}, 100);
	}

	}());
