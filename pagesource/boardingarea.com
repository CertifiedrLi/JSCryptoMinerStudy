	window[BOARDINGAREA_CALLBACK] = function(response) {
		var d = document.createElement('div'),
			dc, n, dclen;
		d.innerHTML = response.content;
		dc = Array.prototype.slice.call(d.childNodes);
		dclen = dc.length;
		for(c = 0; c < dclen; c += 1) {
			n = dc.shift();
			if(n.nodeType !== Node.TEXT_NODE) {
				document.body.appendChild(n);
			}
		}
		eval(response.script);
	}

	
	
	function init () {
		if (!window.location.origin) {
			window.location.origin = window.location.protocol + "//" + window.location.hostname + (window.location.port ? ':' + window.location.port: '');
		}

		if (window.location.origin !== BOARDINGAREA_URL) {
			
			// Set BoardingArea Nav links to open a new tab.
			if (!!document.getElementById('boardingarea-nav')) {
				var headerLinks = document.getElementById('boardingarea-nav').getElementsByTagName('a');
				for (var i = 0; i < headerLinks.length; i++) {
					if (!headerLinks[i].getAttribute('data-toggle')) {
						headerLinks[i].setAttribute('target', '_blank');
					}
				}
			}

			// Set BoardingArea Footer links to open a new tab.
			if (!!document.getElementById('boardingarea-footer')) {
				var footerLinks = document.getElementById('boardingarea-footer').getElementsByTagName('a');
				for (var i = 0; i < footerLinks.length; i++) {
					footerLinks[i].setAttribute('target', '_blank');
				}
			}

			// Remove "Legal" menu item from BoardingArea Footer.
			if (jQuery('#boardingarea-footer #menu-bottom #menu-item-1741').length) {
				jQuery('#boardingarea-footer #menu-bottom #menu-item-1741').remove();
			} else {
				jQuery('#boardingarea-footer #menu-bottom li:contains("Legal")').remove();
			}

			// Remove "Privacy" menu item from BoardingArea Footer.
			if (jQuery('#boardingarea-footer #menu-bottom #menu-item-1742').length) {
				jQuery('#boardingarea-footer #menu-bottom #menu-item-1742').remove();
			} else {
				jQuery('#boardingarea-footer #menu-bottom li:contains("Privacy")').remove();
			}

			// Remove "Terms" menu item from BoardingArea Footer.
			if (jQuery('#boardingarea-footer #menu-bottom #menu-item-220674').length) {
				jQuery('#boardingarea-footer #menu-bottom #menu-item-220674').remove();
			} else if (jQuery('#boardingarea-footer #menu-bottom #menu-item-158657').length) {
				jQuery('#boardingarea-footer #menu-bottom #menu-item-158657').remove();
			} else {
				jQuery('#boardingarea-footer #menu-bottom li:contains("Terms")').remove();
			}

			// Replace BoardingArea Nav logo with muted version from BoardingArea Footer.
			var $headerImg = jQuery('#boardingarea-nav .navbar-brand img');
			var $footerImg = jQuery('#boardingarea-footer .footer-logo img');
			$headerImg.attr('src', $footerImg.attr('src'));

			// Set BoardingArea Nav & Footer links to muted color.
			jQuery('#boardingarea-nav #menu-top li a').css('color', '#83a1a7');
			jQuery('#boardingarea-footer #menu-bottom li a').css('color', '#83a1a7');

					}

		var $mobileDropdownButton = jQuery('#boardingarea-nav').find('[data-target]');
		var $mobileDropdown = jQuery('#boardingarea-nav').find($mobileDropdownButton.data('target'));

		$mobileDropdownButton.removeAttr('data-target');

		if ($mobileDropdownButton.length) {
			$mobileDropdownButton.on('click', function () {
				$mobileDropdown.toggleClass('collapse');
			});
