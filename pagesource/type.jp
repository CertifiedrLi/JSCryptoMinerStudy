<script type="text/javascript" >
$(function(){
  var cookie = getCookies()["JSESSIONID"];
  if (cookie != null) {
    setCookie("JSESSIONID", cookie, null, null, "/", false);
  }

  var histories = JSON.parse(localStorage.getItem('type_SearchHistory'));
  var lastSearchArea = $('#last-search');
  var lastSearchInfo = $('#last-search').find("div");

  if (histories) {
    lastSearchArea.css('display','');
    lastSearchInfo.find('p.mb3').text(histories['SearchHistory'][0]['Date']);
    var conditionName="";

    histories['SearchHistory'].forEach(function(value){
      var length = 0;
      var index = 0;
      for(key in value) {
        if(key != 'Date' && key != 'Url') {
          length++;
        }
      }
      for(key in value) {
        var cond=""
        if(key != 'Date' && key != 'Url') {
          if (key == 'keyword'){
            cond = decodeURI(value[key]);
          } else {
            cond = value[key];
          }
          conditionName = conditionName+ cond;
          if (index < length - 1) {
            conditionName = conditionName + "、";
          }
        index++;
        }
      }
    });
    lastSearchInfo.find('a').text(conditionName);
    lastSearchInfo.find('a').prop('href', histories['SearchHistory'][0]['Url']);
  }
});

function search() {
  $("#search").submit();
}
</script>

