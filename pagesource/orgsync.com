<script type="text/javascript">
  $(document).ready(function(){

    var carousel = $("#highlight-carousel");
    var totalSlides = $("#highlight-carousel").length + 1;
    var randomSlide = Math.ceil(Math.random() * totalSlides);

    function showCarousel() {
      $(".home-header").addClass('animated fadeInDown');
    }

    carousel.owlCarousel({
      transitionStyle : "fade",
      autoPlay : true,
      singleItem : true,
      items : 1,
      paginationSpeed : 4000,
      slideSpeed : 800,
      rewindSpeed : 800,
      lazyLoad : true,
      nav: true,
      animateIn: true,
      pagination : true,
      loop: false,
      afterInit : showCarousel()
    });

    function setCookie(cname, cvalue, exdays) {
      var d = new Date();
      d.setTime(d.getTime() + (exdays*24*60*60*1000));
      var expires = "expires="+d.toUTCString();
      document.cookie = cname + "=" + cvalue + "; " + expires;
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return "";
    }

    function gotoSlide() {
      var visited = getCookie("visited");
      if (visited == "") {
        setCookie("visited", true, 365);
        carousel.trigger('owl.jumpTo', 0);
        showCarousel();
        $('.home').addClass('show-for-previous');
      } else {
        carousel.trigger('owl.jumpTo', randomSlide);
        showCarousel();

        $('#whats-new').appendTo( $('#previously-visited') );
      }
    }

    gotoSlide();


    $("#school-carousel").owlCarousel({
      items : 1,
      itemsCustom : [[0, 3], [400, 5], [700, 6], [1000, 8], [1200, 12], [1600, 12]],
      nav: true,
      animateIn: true,
      pagination : false,
      rewindNav: false,
      paginationSpeed : 3000,
      autoPlay : true,
      scrollPerPage : 1,
      lazyLoad : true,
      lazyFollow : true,
      lazyEffect : "fade",
      afterInit: function(){ $('.school-wrapper').slideToggle() }
    });


    //-----------------------------//
    //        Video Modal          //
    //-----------------------------//

    // This is a hack to add the cta button to the modal
    function addCta() {
      $('.pp_description').html(
          '<strong>Why I Recommend OrgSync</strong><a class="btn-basic blue right" style="margin-top:-8px; padding-top:5px; padding-bottom:5px" href="http://www.orgsync.com/schedule_a_demo">Schedule a Demo</a>'
        );
     };

    $(".vid-olame").prettyPhoto({
      theme: 'os-light',
      show_title: false,
      default_width: 960,
      default_height: 540,
      changepicturecallback: addCta
    });


  });
</script>

