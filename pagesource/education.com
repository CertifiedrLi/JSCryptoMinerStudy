<script>
// Top Nav click events
(function($) {
  if(!$) return;
  var ham_open = false;
  var account_open = false;

  var ham = $('nav.unified-subnav .subnav-more');
  var account = $('header.unified .account-dropdown');

  // Hamburger menu
  ham.on('click','.icon-menu',function(e) {
    ham.toggleClass('open');
    ham_open = ham.hasClass('open');
  });

  // Account menu
  account.on('click','.account-tag',function(e) {
    account.toggleClass('open');
    account_open = true;
  });

  // Hamburger dropdown tabs
  ham.on('click','.dropdown > ul > li > span:not(".link")',function(e) {
    e.preventDefault();
    var p = $(this).parent();
    if(p.hasClass('open')) {
      p.removeClass('open');
    }
    else {
      $('nav.unified-subnav .subnav-more .dropdown > ul > li.open').removeClass('open');
      p.addClass('open');
    }
  });
  // Click on page outside of hamburger/account menu when open
  $(document).on('click',function(e) {
    if(!ham_open && !account_open) return;
    var t = $(e.target);

    if(ham_open) {
      if(!t.hasClass('subnav-more') && !t.closest('.subnav-more').length) {
        ham.removeClass('open');
        ham_open = false;
      }
    }
    if(account_open) {
      if(!t.hasClass('account-dropdown') && !t.closest('.account-dropdown').length) {
      	account.removeClass('open');
      	account_open = false;
      }
    }
  });
})(window.jQuery);
</script>

 

