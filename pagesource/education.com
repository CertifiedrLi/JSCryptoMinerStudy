    <script type="text/javascript">
        // Set ad server timeout
        var PREBID_TIMEOUT = 500;
        // Timeout test
        if(typeof testTimeout != "undefined") PREBID_TIMEOUT = testTimeout;
        var dfpNetwork = '5601'; 
        /* A9 Init */  
        apstag.init({
            pubID: 'c3e5dc78-a521-41d2-b317-26a96be397da',
            adServer: 'googletag'
        });
        var adUnits = adUnits || [];
        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];
        var pbjs = pbjs || {};
        pbjs.que = pbjs.que || [];
        /* A9 Vars */
        var a9Slots = [];
        var a9BidsBack = false;
        // checking whether desktop or mobile
        var viewport_type = window.innerWidth;
            
        (function(){
            // check 'window.pbjs_debug=true' for debugging
            window.prebid_time_started = new Date().getTime();
            window.pbjs_debug = false;
            if(location.search.indexOf('window.pbjs_debug=true') >= 0) {
                window.pbjs_debug = true;
            }            
            
            window.prebid_timeout_hit  = false;
            window.prebid_bids_returned  = false;
            window.prebid_script_loaded = false;
            window.googletag.cmd.push(function() {
                window.googletag.pubads().disableInitialLoad();
            });            
            window.pbjs.que.push(function() {
                window.prebid_script_loaded = true;
                //register a callback handler
                window.pbjs.onEvent('bidResponse', function(adUnitCode){
                    if(window.pbjs_debug) { console.log('________BID RETURNED________' + adUnitCode + '; Took: ' + (new Date().getTime() - window.prebid_time_started) + 'ms'); }
                    window.prebid_bids_returned = true;
                });
            });
            window.sendAdServerRequest = function() {
                if (window.pbjs.adserverRequestSent) return;
                window.pbjs.adserverRequestSent = true;
                window.googletag.cmd.push(function() {
                    /* A9 Set Bids */
                    if(a9BidsBack) apstag.setDisplayBids();
                    window.pbjs.que.push(function() {
                        window.pbjs.setTargetingForGPTAsync();
                        window.googletag.pubads().refresh();
                    });
                });
            }
            setTimeout(function(){
                if (window.pbjs.initAdserverSet) return;
                window.prebid_timeout_hit = true;
                window.sendAdServerRequest();
            }, window.PREBID_TIMEOUT);
            (function () {
                var gads = document.createElement('script');
                gads.async = true;
                gads.type = 'text/javascript';
                var useSSL = 'https:' == document.location.protocol;
                gads.src = (useSSL ? 'https:' : 'http:') +
                '//www.googletagservices.com/tag/js/gpt.js';
                var node = document.getElementsByTagName('script')[0];
                node.parentNode.insertBefore(gads, node);
            })();
            var mappingHorizontal = [[728, 90]];
            if(viewport_type < 748) mappingHorizontal = [[320,50], [300,50]];
            var mappingVertical = [[300, 600],[300,250],[160,600]];
            if(viewport_type <= 1374) mappingVertical = [[160,600]];
            if(viewport_type <= 748) mappingVertical = [[300,250]];
            // START: Defining Adunits
            // All Devices adunits
            adUnits.push({
                    network: dfpNetwork,
                    adunit: (viewport_type >  748) ? "Edcom_Desktop_Top_Horizontal" : "Edcom_Mobile_Top_Horizontal",
                    size: mappingHorizontal,
                    code: 'div-gpt-ad-Edcom_Top_Horizontal',
                    assignToVariableName: false // false if not in use
                });
            adUnits.push({
                    network: dfpNetwork,
                    adunit: (viewport_type >  748) ? "Edcom_Desktop_Bottom_Horizontal" : "Edcom_Mobile_Bottom_Horizontal",
                    size: (viewport_type > 748) ? mappingHorizontal : [[300,250],[320,50],[320,100]],
                    code: 'div-gpt-ad-Edcom_Bottom_Horizontal',
                    assignToVariableName: false // false if not in use
                }); 
            
            if(viewport_type >= 1234 || viewport_type <= 748)
            {// Hide between 1234px and 748px   
                adUnits.push({
                        network: dfpNetwork,
                        adunit: (viewport_type >  768) ? "Edcom_Desktop_Bottom_Vertical" : "Edcom_Mobile_Bottom_Vertical",
                        size: mappingVertical,
                        code: 'div-gpt-ad-Edcom_Bottom_Vertical',
                        assignToVariableName: false // false if not in use
                    }); 
            }
            if(viewport_type >= 1234)
            {// Only 1234px 
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Desktop_Top_Vertical",
                        size: mappingVertical,
                        code: 'div-gpt-ad-Edcom_Desktop_Top_Vertical',
                        assignToVariableName: false // false if not in use
                    });
            }
            if(viewport_type >= 1000)
            {// Only 1000px 
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Desktop_Rectangle_A",
                        size: [[300,250]],
                        code: 'div-gpt-ad-Edcom_Rectangle_A',
                        assignToVariableName: false // false if not in use
                    });
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Desktop_Top_Vertical_B",
                        size: [[300,250], [300, 600]],
                        code: 'div-gpt-ad-Edcom_Top_Vertical_B',
                        assignToVariableName: false // false if not in use
                    });
            } else {
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Mobile_Rectangle_A",
                        size: [[300,250]],
                        code: 'div-gpt-ad-Edcom_Rectangle_A',
                        assignToVariableName: false // false if not in use
                    });
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Mobile_Rectangle_B",
                        size: [[300,250]],
                        code: 'div-gpt-ad-Edcom_Rectangle_B',
                        assignToVariableName: false // false if not in use
                    });
            }
            // ONLY desktop adunits
            if(viewport_type >=  768)
            {
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Desktop_Left_Vertical",
                        size: [[300,250]],
                        code: 'div-gpt-ad-Edcom_Desktop_Left_Vertical',
                        assignToVariableName: false // false if not in use
                    });
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Desktop_Right_Vertical",
                        size: [[300,250]],
                        code: 'div-gpt-ad-Edcom_Desktop_Right_Vertical',
                        assignToVariableName: false // false if not in use
                    });                                 
            } else {
            // ONLY Mobile adunits
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Mobile_Middle_Vertical_A",
                        size: [[300,250]],
                        code: 'div-gpt-ad-Edcom_Mobile_Middle_Vertical_A',
                        assignToVariableName: false // false if not in use
                    }); 
                adUnits.push({
                        network: dfpNetwork,
                        adunit: "Edcom_Mobile_Middle_Vertical_B",
                        size: [[300,250]],
                        code: 'div-gpt-ad-Edcom_Mobile_Middle_Vertical_B',
                        assignToVariableName: false // false if not in use
                    });                 
            }   
                
             // END: Defining Adunits   
            /* A9 Slots */
            if(adUnits){
                if(apstag){
                  for (var i = 0, len = adUnits.length; i < len; i++) {
                          a9Slots.push({slotID: adUnits[i].code,slotName: adUnits[i].network+'/'+adUnits[i].adunit,sizes: adUnits[i].size});
                      }
                 }
            }    
            /* A9 Request Bids */
            apstag.fetchBids({
                slots: a9Slots,
                timeout: PREBID_TIMEOUT
            }, 
            function(bids) {
                a9BidsBack = true;
            });
            googletag.cmd.push(function() {
              window.googletag.refreshables = [];
              if(adUnits){
                var dfpSlots = [];
                for (var i = 0, len = adUnits.length; i < len; i++) {
                  dfpSlots[i] = googletag.defineSlot('/'+adUnits[i].network+'/'+adUnits[i].adunit, adUnits[i].size, adUnits[i].code).addService(googletag.pubads());
                  window.googletag.refreshables.push(dfpSlots[i]);
                  if(adUnits[i].assignToVariableName && (adUnits[i].assignToVariableName !== null)) window[adUnits[i].assignToVariableName] = dfpSlots[i];
                }
              }
            });                 
            window.googletag.cmd.push(function() {
                // Timeout Test
                if(typeof testTimeout !== "undefined") googletag.cmd.push(function() { googletag.pubads().setTargeting("testTimeout", testTimeout.toString()) });
                
                window.googletag.pubads().set("adsense_background_color", "FFFFFF");
                                                                                        
                window.googletag.pubads().setTargeting("keywords", "ros");
                window.googletag.pubads().setTargeting("pagetitle", "Printable Workbooks | Math, Science, Rea");
                window.googletag.pubads().setTargeting("pagename", "UnifiedSearchResults");
                window.googletag.pubads().setTargeting("userinfo", "logged_out");
                window.googletag.pubads().setTargeting("section", "workbooks");
                window.googletag.pubads().setTargeting("env", ["production","www"]);
                window.googletag.pubads().setTargeting("sponsored", "no");
                window.googletag.pubads().setTargeting("prebidTime", PREBID_TIMEOUT.toString());
                window.googletag.pubads().setTargeting("kw", ["ros","Printable Workbooks | Math, Science, Rea","UnifiedSearchResults","logged_out","workbooks","production","www","no","500"]);
                                                            
                window.googletag.pubads().enableSingleRequest();
                //window.googletag.pubads().enableAsyncRendering();
                window.googletag.pubads().set('page_url', (document.location && document.location.protocol ? document.location.protocol : 'http:') + '//www.education.com/workbooks/');
                //googletab.pubads().collapseEmptyDivs(); //<- this doesnt seem to work- breaks all the ads. Test it out to see if they fixed it later.
                window.googletag.enableServices();
            });
        })();
    </script>
	
	 

	
