<script>

    (function ($) {
        var signup_modal = $("#signup-form").modal({
            analytics_name: "Responsive",
            section_name: null,
            thumbnail: null,
            newsletters: {
                worksheet: false,
                activity: false,
                sciencefair: false,
                marketing: true
            },
	        register: false,
	        signin: false,
	        forgot: false,
            confirm: false,
	        reset: false,
            redirectPathOnSuccess: window.location.href,
            emailchecks: 0,
	        setRegisterTitle: function (title) {
		        $('.create-account h3', this.el).html(title);
	        },
	        setRegisterDescription: function (desc) {
		        $('.signup-age-gate .description:first', this.el).html(desc); //For the age gate screen
                $('.signup-form .description:first', this.el).html(desc); // For signup form
	        },
	        setSignInTitle: function (title) {
		        $('.sign-in h3', this.el).html(title);
	        },
            setEmailAddress: function(email) {
                $(".email-address input", this.el).val(email);
            },
            focusOnPassword: function() {
                $(".password input", this.el).focus();
            },
	        setSignInDescription: function (desc) {
		        $('.sign-in .description:first', this.el).html(desc);
	        },
            setRegisterButton: function(text) {
                $(".create-account .submit",this.el).text(text);
            },
            init: function () {
                // Default to create account
                this.showCreateAccount();
                this.setListeners();
            },
            setListeners: function () {
              var self = this;
	            $(this.el).on('click', '.forgot', function (e) {
		            e.preventDefault();
                    self.trackGA('forgot-password link clicked');
                    signup_modal.modal('show');
                    signup_modal.modal('showForgotPassword');
	            });

                $(".email-address input", this.el).on('blur', function(e){
                    self.checkEmailAddress(this);
                }).on('keydown', function() {
                    self.clearFieldError('email-address');
                });
	            $(".create-account",this.el).on('submit', function (e) {
                    e.preventDefault();
                    self.submitData();
                    return false;
                });
                $(".sign-in",this.el).on('submit', function (e) {
                    e.preventDefault();
                    self.submitData();
                    return false;
                });
                $(".forgot-password",this.el).on('submit', function (e) {
                    e.preventDefault();
                    self.submitData();
                    return false;
                });
                $(".reset-password",this.el).on('submit', function (e) {
                    e.preventDefault();
                    self.submitData();
                    return false;
                });
                //For the age gate
                $('.age-gate-pass').on('click', function(e){
                    e.preventDefault();
                    self.trackGA('clicked', 'Age gate - over 13 button');
                    $('.signup-age-gate').css('display', 'none');
                    $('.signup-form').removeClass('hidden');
                });
                $('.age-gate-fail').on('click', function(e){
                    e.preventDefault();
                    self.trackGA('clicked', 'Age gate - not 13 button');
                    $('.signup-age-gate').css('display', 'none');
                    $('.screen-age-gate-fail').css('display', 'block');
                });
                $('.over-13-link').on('click', function(e){
                    e.preventDefault();
                    self.trackGA('clicked', 'Age gate - over 13 link');
                    $('.screen-age-gate-fail').css('display', 'none');
                    $('.signup-form').removeClass('hidden');
                });
            },
            checkEmailAddress: function(el) {
                var em = $(el).val();
                var self = this;

                if(!em.length) {
                  self.clearFieldError('email-address');
                  return;
                }

                // limit to 20 email/mx lookups:
                if(self.emailchecks > 20) {
                    return;
                }
                self.emailchecks++;
                // check the MX record:
                $.post('?', {
                        __json: 'Modal_Registration',
                        action: 'checkemail',
                        email: em,
                        checknum:self.emailchecks
                    }, function (response) {
                        if(!response.status || response.status == "0") {
                            self.addFieldError('email-address', response.msg);
                        } else {
                            self.clearFieldError('email-address');
                        }
                    }
                ).fail(function(r) {});
            },
            setNewsletter: function(name, status) {
                this.newsletters[name] = status;
            },
            setAnalyticsName: function (name) {
                this.analytics_name = name;
            },
            setSectionName: function(name) {
                this.section_name = name;
            },
            setRedirectOverride: function(path) {
                this.redirectPathOnSuccess = path;
            },
            onOpen: function () {
                if (this.open_event_fired) return;
                this.open_event_fired = true;
                this.loadGooglePlatform();
                this.trackGA('opened','Viewed Registration Form');
	            (window.optimizely || []).push('trackEvent', 'registration-modal-open');
            },
            loadGooglePlatform: function() {

                var s = document.createElement('script');
                s.src = "//apis.google.com/js/platform.js";
                document.getElementsByTagName('head')[0].appendChild(s);

            },
            trackGA: function (event, name, method) {
	            if(window.Edu && window.Edu.trackEvent) {
	            	var data = {section: this.section_name || this.analytics_name};
	            	if(method) data.method = method;

		            window.Edu.trackEvent(name,data,{
			            eventCategory: 'modal',
			            eventAction: this.analytics_name + ' registration modal: ' + event
		            });
	            }
            },
            setAllToFalse: function () {
                this.register = false;
                this.signin = false;
                this.forgot = false;
                this.reset = false;
            },
            hideAll: function () {
                $(".sign-in",this.el).hide();
                $(".create-account",this.el).hide();
                $(".forgot-password",this.el).hide();
                $(".confirm-email", this.el).hide();
                $(".reset-password",this.el).hide();
                $('.loginwith #status', this.el).html('');
                $('.error-message', this.el).hide();
                this.clearAllErrors();
            },
            showCreateAccount: function () {
	            this.populateInputFields();
                this.setAllToFalse();
                this.register = true;
                this.hideAll();
	            $(".create-account .forgot",this.el).hide();
                $(".create-account",this.el).show();
            },
            showSignIn: function () {
	            this.populateInputFields();
                this.setAllToFalse();
                this.signin = true;
                this.hideAll();
                $(".sign-in",this.el).show();
            },
            showForgotPassword: function () {
	            this.populateInputFields();
                this.setAllToFalse();
                this.forgot = true;
                this.hideAll();
                $(".forgot-password",this.el).show();
            },
            showEmailConfirmation: function () {
                this.populateInputFields();
                this.setAllToFalse();
                this.confirm = true;
                this.hideAll();
                $(".confirm-email",this.el).show();
            },
            showResetPassword: function (showSubheader) {
                var email = $(".forgot-password .input-row.email-address input",this.el).val();
                $(".reset-password .input-row.email-address input",this.el).val(email);
                $('.reset-password .current-email-address',this.el).text(email);
                // hack because Chrome doesn't allow me to turn autocomplete off
                $("#password-reset-pw").val("");
                this.setAllToFalse();
                this.reset = true;
                this.hideAll();
	            if (showSubheader) {
		            $('.reset-password .subhead', this.el).show();
	            }
	            else {
		            $('.reset-password .subhead', this.el).hide();
	            }
                $(".reset-password",this.el).show();
            },
	        showResetPasswordFromEmailLink: function () {
		        $('.reset-password .subhead', this.el).hide();
		        this.showResetPassword(false);
	        },
            showErrorMessage: function (message) {
                this.hideAll();
                var errorTitle = $('<h3></h3>').text('Set/Reset Password');
                var errorMessage = $('<div></div>').text(message).css('margin-top', '10px');
                $('.error-message', this.el).empty().append(errorTitle).append(errorMessage).show();
                $('.error-message', this.el).append('<div style="text-align: center;display: block;padding: 20px 0 10px 0;"><a class="forgot-below forgot" href="/resetpassword/">Reset Password</a></div>');
            },
	        populateInputFields: function () {
		        var emailSelector;

		        if (this.signin) {
			        emailSelector = '.sign-in';
		        }
		        else if (this.register) {
			        emailSelector = '.create-account';
		        }
		        else if (this.forgot) {
					emailSelector = '.forgot-password';
		        }
		        else {
			        return false;
		        }

		        var emailAddress = $(emailSelector+ ' .input-row.email-address input',this.el).val();
		        if (emailAddress) {
			        $('.input-row.email-address input',this.el).val(emailAddress);
		        }

		        var password = $(emailSelector+ ' .input-row.password input',this.el).val();
		        if (password) {
			        $('.input-row.password input',this.el).val(password);
		        }
	        },
            getFormData: function () {
                var ret = {};

                if (this.register) {
                    ret = {
                        email: $('.create-account .input-row.email-address input',this.el).val(),
                        password: $('.create-account .input-row.password input',this.el).val()
                    };
                    ret.iama = [];
                    $('.create-account .input-row.iama input', this.el).each(function () {
                        var el = $(this);
                        if (el.val() && el.prop('checked')) ret.iama.push(el.val());
                    });

                    /** Getting grades checked */
                    ret.grades = [];
                    $('.create-account .input-row.grades input', this.el).each(function () {
                        var el = $(this);
                        if (el.val() && el.prop('checked')) ret.grades.push(el.val());
                    });
                }
                if (this.signin) {
                    ret = {
                        email: $('.sign-in .input-row.email-address input',this.el).val(),
                        password: $('.sign-in .input-row.password input',this.el).val()
                    };
                }
                if (this.forgot) {
                    ret = {
	                    email: $('.forgot-password .input-row.email-address input',this.el).val()
                    };
                }

                if (this.reset) {
                    ret = {
	                    key: $('.reset-password .input-row.email-verification input',this.el).val(),
	                    password: $('.reset-password .input-row.password input',this.el).val()
                    };
                }

                return ret;
            },
            track: function (event, data, immediate) {
                // TODO: track somewhere?
                // event = this.ca_signin?'ca_si_'+event:(this.register?'ca_'+event:'si_'+event);
                // window.Mongo && window.Mongo.Buyflow.track(event,data||null,immediate);
            },
            validateUserType: function (userType) {
                    if (userType.length > 0) {
                        return true;
                    }

                    this.addFieldError('iama', 'Please select one.');
                    this.track('failure', {error: 'user-type'});
                    return false;
            },
            validateGradesSelected: function(grades) {
                    if (grades.length > 0) {
                        return true;
                    }

                    this.addFieldError('grades', 'Please select one.');
                    this.track('failure', {error: 'grades'});
                    return false;
            },
            validateEmail: function (email) {
                if (email.length > 0) {
                    return true;
                }

                this.addFieldError('email-address', 'Please enter your email address.');
                this.track('failure', {error: 'email'});
                return false;
            },
            validatePasswordPresence: function (password) {
                if (password.length > 0) {
                    return true;
                }

                this.addFieldError('password', 'Please enter your password.');
                this.track('failure', {error: 'password'});
                return false;
            },
            validatePasswordLength: function (password) {
                if (password.length > 32) {
                    this.addFieldError('password', 'Please enter a password less than 32 characters.');
                    this.track('failure', {error: 'password'});
                    return false;
                }
                if (password.length < 6) {
                    this.addFieldError('password', 'Please enter a password at least 6 characters long.');
                    this.track('failure', {error: 'password'});
                    return false;
                }
                return true;
            },
            validate: function (data) {
                var valid = true, general_error = false;
                var emailValidation, passwordValidation, keyValidation;

                this.clearAllErrors();

                if (this.register) {
                    emailValidation = this.validateEmail(data.email);
                    passwordValidation = this.validatePasswordPresence(data.password) && this.validatePasswordLength(data.password);

                                        /** Validate that the user type is checked */
                    userTypeValidation = this.validateUserType(data.iama);

                    /** Validate that the grades are checked */
                    gradesValidation = this.validateGradesSelected(data.grades);
                    valid = emailValidation && passwordValidation && userTypeValidation && gradesValidation;
                    
                }
                else if (this.signin) {
                    emailValidation = this.validateEmail(data.email) ;
                    passwordValidation = this.validatePasswordPresence(data.password);
                    valid = emailValidation && passwordValidation
                }
                else if (this.forgot) {
                    valid = this.validateEmail(data.email);
                }
                else if (this.reset) {
                    valid = this.validatePasswordPresence(data.password) && this.validatePasswordLength(data.password);
                }
                if (!valid && !general_error) {
                    this.addFieldError('general', 'Please fix the errors above in red and try again.');
                }
                return valid;
            },
            addFieldError: function (field, error) {
	            var form = null;
	            if(this.register) form = $(".create-account",this.el);
	            else if(this.signin) form = $(".sign-in",this.el);
	            else if(this.forgot) form = $(".forgot-password",this.el);
	            else if(this.reset) form = $(".reset-password",this.el);

                var field = $('.input-row.' + field, form);
                field.addClass('has-error');
                $('.errormsg', field).html(error);
            },
            clearFieldError: function (field) {
                $('.input-row.' + field, this.el).removeClass('has-error');
            },
            clearAllErrors: function () {
                $('.input-row.has-error', this.el).removeClass('has-error');
                $('.input-row div.errormsg', this.el).text('');
            },
            submitData: function () {
                this.track('submit');
                this.showOverlay();
                var data = this.getFormData();
                if (!this.validate(data)) {
                    this.hideOverlay();
                    return;
                }

                var self = this;

                var callback = function (response) {
                    // Success
                    if (response.status == 1) {
                        self.track('success', null, true);

                        // registering new users
                        if (self.register && !self.ca_signin) {
                            $(document).trigger('member_registration');
	                        (window.optimizely || []).push('trackEvent', 'registration-modal-signup');
                        }

                        window.setTimeout(function () {
                            window.location.href = self.redirectPathOnSuccess;
                        }, 500);
                    }
                    // Failure
                    else {
                        var general_error = false;

                        // Registration error
                        if (self.register) {
                            if (response.exists) {
                                self.addFieldError('password', 'That email is already registered with Education.com, but the password is different.');
                                self.track('failure', {error: 'email_registered'});
                                $('.forgot', self.el).show();
                            }
                            else if (response.errors.emailaddress) {
                                self.track('failure', {error: 'email_invalid'});
                                self.addFieldError('email-address', 'Invalid Email Address');
                            }
                            else if (response.errors.password) {
                                self.track('failure', {error: 'password_invalid'});
                                self.addFieldError('password', response.errors.password);
                            }
                            else if (response.errors.membername) {
	                            self.track('failure', {error: 'membername_invalid'});
	                            self.addFieldError('email-address', response.errors.membername);
                            }
                            // Error unrelated to a specific field
                            else {
                                self.track('failure', {error: 'unknown'});
                                self.addFieldError('general', 'There was an error creating your account.');
                                general_error = true;
                            }
                        }
                        // Sign in error
                        else if (self.signin) {
                            if (response.exists === false) {
                                self.track('failure', {error: 'email_unregistered'});
                                self.addFieldError('email-address', 'No account found with that email address. Do you want to <a href="#" class="create-account-link">Create an Account</a>?');
                            }
                            else if (response.exists === true) {
