<script type="text/javascript">


var saveCall = 0;

var blocks = {
	loadedSettings: [],
	init: function(portal) {
		this.portal = portal;
		this.timeout = 0;
		this.submore = new Array();
	},
	saveLayout: function() {
		var s1 = Sortable.serialize('left1_blocks', { tag:'div' });
		var s2 = Sortable.serialize('left2_blocks', { tag:'div' });
		var s3 = Sortable.serialize('right_blocks', { tag:'div' });

		var link = "/index.php?c_mod=firstpage&op=settings&func=reorder_blocks&portal="+this.portal;

		// increase the timeout
		this.timeout += 5000;

		new Ajax.Request(link, {
					method: 'post',
					parameters: s1+'&'+s2+'&'+s3,
					onSuccess: (function() { this.timeout = 0; }).bind(this),
					onFailure: (function(response) {
							// try again in five seconds
							saveCall = setTimeout("blocks.saveLayout();", this.timeout);
						}).bind(this)
					} );

	},
	toggleSettings: function(block_id) {
		if (!this.loadedSettings.include(block_id)) {
			var link = "/index.php?c_mod=firstpage&op=settings&func=load&portal="+this.portal+"&id="+block_id;

			new Ajax.Request(link, {
						method: 'get',
						onSuccess: (function(response) {
							var block = $('set_'+block_id);
							block.update(response.responseText);
							Effect.toggle(block,'BLIND');
							this.loadedSettings.push(block_id);
						}).bind(this)
					} );
		} else {
			var block = $('set_'+block_id);
			Effect.toggle(block,'blind');
		}
	},
	updateSettings: function(block_id) {
		var link = "/index.php?c_mod=firstpage&op=settings&func=update&portal="+this.portal+"&id="+block_id;
		var form = $('sf_'+block_id);
		if (form) {
			new Ajax.Request(link, {
						method: 'post',
						parameters: form.serialize(true),
						onSuccess: (function(response) {
							this.loadedSettings = this.loadedSettings.without(block_id);
							this.refresh(block_id);
						}).bind(this)
					} );
		}
	},
	updateBlocks: function() {
		var params = '';
		var ids = $$("#lightbox #add_blocks .ablosel");
		ids.each(
			function (e) {
				params += '&ids[]='+e.id;
			} );
		var link = "/index.php?c_mod=firstpage&op=add&portal="+this.portal;
		new Ajax.Request(link, {
					method: 'post',
					parameters: params,
					onSuccess: (function(response) {
						
						window.location.reload();
											}).bind(this)
				} );
	},
	sortSettings: function(block_id) {
		var sort = $('sc_'+block_id);
		if (sort) {
