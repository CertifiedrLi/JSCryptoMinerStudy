function fbShare(url, winWidth, winHeight) {
var winTop = (screen.height / 2) - (winHeight / 2);
var winLeft = (screen.width / 2) - (winWidth / 2);
window.open('http://www.facebook.com/sharer.php?u=' + encodeURIComponent(url), 'SharePopup', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);
}
function twShare(url, winWidth, winHeight) {
url = typeof (url) != "undefined" ? url : window.location.href;
winWidth = typeof (winWidth) == "undefined" ? 500 : winWidth;
winHeight = typeof (winHeight) == "undefined" ? 300 : winHeight;
var winTop = (screen.height / 2) - (winHeight / 2);
var winLeft = (screen.width / 2) - (winWidth / 2);
var text = $('title').text();
text = encodeURIComponent(text);
var openUrl = "http://twitter.com/share?text=" + text + "&url=" + url + "&hashtags=postacomtr&via=postacomtr";
window.open(openUrl, 'twitter', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);
}
function gogleShare(url, winWidth, winHeight) {
var winTop = (screen.height / 2) - (winHeight / 2);
var winLeft = (screen.width / 2) - (winWidth / 2);
window.open('https://plus.google.com/share?url=' + url, 'SharePopup', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);
