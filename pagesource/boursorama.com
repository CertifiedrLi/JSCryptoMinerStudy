        var storageKey = 'lastTimeNewPortalBannerClosed'
        var closedAt = localStorage.getItem(storageKey)
        var shouldOpenBanner = false
        var currentHour = (new Date()).getHours();

        // check if we should close the banned automatically
        // we open banner if user never closed it
        // and if it has been closed for more than 3 days
        // and if we are out of announcement period (18h-8h)
        if (currentHour > 17 || currentHour < 8) {
            if (closedAt === null) {
                shouldOpenBanner = true;
            } else {
                closedAt = new Date(parseInt(closedAt))
                var today = new Date()
                var numberOfDay = Math.round((closedAt - today) / (1000 * 60 * 60 * 24));
                if (numberOfDay > 3) {
                    shouldOpenBanner = true;
                }
            }
        }

        var onBannerClick = function () {
          openBanner();
          // one time event
          $('newPortalBanner').removeEvent('click', onBannerClick);
        }

        if (shouldOpenBanner) {
          openBanner();
        } else {
          $('newPortalBanner').addEvent('click', onBannerClick);
        }

        // close trigger
        // also save the last time user closed banner
